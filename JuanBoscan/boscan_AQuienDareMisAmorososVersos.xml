  <text>
    <body>
      <head>
        <title>¿A quién daré mis amorosos versos,</title>
      </head>
      <lg cert="low" type="Romance heroico">
        <l met="-|2|-|4|-|-|-|8|-|10|-|" n="1">
          <!--¿A quién daré mis amorosos versos,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="quién" type="PT0CS00">quién|</w>
          <w lemma="dar" type="VMIF1S0">da|ré|</w>
          <w lemma="mi" type="DP1CPS">mis|</w>
          <w lemma="amoroso" type="AQ0MP00">a|mo|ro|sos|</w>
          <w lemma="verso" type="NCMP000">ver|sos|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|9|10|-|" n="2">
          <!--que pretienden amor, con virtud junto,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="pretender" type="VMIP3P0">pre|tien|den|</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="virtud" type="NCFS000">vir|tud|</w>
          <w lemma="junto" type="AQ0MS00">jun|to|</w>
        </l>
        <l met="-|-|3|-|-|6|-|8|-|10|-|" n="3">
          <!--y desean también mostrarse hermosos?-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="desear" type="VMIP3P0">de|se|an|</w>
          <w lemma="también" type="RG">tam|bién|</w>
          <w lemma="mostrar" type="VMN0000">mos|trar|</w>
          <w lemma="se" type="PP3CN00">se|</w>
          <w lemma="hermoso" type="AQ0MP00">her|mo|sos|</w>
        </l>
        <l met="-|2|-|4|-|-|7|8|-|10|-|" n="4">
          <!--A ti, señora en quien todo esto cabe,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="ti" type="PP2CSO0">ti|</w>
          <w lemma="señor" type="NCFS000">se|ño|ra</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="quien" type="PR0CS00">quien|</w>
          <w lemma="todo" type="DI0MS0">to|do</w>
          <w lemma="este" type="PD00S00">es|to|</w>
          <w lemma="caber" type="VMIP3S0">ca|be|</w>
        </l>
        <l met="-|2|-|4|-|-|-|-|-|10|-|" n="5">
          <!--a ti se den, por cuanto si carecen-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="ti" type="PP2CSO0">ti|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="dar" type="VMSP3P0">den|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="cuanto" type="RG">cuan|to|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="carecer" type="VMIP3P0">ca|re|cen|</w>
        </l>
        <l met="1|-|3|-|-|6|-|-|-|10|-|" n="6">
          <!--destas cosas que digo que pretienden,-->
	  <w lemma="de" type="SP">de</w>
          <w lemma="este" type="DD0FP0">es|tas|</w>
          <w lemma="cosa" type="NCFP000">co|sas|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="decir" type="VMIP1S0">di|go|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="pretender" type="VMIP3P0">pre|tien|den|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="7">
          <!--en ti las hallarán cumplidamente.-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="ti" type="PP2CSO0">ti|</w>
          <w lemma="él" type="PP3FPA0">las|</w>
          <w lemma="hallar" type="VMIF3P0">ha|lla|rán|</w>
          <w lemma="cumplidamente" type="RG">cum|pli|da|men|te|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="8">
          <!--Recógelos con blanda mansedumbre-->
          <w lemma="recoger" type="VMM02S0">re|co|ge|</w>
          <w lemma="él" type="PP3MPA0">los|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="blando" type="AQ0FS00">blan|da|</w>
          <w lemma="mansedumbre" type="NCFS000">man|se|dum|bre|</w>
        </l>
        <l met="-|2|-|-|5|6|-|-|-|10|-|" n="9">
          <!--si vieres que son blandos, y si no,-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="ver" type="VMSF2S0">vie|res|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="blando" type="AQ0MP00">blan|dos|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="no" type="RN">no|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="10">
          <!--recógelos como ellos merecieren.-->
          <w lemma="recoger" type="VMM02S0">re|co|ge|</w>
          <w lemma="él" type="PP3MPA0">los|</w>
          <w lemma="como" type="CS">co|mo</w>
          <w lemma="ellos" type="PP3MP00">e|llos|</w>
          <w lemma="merecer" type="VMSF3P0">me|re|cie|ren|</w>
        </l>
        <l met="-|-|-|4|-|-|-|8|-|10|-|" n="11">
          <!--Y si después te importunaren mucho-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="después" type="RG">des|pués|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="importunar" type="VMSF3P0">im|por|tu|na|ren|</w>
          <w lemma="mucho" type="RG">mu|cho|</w>
        </l>
        <l met="-|-|3|-|-|6|7|-|-|10|-|" n="12">
          <!--con llorar, porque así suelen hazello,-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="llorar" type="VMN0000">llo|rar|</w>
          <w lemma="porque" type="CS">por|que</w>
          <w lemma="así" type="RG">a|sí|</w>
          <w lemma="soler" type="VMIP3P0">sue|len|</w>
          <w lemma="hacer" type="VMN0000">ha|ze|</w>
	  <w lemma="él" type="PP3MPA0">llos|</w>
        </l>
        <l met="1|-|-|4|-|6|-|8|-|10|-|" n="13">
          <!--no te parezcan mal sus tristes lloros,-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="parecer" type="VMSP3P0">pa|rez|can|</w>
          <w lemma="mal" type="RG">mal|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="triste" type="AQ0CP00">tris|tes|</w>
          <w lemma="lloro" type="NCMP000">llo|ros|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="14">
          <!--que, pues que son sus lágrimas con causa,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="lágrima" type="NCFP000">lá|gri|mas|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="causa" type="NCFS000">cau|sa|</w>
        </l>
        <l met="-|2|3|4|-|6|-|-|-|10|-|" n="15">
          <!--no sólo es gran razón que se consientan,-->
          <w lemma="no" type="RN">no|</w>
	  <w lemma="sólo" type="RG">só|lo</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="gran" type="AQ0CS00">gran|</w>
          <w lemma="razón" type="NCFS000">ra|zón|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="consentir" type="VMSP3P0">con|sien|tan|</w>
        </l>
        <l met="-|2|-|4|-|6|-|-|-|10|-|" n="16">
          <!--mas an de ser dolidas y lloradas-->
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="haber" type="VAIP3P0">an|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="doler" type="VMP00PF">do|li|das|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="llorar" type="VMP00PF">llo|ra|das|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="17">
          <!--por todos los que vieren donde caen.-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="todo" type="DI0MP0">to|dos|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="ver" type="VMSF3P0">vie|ren|</w>
          <w lemma="donde" type="PR00000">don|de|</w>
          <w lemma="caer" type="VMIP3P0">ca|en|</w>
        </l>
        <l met="1|-|-|4|-|6|-|-|-|10|-|" n="18">
          <!--Ellos se van huyendo de mis manos-->
          <w lemma="él" type="PP3MP00">e|llos|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="ir" type="VMIP3P0">van|</w>
          <w lemma="huir" type="VMG0000">hu|yen|do|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="mi" type="DP1CPS">mis|</w>
          <w lemma="mano" type="NCFP000">ma|nos|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="19">
          <!--pensando que podrán bivir doquiera,-->
          <w lemma="pensar" type="VMG0000">pen|san|do|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="poder" type="VMIF3P0">po|drán|</w>
          <w lemma="vivir" type="VMN0000">bi|vir|</w>
          <w lemma="doquier" type="RG">do|quie|ra|</w>
        </l>
        <l met="-|-|-|4|5|6|-|-|-|10|-|" n="20">
          <!--pero, según an sido regalados-->
          <w lemma="pero" type="CC">pe|ro|</w>
          <w lemma="según" type="SP">se|gún|</w>
          <w lemma="haber" type="VAIP3P0">an|</w>
          <w lemma="ser" type="VSP00SM">si|do|</w>
          <w lemma="regalar" type="VMP00PM">re|ga|la|dos|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="21">
          <!--y poco corregidos en sus vicios,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="poco" type="RG">po|co|</w>
          <w lemma="corregir" type="VMP00PM">co|rre|gi|dos|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="vicio" type="NCMP000">vi|cios|</w>
        </l>
        <l met="-|-|3|-|-|6|-|8|9|10|-|" n="22">
          <!--a peligro andarán si en ti no hallan-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="peligro" type="NCMS000">pe|li|gro</w>
          <w lemma="andar" type="VMIF3P0">an|da|rán|</w>
          <w lemma="si" type="CS">si</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="ti" type="PP2CSO0">ti|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="hallar" type="VMIP3P0">ha|llan|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="23">
          <!--manera de bivir en sus regalos-->
          <w lemma="manera" type="NCFS000">ma|ne|ra|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="vivir" type="VMN0000">bi|vir|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="regalo" type="NCMP000">re|ga|los|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="24">
          <!--y amparo por valerse en sus errores.-->
          <w lemma="y" type="CC">y</w>
          <w lemma="amparo" type="NCMS000">am|pa|ro|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="valer" type="VMN0000">va|ler|</w>
          <w lemma="se" type="PP3CN00">se</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="error" type="NCMP000">e|rro|res|</w>
        </l>
        <l met="-|-|3|-|-|6|-|8|-|10|-|" n="25">
          <!--Si pasaren con onra, dales vida,-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="pasar" type="VMSF3P0">pa|sa|ren|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="onra" type="NCFS000">o|nra|</w>
          <w lemma="dar" type="VMM02S0">da|</w>
          <w lemma="él" type="PP3CPD0">les|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
        </l>
        <l met="-|-|3|4|-|6|-|-|-|10|-|" n="26">
          <!--y si no, no les quites el remedio-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="él" type="PP3CPD0">les|</w>
          <w lemma="quitar" type="VMSP2S0">qui|tes|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="remedio" type="NCMS000">re|me|dio|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="27">
          <!--que el tiempo les dará con su justicia:-->
          <w lemma="que" type="CS">que</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="tiempo" type="NCMS000">tiem|po|</w>
          <w lemma="él" type="PP3CPD0">les|</w>
          <w lemma="dar" type="VMIF3S0">da|rá|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="justicia" type="NCFS000">jus|ti|cia|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|-|10|-|" n="28">
          <!--que mueran y que los cubra la tierra,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="morir" type="VMSP3P0">mue|ran|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="él" type="PP3MPA0">los|</w>
          <w lemma="cubrir" type="VMSP3S0">cu|bra|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="tierra" type="NCFS000">tie|rra|</w>
        </l>
        <l met="-|-|3|-|-|6|-|8|-|10|-|" n="29">
          <!--y la tierra será el eterno olvido.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="tierra" type="NCFS000">tie|rra|</w>
          <w lemma="ser" type="VSIF3S0">se|rá</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="eterno" type="AQ0MS00">e|ter|no</w>
          <w lemma="olvido" type="NCMS000">ol|vi|do|</w>
        </l>
      </lg>
    </body>
