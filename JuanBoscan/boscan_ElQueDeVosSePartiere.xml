  <text>
    <body>
      <head>
        <title>El que de vos se partiere</title>
      </head>
      <lg cert="high" type="Canción">
        <l met="-|-|-|4|-|-|7|-|" n="1">
          <!--El que de vos se partiere-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="vos" type="PP2CS0P">vos|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="partir" type="VMSF1S0">par|tie|re|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="2">
          <!--merece nunca bolver.-->
          <w lemma="merecer" type="VMIP3S0">me|re|ce|</w>
          <w lemma="nunca" type="RG">nun|ca|</w>
          <w lemma="volver" type="VMN0000">bol|ver|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="3">
          <!--O, señora si bolviere,-->
          <w lemma="oh" type="I">o|</w>
          <w lemma="señor" type="NCFS000">se|ño|ra|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="volver" type="VMIP3S0">bol|vie|re|</w>
        </l>
        <l met="-|2|-|-|-|6|7|-|" n="4">
          <!--que buelva para no os ver.-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="volver" type="VMSP3S0">buel|va|</w>
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="5">
          <!--No meresco la venida,-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="merecer" type="VMIP1S0">me|res|co|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="venida" type="NCFS000">ve|ni|da|</w>
        </l>
        <l met="-|2|-|-|-|6|7|-|" n="6">
          <!--pues fui para poder irme,-->
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="ir" type="VMIS1S0">fui|</w>
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="poder" type="VMN0000">po|der|</w>
          <w lemma="ir" type="VMN0000">ir|</w>
          <w lemma="me" type="PP1CS00">me|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="7">
          <!--aunque harto va medida-->
          <w lemma="aunque" type="CC">aun|que|</w>
          <w lemma="harto" type="RG">har|to|</w>
          <w lemma="ir" type="VMIP3S0">va|</w>
          <w lemma="medir" type="VMP00SF">me|di|da|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="8">
          <!--con la pena del partirme-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="pena" type="NCFS000">pe|na|</w>
          <w lemma="de" type="SP">de</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="partir" type="VMN0000">par|tir|</w>
          <w lemma="me" type="PP1CS00">me|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="9">
          <!--la culpa de la partida.-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="culpa" type="NCFS000">cul|pa|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="partida" type="NCFS000">par|ti|da|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="10">
          <!--Mas si yo jamás me fuere,-->
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="jamás" type="RG">ja|más|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="ser" type="VSSF1S0">fue|re|</w>
        </l>
        <l met="1|2|-|4|5|-|7|-|" n="11">
          <!--bien sé que no abrá de ser,-->
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="saber" type="VMIP1S0">sé|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="haber" type="VMIF3S0">a|brá|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="12">
          <!--pero quiero, si ello fuere,-->
          <w lemma="pero" type="CC">pe|ro|</w>
          <w lemma="querer" type="VMIP1S0">quie|ro|</w>
          <w lemma="si" type="CS">si</w>
          <w lemma="él" type="PD00S00">e|llo|</w>
          <w lemma="ser" type="VSSF1S0">fue|re|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="13">
          <!--pagallo con nunca os ver.-->
          <w lemma="pagar" type="AQ0CN00">pa|ga|</w>
	  <w lemma="él" type="PP3MSA0">llo</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="nunca" type="RG">nun|ca</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
        </l>
      </lg>
    </body>
  </text>
