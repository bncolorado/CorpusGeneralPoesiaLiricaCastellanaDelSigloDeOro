  <text>
    <body>
      <head>
        <title>Al tronco descansaba de una encina,</title>
      </head>
      <lg cert="high" type="Soneto">
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="1">
          <!--Al tronco descansaba de una encina,-->
          <w lemma="a" type="SP">a</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="tronco" type="NCMS000">tron|co|</w>
          <w lemma="descansar" type="VMII3S0">des|can|sa|ba|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="uno" type="DI0FS0">u|na</w>
          <w lemma="encina" type="NCFS000">en|ci|na|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="2">
          <!--que invidia de los bosques fue lozana,-->
          <w lemma="que" type="CS">que</w>
          <w lemma="invidia" type="VMIP3S0">in|vi|dia|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="bosque" type="NCMP000">bos|ques|</w>
          <w lemma="ser" type="VSIS3S0">fue|</w>
          <w lemma="lozano" type="AQ0FS00">lo|za|na|</w>
        </l>
        <l met="-|-|-|4|-|6|7|-|-|10|-|" n="3">
          <!--cuando segur legal una mañana-->
          <w lemma="cuando" type="CS">cuan|do|</w>
          <w lemma="segur" type="NCFS000">se|gur|</w>
          <w lemma="legal" type="AQ0CS00">le|gal|</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="mañana" type="NCCS000">ma|ña|na|</w>
        </l>
        <l met="1|-|3|-|-|6|-|-|-|10|-|" n="4">
          <!--alto horror me dejó con su rüina.-->
          <w lemma="alto" type="AQ0MS00">al|to</w>
          <w lemma="horror" type="NCMS000">ho|rror|</w>
          <w lemma="me" type="P01CS00">me|</w>
          <w lemma="dejar" type="VMIS3S0">de|jó|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="rüina" type="NCFS000">rü|i|na|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="5">
          <!--Laurel que de sus ramas hizo dina-->
          <w lemma="laurel" type="NP00000">lau|rel|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="rama" type="NCFP000">ra|mas|</w>
          <w lemma="hacer" type="VMIS3S0">hi|zo|</w>
          <w lemma="dina" type="NCFS000">di|na|</w>
        </l>
        <l met="-|2|-|4|-|6|-|-|-|10|-|" n="6">
          <!--mi lira, ruda sí, mas castellana,-->
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="lira" type="NCFS000">li|ra|</w>
          <w lemma="rudo" type="AQ0FS00">ru|da|</w>
          <w lemma="sí" type="RG">sí|</w>
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="castellano" type="AQ0FS00">cas|te|lla|na|</w>
        </l>
        <l met="1|-|3|-|-|6|-|8|-|10|-|" n="7">
          <!--hierro luego fatal su pompa vana-->
          <w lemma="hierro" type="NCMS000">hie|rro|</w>
          <w lemma="luego" type="RG">lue|go|</w>
          <w lemma="fatal" type="AQ0CS00">fa|tal|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="pompa" type="NCFS000">pom|pa|</w>
          <w lemma="vano" type="AQ0FS00">va|na|</w>
        </l>
        <l met="1|-|3|-|-|6|-|-|-|10|-|" n="8">
          <!--(culpa tuya, Calíope) fulmina.-->
          <w lemma="culpa" type="NCFS000">cul|pa|</w>
          <w lemma="tuyo" type="AP0FS2S">tu|ya|</w>
          <w lemma="calíope" type="NP00000">ca|lí|o|pe|</w>
          <w lemma="fulminar" type="VMIP3S0">ful|mi|na|</w>
        </l>
        <l met="-|2|-|4|-|6|-|-|-|10|-|" n="9">
          <!--En verdes hojas cano el de Minerva-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="verde" type="AQ0CP00">ver|des|</w>
          <w lemma="hoja" type="NCFP000">ho|jas|</w>
          <w lemma="cano" type="AQ0MS00">ca|no</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="minerva" type="NP00000">mi|ner|va|</w>
        </l>
        <l met="1|-|3|-|-|6|7|-|-|10|-|" n="10">
          <!--árbol culto, del Sol yace abrasado,-->
          <w lemma="árbol" type="NCMS000">ár|bol|</w>
          <w lemma="culto" type="AQ0MS00">cul|to|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="sol" type="NP00000">sol|</w>
          <w lemma="yacer" type="VMIP3S0">ya|ce</w>
          <w lemma="abrasar" type="VMP00SM">a|bra|sa|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="11">
          <!--aljófar sus cenizas de la yerba.-->
          <w lemma="aljófar" type="NCMS000">al|jó|far|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="ceniza" type="NCFP000">ce|ni|zas|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="yerba" type="NCFS000">yer|ba|</w>
        </l>
        <l met="1|-|-|4|-|6|7|-|-|10|-|" n="12">
          <!--¡Cuánta esperanza miente a un desdichado!-->
          <w lemma="cuánto" type="DE0FS0">cuán|ta</w>
          <w lemma="esperanza" type="NCFS000">es|pe|ran|za|</w>
          <w lemma="mentir" type="VMIP3S0">mien|te</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="desdichado" type="NCMS000">des|di|cha|do|</w>
        </l>
        <l met="-|2|3|-|-|6|-|-|-|10|-|" n="13">
          <!--¿A qué más desengaños me reserva?-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="qué" type="PT00000">qué|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="desengaño" type="NCMP000">de|sen|ga|ños|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="reserva" type="NCCS000">re|ser|va|</w>
        </l>
        <l met="-|2|-|4|-|-|-|8|-|10|-|" n="14">
          <!--¿A qué escarmientos me vincula el hado?-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="qué" type="DT0CN0">qué</w>
          <w lemma="escarmiento" type="NCMP000">es|car|mien|tos|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="vincular" type="VMIP3S0">vin|cu|la</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="hado" type="NCMS000">ha|do|</w>
        </l>
      </lg>
    </body>
  </text>
