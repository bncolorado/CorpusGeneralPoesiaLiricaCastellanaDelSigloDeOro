  <text>
    <body>
      <head>
        <title>Oí, Señor, tu sonido</title>
      </head>
      <lg cert="low" type="Coplas">
        <l met="-|2|-|4|-|-|7|-|" n="1">
          <!--Oí, Señor, tu sonido-->
          <w lemma="oír" type="VMIS1S0">o|í|</w>
          <w lemma="señor" type="NP00000">se|ñor|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="sonido" type="NCMS000">so|ni|do|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="2">
          <!--y temeroso quedé;-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="temeroso" type="AQ0MS00">te|me|ro|so|</w>
          <w lemma="quedar" type="VMIS1S0">que|dé|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="3">
          <!--tus obras consideré-->
          <w lemma="tu" type="DP2CPS">tus|</w>
          <w lemma="obra" type="NCFP000">o|bras|</w>
          <w lemma="considerar" type="VMIS1S0">con|si|de|ré|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="4">
          <!--y quedé despavorido.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="quedar" type="VMIS1S0">que|dé|</w>
          <w lemma="despavorir" type="VMP00SM">des|pa|vo|ri|do|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="5">
          <!--Porque oyendo la grandeza-->
          <w lemma="porque" type="CS">por|que</w>
          <w lemma="oír" type="VMG0000">o|yen|do|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="grandeza" type="NCFS000">gran|de|za|</w>
        </l>
        <l met="-|-|-|-|-|-|7|-|" n="6">
          <!--de la tu divinidad,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="divinidad" type="NCFS000">di|vi|ni|dad|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="7">
          <!--espantóme la humildad-->
          <w lemma="espantar" type="VMIP3S0">es|pan|tó|</w>
	  <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="humildad" type="NCFS000">hu|mil|dad|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="8">
          <!--que escogiste, y la bajeza.-->
          <w lemma="que" type="CS">que</w>
          <w lemma="escoger" type="VMIS2S0">es|co|gis|te</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="bajeza" type="NCFS000">ba|je|za|</w>
        </l>
      </lg>
    </body>
  </text>
