  <text>
    <body>
      <head>
        <title> Mal engañado me has</title>
      </head>
      <lg cert="high" type="Romance">
        <l met="1|-|-|4|-|-|7|-|" n="1">
          <!--Mal engañado me has-->
          <w lemma="mal" type="RG">mal|</w>
          <w lemma="engañar" type="VMP00SM">en|ga|ña|do|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="haber" type="VAIP2S0">has|</w>
        </l>
        <l met="1|-|3|4|-|-|7|-|" n="2">
          <!--mundo, ya siento tus daños,-->
          <w lemma="mundo" type="NCMS000">mun|do|</w>
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="sentar" type="VMIP1S0">sien|to|</w>
          <w lemma="tu" type="DP2CPS">tus|</w>
          <w lemma="daño" type="NCMP000">da|ños|</w>
        </l>
        <l met="1|-|-|4|-|6|7|-|" n="3">
          <!--hasme llevado treinta años,-->
          <w lemma="haber" type="VMIP3S0">has|</w>
	  <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="llevar" type="VMP00SM">lle|va|do|</w>
          <w lemma="30" type="Z">trein|ta</w>
          <w lemma="año" type="NCMP000">a|ños|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="4">
          <!--de lo que me pesa más.-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="pesar" type="VMIP3S0">pe|sa|</w>
          <w lemma="más" type="RG">más|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="5">
          <!--Jugaste con mi moneda-->
          <w lemma="jugar" type="VMIS2S0">ju|gas|te|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="moneda" type="NCFS000">mo|ne|da|</w>
        </l>
        <l met="-|-|3|4|5|6|7|-|" n="6">
          <!--sin poner tú solo un tanto;-->
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="poner" type="VMN0000">po|ner|</w>
          <w lemma="tú" type="PP2CSN0">tú|</w>
          <w lemma="solo" type="AQ0MS00">so|lo</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="tanto" type="NCMS000">tan|to|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="7">
          <!--con pérdida me levanto,-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="pérdida" type="NCFS000">pér|di|da|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="levantar" type="VMIP1S0">le|van|to|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="8">
          <!--por no perder lo que queda.-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="perder" type="VMN0000">per|der|</w>
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="quedar" type="VMIP3S0">que|da|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="9">
          <!--Mas con todo mi dolor,-->
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="todo" type="DI0MS0">to|do|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="dolor" type="NCMS000">do|lor|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="10">
          <!--alegre quedo al partir,-->
          <w lemma="alegre" type="AQ0CS00">a|le|gre|</w>
          <w lemma="quedar" type="VMIP1S0">que|do</w>
          <w lemma="a" type="SP">a</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="partir" type="VMN0000">par|tir|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="11">
          <!--con que te podré decir:-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="poder" type="VMIF1S0">po|dré|</w>
          <w lemma="decir" type="VMN0000">de|cir|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="12">
          <!--allá quedarás, traidor.-->
          <w lemma="allá" type="RG">a|llá|</w>
          <w lemma="quedar" type="VMIF2S0">que|da|rás|</w>
          <w lemma="traidor" type="AQ0MS00">trai|dor|</w>
        </l>
        <l met="1|2|-|-|5|-|7|-|" n="13">
          <!--No tengo de qué alabarme;-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="tener" type="VMIP1S0">ten|go|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="qué" type="PT00000">qué</w>
          <w lemma="alabar" type="VMN0000">a|la|bar|</w>
          <w lemma="me" type="PP1CS00">me|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="14">
          <!--mas tú quedarás corrido-->
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="tú" type="PP2CSN0">tú|</w>
          <w lemma="quedar" type="VMIF2S0">que|da|rás|</w>
          <w lemma="correr" type="VMP00SM">co|rri|do|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="15">
          <!--de verte que me has perdido-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="me" type="PP1CS00">me</w>
          <w lemma="haber" type="VAIP2S0">has|</w>
          <w lemma="perder" type="VMP00SM">per|di|do|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="16">
          <!--donde pensabas gozarme.-->
          <w lemma="donde" type="PR00000">don|de|</w>
          <w lemma="pensar" type="VMII2S0">pen|sa|bas|</w>
          <w lemma="gozar" type="VMN0000">go|zar|</w>
          <w lemma="me" type="PP1CS00">me|</w>
        </l>
        <l met="1|2|-|4|-|-|7|-|" n="17">
          <!--Muy gran peligro y afrenta-->
          <w lemma="muy" type="RG">muy|</w>
          <w lemma="gran" type="AQ0CS00">gran|</w>
          <w lemma="peligro" type="NCMS000">pe|li|gro|</w>
          <w lemma="y" type="CC">y</w>
          <w lemma="afrenta" type="NCFS000">a|fren|ta|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="18">
          <!--es morir la libertad,-->
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="morir" type="VMN0000">mo|rir|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="libertad" type="NCFS000">li|ber|tad|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="19">
          <!--quedando la voluntad-->
          <w lemma="quedar" type="VMG0000">que|dan|do|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="voluntad" type="NCFS000">vo|lun|tad|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="20">
          <!--viva, rebelde y exenta.-->
          <w lemma="vivo" type="AQ0FS00">vi|va|</w>
          <w lemma="rebelde" type="AQ0CS00">re|bel|de|</w>
          <w lemma="y" type="CC">y</w>
          <w lemma="exento" type="AQ0FS00">e|xen|ta|</w>
        </l>
        <l met="1|2|-|-|-|-|7|-|" n="21">
          <!--Vos, Virgen, de cuya cuenta-->
          <w lemma="vos" type="PP2CS0P">vos|</w>
          <w lemma="virgen" type="NP00000">vir|gen|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="cuyo" type="PR0FS00">cu|ya|</w>
          <w lemma="contar" type="VMIP3S0">cuen|ta|</w>
        </l>
        <l met="1|-|3|4|-|-|7|-|" n="22">
          <!--es razón que, esto se escriba,-->
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="razón" type="NCFS000">ra|zón|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="este" type="PD00S00">es|to|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="escribir" type="VMSP3S0">es|cri|ba|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="23">
          <!--haced que muera la viva,-->
          <w lemma="hacer" type="VMM02P0">ha|ced|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="morir" type="VMSP3S0">mue|ra|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="vivo" type="AQ0FS00">vi|va|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="24">
          <!--porque la muerta consienta.-->
          <w lemma="porque" type="CS">por|que|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="morir" type="VMP00SF">muer|ta|</w>
          <w lemma="consentir" type="VMSP3S0">con|sien|ta|</w>
        </l>
      </lg>
    </body>
  </text>
