    <body>
      <head>
        <title>Del mar, y no de Huelva,</title>
      </head>
      <lg cert="high" type="Canción">
        <l met="-|2|-|4|-|6|-|" n="1">
          <!--Del mar, y no de Huelva,-->
          <w lemma="de" type="SP">de</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="mar" type="NCCS000">mar|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="huelva" type="NP00000">huel|va|</w>
        </l>
        <l met="-|-|3|-|-|6|-|8|-|10|-|" n="2">
          <!--los escollos el sol, los muros raya;-->
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="escollo" type="NCMP000">es|co|llos|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="sol" type="NCMS000">sol|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="muro" type="NCMP000">mu|ros|</w>
          <w lemma="raer" type="VMSP3S0">ra|ya|</w>
        </l>
        <l met="-|2|-|-|-|6|7|-|-|10|-|" n="3">
          <!--gimiendo el alcïón era en la playa-->
          <w lemma="gemir" type="VMG0000">gi|mien|do|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="alcïón" type="NCFS000">alcï|ón|</w>
          <w lemma="ser" type="VSII3S0">e|ra</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FS0">l|a</w>
          <w lemma="playa" type="NCFS000">pla|ya|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="4">
          <!--ruiseñor en la selva,-->
          <w lemma="ruiseñor" type="NCMS000">rui|se|ñor|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="selva" type="NCFS000">sel|va|</w>
        </l>
        <l met="-|-|-|-|5|6|-|" n="5">
          <!--cuando pescador pobre-->
          <w lemma="cuando" type="CS">cuan|do|</w>
          <w lemma="pescador" type="NCMS000">pes|ca|dor|</w>
          <w lemma="pobre" type="AQ0CS00">po|bre|</w>
        </l>
        <l met="1|-|-|4|-|6|-|8|-|10|-|" n="6">
          <!--mucha despide red de poco robre.-->
          <w lemma="mucho" type="DI0FS0">mu|cha|</w>
          <w lemma="despedir" type="VMIP3S0">des|pi|de|</w>
          <w lemma="red" type="NCFS000">red|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="poco" type="DI0MS0">po|co|</w>
          <w lemma="robre" type="NCMS000">ro|bre|</w>
        </l>
        <l met="-|-|-|-|5|6|-|" n="7">
          <!--Al que le escuchó en vano-->
          <w lemma="a" type="SP">a</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="le" type="PP3CSD0">le</w>
          <w lemma="escuchar" type="VMIS3S0">es|cu|chó</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="vano" type="NCMS000">va|no|</w>
        </l>
        <l met="1|-|-|-|-|6|-|8|-|10|-|" n="8">
          <!--golfo, a pesar del Norte, siempre inquieto,-->
          <w lemma="golfo" type="AQ0MS00">gol|fo</w>
          <w lemma="a_pesar_de" type="SP">a_|pe|sar_|de</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="norte" type="NP00000">nor|te|</w>
          <w lemma="siempre" type="RG">siem|pre</w>
          <w lemma="inquieto" type="AQ0MS00">in|quie|to|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="9">
          <!--se queja del Amor, a quien sujeto-->
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="quejar" type="VMIP3S0">que|ja|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="amor" type="NP00000">a|mor|</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="quien" type="PR0CS00">quien|</w>
          <w lemma="sujeto" type="NCMS000">su|je|to|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="10">
          <!--obedece tirano,-->
          <w lemma="obedecer" type="VMIP3S0">o|be|de|ce|</w>
          <w lemma="tirano" type="NCMS000">ti|ra|no|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="11">
          <!--en las prisiones bellas-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="prisión" type="NCFP000">pri|sio|nes|</w>
          <w lemma="bello" type="AQ0FP00">be|llas|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="12">
          <!--de la esfera mayor de sus centellas.-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="esfera" type="NCFS000">es|fe|ra|</w>
          <w lemma="mayor" type="AQ0CS00">ma|yor|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="centella" type="NCFP000">cen|te|llas|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="13">
          <!--Escollo cristalino,-->
          <w lemma="escollo" type="NCMS000">es|co|llo|</w>
          <w lemma="cristalino" type="AQ0MS00">cris|ta|li|no|</w>
        </l>
        <l met="-|-|-|-|-|6|-|-|-|10|-|" n="14">
          <!--a quien el pescador cuanto padece,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="quien" type="PR0CS00">quien|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="pescador" type="NCMS000">pes|ca|dor|</w>
          <w lemma="cuanto" type="PR0MS00">cuan|to|</w>
          <w lemma="padecer" type="VMIP3S0">pa|de|ce|</w>
        </l>
        <l met="-|2|-|-|-|6|7|-|-|10|-|" n="15">
          <!--sentado en su crueldad, dulce le ofrece,-->
          <w lemma="sentar" type="VMP00SM">sen|ta|do</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="crueldad" type="NCFS000">cruel|dad|</w>
          <w lemma="dulce" type="NCMS000">dul|ce|</w>
          <w lemma="le" type="PP3CSD0">le</w>
          <w lemma="ofrecer" type="VMIP3S0">o|fre|ce|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="16">
          <!--sin hallar el divino-->
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="hallar" type="VMN0000">ha|llar|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="divino" type="AQ0MS00">di|vi|no|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="17">
          <!--canto alivio a sus quejas.-->
          <w lemma="canto" type="NCMS000">can|to</w>
          <w lemma="alivio" type="NCMS000">a|li|vio</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="queja" type="NCFP000">que|jas|</w>
        </l>
        <l met="1|-|-|4|-|6|-|8|-|10|-|" n="18">
          <!--¡Triste del que a una roca pide orejas!-->
          <w lemma="triste" type="AQ0CS00">tris|te|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="roca" type="NCFS000">ro|ca|</w>
          <w lemma="pedir" type="VMIP3S0">pi|de</w>
          <w lemma="oreja" type="NCFP000">o|re|jas|</w>
        </l>
      </lg>
    </body>
