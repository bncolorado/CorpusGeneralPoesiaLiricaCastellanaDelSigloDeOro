    <body>
      <head>
        <title>Pomos de olor son al prado</title>
      </head>
      <lg cert="high" type="Romance">
        <l met="1|-|-|4|5|-|7|-|" n="1">
          <!--Pomos de olor son al prado-->
          <w lemma="pomo" type="NCMP000">po|mos|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="olor" type="NCMS000">o|lor|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="prado" type="NCMS000">pra|do|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="2">
          <!--en el brasero de sol-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="brasero" type="NCMS000">bra|se|ro|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="sol" type="NCMS000">sol|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="3">
          <!--estos naranjos hermosos,-->
          <w lemma="este" type="DD0MP0">es|tos|</w>
          <w lemma="naranjo" type="NCMP000">na|ran|jos|</w>
          <w lemma="hermoso" type="AQ0MP00">her|mo|sos|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="4">
          <!--que ámbar exhala su flor.-->
          <w lemma="que" type="CS">que</w>
          <w lemma="ámbar" type="NCMS000">ám|bar|</w>
          <w lemma="exhalar" type="VMIP3S0">e|xha|la|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="flor" type="NCFS000">flor|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="5">
          <!--Perpetua esmeralda bella,-->
          <w lemma="perpetuo" type="AQ0FS00">per|pe|tua</w>
          <w lemma="esmeralda" type="NCFS000">es|me|ral|da|</w>
          <w lemma="bello" type="AQ0FS00">be|lla|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="6">
          <!--donde, en numerosa voz,-->
          <w lemma="donde" type="PR00000">don|de</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="numeroso" type="AQ0FS00">nu|me|ro|sa|</w>
          <w lemma="voz" type="NCFS000">voz|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="7">
          <!--mil parlerías nos canta-->
          <w lemma="1000" type="Z">mil|</w>
          <w lemma="parlerías" type="NCFP000">pa|rle|rí|as|</w>
          <w lemma="nos" type="PP1CP00">nos|</w>
          <w lemma="cantar" type="VMIP3S0">can|ta|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="8">
          <!--el bachiller ruiseñor;-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="bachiller" type="NCMS000">ba|chi|ller|</w>
          <w lemma="ruiseñor" type="NCMS000">rui|se|ñor|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="9">
          <!--entre cuyas tiernas hojas-->
          <w lemma="entre" type="SP">en|tre|</w>
          <w lemma="cuyo" type="PR0FP00">cu|yas|</w>
          <w lemma="tierno" type="AQ0FP00">tier|nas|</w>
          <w lemma="hoja" type="NCFP000">ho|jas|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="10">
          <!--las flores que abril formó-->
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="flor" type="NCFP000">flo|res|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="[??:??/4/??:??.??:??]" type="W">a|bril|</w>
          <w lemma="formar" type="VMIS3S0">for|mó|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="11">
          <!--de estrellas breves de nieve-->
          <w lemma="de" type="SP">de</w>
          <w lemma="estrella" type="NCFP000">es|tre|llas|</w>
          <w lemma="breve" type="AQ0CP00">bre|ves|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="nieve" type="NCFS000">nie|ve|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="12">
          <!--racimos fragantes son.-->
          <w lemma="racimo" type="NCMP000">ra|ci|mos|</w>
          <w lemma="fragante" type="AQ0CP00">fra|gan|tes|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="13">
          <!--Metamorfóseos del tiempo-->
          <w lemma="metamorfóseos" type="NP00000">me|ta|mor|fó|seos|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="tiempo" type="NCMS000">tiem|po|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="14">
          <!--que, en dulce transformación,-->
          <w lemma="que" type="CS">que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="dulce" type="AQ0CS00">dul|ce|</w>
          <w lemma="transformación" type="NCFS000">trans|for|ma|ción|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="15">
          <!--hará topacios mañana-->
          <w lemma="hacer" type="VMIF3S0">ha|rá|</w>
          <w lemma="topacio" type="NCMP000">to|pa|cios|</w>
          <w lemma="mañana" type="RG">ma|ña|na|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="16">
          <!--los que son diamantes hoy,-->
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="diamante" type="NCMP000">dia|man|tes|</w>
          <w lemma="hoy" type="RG">hoy|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="17">
          <!--a cuyas libreas verdes-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="cuyo" type="PR0FP00">cu|yas|</w>
          <w lemma="librea" type="NCFP000">li|bre|as|</w>
          <w lemma="verde" type="AQ0CP00">ver|des|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="18">
          <!--dan vistosa guarnición-->
          <w lemma="dar" type="VMIP3P0">dan|</w>
          <w lemma="vistoso" type="AQ0FS00">vis|to|sa|</w>
          <w lemma="guarnición" type="NCFS000">guar|ni|ción|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="19">
          <!--ramilletes de cristal,-->
          <w lemma="ramillete" type="NCMP000">ra|mi|lle|tes|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="cristal" type="NCMS000">cris|tal|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="20">
          <!--fragantísimo candor.-->
          <w lemma="fragante" type="AQSMS00">fra|gan|tí|si|mo|</w>
          <w lemma="candor" type="NCMS000">can|dor|</w>
        </l>
        <l met="1|-|-|-|5|-|7|-|" n="21">
          <!--Rico mineral del valle,-->
          <w lemma="rico" type="AQ0MS00">ri|co|</w>
          <w lemma="mineral" type="NCMS000">mi|ne|ral|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="valle" type="NCMS000">va|lle|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="22">
          <!--adonde franco nos dio-->
          <w lemma="adonde" type="CS">a|don|de|</w>
          <w lemma="franco" type="NCMS000">fran|co|</w>
          <w lemma="nos" type="PP1CP00">nos|</w>
          <w lemma="dar" type="VMIS3S0">dio|</w>
        </l>
        <l met="1|-|-|-|-|-|7|-|" n="23">
          <!--oro el enero encogido;-->
          <w lemma="oro" type="NCMS000">o|ro</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="[??:??/1/??:??.??:??]" type="W">e|ne|ro</w>
          <w lemma="encoger" type="VMP00SM">en|co|gi|do|</w>
        </l>
        <l met="1|-|-|-|-|-|7|-|" n="24">
          <!--plata el mayo ostentador.-->
          <w lemma="plata" type="NCFS000">pla|ta</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="[??:??/5/??:??.??:??]" type="W">ma|yo</w>
          <w lemma="ostentador" type="AQ0MS00">os|ten|ta|dor|</w>
        </l>
      </lg>
    </body>
