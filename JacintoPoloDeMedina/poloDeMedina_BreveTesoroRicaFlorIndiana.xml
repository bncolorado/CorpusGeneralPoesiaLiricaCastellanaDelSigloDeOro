  <text>
    <body>
      <head>
        <title>Breve tesoro, rica flor indiana,</title>
      </head>
      <lg cert="high" type="Silva">
        <l met="1|-|-|4|-|6|-|8|-|10|-|" n="1">
          <!--Breve tesoro, rica flor indiana,-->
          <w lemma="breve" type="AQ0CS00">bre|ve|</w>
          <w lemma="tesoro" type="NCMS000">te|so|ro|</w>
          <w lemma="rico" type="AQ0FS00">ri|ca|</w>
          <w lemma="flor" type="NCFS000">flor|</w>
          <w lemma="indiano" type="AQ0FS00">in|dia|na|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="2">
          <!--y sol rizado en hojas,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="sol" type="NCMS000">sol|</w>
          <w lemma="rizar" type="VMP00SM">ri|za|do</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="hoja" type="NCFP000">ho|jas|</w>
        </l>
        <l met="1|-|-|4|-|-|-|8|-|10|-|" n="3">
          <!--oro florido que tu patria niegas,-->
          <w lemma="oro" type="NCMS000">o|ro|</w>
          <w lemma="florido" type="AQ0MS00">flo|ri|do|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="patria" type="NCFS000">pa|tria|</w>
          <w lemma="negar" type="VMIP2S0">nie|gas|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="4">
          <!--que a tu oriente despojas-->
          <w lemma="que" type="CS">que</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="tu" type="DP2CSS">tu</w>
          <w lemma="oriente" type="NCMS000">o|rien|te|</w>
          <w lemma="despojar" type="VMIP2S0">des|po|jas|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="5">
          <!--y en extranjeros valles te avecinas,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="extranjero" type="AQ0MP00">ex|tran|je|ros|</w>
          <w lemma="valle" type="NCMP000">va|lles|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="avecinar" type="VMIP2S0">a|ve|ci|nas|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="6">
          <!--y a ser desvelo llegas-->
          <w lemma="y" type="CC">y</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="desvelo" type="NCMS000">des|ve|lo|</w>
          <w lemma="llegar" type="VMIP2S0">lle|gas|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="7">
          <!--de laureles y rústicas encinas.-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="laurel" type="NCMP000">lau|re|les|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="rústico" type="AQ0FP00">rús|ti|cas|</w>
          <w lemma="encina" type="NCFP000">en|ci|nas|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="8">
          <!--Por ti en alado pino,-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="ti" type="PP2CSO0">ti</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="alado" type="AQ0MS00">a|la|do|</w>
          <w lemma="pino" type="NCMS000">pi|no|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="9">
          <!--por selvas de coral pasó animoso-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="selva" type="NCFP000">sel|vas|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="coral" type="NCCS000">co|ral|</w>
          <w lemma="pasar" type="VMIS3S0">pa|só</w>
          <w lemma="animoso" type="AQ0MS00">a|ni|mo|so|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="10">
          <!--el avariento, el vano, el codicioso,-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="avariento" type="NCMS000">a|va|rien|to</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="vano" type="NCMS000">va|no</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="codicioso" type="NCMS000">co|di|cio|so|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="11">
          <!--sin que el fatal destino-->
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="fatal" type="AQ0CS00">fa|tal|</w>
          <w lemma="destino" type="NCMS000">des|ti|no|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="12">
          <!--que le asalte, presuma-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="le" type="PP3CSD0">le</w>
          <w lemma="asaltar" type="VMSP3S0">a|sal|te|</w>
          <w lemma="presumir" type="VMSP3S0">pre|su|ma|</w>
        </l>
        <l met="-|2|-|-|-|6|7|-|-|10|-|" n="13">
          <!--en valles de cristal, montes de espuma.-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="valle" type="NCMP000">va|lles|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="cristal" type="NCMS000">cris|tal|</w>
          <w lemma="monte" type="NCMP000">mon|tes|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="espuma" type="NCFS000">es|pu|ma|</w>
        </l>
      </lg>
    </body>
  </text>
