    <body>
      <head>
        <title>Todo el mayo volaba</title>
      </head>
      <lg cert="high" type="Sextetos-lira">
        <l met="1|-|-|-|-|-|7|-|" n="1">
          <!--Todo el mayo volaba-->
          <w lemma="todo" type="DI0MS0">to|do|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="[??:??/5/??:??.??:??]" type="W">ma|yo|</w>
          <w lemma="volar" type="VMII3S0">vo|la|ba|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="2">
          <!--en un pájaro hermoso,-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="pájaro" type="NCMS000">pá|ja|ro|</w>
          <w lemma="hermoso" type="AQ0MS00">her|mo|so|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="3">
          <!--que a carreras furioso-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="carrera" type="NCFP000">ca|rre|ras|</w>
          <w lemma="furioso" type="AQ0MS00">fu|rio|so|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="4">
          <!--un halcón lo acosaba;-->
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="halcón" type="NCMS000">hal|cón|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="acosar" type="VMII3S0">a|co|sa|ba|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="5">
          <!--de unas ramas se abrigaba-->
          <w lemma="de" type="SP">de</w>
          <w lemma="uno" type="DI0FP0">u|nas|</w>
          <w lemma="rama" type="NCFP000">ra|mas|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="abrigar" type="VMII3S0">a|bri|ga|ba|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="6">
          <!--y huyendo del peligro da en la liga.-->
          <w lemma="y" type="CC">y</w>
          <w lemma="huir" type="VMG0000">hu|yen|do|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="peligro" type="NCMS000">pe|li|gro|</w>
          <w lemma="dar" type="VMIP3S0">da</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="liga" type="NCFS000">li|ga|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="7">
          <!--La corderilla mansa-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="corderillo" type="NCFS000">cor|de|ri|lla|</w>
          <w lemma="manso" type="AQ0FS00">man|sa|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="8">
          <!--(felpa viva) se pierde-->
          <w lemma="felpa" type="NCFS000">fel|pa|</w>
          <w lemma="vivo" type="AQ0FS00">vi|va|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="perder" type="VMIP3S0">pier|de|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="9">
          <!--entre la selva verde,-->
          <w lemma="entre" type="SP">en|tre|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="selva" type="NCFS000">sel|va|</w>
          <w lemma="verde" type="AQ0CS00">ver|de|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="10">
          <!--y en dar voces se cansa;-->
          <w lemma="y" type="CC">y</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="dar" type="VMN0000">dar|</w>
          <w lemma="voz" type="NCFP000">vo|ces|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="cansar" type="VMIP3S0">can|sa|</w>
        </l>
        <l met="-|-|3|-|5|6|-|" n="11">
          <!--y las voces que ha dado-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="voz" type="NCFP000">vo|ces|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="haber" type="VAIP3S0">ha|</w>
          <w lemma="dar" type="VMP00SM">da|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="12">
          <!--las oye su peligro, y no el ganado.-->
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="oír" type="VMIP3S0">o|ye|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="peligro" type="NCMS000">pe|li|gro</w>
          <w lemma="y" type="CC">y</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="el" type="DA0MS0">e|l</w>
          <w lemma="ganado" type="NCMS000">ga|na|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="13">
          <!--Manchado de colores-->
          <w lemma="manchar" type="VMP00SM">man|cha|do|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="color" type="NCMP000">co|lo|res|</w>
        </l>
        <l met="1|2|-|-|-|6|-|" n="14">
          <!--(ya tigre de las aves)-->
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="tigre" type="NCMS000">ti|gre|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="ave" type="NCFP000">a|ves|</w>
        </l>
        <l met="-|-|-|4|5|-|" n="15">
          <!--el colorín, suaves,-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="colorín" type="NCMS000">co|lo|rín|</w>
          <w lemma="suave" type="AQ0CP00">sua|ves|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="16">
          <!--cantaba sus amores;-->
          <w lemma="cantar" type="VMII3S0">can|ta|ba|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="amor" type="NCMP000">a|mo|res|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="17">
          <!--el cazador lo oía-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cazador" type="NCMS000">ca|za|dor|</w>
          <w lemma="lo" type="PP3MSA0">lo</w>
          <w lemma="oír" type="VMII3S0">o|í|a|</w>
        </l>
        <l met="-|-|3|-|5|6|-|8|-|10|-|" n="18">
          <!--y su canto fue muerte, y no armonía.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="canto" type="NCMS000">can|to|</w>
          <w lemma="ser" type="VSIS3S0">fue|</w>
          <w lemma="muerte" type="NCFS000">muer|te</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="armonía" type="NCFS000">ar|mo|ní|a|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="19">
          <!--Relumbra allá en el risco-->
          <w lemma="relumbrar" type="VMIP3S0">re|lum|bra</w>
          <w lemma="allá" type="RG">a|llá</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">e|l</w>
          <w lemma="risco" type="NCMS000">ris|co|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="20">
          <!--(carbunclo de su pecho)-->
          <w lemma="carbunclo" type="NCMS000">car|bun|clo|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="pecho" type="NCMS000">pe|cho|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="21">
          <!--la llama que se ha hecho-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="llama" type="NCFS000">lla|ma|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="haber" type="VAIP3S0">ha</w>
          <w lemma="hacer" type="VMP00SM">he|cho|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="22">
          <!--por calor del aprisco;-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="calor" type="NCMS000">ca|lor|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="aprisco" type="NCMS000">a|pris|co|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="23">
          <!--y a que lo robe fiero-->
          <w lemma="y" type="CC">y</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="robar" type="VMSP3S0">ro|be|</w>
          <w lemma="fiero" type="AQ0MS00">fie|ro|</w>
        </l>
        <l met="1|-|3|4|-|6|-|-|-|10|-|" n="24">
          <!--ella misma es quien llama al bandolero.-->
          <w lemma="él" type="PP3FS00">e|lla|</w>
          <w lemma="mismo" type="AQ0FS00">mis|ma</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="quien" type="PR0CS00">quien|</w>
          <w lemma="llamar" type="VMIP3S0">lla|ma</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="bandolero" type="NCMS000">ban|do|le|ro|</w>
        </l>
        <l met="1|-|-|4|-|6|-|" n="25">
          <!--Corre, listón de nieve,-->
          <w lemma="correr" type="VMIP3S0">co|rre|</w>
          <w lemma="listón" type="NCMS000">lis|tón|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="nieve" type="NCFS000">nie|ve|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="26">
          <!--arroyuelo, que, helado,-->
          <w lemma="arroyuelo" type="NCMS000">a|rro|yue|lo|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="helar" type="VMP00SM">he|la|do|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="27">
          <!--era alcorza del prado,-->
          <w lemma="ser" type="VSII3S0">e|ra</w>
          <w lemma="alcorza" type="NCFS000">al|cor|za|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="prado" type="NCMS000">pra|do|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="28">
          <!--y los pasos que mueve,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="paso" type="NCMP000">pa|sos|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="mover" type="VMIP3S0">mue|ve|</w>
        </l>
        <l met="1|-|-|4|-|6|-|" n="29">
          <!--dando en el mar, ¡ay cielo!,-->
          <w lemma="dar" type="VMG0000">dan|do</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="mar" type="NCCS000">mar|</w>
          <w lemma="ay" type="I">ay|</w>
          <w lemma="cielo" type="NCMS000">cie|lo|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="30">
          <!--ni lo dejan alcorza ni arroyuelo.-->
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="dejar" type="VMIP3P0">de|jan|</w>
          <w lemma="alcorza" type="NCFS000">al|cor|za|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="arroyuelo" type="NCMS000">a|rro|yue|lo|</w>
        </l>
      </lg>
    </body>
