  <text>
    <body>
      <head>
        <title>Ayer, viniendo del prado,</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="-|2|-|4|-|-|7|-|" n="1">
          <!--Ayer, viniendo del prado,-->
          <w lemma="ayer" type="RG">a|yer|</w>
          <w lemma="venir" type="VMG0000">vi|nien|do|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="prado" type="NCMS000">pra|do|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="2">
          <!--te encontré con un vestido,-->
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="encontrar" type="VMIS1S0">en|con|tré|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="vestir" type="VMP00SM">ves|ti|do|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="3">
          <!--Luis, aunque bien guarnecido,-->
          <w lemma="luis" type="NP00000">luis|</w>
          <w lemma="aunque" type="CC">aun|que|</w>
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="guarnecer" type="VMP00SM">guar|ne|ci|do|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="4">
          <!--fieramente acuchillado.-->
          <w lemma="fieramente" type="RG">fie|ra|men|te</w>
          <w lemma="acuchillar" type="VMP00SM">a|cu|chi|lla|do|</w>
        </l>
        <l met="1|2|3|-|-|-|7|-|" n="5">
          <!--Tan fuera eran de compás-->
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="fuera" type="RG">fue|ra</w>
          <w lemma="ser" type="VSII3P0">e|ran|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="compás" type="NCMS000">com|pás|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="6">
          <!--y grandes sus cuchilladas,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="grande" type="AQ0CP00">gran|des|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="cuchillada" type="NCFP000">cu|chi|lla|das|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="7">
          <!--que juzgué serían dadas-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="juzgar" type="VMIS1S0">juz|gué|</w>
          <w lemma="ser" type="VSIC3P0">se|rí|an|</w>
          <w lemma="dar" type="VMP00PF">da|das|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="8">
          <!--por Orlando o Fierabrás.-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="orlando" type="NP00000">o|rlan|do</w>
          <w lemma="o" type="CC">o</w>
          <w lemma="fierabrás" type="NP00000">fie|ra|brás|</w>
        </l>
      </lg>
    </body>
