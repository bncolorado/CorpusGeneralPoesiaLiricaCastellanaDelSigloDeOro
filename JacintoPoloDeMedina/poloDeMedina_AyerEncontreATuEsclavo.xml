  <text>
    <body>
      <head>
        <title>Ayer encontré a tu esclavo</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="-|2|-|-|5|-|7|-|" n="1">
          <!--Ayer encontré a tu esclavo-->
          <w lemma="ayer" type="RG">a|yer|</w>
          <w lemma="encontrar" type="VMIS1S0">en|con|tré</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="tu" type="DP2CSS">tu</w>
          <w lemma="esclavo" type="NCMS000">es|cla|vo|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="2">
          <!--luciendo de luto y duelo-->
          <w lemma="lucir" type="VMG0000">lu|cien|do|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="luto" type="NCMS000">lu|to</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="duelo" type="NCMS000">due|lo|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="3">
          <!--vestido, y barriendo el suelo-->
          <w lemma="vestir" type="VMP00SM">ves|ti|do</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="barrer" type="VMG0000">ba|rrien|do</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="suelo" type="NCMS000">sue|lo|</w>
        </l>
        <l met="-|2|-|4|5|-|7|-|" n="4">
          <!--con una gran cola o rabo.-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="gran" type="AQ0CS00">gran|</w>
          <w lemma="cola" type="NCFS000">co|la</w>
          <w lemma="o" type="CC">o</w>
          <w lemma="rabo" type="NCMS000">ra|bo|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="5">
          <!--Vilo y dije: Bien lo gasta-->
          <w lemma="vilo" type="NP00000">vi|lo</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="decir" type="VMIS1S0">di|je|</w>
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="lo" type="PP3MSA0">lo</w>
          <w lemma="gastar" type="VMIP3S0">gas|ta|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="6">
          <!--quien por derecho lo tiene,-->
          <w lemma="quien" type="PR0CS00">quien|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="derecho" type="NCMS000">de|re|cho|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="tener" type="VMIP3S0">tie|ne|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="7">
          <!--que siempre al galgo le viene-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="siempre" type="RG">siem|pre</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="galgo" type="NCMS000">gal|go|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="venir" type="VMIP3S0">vie|ne|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="8">
          <!--ser rabilargo de casta.-->
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="rabilargo" type="AQ0MS00">ra|bi|lar|go|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="casta" type="NCFS000">cas|ta|</w>
        </l>
      </lg>
    </body>
  </text>
