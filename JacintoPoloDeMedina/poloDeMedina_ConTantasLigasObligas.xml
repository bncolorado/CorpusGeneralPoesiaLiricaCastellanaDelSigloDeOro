  <text>
    <body>
      <head>
        <title>Con tantas ligas obligas</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="-|2|-|4|-|-|7|-|" n="1">
          <!--Con tantas ligas obligas-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="tanto" type="DI0FP0">tan|tas|</w>
          <w lemma="liga" type="NCFP000">li|gas|</w>
          <w lemma="obligar" type="VMIP2S0">o|bli|gas|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="2">
          <!--a que se dude, Damón,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="dudar" type="VMSP3S0">du|de|</w>
          <w lemma="damón" type="NP00000">da|món|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="3">
          <!--si tus flacas piernas son-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="tu" type="DP2CPS">tus|</w>
          <w lemma="flaco" type="AQ0FP00">fla|cas|</w>
          <w lemma="pierna" type="NCFP000">pier|nas|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="4">
          <!--rapacejos de tus ligas.-->
          <w lemma="rapacejo" type="NCMP000">ra|pa|ce|jos|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="tu" type="DP2CPS">tus|</w>
          <w lemma="liga" type="NCFP000">li|gas|</w>
        </l>
        <l met="-|2|-|4|5|-|7|-|" n="5">
          <!--De no poder ser casado-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="poder" type="VMN0000">po|der|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="casar" type="VMP00SM">ca|sa|do|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="6">
          <!--nos das claro testimonio,-->
          <w lemma="nos" type="PP1CP00">nos|</w>
          <w lemma="dar" type="VMIP2S0">das|</w>
          <w lemma="claro" type="AQ0MS00">cla|ro|</w>
          <w lemma="testimonio" type="NCMS000">tes|ti|mo|nio|</w>
        </l>
        <l met="-|-|-|-|-|-|7|-|" n="7">
          <!--porque para matrimonio-->
          <w lemma="porque" type="CS">por|que|</w>
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="matrimonio" type="NCMS000">ma|tri|mo|nio|</w>
        </l>
        <l met="-|2|-|4|5|-|7|-|" n="8">
          <!--estás, Damón, muy ligado.-->
          <w lemma="estar" type="VMIP2S0">es|tás|</w>
          <w lemma="damón" type="NP00000">da|món|</w>
          <w lemma="muy" type="RG">muy|</w>
          <w lemma="ligar" type="VMP00SM">li|ga|do|</w>
        </l>
      </lg>
    </body>
  </text>
