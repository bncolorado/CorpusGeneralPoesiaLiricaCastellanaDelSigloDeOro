  <text>
    <body>
      <head>
        <title>A escarmentar el prado</title>
      </head>
      <lg cert="high" type="Silva">
        <l met="-|-|-|4|-|6|-|" n="1">
          <!--A escarmentar el prado-->
          <w lemma="a" type="SP">a</w>
          <w lemma="escarmentar" type="VMN0000">es|car|men|tar|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="prado" type="NCMS000">pra|do|</w>
        </l>
        <l met="-|-|3|-|-|6|-|8|-|10|-|" n="2">
          <!--maravilla naciste, flor, y en ella-->
          <w lemma="maravilla" type="NCFS000">ma|ra|vi|lla|</w>
          <w lemma="nacer" type="VMIS2S0">na|cis|te|</w>
          <w lemma="flor" type="NCFS000">flor|</w>
          <w lemma="y" type="CC">y</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="él" type="PP3FS00">e|lla|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="3">
          <!--escrita la siniestra infausta estrella-->
          <w lemma="escribir" type="VMP00SF">es|cri|ta|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="siniestro" type="AQ0FS00">si|nies|tra</w>
          <w lemma="infausto" type="AQ0FS00">in|faus|ta</w>
          <w lemma="estrella" type="NCFS000">es|tre|lla|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="4">
          <!--que anochece tu vida con el alba;-->
          <w lemma="que" type="CS">que</w>
          <w lemma="anochecer" type="VMIP3S0">a|no|che|ce|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="alba" type="NCFS000">al|ba|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="5">
          <!--clamores son la salva-->
          <w lemma="clamor" type="NCMP000">cla|mo|res|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="salva" type="NCFS000">sal|va|</w>
        </l>
        <l met="-|-|-|4|-|6|-|8|-|10|-|" n="6">
          <!--que Filomena dulcemente llora:-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="filomena" type="NP00000">fi|lo|me|na|</w>
          <w lemma="dulcemente" type="RG">dul|ce|men|te|</w>
          <w lemma="llorar" type="VMIP3S0">llo|ra|</w>
        </l>
        <l met="1|2|3|-|5|-|-|-|-|10|-|" n="7">
          <!--aun no quieren fiarte hasta la Aurora,-->
          <w lemma="aun" type="RG">aun|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="querer" type="VMIP3P0">quie|ren|</w>
          <w lemma="fiar" type="VMN0000">fiar|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="hasta" type="SP">has|ta|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="aurora" type="NP00000">au|ro|ra|</w>
        </l>
        <l met="-|2|3|-|-|6|-|-|-|10|-|" n="8">
          <!--pues no llega con vida a conocerte;-->
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="llegar" type="VMIP3S0">lle|ga|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="vida" type="NCFS000">vi|da</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="conocer" type="VMN0000">co|no|cer|</w>
          <w lemma="te" type="PP2CS00">te|</w>
        </l>
        <l met="1|-|3|-|-|6|-|-|-|10|-|" n="9">
          <!--sólo saben las flores de tu muerte.-->
          <w lemma="sólo" type="RG">só|lo|</w>
          <w lemma="saber" type="VMIP3P0">sa|ben|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="flor" type="NCFP000">flo|res|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="muerte" type="NCFS000">muer|te|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="10">
          <!--¡Oh malograda vida,-->
          <w lemma="oh" type="I">oh|</w>
          <w lemma="malograr" type="VMP00SF">ma|lo|gra|da|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="11">
          <!--en la muerte nacida!-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="muerte" type="NCFS000">muer|te|</w>
          <w lemma="nacer" type="VMP00SF">na|ci|da|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="12">
          <!--¡Oh vida malograda,-->
          <w lemma="oh" type="I">oh|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
          <w lemma="malograr" type="VMP00SF">ma|lo|gra|da|</w>
        </l>
        <l met="1|-|-|4|-|6|7|-|-|10|-|" n="13">
          <!--no conseguida, no, sólo intentada!-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="conseguir" type="VMP00SF">con|se|gui|da|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="sólo" type="RG">só|lo</w>
          <w lemma="intentar" type="VMP00SF">in|ten|ta|da|</w>
        </l>
        <l met="-|-|3|4|-|6|-|" n="14">
          <!--¡Pero qué más dichosa-->
          <w lemma="pero" type="CC">pe|ro|</w>
          <w lemma="qué" type="PT00000">qué|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="dichoso" type="AQ0FS00">di|cho|sa|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|9|10|-|" n="15">
          <!--se podía esperar quien nacía hermosa!-->
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="poder" type="VMII3S0">po|día|</w>
          <w lemma="esperar" type="VMN0000">es|pe|rar|</w>
          <w lemma="quien" type="PR0CS00">quien|</w>
          <w lemma="nacer" type="VMII3S0">na|cía</w>
          <w lemma="hermoso" type="AQ0FS00">her|mo|sa|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="16">
          <!--Que entre tanta hermosura-->
          <w lemma="que" type="CS">que</w>
          <w lemma="entre" type="SP">en|tre|</w>
          <w lemma="tanto" type="DI0FS0">tan|ta</w>
          <w lemma="hermosura" type="NCFS000">her|mo|su|ra|</w>
        </l>
        <l met="1|-|3|-|-|6|-|8|-|10|-|" n="17">
          <!--fuera yerro esperar mayor ventura.-->
          <w lemma="fuera" type="RG">fue|ra|</w>
          <w lemma="errar" type="VMIP1S0">ye|rro</w>
          <w lemma="esperar" type="VMN0000">es|pe|rar|</w>
          <w lemma="mayor" type="AQ0CS00">ma|yor|</w>
          <w lemma="ventura" type="NCFS000">ven|tu|ra|</w>
        </l>
      </lg>
    </body>
  </text>
