  <text>
    <body>
      <head>
        <title>Narciso bello, que en papel bruñido,</title>
      </head>
      <lg cert="high" type="Silva">
        <l met="-|2|-|4|-|-|-|8|-|10|-|" n="1">
          <!--Narciso bello, que en papel bruñido,-->
          <w lemma="narciso" type="NP00000">nar|ci|so|</w>
          <w lemma="bello" type="AQ0MS00">be|llo|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="papel" type="NCMS000">pa|pel|</w>
          <w lemma="bruñir" type="VMP00SM">bru|ñi|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="2">
          <!--o en lienzo transparente,-->
          <w lemma="o" type="CC">o</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="lienzo" type="NCMS000">lien|zo|</w>
          <w lemma="transparente" type="AQ0CS00">trans|pa|ren|te|</w>
        </l>
        <l met="-|-|3|-|-|6|-|8|-|10|-|" n="3">
          <!--del cristal detenido de una fuente-->
          <w lemma="del" type="SP">del|</w>
          <w lemma="cristal" type="NCMS000">cris|tal|</w>
          <w lemma="detener" type="VMP00SM">de|te|ni|do|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="fuente" type="NCFS000">fuen|te|</w>
        </l>
        <l met="1|-|-|-|-|6|-|-|-|10|-|" n="4">
          <!--copias tu original, que te enamora,-->
          <w lemma="copia" type="NCFP000">co|pias|</w>
          <w lemma="tu" type="DP2CSS">tu</w>
          <w lemma="original" type="AQ0CS00">o|ri|gi|nal|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="enamorar" type="VMIP3S0">e|na|mo|ra|</w>
        </l>
        <l met="1|-|-|4|-|-|-|8|-|10|-|" n="5">
          <!--sordo al peñasco, que con voz te llora,-->
          <w lemma="sordo" type="AQ0MS00">sor|do</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="peñasco" type="NCMS000">pe|ñas|co|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="voz" type="NCFS000">voz|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="llorar" type="VMIP3S0">llo|ra|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="6">
          <!--y al monte, que con ecos te suspira.-->
          <w lemma="y" type="CC">y</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="monte" type="NCMS000">mon|te|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="eco" type="NCMP000">e|cos|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="suspirar" type="VMIP3S0">sus|pi|ra|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="7">
          <!--Si el que no te merece te retira-->
          <w lemma="si" type="CS">si</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="merecer" type="VMIP3S0">me|re|ce|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="retirar" type="VMIP3S0">re|ti|ra|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="8">
          <!--(pues ninguna nació para igualarte,-->
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="ninguno" type="PI0FS00">nin|gu|na|</w>
          <w lemma="nacer" type="VMIS3S0">na|ció|</w>
          <w lemma="para" type="SP">pa|ra</w>
          <w lemma="igualar" type="VMN0000">i|gua|lar|</w>
          <w lemma="te" type="PP2CS00">te|</w>
        </l>
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="9">
          <!--y nadie espera tan hermosa suerte)-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="nadie" type="PI0CS00">na|die</w>
          <w lemma="esperar" type="VMIP3S0">es|pe|ra|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="hermoso" type="AQ0FS00">her|mo|sa|</w>
          <w lemma="suerte" type="NCFS000">suer|te|</w>
        </l>
        <l met="1|2|-|-|-|6|-|-|-|10|-|" n="10">
          <!--no lleguen por tu mérito a alcanzarte,-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="llegar" type="VMSP3P0">lle|guen|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="mérito" type="NCMS000">mé|ri|to</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="alcanzar" type="VMN0000">al|can|zar|</w>
          <w lemma="te" type="PP2CS00">te|</w>
        </l>
        <l met="1|-|-|-|-|6|-|-|-|10|-|" n="11">
          <!--lleguen por tu piedad a merecerte.-->
          <w lemma="llegar" type="VMM03P0">lle|guen|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="piedad" type="NCFS000">pie|dad|</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="merecer" type="VMN0000">me|re|cer|</w>
          <w lemma="te" type="PP2CS00">te|</w>
        </l>
      </lg>
    </body>
  </text>
