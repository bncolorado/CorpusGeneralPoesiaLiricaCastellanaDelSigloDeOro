  <text>
    <body>
      <head>
        <title>Al arrabal se murmura</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="-|-|-|4|-|-|7|-|" n="1">
          <!--Al arrabal se murmura-->
          <w lemma="a" type="SP">a</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="arrabal" type="NCMS000">a|rra|bal|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="murmurar" type="VMIP3S0">mur|mu|ra|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="2">
          <!--que acudes enamorado,-->
          <w lemma="que" type="CS">que</w>
          <w lemma="acudir" type="VMIP2S0">a|cu|des|</w>
          <w lemma="enamorar" type="VMP00SM">e|na|mo|ra|do|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="3">
          <!--de oculta pasión picado,-->
          <w lemma="de" type="SP">de</w>
          <w lemma="oculto" type="AQ0FS00">o|cul|ta|</w>
          <w lemma="pasión" type="NCFS000">pa|sión|</w>
          <w lemma="picar" type="VMP00SM">pi|ca|do|</w>
        </l>
        <l met="-|-|3|4|-|-|7|-|" n="4">
          <!--a picar cierta hermosura.-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="picar" type="VMN0000">pi|car|</w>
          <w lemma="cierto" type="DI0FS0">cier|ta</w>
          <w lemma="hermosura" type="NCFS000">her|mo|su|ra|</w>
        </l>
        <l met="1|2|-|4|5|6|7|-|" n="5">
          <!--Si esto es así, cosa es llana,-->
          <w lemma="si" type="CS">si</w>
          <w lemma="este" type="PD00S00">es|to</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="así" type="RG">a|sí|</w>
          <w lemma="cosa" type="NCFS000">co|sa</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="llano" type="AQ0FS00">lla|na|</w>
        </l>
        <l met="1|-|-|-|5|-|7|-|" n="6">
          <!--Fabio, que si acudes tal-->
          <w lemma="fabio" type="NP00000">fa|bio|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="si" type="CS">si</w>
          <w lemma="acudir" type="VMIP2S0">a|cu|des|</w>
          <w lemma="tal" type="DD0CS0">tal|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="7">
          <!--a picar al arrabal,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="picar" type="VMN0000">pi|car|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="arrabal" type="NCMS000">a|rra|bal|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="8">
          <!--que eres amante almorrana.-->
          <w lemma="que" type="CS">que</w>
          <w lemma="ser" type="VSIP2S0">e|res|</w>
          <w lemma="amante" type="NCCS000">a|man|te</w>
          <w lemma="almorrana" type="NCFS000">al|mo|rra|na|</w>
        </l>
      </lg>
    </body>
