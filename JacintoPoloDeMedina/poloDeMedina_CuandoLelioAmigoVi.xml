    <body>
      <head>
        <title> Cuando, Lelio amigo, vi</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="-|-|3|-|5|-|7|-|" n="1">
          <!--Cuando, Lelio amigo, vi-->
          <w lemma="cuando" type="CS">cuan|do|</w>
          <w lemma="lelio" type="NP00000">le|lio</w>
          <w lemma="amigo" type="NCMS000">a|mi|go|</w>
          <w lemma="ver" type="VMIS1S0">vi|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="2">
          <!--tu cara, quedé confuso,-->
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="cara" type="NCFS000">ca|ra|</w>
          <w lemma="quedar" type="VMIS1S0">que|dé|</w>
          <w lemma="confuso" type="AQ0MS00">con|fu|so|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="3">
          <!--pues como la espada al uso,-->
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="como" type="CS">co|mo|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="espada" type="NCFS000">es|pa|da</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="uso" type="NCMS000">u|so|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="4">
          <!--la llevas con tahalí.-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="llevar" type="VMIP2S0">lle|vas|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="tahalí" type="NCMS000">ta|ha|lí|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="5">
          <!--Come huevos si te agrada-->
          <w lemma="comer" type="VMIP3S0">co|me|</w>
          <w lemma="huevo" type="NCMP000">hue|vos|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="agradar" type="VMIP3S0">a|gra|da|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="6">
          <!--en las cuaresmas solenes,-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="cuaresma" type="NCFP000">cua|res|mas|</w>
          <w lemma="solenes" type="AQ0CP00">so|le|nes|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="7">
          <!--pues siempre en la cara tienes-->
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="siempre" type="RG">siem|pre</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="cara" type="NCFS000">ca|ra|</w>
          <w lemma="tener" type="VMIP2S0">tie|nes|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="8">
          <!--la bula de la Cruzada.-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="bula" type="NCFS000">bu|la|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="cruzada" type="NP00000">cru|za|da|</w>
        </l>
