  <text>
    <body>
      <head>
        <title>Cavando un sepulcro un hombre,</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="-|2|3|-|5|6|7|-|" n="1">
          <!--Cavando un sepulcro un hombre,-->
          <w lemma="cavar" type="VMG0000">ca|van|do</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="sepulcro" type="NCMS000">se|pul|cro</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="hombre" type="NCMS000">hom|bre|</w>
        </l>
        <l met="-|2|3|-|5|-|7|-|" n="2">
          <!--sacó largo, corvo y grueso,-->
          <w lemma="sacar" type="VMIS3S0">sa|có|</w>
          <w lemma="largo" type="AQ0MS00">lar|go|</w>
          <w lemma="corvo" type="AQ0MS00">cor|vo</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="grueso" type="AQ0MS00">grue|so|</w>
        </l>
        <l met="-|2|-|4|-|6|7|-|" n="3">
          <!--entre otros muchos, un hueso-->
          <w lemma="entre" type="SP">en|tre</w>
          <w lemma="otro" type="DI0MP0">o|tros|</w>
          <w lemma="mucho" type="PI0MP00">mu|chos|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="hueso" type="NCMS000">hue|so|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="4">
          <!--que tiene cuerno por nombre.-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="tener" type="VMIP3S0">tie|ne|</w>
          <w lemma="cuerno" type="NCMS000">cuer|no|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="nombre" type="NCMS000">nom|bre|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="5">
          <!--Volviolo al sepulcro al punto,-->
          <w lemma="volviolo" type="NP00000">vol|vio|lo</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="sepulcro" type="NCMS000">se|pul|cro</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="punto" type="NCMS000">pun|to|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="6">
          <!--y viéndolo un cortesano,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="ver" type="VMG0000">vien|do|</w>
          <w lemma="lo" type="PP3MSA0">lo</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="cortesano" type="NCMS000">cor|te|sa|no|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="7">
          <!--dijo: Bien hacéis, hermano,-->
          <w lemma="decir" type="VMIS3S0">di|jo|</w>
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="hacer" type="VMIP2P0">ha|céis|</w>
          <w lemma="hermano" type="NCMS000">her|ma|no|</w>
        </l>
        <l met="1|2|-|4|-|-|7|-|" n="8">
          <!--que es hueso de ese difunto.-->
          <w lemma="que" type="CS">que</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="hueso" type="NCMS000">hue|so|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="ese" type="DD0MS0">e|se|</w>
          <w lemma="difunto" type="NCMS000">di|fun|to|</w>
        </l>
      </lg>
    </body>
