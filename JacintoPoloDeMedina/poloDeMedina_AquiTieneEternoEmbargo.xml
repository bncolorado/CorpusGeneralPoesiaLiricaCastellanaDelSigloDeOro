  <text>
    <body>
      <head>
        <title> Aquí tiene eterno embargo</title>
      </head>
      <lg cert="high" type="Décima">
        <l met="-|2|3|-|5|-|7|-|" n="1">
          <!--Aquí tiene eterno embargo-->
          <w lemma="aquí" type="RG">a|quí|</w>
          <w lemma="tener" type="VMIP3S0">tie|ne</w>
          <w lemma="eterno" type="AQ0MS00">e|ter|no</w>
          <w lemma="embargo" type="NCMS000">em|bar|go|</w>
        </l>
        <l met="1|2|-|4|-|-|7|-|" n="2">
          <!--un hombre tan sin provecho,-->
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="hombre" type="NCMS000">hom|bre|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="provecho" type="NCMS000">pro|ve|cho|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="3">
          <!--que reventó por estrecho,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="reventar" type="VMIS3S0">re|ven|tó|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="estrecho" type="NCMS000">es|tre|cho|</w>
        </l>
        <l met="-|-|3|4|-|-|7|-|" n="4">
          <!--ya que no puede por largo.-->
          <w lemma="ya_que" type="CS">ya_|que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="poder" type="VMIP3S0">pue|de|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="largo" type="AQ0MS00">lar|go|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="5">
          <!--De su muerte el fin amargo-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="muerte" type="NCFS000">muer|te</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="fin" type="NCMS000">fin|</w>
          <w lemma="amargo" type="AQ0MS00">a|mar|go|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="6">
          <!--por cámara despachó,-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="cámara" type="NCFS000">cá|ma|ra|</w>
          <w lemma="despachar" type="VMIS3S0">des|pa|chó|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="7">
          <!--y aun el pesar le ayudó,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="aun" type="RG">aun|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="pesar" type="NCMS000">pe|sar|</w>
          <w lemma="le" type="PP3CSD0">le</w>
          <w lemma="ayudar" type="VMIS3S0">a|yu|dó|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="8">
          <!--de verse tan liberal,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
          <w lemma="se" type="PP3CN00">se|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="liberal" type="AQ0CS00">li|be|ral|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="9">
          <!--por la parte occidental,-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="parte" type="NCCS000">par|te</w>
          <w lemma="occidental" type="AQ0CS00">oc|ci|den|tal|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="10">
          <!--de lo que no recibió.-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="recibir" type="VMIS3S0">re|ci|bió|</w>
        </l>
      </lg>
    </body>
