  <text>
    <body>
      <head>
        <title> Cierto galán tan discreto</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="1|-|-|4|5|-|7|-|" n="1">
          <!--Cierto galán tan discreto-->
          <w lemma="cierto" type="DI0MS0">cier|to|</w>
          <w lemma="galán" type="NCMS000">ga|lán|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="discreto" type="AQ0MS00">dis|cre|to|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="2">
          <!--que Cicerón se imagina,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="cicerón" type="NP00000">ci|ce|rón|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="imaginar" type="VMIP3S0">i|ma|gi|na|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="3">
          <!--sin ser gallo ni gallina,-->
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="gallo" type="NCMS000">ga|llo|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="gallina" type="NCFS000">ga|lli|na|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="4">
          <!--porque es capón en efeto,-->
          <w lemma="porque" type="CS">por|que</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="capón" type="AQ0MS00">ca|pón|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="efeto" type="NCMS000">e|fe|to|</w>
        </l>
        <l met="1|2|-|4|-|-|7|-|" n="5">
          <!--a un fraile padre llamó,-->
          <w lemma="a" type="SP">a</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="fraile" type="NCMS000">frai|le|</w>
          <w lemma="padre" type="NCMS000">pa|dre|</w>
          <w lemma="llamar" type="VMIS3S0">lla|mó|</w>
        </l>
        <l met="-|-|-|4|5|-|7|-|" n="6">
          <!--y respondió: No os corráis,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="responder" type="VMIS3S0">res|pon|dió|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="correr" type="VMSP2P0">co|rráis|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="7">
          <!--que ese nombre que me dais-->
          <w lemma="que" type="CS">que</w>
          <w lemma="ese" type="DD0MS0">e|se|</w>
          <w lemma="nombre" type="NCMS000">nom|bre|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="dar" type="VMIP2P0">dais|</w>
        </l>
        <l met="1|2|-|4|-|-|7|-|" n="8">
          <!--no puedo dároslo yo.-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="poder" type="VMIP1S0">pue|do|</w>
          <w lemma="dar" type="VMN0000">dar|</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
        </l>
      </lg>
    </body>
