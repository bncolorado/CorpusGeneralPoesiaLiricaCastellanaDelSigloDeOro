  <text>
    <body>
      <head>
        <title>Fabio, no es mucho os inquiete</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="1|-|3|4|-|-|7|-|" n="1">
          <!--Fabio, no es mucho os inquiete-->
          <w lemma="fabio" type="NP00000">fa|bio|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="mucho" type="RG">mu|cho</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="inquietar" type="VMSP3S0">in|quie|te|</w>
        </l>
        <l met="1|-|3|4|-|-|7|-|" n="2">
          <!--este mal crudo y traidor-->
          <w lemma="este" type="DD0MS0">es|te|</w>
          <w lemma="mal" type="AQ0MS00">mal|</w>
          <w lemma="crudo" type="NCMS000">cru|do</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="traidor" type="NCMS000">trai|dor|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="3">
          <!--que con sangriento rigor-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="sangriento" type="AQ0MS00">san|grien|to|</w>
          <w lemma="rigor" type="NCMS000">ri|gor|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="4">
          <!--por detrás os acomete.-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="detrás" type="RG">de|trás|</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="acometer" type="VMIP3S0">a|co|me|te|</w>
        </l>
        <l met="-|2|-|4|5|-|7|-|" n="5">
          <!--Saberse no os cause enojo,-->
          <w lemma="saber" type="VMN0000">sa|ber|</w>
          <w lemma="se" type="PP3CN00">se|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="causar" type="VMSP3S0">cau|se</w>
          <w lemma="enojo" type="NCMS000">e|no|jo|</w>
        </l>
        <l met="-|-|-|-|-|-|7|-|" n="6">
          <!--porque cuantos lo sabrán,-->
          <w lemma="porque" type="CS">por|que|</w>
          <w lemma="cuanto" type="PR0MP00">cuan|tos|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="saber" type="VMIF3P0">sa|brán|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="7">
          <!--que sois, con razón dirán-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="ser" type="VSIP2P0">sois|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="razón" type="NCFS000">ra|zón|</w>
          <w lemma="decir" type="VMIF3P0">di|rán|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="8">
          <!--hombre de sangre en el ojo.-->
          <w lemma="hombre" type="NCMS000">hom|bre|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="sangre" type="NCFS000">san|gre</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="ojo" type="NCMS000">o|jo|</w>
        </l>
      </lg>
    </body>
