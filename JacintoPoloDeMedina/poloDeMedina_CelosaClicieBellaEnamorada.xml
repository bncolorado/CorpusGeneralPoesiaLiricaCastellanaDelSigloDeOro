  <text>
    <body>
      <head>
        <title>Celosa Clicie, bella enamorada,</title>
      </head>
      <lg cert="high" type="Silva">
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="1">
          <!--Celosa Clicie, bella enamorada,-->
          <w lemma="celosa_clicie" type="NP00000">ce|lo|sa_|cli|cie|</w>
          <w lemma="bello" type="AQ0FS00">be|lla</w>
          <w lemma="enamorado" type="NCFS000">e|na|mo|ra|da|</w>
        </l>
        <l met="1|-|-|-|-|6|-|" n="2">
          <!--águila de las flores,-->
          <w lemma="águila" type="NCFS000">á|gui|la|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="flor" type="NCFP000">flo|res|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="3">
          <!--que atenta le examinas rayo a rayo-->
          <w lemma="que" type="CS">que</w>
          <w lemma="atentar" type="VMIP3S0">a|ten|ta|</w>
          <w lemma="le" type="PP3CSD0">le</w>
          <w lemma="examinar" type="VMIP2S0">e|xa|mi|nas|</w>
          <w lemma="rayo" type="NCMS000">ra|yo</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="rayo" type="NCMS000">ra|yo|</w>
        </l>
        <l met="-|2|-|4|-|6|-|-|-|10|-|" n="4">
          <!--al sol los más despiertos resplandores,-->
          <w lemma="al" type="SP">al|</w>
          <w lemma="sol" type="NCMS000">sol|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="despierto" type="AQ0MP00">des|pier|tos|</w>
          <w lemma="resplandor" type="NCMP000">res|plan|do|res|</w>
        </l>
        <l met="-|-|-|4|-|6|-|8|-|10|-|" n="5">
          <!--de tu durable amor continuo ensayo-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="durable" type="AQ0CS00">du|ra|ble</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
          <w lemma="continuo" type="AQ0MS00">con|ti|nuo</w>
          <w lemma="ensayo" type="NCMS000">en|sa|yo|</w>
        </l>
        <l met="1|-|-|4|-|-|-|8|-|10|-|" n="6">
          <!--(no a los desdenes de su luz rendida-->
          <w lemma="no" type="RN">no</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="desdén" type="NCMP000">des|de|nes|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="luz" type="NCFS000">luz|</w>
          <w lemma="rendir" type="VMP00SF">ren|di|da|</w>
        </l>
        <l met="-|2|-|4|-|-|-|8|-|10|-|" n="7">
          <!--tu vista clara ni tu amante vida).-->
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="vista" type="NCFS000">vis|ta|</w>
          <w lemma="claro" type="AQ0FS00">cla|ra|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="amante" type="NCCS000">a|man|te|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="8">
          <!--Sol el valle te aclama,-->
          <w lemma="sol" type="NP00000">sol|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="valle" type="NCMS000">va|lle|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="aclamar" type="VMIP3S0">a|cla|ma|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="9">
          <!--que se convierte amor en lo que ama;-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="convertir" type="VMIP3S0">con|vier|te</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="amar" type="VMIP3S0">a|ma|</w>
        </l>
        <l met="-|-|-|-|-|6|7|-|-|10|-|" n="10">
          <!--ya que tu castidad, Clicie, perdiste,-->
          <w lemma="ya_que" type="CS">ya_|que|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="castidad" type="NCFS000">cas|ti|dad|</w>
          <w lemma="clicie" type="NP00000">cli|cie|</w>
          <w lemma="perder" type="VMIS2S0">per|dis|te|</w>
        </l>
        <l met="1|-|3|-|5|6|-|-|-|10|-|" n="11">
          <!--no se niegue el buen gusto que tuviste,-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="negar" type="VMSP3S0">nie|gue</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="bueno" type="AQ0MS00">buen|</w>
          <w lemma="gusto" type="NCMS000">gus|to|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="tener" type="VMIS2S0">tu|vis|te|</w>
        </l>
        <l met="-|-|-|4|-|-|-|8|-|10|-|" n="12">
          <!--pues por blasón de tu mayor firmeza-->
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="blasón" type="NCMS000">bla|són|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="mayor" type="AQ0CS00">ma|yor|</w>
          <w lemma="firmeza" type="NCFS000">fir|me|za|</w>
        </l>
        <l met="1|-|3|-|-|6|-|-|-|10|-|" n="13">
          <!--sólo al sol se le rinde tu belleza.-->
          <w lemma="sólo" type="RG">só|lo</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="sol" type="NCMS000">sol|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="rendir" type="VMIP3S0">rin|de|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="belleza" type="NCFS000">be|lle|za|</w>
        </l>
      </lg>
    </body>
  </text>
