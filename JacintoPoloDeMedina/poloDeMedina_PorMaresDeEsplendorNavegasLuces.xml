  <text>
    <body>
      <head>
        <title>Por mares de esplendor navegas luces</title>
      </head>
      <lg cert="high" type="Silva">
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="1">
          <!--Por mares de esplendor navegas luces-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="mar" type="NCMP000">ma|res|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="esplendor" type="NCMS000">es|plen|dor|</w>
          <w lemma="navegar" type="VMIP2S0">na|ve|gas|</w>
          <w lemma="luz" type="NCFP000">lu|ces|</w>
        </l>
        <l met="-|2|-|4|-|6|-|-|-|10|-|" n="2">
          <!--con blandos remos, Ícaro atrevido,-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="blando" type="AQ0MP00">blan|dos|</w>
          <w lemma="remo" type="NCMP000">re|mos|</w>
          <w lemma="ícaro" type="NP00000">í|ca|ro</w>
          <w lemma="atrever" type="VMP00SM">a|tre|vi|do|</w>
        </l>
        <l met="-|-|3|-|-|6|7|-|-|10|-|" n="3">
          <!--a perderte en el sol vas, mariposa;-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="perder" type="VMN0000">per|der|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="sol" type="NCMS000">sol|</w>
          <w lemma="vas" type="NCMS000">vas|</w>
          <w lemma="mariposa" type="NCFS000">ma|ri|po|sa|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="4">
          <!--mas una ola furiosa-->
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="uno" type="DI0FS0">u|na</w>
          <w lemma="ola" type="NCFS000">o|la|</w>
          <w lemma="furioso" type="AQ0FS00">fu|rio|sa|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="5">
          <!--te despeña, encendido,-->
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="despeñar" type="VMIP3S0">des|pe|ña</w>
          <w lemma="encender" type="VMP00SM">en|cen|di|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="6">
          <!--penacho, destrozado por las nubes,-->
          <w lemma="penacho" type="NCMS000">pe|na|cho|</w>
          <w lemma="destrozar" type="VMP00SM">des|tro|za|do|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="nube" type="NCFP000">nu|bes|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="7">
          <!--porque al dorado océano te subes;-->
          <w lemma="porque" type="CS">por|que</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="dorar" type="VMP00SM">do|ra|do</w>
          <w lemma="océano" type="NCMS000">o|cé|a|no|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="subir" type="VMIP2S0">su|bes|</w>
        </l>
        <l met="-|-|3|-|-|6|-|8|-|10|-|" n="8">
          <!--y en veloz precipicio vuelves luego,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="veloz" type="AQ0CS00">ve|loz|</w>
          <w lemma="precipicio" type="NCMS000">pre|ci|pi|cio|</w>
          <w lemma="volver" type="VMIP2S0">vuel|ves|</w>
          <w lemma="luego" type="RG">lue|go|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="9">
          <!--y con alas de fuego-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="ala" type="NCFP000">a|las|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="fuego" type="NCMS000">fue|go|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="10">
          <!--pretendes en el húmedo elemento-->
          <w lemma="pretender" type="VMIP2S0">pre|ten|des|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="húmedo" type="AQ0MS00">hú|me|do</w>
          <w lemma="elemento" type="NCMS000">e|le|men|to|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="11">
          <!--los vientos de cristal volar sediento;-->
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="viento" type="NCMP000">vien|tos|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="cristal" type="NCMS000">cris|tal|</w>
          <w lemma="volar" type="VMN0000">vo|lar|</w>
          <w lemma="sediento" type="AQ0MS00">se|dien|to|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="12">
          <!--pero dan las espumas-->
          <w lemma="pero" type="CC">pe|ro|</w>
          <w lemma="dar" type="VMIP3P0">dan|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="espuma" type="NCFP000">es|pu|mas|</w>
        </l>
        <l met="1|-|-|4|-|-|-|8|-|10|-|" n="13">
          <!--blanco sepulcro a tus flamantes plumas.-->
          <w lemma="blanco" type="AQ0MS00">blan|co|</w>
          <w lemma="sepulcro" type="NCMS000">se|pul|cro</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="tu" type="DP2CPS">tus|</w>
          <w lemma="flamante" type="AQ0CP00">fla|man|tes|</w>
          <w lemma="pluma" type="NCFP000">plu|mas|</w>
        </l>
      </lg>
    </body>
  </text>
