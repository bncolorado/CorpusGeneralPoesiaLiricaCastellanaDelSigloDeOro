  <text>
    <body>
      <head>
        <title>Con muy sobrada razón,</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="-|2|-|4|-|-|7|-|" n="1">
          <!--Con muy sobrada razón,-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="muy" type="RG">muy|</w>
          <w lemma="sobrar" type="VMP00SF">so|bra|da|</w>
          <w lemma="razón" type="NCFS000">ra|zón|</w>
        </l>
        <l met="-|-|3|4|-|-|7|-|" n="2">
          <!--las llamó cierto poeta-->
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="llamar" type="VMIS3S0">lla|mó|</w>
          <w lemma="cierto" type="AQ0MS00">cier|to|</w>
          <w lemma="poeta" type="NCMS000">po|e|ta|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="3">
          <!--a tus barbas de bayeta,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="tu" type="DP2CPS">tus|</w>
          <w lemma="barba" type="NCFP000">bar|bas|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="bayeta" type="NCFS000">ba|ye|ta|</w>
        </l>
        <l met="1|-|-|-|-|-|7|-|" n="4">
          <!--barbas de Kirieleysón.-->
          <w lemma="barba" type="NCFP000">bar|bas|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="kirieleysón" type="NP00000">ki|rie|ley|són|</w>
        </l>
        <l met="1|-|-|4|-|6|7|-|" n="5">
          <!--Pueden servir, si tú, Alonso,-->
          <w lemma="poder" type="VMIP3P0">pue|den|</w>
          <w lemma="servir" type="VMN0000">ser|vir|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="tú" type="PP2CSN0">tú</w>
          <w lemma="alonso" type="NP00000">a|lon|so|</w>
        </l>
        <l met="1|-|-|-|-|-|7|-|" n="6">
          <!--mueres en la juventud,-->
          <w lemma="morir" type="VMIP2S0">mue|res|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="juventud" type="NCFS000">ju|ven|tud|</w>
        </l>
        <l met="-|-|3|-|-|-|-|8|-|" n="7">
          <!--para aforro en tu ataúd,-->
          <w lemma="para" type="SP">pa|ra</w>
          <w lemma="aforro" type="NCMS000">a|fo|rro</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="ataúd" type="NCMS000">a|ta|úd|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="8">
          <!--y a tu entierro de responso.-->
          <w lemma="y" type="CC">y</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="tu" type="DP2CSS">tu</w>
          <w lemma="entierro" type="NCMS000">en|tie|rro|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="responso" type="NCMS000">res|pon|so|</w>
        </l>
      </lg>
    </body>
