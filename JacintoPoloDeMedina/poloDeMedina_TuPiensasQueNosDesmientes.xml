  <text>
    <body>
      <head>
        <title>¿Tú piensas que nos desmientes</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="1|2|-|-|-|-|7|-|" n="1">
          <!--¿Tú piensas que nos desmientes-->
          <w lemma="tú" type="PP2CSN0">tú|</w>
          <w lemma="pensar" type="VMIP2S0">pien|sas|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="nos" type="PP1CP00">nos|</w>
          <w lemma="desmentir" type="VMIP2S0">des|mien|tes|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="2">
          <!--con el palillo pulido-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="palillo" type="NCMS000">pa|li|llo|</w>
          <w lemma="pulir" type="VMP00SM">pu|li|do|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="3">
          <!--con que, sin haber comido,-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="haber" type="VAN0000">ha|ber|</w>
          <w lemma="comer" type="VMP00SM">co|mi|do|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="4">
          <!--Tristán, te limpias los dientes?-->
          <w lemma="tristán" type="NP00000">tris|tán|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="limpiar" type="VMIP2S0">lim|pias|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="diente" type="NCMP000">dien|tes|</w>
        </l>
        <l met="1|2|-|4|-|6|-|" n="5">
          <!--No tal, el hambre cruel-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="tal" type="PD0CS00">tal|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="hambre" type="NCFS000">ham|bre|</w>
          <w lemma="cruel" type="AQ0CS00">cruel|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="6">
          <!--da en comerte y en picarte,-->
          <w lemma="dar" type="VMIP3S0">da</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="comer" type="VMN0000">co|mer|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="y" type="CC">y</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="picar" type="VMN0000">pi|car|</w>
          <w lemma="te" type="PP2CS00">te|</w>
        </l>
        <l met="-|2|-|-|5|6|-|8|-|" n="7">
          <!--de suerte, que no es limpiarte,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="suerte" type="NCFS000">suer|te|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="limpiar" type="VMN0000">lim|piar|</w>
          <w lemma="te" type="PP2CS00">te|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="8">
          <!--sino rascarte con él.-->
          <w lemma="sino" type="CC">si|no|</w>
          <w lemma="rascar" type="VMN0000">ras|car|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="él" type="PP3MS00">él|</w>
        </l>
      </lg>
    </body>
