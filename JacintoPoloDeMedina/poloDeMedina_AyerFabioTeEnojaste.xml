    <body>
      <head>
        <title> Ayer, Fabio, te enojaste</title>
      </head>
      <lg cert="high" type="Redondillas">
        <l met="-|2|3|-|-|-|7|-|" n="1">
          <!--Ayer, Fabio, te enojaste-->
          <w lemma="ayer" type="RG">a|yer|</w>
          <w lemma="fabio" type="NP00000">fa|bio|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="enojar" type="VMIS2S0">e|no|jas|te|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="2">
          <!--en cierta conversación,-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="cierto" type="DI0FS0">cier|ta|</w>
          <w lemma="conversación" type="NCFS000">con|ver|sa|ción|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="3">
          <!--y en mi ausencia fanfarrón,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="mi" type="DP1CSS">mi</w>
          <w lemma="ausencia" type="NCFS000">au|sen|cia|</w>
          <w lemma="fanfarrón" type="AQ0MS00">fan|fa|rrón|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="4">
          <!--de puerco me motejaste.-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="puerco" type="NCMS000">puer|co|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="motejar" type="VMIS2S0">mo|te|jas|te|</w>
        </l>
        <l met="1|2|-|-|-|-|7|-|" n="5">
          <!--Yo sé que cuando lo fuera,-->
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="saber" type="VMIP1S0">sé|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="cuando" type="CS">cuan|do|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="ser" type="VSSI3S0">fue|ra|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="6">
          <!--y contigo me encontrara,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="contigo" type="PP2CSO0">con|ti|go|</w>
          <w lemma="me" type="PP1CS00">me</w>
          <w lemma="encontrar" type="VMSI3S0">en|con|tra|ra|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="7">
          <!--ni tu mano me matara-->
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="mano" type="NCFS000">ma|no|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="matar" type="VMSI3S0">ma|ta|ra|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="8">
          <!--ni tu boca me comiera.-->
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="boca" type="NCFS000">bo|ca|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="comer" type="VMSI3S0">co|mie|ra|</w>
        </l>
      </lg>
    </body>
