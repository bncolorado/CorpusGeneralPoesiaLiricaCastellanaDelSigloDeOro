    <body>
      <head>
        <title>Que ha sido vuestra sangría</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="1|2|-|-|-|-|7|-|" n="1">
          <!--Que ha sido vuestra sangría-->
          <w lemma="que" type="CS">que</w>
          <w lemma="haber" type="VAIP3S0">ha|</w>
          <w lemma="ser" type="VSP00SM">si|do|</w>
          <w lemma="vuestro" type="DP2FSP">vues|tra|</w>
          <w lemma="sangría" type="NCFS000">san|grí|a|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="2">
          <!--acertada, dicen cuantos-->
          <w lemma="acertar" type="VMP00SF">a|cer|ta|da|</w>
          <w lemma="decir" type="VMIP3P0">di|cen|</w>
          <w lemma="cuanto" type="DI0MP0">cuan|tos|</w>
        </l>
        <l met="1|-|3|-|-|6|7|-|" n="3">
          <!--saben, Gil, que tenéis tantos-->
          <w lemma="saber" type="VMIP3P0">sa|ben|</w>
          <w lemma="gil" type="NP00000">gil|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="tener" type="VMIP2P0">te|néis|</w>
          <w lemma="tanto" type="DI0MP0">tan|tos|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="4">
          <!--pujamientos de poesía.-->
          <w lemma="pujamiento" type="NCMP000">pu|ja|mien|tos|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="poesía" type="NCFS000">poe|sí|a|</w>
        </l>
        <l met="-|2|3|-|5|-|7|-|" n="5">
          <!--Mas yo digo que es engaño;-->
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="decir" type="VMIP1S0">di|go|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="engaño" type="NCMS000">en|ga|ño|</w>
        </l>
        <l met="-|2|-|4|5|-|7|-|" n="6">
          <!--y afirmo no ha sido buena-->
          <w lemma="y" type="CC">y</w>
          <w lemma="afirmar" type="VMIP1S0">a|fir|mo|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="haber" type="VAIP3S0">ha|</w>
          <w lemma="ser" type="VSP00SM">si|do|</w>
          <w lemma="bueno" type="AQ0FS00">bue|na|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="7">
          <!--la sangría de esa vena,-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="sangría" type="NCFS000">san|gría|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="ese" type="DD0FS0">e|sa|</w>
          <w lemma="vena" type="NCFS000">ve|na|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="8">
          <!--si tenéis en otra el daño.-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="tener" type="VMIP2P0">te|néis|</w>
          <w lemma="en" type="SP">e|n</w>
          <w lemma="otro" type="DI0FS0">o|tra</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="daño" type="NCMS000">da|ño|</w>
        </l>
      </lg>
