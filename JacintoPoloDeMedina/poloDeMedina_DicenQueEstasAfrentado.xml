  <text>
    <body>
      <head>
        <title>Dicen que estás afrentado</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="1|-|-|4|-|-|7|-|" n="1">
          <!--Dicen que estás afrentado-->
          <w lemma="decir" type="VMIP3P0">di|cen|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="estar" type="VMIP2S0">es|tás|</w>
          <w lemma="afrentar" type="VMP00SM">a|fren|ta|do|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="2">
          <!--los que la giba te ven,-->
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="giba" type="NCFS000">gi|ba|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="ver" type="VMIP3P0">ven|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="3">
          <!--y algunos, Fabio, lo creen,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="alguno" type="PI0MP00">al|gu|nos|</w>
          <w lemma="fabio" type="NP00000">fa|bio|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="creer" type="VMIP3P0">cre|en|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="4">
          <!--porque siempre estás cargado.-->
          <w lemma="porque" type="CS">por|que|</w>
          <w lemma="siempre" type="RG">siem|pre</w>
          <w lemma="estar" type="VMIP2S0">es|tás|</w>
          <w lemma="cargar" type="VMP00SM">car|ga|do|</w>
        </l>
        <l met="1|2|-|4|-|-|7|-|" n="5">
          <!--Yo digo que eres pipote-->
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="decir" type="VMIP1S0">di|go|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="ser" type="VSIP2S0">e|res|</w>
          <w lemma="pipote" type="NCMS000">pi|po|te|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="6">
          <!--con alma, y hombre en brevete-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="alma" type="NCFS000">al|ma</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="hombre" type="NCMS000">hom|bre</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="brevete" type="NCMS000">bre|ve|te|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="7">
          <!--que en la espalda trae juanete,-->
          <w lemma="que" type="CS">que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="espalda" type="NCFS000">es|pal|da|</w>
          <w lemma="traer" type="VMIP3S0">trae|</w>
          <w lemma="juanete" type="NCMS000">jua|ne|te|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="8">
          <!--o cual soneto, estrambote.-->
          <w lemma="o" type="CC">o|</w>
          <w lemma="cual" type="PR0CS00">cual|</w>
          <w lemma="soneto" type="NCMS000">so|ne|to</w>
          <w lemma="estrambote" type="NCMS000">es|tram|bo|te|</w>
        </l>
      </lg>
    </body>
  </text>
