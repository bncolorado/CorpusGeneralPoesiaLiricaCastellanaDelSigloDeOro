    <body>
      <head>
        <title>Tu nariz, en calidad,</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="-|-|3|-|-|-|7|-|" n="1">
          <!--Tu nariz, en calidad,-->
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="nariz" type="NCFS000">na|riz|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="calidad" type="NCFS000">ca|li|dad|</w>
        </l>
        <l met="1|-|-|-|-|-|7|-|" n="2">
          <!--es, por su naturaleza,-->
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="naturaleza" type="NCFS000">na|tu|ra|le|za|</w>
        </l>
        <l met="1|-|-|-|-|-|7|-|" n="3">
          <!--símbolo de la largueza,-->
          <w lemma="símbolo" type="NCMS000">sím|bo|lo|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="largueza" type="NCFS000">lar|gue|za|</w>
        </l>
        <l met="1|-|-|-|-|-|7|-|" n="4">
          <!--cifra de la inmensidad.-->
          <w lemma="cifra" type="NCFS000">ci|fra|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="inmensidad" type="NCFS000">in|men|si|dad|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="5">
          <!--Primero que tú, Beatriz,-->
          <w lemma="primero" type="RG">pri|me|ro|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="tú" type="PP2CSN0">tú|</w>
          <w lemma="beatriz" type="NP00000">bea|triz|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="6">
          <!--sale siempre de tu casa;-->
          <w lemma="salir" type="VMIP3S0">sa|le|</w>
          <w lemma="siempre" type="RG">siem|pre|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="casa" type="NCFS000">ca|sa|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="7">
          <!--y tan adelante pasa,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="adelante" type="RG">a|de|lan|te|</w>
          <w lemma="pasar" type="VMIP3S0">pa|sa|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="8">
          <!--que ya pasa de nariz.-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="pasar" type="VMIP3S0">pa|sa|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="nariz" type="NCFS000">na|riz|</w>
        </l>
      </lg>
    </body>
