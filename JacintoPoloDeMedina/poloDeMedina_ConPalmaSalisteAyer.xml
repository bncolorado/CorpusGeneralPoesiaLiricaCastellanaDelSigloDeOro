  <text>
    <body>
      <head>
        <title>Con palma saliste ayer;</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="-|2|-|-|5|-|7|-|" n="1">
          <!--Con palma saliste ayer;-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="palma" type="NCFS000">pal|ma|</w>
          <w lemma="salir" type="VMIS2S0">sa|lis|te</w>
          <w lemma="ayer" type="RG">a|yer|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="2">
          <!--si es de victoria, se calla,-->
          <w lemma="si" type="CS">si</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="victoria" type="NCFS000">vic|to|ria|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="callar" type="VMIP3S0">ca|lla|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="3">
          <!--que quien nunca entró en batalla-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="quien" type="PR0CS00">quien|</w>
          <w lemma="nunca" type="RG">nun|ca</w>
          <w lemma="entrar" type="VMIS3S0">en|tró</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="batalla" type="NCFS000">ba|ta|lla|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="4">
          <!--mal podrá, capón, vencer.-->
          <w lemma="mal" type="RG">mal|</w>
          <w lemma="poder" type="VMIF3S0">po|drá|</w>
          <w lemma="capón" type="NCMS000">ca|pón|</w>
          <w lemma="vencer" type="VMN0000">ven|cer|</w>
        </l>
        <l met="1|2|-|4|-|-|7|-|" n="5">
          <!--Muy bien la palma te está;-->
          <w lemma="muy" type="RG">muy|</w>
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="palma" type="NCFS000">pal|ma|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="estar" type="VMIP3S0">es|tá|</w>
        </l>
        <l met="-|-|3|4|-|-|7|-|" n="6">
          <!--pero si es cosa notoria-->
          <w lemma="pero" type="CC">pe|ro|</w>
          <w lemma="si" type="CS">si</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="cosa" type="NCFS000">co|sa|</w>
          <w lemma="notorio" type="AQ0FS00">no|to|ria|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="7">
          <!--que no es palma de victoria,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="palma" type="NCFS000">pal|ma|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="victoria" type="NCFS000">vic|to|ria|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="8">
          <!--palma de virgen será.-->
          <w lemma="palma" type="NCFS000">pal|ma|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="virgen" type="NCCS000">vir|gen|</w>
          <w lemma="ser" type="VSIF3S0">se|rá|</w>
        </l>
      </lg>
    </body>
