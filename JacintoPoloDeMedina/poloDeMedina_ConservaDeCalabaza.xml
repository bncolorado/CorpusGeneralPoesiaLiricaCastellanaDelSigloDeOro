  <text>
    <body>
      <head>
        <title>Conserva de calabaza</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="-|2|-|-|-|-|7|-|" n="1">
          <!--Conserva de calabaza-->
          <w lemma="conservar" type="VMIP3S0">con|ser|va|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="calabaza" type="NCFS000">ca|la|ba|za|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="2">
          <!--os envío, que interpreta-->
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="envío" type="NCMS000">en|vío|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="interpretar" type="VMIP3S0">in|ter|pre|ta|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="3">
          <!--que tendréis, como poeta,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="tener" type="VMIF2P0">ten|dréis|</w>
          <w lemma="como" type="CS">co|mo|</w>
          <w lemma="poeta" type="NCMS000">po|e|ta|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="4">
          <!--cabeza a la misma traza.-->
          <w lemma="cabeza" type="NCCS000">ca|be|za</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mismo" type="AQ0FS00">mis|ma|</w>
          <w lemma="traza" type="NCFS000">tra|za|</w>
        </l>
        <l met="-|2|-|-|5|-|-|" n="5">
          <!--Comedla, pues yo la como;-->
          <w lemma="comer" type="VMM02P0">co|med|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="como" type="CS">co|mo|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="6">
          <!--y pues el caso la obliga,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="caso" type="NCMS000">ca|so|</w>
          <w lemma="lo" type="PP3FSA0">la</w>
          <w lemma="obligar" type="VMIP3S0">o|bli|ga|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="7">
          <!--memento, poeta, os diga,-->
          <w lemma="memento" type="NCMS000">me|men|to|</w>
          <w lemma="poeta" type="NCMS000">poe|ta|</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="decir" type="VMSP3S0">di|ga|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="8">
          <!--en vez de memento, homo.-->
          <w lemma="en_vez_de" type="SP">en_|vez_|de|</w>
          <w lemma="memento" type="NCMS000">me|men|to|</w>
          <w lemma="homo" type="AQ0CN00">ho|mo|</w>
        </l>
      </lg>
    </body>
