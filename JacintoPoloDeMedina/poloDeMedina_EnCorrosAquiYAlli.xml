  <text>
    <body>
      <head>
        <title>En corros aquí y allí,</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="-|2|-|-|5|-|7|-|" n="1">
          <!--En corros aquí y allí,-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="corro" type="NCMP000">co|rros|</w>
          <w lemma="aquí" type="RG">a|quí</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="allí" type="RG">a|llí|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="2">
          <!--Silvio, sin darte ocasión,-->
          <w lemma="silvio" type="NP00000">sil|vio|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="dar" type="VMN0000">dar|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="ocasión" type="NCFS000">o|ca|sión|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="3">
          <!--con malicia y sin razón-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="malicia" type="NCFS000">ma|li|cia</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="razón" type="NCFS000">ra|zón|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="4">
          <!--vas diciendo mal de mí.-->
          <w lemma="ir" type="VMIP2S0">vas|</w>
          <w lemma="decir" type="VMG0000">di|cien|do|</w>
          <w lemma="mal" type="RG">mal|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="5">
          <!--Y aunque esta falta imagino-->
          <w lemma="y" type="CC">y</w>
          <w lemma="aunque" type="CC">aun|que</w>
          <w lemma="este" type="DD0FS0">es|ta|</w>
          <w lemma="falta" type="NCFS000">fal|ta</w>
          <w lemma="imaginar" type="VMIP1S0">i|ma|gi|no|</w>
        </l>
        <l met="1|-|3|4|-|-|7|-|" n="6">
          <!--que es en ti muy natural,-->
          <w lemma="que" type="CS">que</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="ti" type="PP2CSO0">ti|</w>
          <w lemma="muy" type="RG">muy|</w>
          <w lemma="natural" type="AQ0CS00">na|tu|ral|</w>
        </l>
        <l met="1|2|-|-|5|6|7|-|" n="7">
          <!--no digas de mí más mal;-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="decir" type="VMSP2S0">di|gas|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="mal" type="AQ0MS00">mal|</w>
        </l>
        <l met="1|-|-|4|5|-|7|-|" n="8">
          <!--mira que no soy tocino.-->
          <w lemma="mirar" type="VMM02S0">mi|ra|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="ser" type="VSIP1S0">soy|</w>
          <w lemma="tocino" type="NCMS000">to|ci|no|</w>
        </l>
      </lg>
    </body>
  </text>
