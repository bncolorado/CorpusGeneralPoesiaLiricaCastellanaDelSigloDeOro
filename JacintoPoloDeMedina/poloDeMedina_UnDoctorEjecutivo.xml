  <text>
    <body>
      <head>
        <title>Un doctor ejecutivo,</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="1|-|3|-|-|-|7|-|" n="1">
          <!--Un doctor ejecutivo,-->
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="doctor" type="NCMS000">doc|tor|</w>
          <w lemma="ejecutivo" type="AQ0MS00">e|je|cu|ti|vo|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="2">
          <!--tan experto y liberal,-->
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="experto" type="AQ0MS00">ex|per|to</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="liberal" type="AQ0CS00">li|be|ral|</w>
        </l>
        <l met="-|-|-|-|-|-|7|-|" n="3">
          <!--que, como a lo racional,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="como" type="CS">co|mo</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="racional" type="NCMS000">ra|cio|nal|</w>
        </l>
        <l met="1|2|-|-|-|-|7|-|" n="4">
          <!--da muerte a lo sensitivo,-->
          <w lemma="dar" type="VMIP3S0">da|</w>
          <w lemma="muerte" type="NCFS000">muer|te</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="sensitivo" type="NCMS000">sen|si|ti|vo|</w>
        </l>
        <l met="-|-|3|4|-|-|7|-|" n="5">
          <!--disparó, diestro y activo-->
          <w lemma="disparar" type="VMIS3S0">dis|pa|ró|</w>
          <w lemma="diestro" type="AQ0MS00">dies|tro</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="activo" type="AQ0MS00">ac|ti|vo|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="6">
          <!--en matar y deshacer-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="matar" type="VMN0000">ma|tar|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="deshacer" type="VMN0000">de|sha|cer|</w>
        </l>
        <l met="1|-|3|4|5|-|7|-|" n="7">
          <!--a un conejo un tiro ayer;-->
          <w lemma="a" type="SP">a</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="conejo" type="NCMS000">co|ne|jo</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="tiro" type="NCMS000">ti|ro</w>
          <w lemma="ayer" type="RG">a|yer|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="8">
          <!--matole, porque se crea-->
          <w lemma="matole" type="VMSP3S0">ma|to|le|</w>
          <w lemma="porque" type="CS">por|que|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="creer" type="VMSP3S0">cre|a|</w>
        </l>
        <l met="1|2|-|-|-|-|7|-|" n="9">
          <!--que hay pólvora escamonea,-->
          <w lemma="que" type="CS">que</w>
          <w lemma="haber" type="VMIP3S0">hay|</w>
          <w lemma="pólvora" type="NCFS000">pól|vo|ra</w>
          <w lemma="escamonea" type="AQ0FS00">es|ca|mo|ne|a|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="10">
          <!--como escopeta clister.-->
          <w lemma="como" type="CS">co|mo</w>
          <w lemma="escopeta" type="NCFS000">es|co|pe|ta|</w>
          <w lemma="clister" type="NCMS000">clis|ter|</w>
        </l>
      </lg>
    </body>
