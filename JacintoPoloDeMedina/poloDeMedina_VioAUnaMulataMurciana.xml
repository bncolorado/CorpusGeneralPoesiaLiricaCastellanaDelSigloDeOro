  <text>
    <body>
      <head>
        <title>Vio a una mulata murciana,</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="1|-|-|4|-|-|7|-|" n="1">
          <!--Vio a una mulata murciana,-->
          <w lemma="ver" type="VMIS3S0">vio</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="mulato" type="NCFS000">mu|la|ta|</w>
          <w lemma="murciano" type="AQ0FS00">mur|cia|na|</w>
        </l>
        <l met="1|2|-|-|5|6|7|-|" n="2">
          <!--un hombre, asomado un día-->
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="hombre" type="NCMS000">hom|bre</w>
          <w lemma="asomar" type="VMP00SM">a|so|ma|do</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="día" type="NCMS000">dí|a|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="3">
          <!--a un esconce que servía-->
          <w lemma="a" type="SP">a</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="esconce" type="NCMS000">es|con|ce|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="servir" type="VMII3S0">ser|ví|a|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="4">
          <!--de chimenea y ventana.-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="chimenea" type="NCFS000">chi|me|nea|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="ventana" type="NCFS000">ven|ta|na|</w>
        </l>
        <l met="1|-|-|-|5|-|7|-|" n="5">
          <!--Ella se le queja, viendo-->
          <w lemma="él" type="PP3FS00">e|lla|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="quejar" type="VMIP3S0">que|ja|</w>
          <w lemma="ver" type="VMG0000">vien|do|</w>
        </l>
        <l met="1|-|3|4|-|-|7|-|" n="6">
          <!--que aun cuando es de él conocida-->
          <w lemma="que" type="CS">que</w>
          <w lemma="aun" type="RG">aun|</w>
          <w lemma="cuando" type="CS">cuan|do</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="él" type="PP3MS00">él|</w>
          <w lemma="conocer" type="VMP00SF">co|no|ci|da|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="7">
          <!--no le habla de corrida,-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="hablar" type="VMIP3S0">ha|bla|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="corrida" type="NCFS000">co|rri|da|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="8">
          <!--y él se disculpa diciendo:-->
          <w lemma="y" type="CC">y</w>
          <w lemma="él" type="PP3MS00">él|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="disculpar" type="VMIP3S0">dis|cul|pa|</w>
          <w lemma="decir" type="VMG0000">di|cien|do|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="9">
          <!--Que pase, mire y te vea-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="pasar" type="VMM03S0">pa|se|</w>
          <w lemma="mirar" type="VMM03S0">mi|re</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="ver" type="VMSP3S0">ve|a|</w>
        </l>
        <l met="-|-|3|4|5|-|7|-|" n="10">
          <!--sin hablar no es mucho, Clara,-->
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="hablar" type="VMN0000">ha|blar|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="mucho" type="RG">mu|cho|</w>
          <w lemma="clara" type="NP00000">cla|ra|</w>
        </l>
        <l met="-|-|3|4|-|-|7|-|" n="11">
          <!--que entendí que era tu cara-->
          <w lemma="que" type="CS">que</w>
          <w lemma="entender" type="VMIS1S0">en|ten|dí|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="ser" type="VSII3S0">e|ra|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="cara" type="NCFS000">ca|ra|</w>
        </l>
        <l met="1|-|-|4|-|-|-|8|-|" n="12">
          <!--humo de esa chimenea.-->
          <w lemma="humo" type="NCMS000">hu|mo|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="ese" type="DD0FS0">e|sa|</w>
          <w lemma="chimenea" type="NCFS000">chi|me|ne|a|</w>
        </l>
      </lg>
    </body>
