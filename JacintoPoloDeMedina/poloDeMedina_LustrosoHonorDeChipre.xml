    <body>
      <head>
        <title>Lustroso honor de Chipre,</title>
      </head>
      <lg cert="high" type="Silva">
        <l met="-|2|-|4|-|6|-|" n="1">
          <!--Lustroso honor de Chipre,-->
          <w lemma="lustroso" type="AQ0MS00">lus|tro|so</w>
          <w lemma="honor" type="NCMS000">ho|nor|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="chipre" type="NP00000">chi|pre|</w>
        </l>
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="2">
          <!--Aurora, que a una flor tu llanto quiere-->
          <w lemma="aurora" type="NP00000">au|ro|ra|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="flor" type="NCFS000">flor|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="llanto" type="NCMS000">llan|to|</w>
          <w lemma="querer" type="VMIP3S0">quie|re|</w>
        </l>
        <l met="-|-|-|4|-|6|-|8|-|10|-|" n="3">
          <!--amanecer segunda vez la vida,-->
          <w lemma="amanecer" type="VMN0000">a|ma|ne|cer|</w>
          <w lemma="2" type="AO0FS00">se|gun|da|</w>
          <w lemma="vez" type="NCFS000">vez|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
        </l>
        <l met="1|-|-|4|-|6|-|-|-|10|-|" n="4">
          <!--de un jabalí robada, que la hiere,-->
          <w lemma="de" type="SP">de</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="jabalí" type="NCMS000">ja|ba|lí|</w>
          <w lemma="robar" type="VMP00SF">ro|ba|da|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="herir" type="VMIP3S0">hie|re|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="5">
          <!--y tú se la suspiras en la boca-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="tú" type="PP2CSN0">tú|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="suspirar" type="VMIP2S0">sus|pi|ras|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="boca" type="NCFS000">bo|ca|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="6">
          <!--cerrándole la llaga con la toca,-->
          <w lemma="cerrar" type="VMG0000">ce|rran|do|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="llaga" type="NCFS000">lla|ga|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="toca" type="NCFS000">to|ca|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="7">
          <!--porque no se le ausente con la herida;-->
          <w lemma="porque" type="CS">por|que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="le" type="PP3CSD0">le</w>
          <w lemma="ausentar" type="VMSP3S0">au|sen|te|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="herida" type="NCFS000">he|ri|da|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="8">
          <!--en vano prevenida-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="vano" type="NCMS000">va|no|</w>
          <w lemma="prevenir" type="VMP00SF">pre|ve|ni|da|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="9">
          <!--contra el rigor celoso de la fiera-->
          <w lemma="contra" type="SP">con|tra</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="rigor" type="NCMS000">ri|gor|</w>
          <w lemma="celoso" type="AQ0MS00">ce|lo|so|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="fiera" type="NCFS000">fie|ra|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="10">
          <!--el alma le conmutas con tu aliento,-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="alma" type="NCFS000">al|ma|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="conmuta" type="NCFP000">con|mu|tas|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="tu" type="DP2CSS">tu</w>
          <w lemma="aliento" type="NCMS000">a|lien|to|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="11">
          <!--si en filigranas borda la ribera-->
          <w lemma="si" type="CS">si</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="filigrana" type="NCFP000">fi|li|gra|nas|</w>
          <w lemma="bordar" type="VMIP3S0">bor|da|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="ribera" type="NCFS000">ri|be|ra|</w>
        </l>
        <l met="-|-|-|4|-|-|-|8|-|10|-|" n="12">
          <!--desvanecida con humor sangriento-->
          <w lemma="desvanecer" type="VMP00SF">des|va|ne|ci|da|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="humor" type="NCMS000">hu|mor|</w>
          <w lemma="sangriento" type="AQ0MS00">san|grien|to|</w>
        </l>
        <l met="-|-|3|-|-|6|7|-|-|10|-|" n="13">
          <!--y manchó de coral todas las flores-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="manchar" type="VMIS3S0">man|chó|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="coral" type="NCCS000">co|ral|</w>
          <w lemma="todo" type="DI0FP0">to|das|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="flor" type="NCFP000">flo|res|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="14">
          <!--rotulando en las hojas sus amores;-->
          <w lemma="rotular" type="VMG0000">ro|tu|lan|do</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="hoja" type="NCFP000">ho|jas|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="amor" type="NCMP000">a|mo|res|</w>
        </l>
        <l met="-|-|-|4|-|6|-|8|-|10|-|" n="15">
          <!--de donde en flor la copia, el prado umbroso-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="donde" type="PR00000">don|de</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="flor" type="NCFS000">flor|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="copia" type="NCFS000">co|pia</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="prado" type="NCMS000">pra|do</w>
          <w lemma="umbroso" type="AQ0MS00">um|bro|so|</w>
        </l>
        <l met="1|-|-|4|-|-|-|8|-|10|-|" n="16">
          <!--pira de Adonis, monumento hojoso.-->
          <w lemma="pirar" type="VMIP3S0">pi|ra|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="adonis" type="NP00000">a|do|nis|</w>
          <w lemma="monumento" type="NCMS000">mo|nu|men|to</w>
          <w lemma="hojoso" type="AQ0MS00">ho|jo|so|</w>
        </l>
      </lg>
