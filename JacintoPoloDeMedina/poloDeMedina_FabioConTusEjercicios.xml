  <text>
    <body>
      <head>
        <title> Fabio, con tus ejercicios</title>
      </head>
      <lg cert="high" type="Epigrama">
        <l met="1|-|-|-|-|-|7|-|" n="1">
          <!--Fabio, con tus ejercicios-->
          <w lemma="fabio" type="NP00000">fa|bio|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="tu" type="DP2CPS">tus|</w>
          <w lemma="ejercicio" type="NCMP000">e|jer|ci|cios|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="2">
          <!--nos das a entender que puedes-->
          <w lemma="nos" type="PP1CP00">nos|</w>
          <w lemma="dar" type="VMIP2S0">das|</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="entender" type="VMN0000">en|ten|der|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="poder" type="VMIP2S0">pue|des|</w>
        </l>
        <l met="-|-|3|4|-|-|7|-|" n="3">
          <!--alcanzar muchas mercedes,-->
          <w lemma="alcanzar" type="VMN0000">al|can|zar|</w>
          <w lemma="mucho" type="DI0FP0">mu|chas|</w>
          <w lemma="merced" type="NCFP000">mer|ce|des|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="4">
          <!--pues vives de hacer servicios.-->
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="vivir" type="VMIP2S0">vi|ves|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="hacer" type="VMN0000">ha|cer|</w>
          <w lemma="servicio" type="NCMP000">ser|vi|cios|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="5">
          <!--Los reyes tendrás propicios-->
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="rey" type="NCMP000">re|yes|</w>
          <w lemma="tener" type="VMIF2S0">ten|drás|</w>
          <w lemma="propicio" type="AQ0MP00">pro|pi|cios|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="6">
          <!--y siempre muy satisfecho-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="siempre" type="RG">siem|pre|</w>
          <w lemma="muy" type="RG">muy|</w>
          <w lemma="satisfacer" type="VMP00SM">sa|tis|fe|cho|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="7">
          <!--podrá con justo derecho-->
          <w lemma="poder" type="VMIF3S0">po|drá|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="justo" type="AQ0MS00">jus|to|</w>
          <w lemma="derecho" type="NCMS000">de|re|cho|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="8">
          <!--alcanzar mercedes tales,-->
          <w lemma="alcanzar" type="VMN0000">al|can|zar|</w>
          <w lemma="merced" type="NCFP000">mer|ce|des|</w>
          <w lemma="tal" type="DD0CP0">ta|les|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="9">
          <!--quien con manos liberales-->
          <w lemma="quien" type="PR0CS00">quien|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="mano" type="NCFP000">ma|nos|</w>
          <w lemma="liberal" type="AQ0CP00">li|be|ra|les|</w>
        </l>
        <l met="1|-|-|4|-|6|7|-|" n="10">
          <!--tantos servicios ha hecho.-->
          <w lemma="tanto" type="DI0MP0">tan|tos|</w>
          <w lemma="servicio" type="NCMP000">ser|vi|cios|</w>
          <w lemma="haber" type="VAIP3S0">ha|</w>
          <w lemma="hacer" type="VMP00SM">he|cho|</w>
        </l>
      </lg>
    </body>
