          <w lemma="no" type="RN">no|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="presumir" type="VMSP3S0">pre|su|ma|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="7">
