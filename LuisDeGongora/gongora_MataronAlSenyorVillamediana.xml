  <text>
    <body>
      <head>
        <title>Mataron al señor Villamediana.</title>
      </head>
      <lg cert="high" type="Octavas">
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="1">
          <!--Mataron al señor Villamediana.-->
          <w lemma="matar" type="VMIS3P0">ma|ta|ron|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="señor" type="NCMS000">se|ñor|</w>
          <w lemma="villamediana" type="NP00000">vi|lla|me|dia|na|</w>
        </l>
        <l met="1|-|-|-|5|6|-|8|-|10|-|" n="2">
          <!--Dúdase con cuál arma fuese muerto:-->
          <w lemma="dudar" type="VMM02S0">du|da|</w>
          <w lemma="se" type="PP3CN00">se|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="cuál" type="DT0CS0">cuál|</w>
          <w lemma="arma" type="NCFS000">ar|ma|</w>
          <w lemma="ser" type="VSSI3S0">fue|se|</w>
          <w lemma="morir" type="VMP00SM">muer|to|</w>
        </l>
        <l met="1|2|-|-|5|6|-|-|-|10|-|" n="3">
          <!--quién dice que fue media partesana;-->
          <w lemma="quién" type="PT0CS00">quién|</w>
          <w lemma="decir" type="VMIP3S0">di|ce|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="ser" type="VSIS3S0">fue|</w>
          <w lemma="medio" type="AQ0FS00">me|dia|</w>
          <w lemma="partesana" type="NCFS000">par|te|sa|na|</w>
        </l>
        <l met="1|-|3|-|-|6|-|8|-|10|-|" n="4">
          <!--quién alfanje, de puro corvo tuerto;-->
          <w lemma="quién" type="PE0CS00">quién|</w>
          <w lemma="alfanje" type="NCMS000">al|fan|je|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="puro" type="AQ0MS00">pu|ro|</w>
          <w lemma="corvo" type="NCMS000">cor|vo|</w>
          <w lemma="tuerto" type="AQ0MS00">tuer|to|</w>
        </l>
        <l met="1|-|3|-|-|6|-|-|-|10|-|" n="5">
          <!--quién el golpe atribuye a Durindana,-->
          <w lemma="quién" type="PT0CS00">quién|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="golpe" type="NCMS000">gol|pe</w>
          <w lemma="atribuir" type="VMIP3S0">a|tri|bu|ye</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="durindana" type="NP00000">du|rin|da|na|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="6">
          <!--y en lo horrible tuviéralo por cierto,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA00S0">lo</w>
          <w lemma="horrible" type="AQ0CS00">ho|rri|ble|</w>
          <w lemma="tuviéralo" type="NCMS000">tu|vié|ra|lo|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="cierto" type="AQ0MS00">cier|to|</w>
        </l>
        <l met="-|2|3|4|-|6|-|-|-|10|-|" n="7">
          <!--a no haber un alcalde averiguado-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="haber" type="VMN0000">ha|ber|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="alcalde" type="NCMS000">al|cal|de</w>
          <w lemma="averiguar" type="VMP00SM">a|ve|ri|gua|do|</w>
        </l>
        <l met="-|-|3|-|-|6|-|8|-|10|-|" n="8">
          <!--que le dieron con un doctor Collado.-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="dar" type="VMIS3P0">die|ron|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="doctor" type="NCMS000">doc|tor|</w>
          <w lemma="collado" type="NP00000">co|lla|do|</w>
        </l>
      </lg>
    </body>
