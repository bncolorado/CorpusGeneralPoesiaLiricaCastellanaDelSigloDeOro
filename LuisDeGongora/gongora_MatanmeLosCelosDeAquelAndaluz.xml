    <body>
      <head>
        <title>Mátanme los celos de aquel andaluz:</title>
      </head>
      <lg cert="high" type="Seguidillas y canción">
        <l met="1|-|-|-|5|-|-|-|9|-|-|12|-|" n="1">
          <!--Mátanme los celos de aquel andaluz:-->
          <w lemma="mátanme" type="NP00000">má|tan|me|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="celo" type="NCMP000">ce|los|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="aquel" type="DD0MS0">a|quel|</w>
          <w lemma="andaluz" type="NCMS000">an|da|luz|</w>
        </l>
        <l met="1|-|-|-|-|6|-|-|-|10|-|-|13|-|" n="2">
          <!--hágame, si muriere,          la mortaja azul.-->
          <w lemma="hacer" type="VMM03S0">ha|ga|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="morir" type="VMSF1S0">mu|rie|re|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mortaja" type="NCFS000">mor|ta|ja|</w>
          <w lemma="azul" type="AQ0CS00">a|zul|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|9|-|-|12|-|" n="3">
          <!--Perdí la esperanza          de ver mi ausente:-->
          <w lemma="perder" type="VMIS1S0">per|dí|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="esperanza" type="NCFS000">es|pe|ran|za|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="ausente" type="NCCS000">au|sen|te|</w>
        </l>
        <l met="1|-|-|-|-|6|-|-|-|10|-|12|-|" n="4">
          <!--Háganme, si muriere,          la mortaja verde.-->
          <w lemma="hacer" type="VMM03P0">ha|gan|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="morir" type="VMSF1S0">mu|rie|re|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mortaja" type="NCFS000">mor|ta|ja|</w>
          <w lemma="verde" type="AQ0CS00">ver|de|</w>
        </l>
        <l met="1|-|-|4|5|-|7|8|-|10|-|" n="5">
          <!--Madre, sin ser monja,          soy ya descalza,-->
          <w lemma="madre" type="NP00000">ma|dre|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="monje" type="NCFS000">mon|ja|</w>
          <w lemma="ser" type="VSIP1S0">soy|</w>
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="descalzo" type="AQ0FS00">des|cal|za|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|-|11|-|" n="6">
          <!--pues me tiene la ausencia          sin mi Zapata.-->
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="tener" type="VMIP3S0">tie|ne|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="ausencia" type="NCFS000">au|sen|cia|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="zapata" type="NP00000">za|pa|ta|</w>
        </l>
        <l met="-|-|3|-|5|-|-|8|-|-|11|-|" n="7">
          <!--La mitad del alma          me lleva la mar;-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mitad" type="NCFS000">mi|tad|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="alma" type="NCFS000">al|ma|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="llevar" type="VMIP3S0">lle|va|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mar" type="NCCS000">mar|</w>
        </l>
        <l met="-|2|-|-|5|-|-|8|-|-|11|-|" n="8">
          <!--volved, galeritas,          por la otra mitad.-->
          <w lemma="volver" type="VMM02P0">vol|ved|</w>
          <w lemma="galera" type="NCFP00V">ga|le|ri|tas|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="otro" type="DI0FS0">o|tra|</w>
          <w lemma="mitad" type="NCFS000">mi|tad|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|-|10|-|" n="9">
          <!--Muera yo en tu playa,          Nápoles bella,-->
          <w lemma="muera" type="NCFS000">mue|ra|</w>
          <w lemma="yo" type="PP1CSN0">yo</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="playa" type="NCFS000">pla|ya|</w>
          <w lemma="nápoles" type="NP00000">ná|po|les|</w>
          <w lemma="bello" type="AQ0FS00">be|lla|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|-|10|-|" n="10">
          <!--y serás sepulcro          de otra sirena.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="ser" type="VSIF2S0">se|rás|</w>
          <w lemma="sepulcro" type="NCMS000">se|pul|cro|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="otro" type="DI0FS0">o|tra|</w>
          <w lemma="sirena" type="NCFS000">si|re|na|</w>
        </l>
        <l met="1|-|-|-|5|-|7|-|-|10|-|" n="11">
          <!--Pídenme que cante,          canto forzada;-->
          <w lemma="pídenme" type="NP00000">pí|den|me|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="cantar" type="VMSP3S0">can|te|</w>
          <w lemma="canto" type="NCMS000">can|to|</w>
          <w lemma="forzar" type="VMP00SF">for|za|da|</w>
        </l>
        <l met="1|-|3|-|-|-|-|8|-|-|11|-|" n="12">
          <!--¡quién lo fuera vuestro,          galeras de España!-->
          <w lemma="quién" type="PT0CS00">quién|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="ir" type="VMSI1S0">fue|ra|</w>
          <w lemma="vuestro" type="DP2MSP">vues|tro|</w>
          <w lemma="galera" type="NCFP000">ga|le|ras|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="españa" type="NP00000">es|pa|ña|</w>
        </l>
        <l met="-|-|3|-|5|-|-|-|-|10|-|" n="13">
          <!--Mientras hago treguas          con mi dolor,-->
          <w lemma="mientras" type="CS">mien|tras|</w>
          <w lemma="hacer" type="VMIP1S0">ha|go|</w>
          <w lemma="tregua" type="NCFP000">tre|guas|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="dolor" type="NCMS000">do|lor|</w>
        </l>
        <l met="-|-|3|-|-|6|-|8|-|-|11|-|" n="14">
          <!--si descansan los ojos,          llore la voz.-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="descansar" type="VMIP3P0">des|can|san|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="ojo" type="NCMP000">o|jos|</w>
          <w lemma="llorar" type="VMSP3S0">llo|re|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="voz" type="NCFS000">voz|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="15">
          <!--Ausente de mi vida,-->
          <w lemma="ausente" type="NCCS000">au|sen|te|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
        </l>
        <l met="1|2|-|4|-|6|-|" n="16">
          <!--tú en agua, yo navego-->
          <w lemma="tú" type="PP2CSN0">tú</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="agua" type="NCCS000">a|gua|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="navegar" type="VMIP1S0">na|ve|go|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="17">
          <!--en lágrimas de fuego-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="lágrima" type="NCFP000">lá|gri|mas|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="fuego" type="NCMS000">fue|go|</w>
        </l>
        <l met="-|-|-|-|-|6|-|" n="18">
          <!--después de tu partida.-->
          <w lemma="después_de" type="SP">des|pués_|de|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="partida" type="NCFS000">par|ti|da|</w>
        </l>
        <l met="1|-|-|4|-|6|-|" n="19">
          <!--Esta mi voz perdida-->
          <w lemma="este" type="DD0FS0">es|ta|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="voz" type="NCFS000">voz|</w>
          <w lemma="perder" type="VMP00SF">per|di|da|</w>
        </l>
        <l met="1|-|-|-|-|6|-|8|-|10|-|" n="20">
          <!--dulce te seguirá, pues dulce vuela;-->
          <w lemma="dulce" type="NCMS000">dul|ce|</w>
          <w lemma="dulce" type="NCMS000">dul|ce|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="seguir" type="VMIF3S0">se|gui|rá|</w>
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="dulce" type="NCMS000">dul|ce|</w>
          <w lemma="dulce" type="NCMS000">dul|ce|</w>
          <w lemma="volar" type="VMIP3S0">vue|la|</w>
        </l>
        <l met="-|2|-|4|-|-|-|8|-|10|-|" n="21">
          <!--suspiros no, que abrasarán tu vela.-->
          <w lemma="suspiro" type="NCMP000">sus|pi|ros|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="abrasar" type="VMIF3P0">a|bra|sa|rán|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="vela" type="NCFS000">ve|la|</w>
        </l>
        <l met="1|-|-|4|-|6|-|" n="22">
          <!--No de tu media luna-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="medio" type="AQ0FS00">me|dia|</w>
          <w lemma="luna" type="NCFS000">lu|na|</w>
        </l>
        <l met="1|2|-|4|-|6|-|" n="23">
          <!--ha sido, Amor, flechada-->
          <w lemma="haber" type="VAIP3S0">ha|</w>
          <w lemma="ser" type="VSP00SM">si|do</w>
          <w lemma="amor" type="NP00000">a|mor|</w>
          <w lemma="flechar" type="VMP00SF">fle|cha|da|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="24">
          <!--saeta más alada-->
          <w lemma="saeta" type="NCFS000">sa|e|ta|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="alado" type="AQ0FS00">a|la|da|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="25">
          <!--que la ausencia importuna.-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="ausencia" type="NCFS000">au|sen|cia</w>
          <w lemma="importuno" type="AQ0FS00">im|por|tu|na|</w>
        </l>
        <l met="-|2|-|4|5|6|-|" n="26">
          <!--Defensa hay sola una-->
          <w lemma="defensa" type="NP00000">de|fen|sa|</w>
          <w lemma="haber" type="VMIP3S0">hay|</w>
          <w lemma="solo" type="AQ0FS00">so|la</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
        </l>
        <l met="-|-|-|-|-|6|-|8|-|10|-|" n="27">
          <!--contra su penetrante vuelo, y esa-->
          <w lemma="contra" type="SP">con|tra|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="penetrante" type="AQ0CS00">pe|ne|tran|te|</w>
          <w lemma="vuelo" type="NCMS000">vue|lo</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="ese" type="DD0FS0">e|sa|</w>
        </l>
        <l met="-|2|3|4|-|6|-|8|-|10|-|" n="28">
          <!--el duro es mármol de una breve huesa.-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="duro" type="NCMS000">du|ro</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="mármol" type="NCMS000">már|mol|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="breve" type="AQ0CS00">bre|ve|</w>
          <w lemma="huesa" type="NCFS000">hue|sa|</w>
        </l>
