  <text>
    <body>
      <head>
        <title>De la florida falda</title>
      </head>
      <lg cert="high" type="Canción">
        <l met="-|-|-|4|-|6|-|" n="1">
          <!--De la florida falda-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="florido" type="AQ0FS00">flo|ri|da|</w>
          <w lemma="falda" type="NCFS000">fal|da|</w>
        </l>
        <l met="1|-|3|-|-|6|7|-|-|10|-|" n="2">
          <!--que hoy de perlas bordó la alba luciente,-->
          <w lemma="que" type="CS">que</w>
          <w lemma="hoy" type="RG">hoy|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="perla" type="NCFP000">pe|rlas|</w>
          <w lemma="bordar" type="VMIS3S0">bor|dó|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="alba" type="NCFS000">al|ba|</w>
          <w lemma="luciente" type="AQ0CS00">lu|cien|te|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="3">
          <!--tejidos en guirnalda-->
          <w lemma="tejido" type="NCMP000">te|ji|dos|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="guirnalda" type="NCFS000">guir|nal|da|</w>
        </l>
        <l met="-|2|3|-|-|6|-|-|-|10|-|" n="4">
          <!--traslado estos jazmines a tu frente,-->
          <w lemma="trasladar" type="VMIP1S0">tras|la|do</w>
          <w lemma="este" type="DD0MP0">es|tos|</w>
          <w lemma="jazmín" type="NCMP000">jaz|mi|nes|</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="frente" type="NCCS000">fren|te|</w>
        </l>
        <l met="-|2|-|-|5|6|-|" n="5">
          <!--que piden, con ser flores,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="pedir" type="VMIP3P0">pi|den|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="flor" type="NCFP000">flo|res|</w>
        </l>
        <l met="1|-|-|4|-|-|-|8|-|10|-|" n="6">
          <!--blanco a tus sienes y a tu boca olores.-->
          <w lemma="blanco" type="AQ0MS00">blan|co</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="tu" type="DP2CPS">tus|</w>
          <w lemma="sien" type="NCFP000">sie|nes|</w>
          <w lemma="y" type="CC">y</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="boca" type="NCFS000">bo|ca</w>
          <w lemma="olor" type="NCMP000">o|lo|res|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="7">
          <!--Guarda de estos jazmines-->
          <w lemma="guarda" type="NCMS000">guar|da|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="este" type="DD0MP0">es|tos|</w>
          <w lemma="jazmín" type="NCMP000">jaz|mi|nes|</w>
        </l>
        <l met="-|2|-|4|5|-|-|8|-|10|-|" n="8">
          <!--de abejas era un escuadrón volante,-->
          <w lemma="de" type="SP">de</w>
          <w lemma="abeja" type="NCFP000">a|be|jas|</w>
          <w lemma="ser" type="VSII3S0">e|ra</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="escuadrón" type="NCMS000">es|cua|drón|</w>
          <w lemma="volante" type="NCMS000">vo|lan|te|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="9">
          <!--ronco, sí, de clarines,-->
          <w lemma="ronco" type="AQ0MS00">ron|co|</w>
          <w lemma="sí" type="RG">sí|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="clarín" type="NCMP000">cla|ri|nes|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="10">
          <!--mas de puntas armado de diamante;-->
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="punta" type="NCFP000">pun|tas|</w>
          <w lemma="armar" type="VMP00SM">ar|ma|do|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="diamante" type="NCMS000">dia|man|te|</w>
        </l>
        <l met="1|-|-|-|-|6|-|" n="11">
          <!--púselas en huida,-->
          <w lemma="poner" type="VMIS1S0">pú|se|</w>
	  <w lemma="lo" type="PP3FPA0">las|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="huida" type="NCFS000">hu|i|da|</w>
        </l>
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="12">
          <!--y cada flor me cuesta una herida.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="cada" type="DI0CS0">ca|da|</w>
          <w lemma="flor" type="NCFS000">flor|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="costar" type="VMIP3S0">cues|ta|</w>
          <w lemma="uno" type="DI0FS0">u|na</w>
          <w lemma="herida" type="NCFS000">he|ri|da|</w>
        </l>
        <l met="1|2|-|4|-|6|-|" n="13">
          <!--Más, Clori, que he tejido-->
          <w lemma="más" type="RG">más|</w>
          <w lemma="clori" type="NP00000">clo|ri|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="haber" type="VAIP1S0">he|</w>
          <w lemma="tejer" type="VMP00SM">te|ji|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="14">
          <!--jazmines al cabello desatado,-->
          <w lemma="jazmín" type="NCMP000">jaz|mi|nes|</w>
          <w lemma="a" type="SP">a</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cabello" type="NCMS000">ca|be|llo|</w>
          <w lemma="desatar" type="VMP00SM">de|sa|ta|do|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="15">
          <!--y más besos te pido-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="beso" type="NCMP000">be|sos|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="pedir" type="VMIP1S0">pi|do|</w>
        </l>
        <l met="-|2|-|4|-|-|-|8|-|10|-|" n="16">
          <!--que abejas tuvo el escuadrón armado;-->
          <w lemma="que" type="CS">que</w>
          <w lemma="abeja" type="NCFP000">a|be|jas|</w>
          <w lemma="tener" type="VMIS3S0">tu|vo</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="escuadrón" type="NCMS000">es|cua|drón|</w>
          <w lemma="armar" type="VMP00SM">ar|ma|do|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="17">
          <!--lisonjas son iguales-->
          <w lemma="lisonja" type="NCFP000">li|son|jas|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="igual" type="AQ0CP00">i|gua|les|</w>
        </l>
        <l met="-|2|3|4|-|-|7|8|-|10|-|" n="18">
          <!--servir yo en flores, pagar tú en panales.-->
          <w lemma="servir" type="VMN0000">ser|vir|</w>
          <w lemma="yo" type="PP1CSN0">yo</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="flor" type="NCFP000">flo|res|</w>
          <w lemma="pagar" type="VMN0000">pa|gar|</w>
          <w lemma="tú" type="PP2CSN0">tú</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="panal" type="NCMP000">pa|na|les|</w>
        </l>
      </lg>
    </body>
  </text>
