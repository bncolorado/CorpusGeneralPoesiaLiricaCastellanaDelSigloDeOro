  <text>
    <body>
      <head>
        <title>La bella Lira muda yace ahora</title>
      </head>
      <lg cert="high" type="Madrigal">
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="1">
          <!--La bella Lira muda yace ahora-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="bello" type="AQ0FS00">be|lla|</w>
          <w lemma="lira" type="NP00000">li|ra|</w>
          <w lemma="mudo" type="AQ0FS00">mu|da|</w>
          <w lemma="yacer" type="VMIP3S0">ya|ce</w>
          <w lemma="ahora" type="RG">a|ho|ra|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="2">
          <!--debajo de este mármol, que sin duda-->
          <w lemma="debajo_de" type="SP">de|ba|jo_|de</w>
          <w lemma="este" type="DD0MS0">es|te|</w>
          <w lemma="mármol" type="NCMS000">már|mol|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="duda" type="NCFS000">du|da|</w>
        </l>
        <l met="1|-|-|4|-|6|-|" n="3">
          <!--lo ha convocado muda-->
          <w lemma="lo" type="PP3MSA0">lo</w>
          <w lemma="haber" type="VAIP3S0">ha|</w>
          <w lemma="convocar" type="VMP00SM">con|vo|ca|do|</w>
          <w lemma="mudo" type="AQ0FS00">mu|da|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="4">
          <!--como solía canora.-->
          <w lemma="como" type="CS">co|mo|</w>
          <w lemma="soler" type="VMII3S0">so|lía|</w>
          <w lemma="canoro" type="AQ0FS00">ca|no|ra|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="5">
          <!--Si el Tajo arenas dora,-->
          <w lemma="si" type="CS">si</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="tajo" type="NP00000">ta|jo</w>
          <w lemma="arena" type="NCFP000">a|re|nas|</w>
          <w lemma="dorar" type="VMIP3S0">do|ra|</w>
        </l>
        <l met="-|2|-|4|-|6|-|-|-|10|-|" n="6">
          <!--ilustre piedras: culto monumento-->
          <w lemma="ilustre" type="AQ0CS00">i|lus|tre|</w>
          <w lemma="piedra" type="NCFP000">pie|dras|</w>
          <w lemma="culto" type="AQ0MS00">cul|to|</w>
          <w lemma="monumento" type="NCMS000">mo|nu|men|to|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="7">
          <!--a este de las Musas instrumento.-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="este" type="DD0MS0">es|te|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="musas" type="NP00000">mu|sas|</w>
          <w lemma="instrumento" type="NCMS000">ins|tru|men|to|</w>
        </l>
      </lg>
    </body>
  </text>
