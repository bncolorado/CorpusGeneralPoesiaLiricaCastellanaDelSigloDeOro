  <text>
    <body>
      <head>
        <title>¿Qué haré, que por quereros</title>
      </head>
      <lg cert="high" type="Canción">
        <l met="1|-|3|-|-|-|7|-|" n="1">
          <!--¿Qué haré, que por quereros-->
          <w lemma="qué" type="PT00000">qué|</w>
          <w lemma="hacer" type="VMIF1S0">ha|ré|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="querer" type="VMN0000">que|rer|</w>
          <w lemma="os" type="PP2CP00">os|</w>
        </l>
        <l met="-|-|3|-|5|6|7|-|" n="2">
          <!--mis estremos son tan claros-->
          <w lemma="mi" type="DP1CPS">mis|</w>
          <w lemma="estremos" type="NCMP000">es|tre|mos|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="claro" type="AQ0MP00">cla|ros|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="3">
          <!--que ni soy para miraros-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="ni" type="RN">ni|</w>
          <w lemma="ser" type="VSIP1S0">soy|</w>
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="mirar" type="VMN0000">mi|rar|</w>
          <w lemma="os" type="PP2CP00">os|</w>
        </l>
        <l met="1|2|-|-|5|-|7|-|" n="4">
          <!--ni puedo dexar de veros?-->
          <w lemma="ni" type="RN">ni|</w>
          <w lemma="poder" type="VMIP1S0">pue|do|</w>
          <w lemma="dexar" type="VMN0000">de|xar|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
          <w lemma="os" type="PP2CP00">os|</w>
        </l>
        <l met="1|2|3|-|-|-|7|-|" n="5">
          <!--Yo no sé con vuestra ausencia-->
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="saber" type="VMIP1S0">sé|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="vuestro" type="DP2FSP">vues|tra</w>
          <w lemma="ausencia" type="NCFS000">au|sen|cia|</w>
        </l>
        <l met="1|2|-|-|5|-|7|-|" n="6">
          <!--un punto bivir ausente,-->
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="punto" type="NCMS000">pun|to|</w>
          <w lemma="bivir" type="VMN0000">bi|vir|</w>
          <w lemma="ausente" type="NCCS000">au|sen|te|</w>
        </l>
        <l met="1|2|-|-|5|-|7|-|" n="7">
          <!--ni puedo sufrir presente,-->
          <w lemma="ni" type="RN">ni|</w>
          <w lemma="poder" type="VMIP1S0">pue|do|</w>
          <w lemma="sufrir" type="VMN0000">su|frir|</w>
          <w lemma="presente" type="AQ0CS00">pre|sen|te|</w>
        </l>
        <l met="-|2|-|4|5|-|7|-|" n="8">
          <!--señora, tan gran presencia.-->
          <w lemma="señor" type="NCFS000">se|ño|ra|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="gran" type="AQ0CS00">gran|</w>
          <w lemma="presencia" type="NCFS000">pre|sen|cia|</w>
        </l>
        <l met="-|-|-|-|-|-|7|-|" n="9">
          <!--De suerte que, por quereros,-->
          <w lemma="de_suerte_que" type="CS">de_|suer|te_|que|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="querer" type="VMN0000">que|rer|</w>
          <w lemma="os" type="PP2CP00">os|</w>
        </l>
        <l met="-|-|3|-|5|6|7|-|" n="10">
          <!--mis estremos son tan claros-->
          <w lemma="mi" type="DP1CPS">mis|</w>
          <w lemma="estremos" type="NCMP000">es|tre|mos|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="claro" type="AQ0MP00">cla|ros|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="11">
          <!--que ni soy para miraros-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="ni" type="RN">ni|</w>
          <w lemma="ser" type="VSIP1S0">soy|</w>
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="mirar" type="VMN0000">mi|rar|</w>
          <w lemma="os" type="PP2CP00">os|</w>
        </l>
        <l met="1|2|-|-|5|-|7|-|" n="12">
          <!--ni puedo dexar de veros.-->
          <w lemma="ni" type="RN">ni|</w>
          <w lemma="poder" type="VMIP1S0">pue|do|</w>
          <w lemma="dexar" type="VMN0000">de|xar|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
          <w lemma="os" type="PP2CP00">os|</w>
        </l>
      </lg>
