    <body>
      <head>
        <title>Si en alta mar, Licino,</title>
      </head>
      <lg cert="high" type="Sextetos-lira">
        <l met="-|2|-|4|-|6|-|" n="1">
          <!--Si en alta mar, Licino,-->
          <w lemma="si" type="CS">si</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="alto" type="AQ0FS00">al|ta|</w>
          <w lemma="mar" type="NCCS000">mar|</w>
          <w lemma="licino" type="NP00000">li|ci|no|</w>
        </l>
        <l met="1|-|-|4|-|6|-|-|-|10|-|" n="2">
          <!--no te engolfares mucho, ni temiendo-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="engolfar" type="VMSF2S0">en|gol|fa|res|</w>
          <w lemma="mucho" type="RG">mu|cho|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="temer" type="VMG0000">te|mien|do|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="3">
          <!--la tormenta, el camino-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="tormenta" type="NCFS000">tor|men|ta</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="camino" type="NCMS000">ca|mi|no|</w>
        </l>
        <l met="-|2|-|4|-|6|-|-|-|10|-|" n="4">
          <!--te fueres costa a costa prosiguiendo,-->
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="ir" type="VMSF2S0">fue|res|</w>
          <w lemma="costa" type="NCFS000">cos|ta</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="costa" type="NCFS000">cos|ta</w>
          <w lemma="proseguir" type="VMG0000">pro|si|guien|do|</w>
        </l>
        <l met="-|-|-|-|5|6|-|" n="5">
          <!--entre la demás gente-->
          <w lemma="entre" type="SP">en|tre|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="demás" type="DI0CP0">de|más|</w>
