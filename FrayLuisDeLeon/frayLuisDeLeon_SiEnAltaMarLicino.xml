          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="combatir" type="VMP00SM">com|ba|ti|do|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="24">
