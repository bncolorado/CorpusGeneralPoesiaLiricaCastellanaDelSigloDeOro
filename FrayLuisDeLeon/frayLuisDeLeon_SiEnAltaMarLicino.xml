          <w lemma="monte" type="NCMP000">mon|tes|</w>
          <w lemma="ensalzar" type="VMP00PM">en|sal|za|dos|</w>
        </l>
        <l met="1|2|-|-|-|6|-|8|-|10|-|" n="18">
