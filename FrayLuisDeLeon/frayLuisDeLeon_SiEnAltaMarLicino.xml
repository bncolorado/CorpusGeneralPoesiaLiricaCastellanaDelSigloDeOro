          <w lemma="con" type="SP">con|</w>
          <w lemma="lluvia" type="NCFS000">llu|via</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="noche" type="NCFS000">no|che</w>
          <w lemma="escura" type="NCFS000">es|cu|ra|</w>
        </l>
        <l met="-|2|-|-|-|6|7|-|-|10|-|" n="26">
          <!--si el cielo se escurece, él se serena;-->
