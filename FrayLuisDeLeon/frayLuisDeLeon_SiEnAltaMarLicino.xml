          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="mostrar" type="VMIP3S0">mues|tra|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="animoso" type="AQ0MS00">a|ni|mo|so</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="fuerte" type="AQ0CS00">fuer|te|</w>
          <w lemma="pecho" type="NCMS000">pe|cho|</w>
