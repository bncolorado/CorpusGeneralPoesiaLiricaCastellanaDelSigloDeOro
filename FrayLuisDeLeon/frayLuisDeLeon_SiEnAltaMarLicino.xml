          <w lemma="del" type="SP">del|</w>
          <w lemma="palacio" type="NCMS000">pa|la|cio|</w>
          <w lemma="real" type="AQ0CS00">re|al|</w>
          <w lemma="rico" type="AQ0MS00">ri|co</w>
          <w lemma="envidiar" type="VMP00SM">en|vi|dia|do|</w>
        </l>
        <l met="-|2|3|4|-|6|-|" n="13">
