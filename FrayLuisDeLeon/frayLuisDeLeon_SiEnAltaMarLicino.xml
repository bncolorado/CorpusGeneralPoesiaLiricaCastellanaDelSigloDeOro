          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cielo" type="NCMS000">cie|lo|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="escurece" type="VMIP3S0">es|cu|re|ce</w>
          <w lemma="él" type="PP3MS00">él|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="serenar" type="VMIP3S0">se|re|na|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="27">
          <!--no, si falta ventura-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="falta" type="NCFS000">fal|ta|</w>
          <w lemma="venturo" type="AQ0FS00">ven|tu|ra|</w>
        </l>
        <l met="-|2|3|-|-|6|7|-|-|10|-|" n="28">
