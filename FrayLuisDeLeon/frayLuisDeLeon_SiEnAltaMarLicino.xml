          <w lemma="aágora" type="NCFS000">a|go|ra</w>
          <w lemma="haber" type="VMIP3S0">ha|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="durar" type="VMN0000">du|rar|</w>
