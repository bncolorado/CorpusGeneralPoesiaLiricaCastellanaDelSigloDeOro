          <w lemma="más" type="RG">más|</w>
          <w lemma="vez" type="NCFP000">ve|ces|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="rayo" type="NCMP000">ra|yos|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="tocar" type="VMP00PM">to|ca|dos|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="19">
          <!--En los casos aviesos-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="caso" type="NCMP000">ca|sos|</w>
          <w lemma="avieso" type="AQ0MP00">a|vie|sos|</w>
        </l>
        <l met="1|2|-|-|-|6|-|-|-|10|-|" n="20">
