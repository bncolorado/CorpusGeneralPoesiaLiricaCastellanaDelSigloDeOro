  <text>
    <body>
      <head>
        <title>HORACIO</title>
      </head>
      <lg cert="high" type="Liras">
        <l met="-|2|-|" n="1">
          <!--HORACIO-->
          <w lemma="horacio" type="NP00000">ho|ra|cio|</w>
        </l>
        <l met="-|-|-|-|-|6|-|" n="2">
          <!--Mientras que te agradaba,-->
          <w lemma="mientras_que" type="CS">mien|tras_|que|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="agradar" type="VMII3S0">a|gra|da|ba|</w>
        </l>
        <l met="-|-|-|-|-|6|-|8|-|10|-|" n="3">
          <!--y mientras que ninguno más dichoso-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="mientras_que" type="CS">mien|tras_|que|</w>
          <w lemma="ninguno" type="PI0MS00">nin|gu|no|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="dichoso" type="AQ0MS00">di|cho|so|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="4">
          <!--los brazos añudaba-->
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="brazo" type="NCMP000">bra|zos|</w>
          <w lemma="añudaba" type="VMII3S0">a|ñu|da|ba|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="5">
          <!--al blanco cuello hermoso,-->
          <w lemma="al" type="SP">al|</w>
          <w lemma="blanco" type="AQ0MS00">blan|co|</w>
          <w lemma="cuello" type="NCMS000">cue|llo</w>
          <w lemma="hermoso" type="AQ0MS00">her|mo|so|</w>
        </l>
        <l met="1|-|-|4|-|6|7|-|-|10|-|" n="6">
          <!--más que el persiano Rey fui venturoso.-->
          <w lemma="más" type="RG">más|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="persiano" type="NCMS000">per|sia|no|</w>
          <w lemma="rey" type="NP00000">rey|</w>
          <w lemma="ser" type="VSIS1S0">fui|</w>
          <w lemma="venturoso" type="AQ0MS00">ven|tu|ro|so|</w>
        </l>
        <l met="1|-|" n="7">
          <!--LYDIA-->
          <w lemma="lydia" type="NP00000">ly|dia|</w>
        </l>
        <l met="-|2|-|-|5|6|-|" n="8">
          <!--Y yo mientras no amaste-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="mientras" type="CS">mien|tras|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="amar" type="VMIS2S0">a|mas|te|</w>
        </l>
        <l met="-|2|-|4|-|6|-|-|-|10|-|" n="9">
          <!--a otra más que a mí, ni, desechada,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="otro" type="PI0FS00">o|tra|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="desechar" type="VMP00SF">de|se|cha|da|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="10">
          <!--por Cloe me dejaste,-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="cloe" type="NP00000">clo|e|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="dejar" type="VMIS2S0">de|jas|te|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="11">
          <!--de todos alabada,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="todo" type="PI0MP00">to|dos|</w>
          <w lemma="alabar" type="VMP00SF">a|la|ba|da|</w>
        </l>
        <l met="-|2|3|-|5|-|-|-|9|-|" n="12">
          <!--y más fue que la Ilia celebrada.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="ser" type="VSIS3S0">fue|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="ilia" type="NP00000">i|lia|</w>
          <w lemma="celebrar" type="VMP00SF">ce|le|bra|da|</w>
        </l>
        <l met="-|2|-|" n="13">
          <!--HORACIO-->
          <w lemma="horacio" type="NP00000">ho|ra|cio|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="14">
          <!--A mí me manda agora-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="mandar" type="VMIP3S0">man|da</w>
          <w lemma="aágora" type="NCFS000">a|go|ra|</w>
        </l>
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="15">
          <!--la Cloe, que canta y tañe dulcemente-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="cloe" type="NP00000">cloe|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="cantar" type="VMIP3S0">can|ta</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="tañer" type="VMIP3S0">ta|ñe|</w>
          <w lemma="dulcemente" type="RG">dul|ce|men|te|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="16">
          <!--la vihuela sonora;-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="vihuela" type="NCFS000">vihue|la|</w>
          <w lemma="sonoro" type="AQ0FS00">so|no|ra|</w>
        </l>
        <l met="-|-|-|-|-|6|-|" n="17">
          <!--y porque se acreciente-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="porque" type="CS">por|que|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="acrecentar" type="VMSP3S0">a|cre|cien|te|</w>
        </l>
        <l met="-|2|-|-|-|6|7|8|-|10|-|" n="18">
          <!--su vida, moriré yo alegremente.-->
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
          <w lemma="morir" type="VMIF1S0">mo|ri|ré|</w>
          <w lemma="yo" type="PP1CSN0">yo</w>
          <w lemma="alegremente" type="RG">a|le|gre|men|te|</w>
        </l>
        <l met="1|-|" n="19">
          <!--LYDIA-->
          <w lemma="lydia" type="NP00000">ly|dia|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="20">
          <!--Y yo con inflamado-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="inflamar" type="VMP00SM">in|fla|ma|do|</w>
        </l>
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="21">
          <!--amor al Calais quiero y soy querida;-->
          <w lemma="amor" type="NCMS000">a|mor|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="calais" type="NP00000">ca|lais|</w>
          <w lemma="querer" type="VMIP1S0">quie|ro</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="ser" type="VSIP1S0">soy|</w>
          <w lemma="querer" type="VMP00SF">que|ri|da|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="22">
          <!--y si el benigno hado-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="si" type="CS">si</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="benigno" type="AQ0MS00">be|nig|no|</w>
          <w lemma="hado" type="NCMS000">ha|do|</w>
        </l>
        <l met="-|2|3|4|-|6|-|" n="23">
          <!--le da más larga vida,-->
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="dar" type="VMIP3S0">da|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="largo" type="AQ0FS00">lar|ga|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
        </l>
        <l met="-|2|-|4|5|-|7|-|9|-|" n="24">
          <!--la mía daré yo por bien perdida.-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mío" type="AP0FS1S">mía|</w>
          <w lemma="dar" type="VMIF1S0">da|ré|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="perder" type="VMP00SF">per|di|da|</w>
        </l>
        <l met="-|2|-|" n="25">
          <!--HORACIO-->
          <w lemma="horacio" type="NP00000">ho|ra|cio|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="26">
          <!--Mas ¿qué, si torna al juego-->
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="qué" type="PT00000">qué|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="tornar" type="VMIP3S0">tor|na</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="juego" type="NCMS000">jue|go|</w>
        </l>
        <l met="-|2|-|4|-|6|7|-|-|10|-|" n="27">
          <!--Amor, y torna a dar firme lanzada?-->
          <w lemma="amor" type="NP00000">a|mor|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="tornar" type="VMIP3S0">tor|na</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="dar" type="VMN0000">dar|</w>
          <w lemma="firme" type="AQ0CS00">fir|me|</w>
          <w lemma="lanzada" type="NCFS000">lan|za|da|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="28">
          <!--¿Si de mi puerta luego-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="puerta" type="NCFS000">puer|ta|</w>
          <w lemma="luego" type="RG">lue|go|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="29">
          <!--la rubia Cloe apartada,-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="rubia" type="NCFS000">ru|bia|</w>
          <w lemma="cloe" type="NP00000">cloe</w>
          <w lemma="apartar" type="VMP00SF">a|par|ta|da|</w>
        </l>
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="30">
          <!--a Lydia queda abierta y libre entrada?-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="lydia" type="NP00000">ly|dia|</w>
          <w lemma="quedar" type="VMIP3S0">que|da</w>
          <w lemma="abrir" type="VMP00SF">a|bier|ta</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="libre" type="AQ0CS00">li|bre</w>
          <w lemma="entrada" type="NCFS000">en|tra|da|</w>
        </l>
        <l met="1|-|" n="31">
          <!--LYDIA-->
          <w lemma="lydia" type="NP00000">ly|dia|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="32">
          <!--Aunque Calais hermoso-->
          <w lemma="aunque" type="CC">aun|que|</w>
          <w lemma="calais" type="NP00000">ca|lais|</w>
          <w lemma="hermoso" type="AQ0MS00">her|mo|so|</w>
        </l>
        <l met="1|2|-|4|-|6|7|8|-|10|-|" n="33">
          <!--es más que el sol, y tú más bravo y fiero-->
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="sol" type="NCMS000">sol|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="tú" type="PP2CSN0">tú|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="bravo" type="AQ0MS00">bra|vo</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="fiero" type="AQ0MS00">fie|ro|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="34">
          <!--que mar tempestuoso,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="mar" type="NCCS000">mar|</w>
          <w lemma="tempestuoso" type="AQ0MS00">tem|pes|tuo|so|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="35">
          <!--más que pluma ligero,-->
          <w lemma="más" type="RG">más|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="pluma" type="NCFS000">plu|ma|</w>
          <w lemma="ligero" type="AQ0MS00">li|ge|ro|</w>
        </l>
        <l met="-|2|3|-|-|6|-|-|9|10|-|" n="36">
          <!--vivir quiero contigo y morir quiero.-->
          <w lemma="vivir" type="VMN0000">vi|vir|</w>
          <w lemma="querer" type="VMIP1S0">quie|ro|</w>
          <w lemma="contigo" type="PP2CSO0">con|ti|go</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="morir" type="VMN0000">mo|rir|</w>
          <w lemma="querer" type="VMIP1S0">quie|ro||</w>
        </l>
      </lg>
    </body>
