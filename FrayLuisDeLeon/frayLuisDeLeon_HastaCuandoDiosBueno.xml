  <text>
    <body>
      <head>
        <title> ¿Hasta cuándo, Dios bueno,</title>
      </head>
      <lg cert="high" type="Liras">
        <l met="-|-|3|-|5|6|-|" n="1">
          <!--¿Hasta cuándo, Dios bueno,-->
          <w lemma="hasta" type="SP">has|ta|</w>
          <w lemma="cuándo" type="PT00000">cuán|do|</w>
          <w lemma="dios" type="NP00000">dios|</w>
          <w lemma="bueno" type="AQ0MS00">bue|no|</w>
        </l>
        <l met="-|-|3|-|-|6|-|8|-|10|-|" n="2">
          <!--hasta cuándo estaréis de mí olvidado?-->
          <w lemma="hasta" type="SP">has|ta|</w>
          <w lemma="cuándo" type="PT00000">cuán|do</w>
          <w lemma="estar" type="VMIF2P0">es|ta|réis|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="mí" type="PP1CSO0">mí</w>
          <w lemma="olvidar" type="VMP00SM">ol|vi|da|do|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="3">
          <!--Y ese rostro sereno,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="ese" type="DD0MS0">e|se|</w>
          <w lemma="rostro" type="NCMS000">ros|tro|</w>
          <w lemma="sereno" type="AQ0MS00">se|re|no|</w>
        </l>
        <l met="-|-|3|-|5|6|-|" n="4">
          <!--¿hasta cuándo de un lado-->
          <w lemma="hasta" type="SP">has|ta|</w>
          <w lemma="cuándo" type="PT00000">cuán|do|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="lado" type="NCMS000">la|do|</w>
        </l>
        <l met="-|-|3|-|-|6|7|-|-|10|-|" n="5">
          <!--ha de estar para mí, triste, cuitado?-->
          <w lemma="ha" type="I">ha|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="estar" type="VMN0000">es|tar|</w>
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
          <w lemma="triste" type="AQ0CS00">tris|te|</w>
          <w lemma="cuitado" type="AQ0MS00">cui|ta|do|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="6">
          <!--¿Hasta cuándo, pasmada,-->
          <w lemma="hasta" type="SP">has|ta|</w>
          <w lemma="cuándo" type="PT00000">cuán|do|</w>
          <w lemma="pasmar" type="VMP00SF">pas|ma|da|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="7">
          <!--entre varios consejos vacilando-->
          <w lemma="entre" type="SP">en|tre|</w>
          <w lemma="varios" type="DI0MP0">va|rios|</w>
          <w lemma="consejo" type="NCMP000">con|se|jos|</w>
          <w lemma="vacilar" type="VMG0000">va|ci|lan|do|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="8">
          <!--tendré esta alma cuitada?-->
          <w lemma="tener" type="VMIF1S0">ten|dré</w>
          <w lemma="este" type="DD0FS0">es|ta</w>
          <w lemma="alma" type="NCFS000">al|ma|</w>
          <w lemma="cuitado" type="AQ0FS00">cui|ta|da|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="9">
          <!--Y el dolor, ¿hasta cuándo-->
          <w lemma="y" type="CC">y</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="dolor" type="NCMS000">do|lor|</w>
          <w lemma="hasta" type="SP">has|ta|</w>
          <w lemma="cuándo" type="PT00000">cuán|do|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="10">
          <!--ha de estar mis entrañas traspasando?-->
          <w lemma="ha" type="I">ha|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="estar" type="VMN0000">es|tar|</w>
          <w lemma="mi" type="DP1CPS">mis|</w>
          <w lemma="entraña" type="NCFP000">en|tra|ñas|</w>
          <w lemma="traspasar" type="VMG0000">tras|pa|san|do|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="11">
          <!--A mi enemigo airado,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="mi" type="DP1CSS">mi</w>
          <w lemma="enemigo" type="NCMS000">e|ne|mi|go</w>
          <w lemma="airar" type="VMP00SM">ai|ra|do|</w>
        </l>
        <l met="-|-|3|4|-|6|-|8|-|10|-|" n="12">
          <!--¿hasta cuándo he de estar, Señor, rendido?-->
          <w lemma="hasta" type="SP">has|ta|</w>
          <w lemma="cuándo" type="PT00000">cuán|do</w>
          <w lemma="haber" type="VMIP1S0">he|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="estar" type="VMN0000">es|tar|</w>
          <w lemma="señor" type="NP00000">se|ñor|</w>
          <w lemma="rendir" type="VMP00SM">ren|di|do|</w>
        </l>
        <l met="1|2|-|-|-|6|-|" n="13">
          <!--Ya basta lo pasado,-->
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="bastar" type="VMIP3S0">bas|ta|</w>
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="pasar" type="VMP00SM">pa|sa|do|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="14">
          <!--si vos atento oido-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="vos" type="PP2CS0P">vos|</w>
          <w lemma="atento" type="AQ0MS00">a|ten|to|</w>
          <w lemma="oido" type="VMP00SM">oi|do|</w>
        </l>
        <l met="-|2|-|4|-|6|-|-|-|10|-|" n="15">
          <!--volvéis y rostro alegre al afligido.-->
          <w lemma="volver" type="VMIP2P0">vol|véis|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="rostro" type="NCMS000">ros|tro</w>
          <w lemma="alegre" type="AQ0CS00">a|le|gre</w>
          <w lemma="al" type="SP">a|l</w>
          <w lemma="afligir" type="VMP00SM">a|fli|gi|do|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="16">
          <!--Si sola una centella-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="solo" type="AQ0FS00">so|la</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="centella" type="NCFS000">cen|te|lla|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="17">
          <!--de vuestra luz tuviese en mi sentido,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="vuestro" type="DP2FSP">vues|tra|</w>
          <w lemma="luz" type="NCFS000">luz|</w>
          <w lemma="tener" type="VMSI3S0">tu|vie|se</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="sentido" type="NCMS000">sen|ti|do|</w>
        </l>
        <l met="1|-|-|4|-|6|-|" n="18">
          <!--yo quedaré con ella-->
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="quedar" type="VMIF1S0">que|da|ré|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="él" type="PP3FS00">e|lla|</w>
        </l>
        <l met="1|2|-|4|-|6|-|" n="19">
          <!--tan vivo y tan lucido,-->
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="vivo" type="AQ0MS00">vi|vo</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="lucir" type="VMP00SM">lu|ci|do|</w>
        </l>
        <l met="-|2|-|-|5|6|-|8|-|10|-|" n="20">
          <!--que nunca en mortal sueño esté dormido.-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="nunca" type="RG">nun|ca</w>
          <w lemma="en" type="SP">e|n</w>
          <w lemma="mortal" type="AQ0CS00">mor|tal|</w>
          <w lemma="sueño" type="NCMS000">sue|ño</w>
          <w lemma="estar" type="VMSP3S0">es|té|</w>
          <w lemma="dormir" type="VMP00SM">dor|mi|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="21">
          <!--Y ansí ni mi enemigo-->
          <w lemma="y" type="CC">y</w>
          <w lemma="ansí" type="RG">an|sí|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="mi" type="DP1CSS">mi</w>
          <w lemma="enemigo" type="NCMS000">e|ne|mi|go|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="22">
          <!--se ufanará de haberme contrastado,-->
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="ufanar" type="VMIF3S0">u|fa|na|rá|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="haber" type="VAN0000">ha|ber|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="contrastar" type="VMP00SM">con|tras|ta|do|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="23">
          <!--ni dirá que conmigo-->
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="decir" type="VMIF3S0">di|rá|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="conmigo" type="PP1CSO0">con|mi|go|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="24">
          <!--sus fuerzas ha mostrado,-->
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="fuerza" type="NCFP000">fuer|zas|</w>
          <w lemma="haber" type="VAIP3S0">ha|</w>
          <w lemma="mostrar" type="VMP00SM">mos|tra|do|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="25">
          <!--y que me deja ya domesticado.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="dejar" type="VMIP3S0">de|ja|</w>
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="domesticar" type="VMP00SM">do|mes|ti|ca|do|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="26">
          <!--Tendrá el que mal me quiere,-->
          <w lemma="tener" type="VMIF3S0">ten|drá</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="mal" type="RG">mal|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="querer" type="VMIP3S0">quie|re|</w>
        </l>
        <l met="-|-|3|-|-|6|-|8|-|10|-|" n="27">
          <!--si me quiere vencido, gran pujanza;-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="querer" type="VMIP3S0">quie|re|</w>
          <w lemma="vencer" type="VMP00SM">ven|ci|do|</w>
          <w lemma="gran" type="AQ0CS00">gran|</w>
          <w lemma="pujanza" type="NCFS000">pu|jan|za|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="28">
          <!--pero si yo pusiere,-->
          <w lemma="pero" type="CC">pe|ro|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="poner" type="VMSF1S0">pu|sie|re|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="29">
          <!--Dios mío, mi esperanza-->
          <w lemma="dios" type="I">dios|</w>
          <w lemma="mío" type="AP0MS1S">mío|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="esperanza" type="NCFS000">es|pe|ran|za|</w>
        </l>
        <l met="-|2|3|-|-|6|-|8|-|10|-|" n="30">
          <!--en Vos, ¿quién tomará de mí venganza?-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="vos" type="NP00000">vos|</w>
          <w lemma="quién" type="PT0CS00">quién|</w>
          <w lemma="tomar" type="VMIF3S0">to|ma|rá|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
          <w lemma="venganza" type="NCFS000">ven|gan|za|</w>
        </l>
        <l met="-|-|-|4|5|6|-|" n="31">
          <!--Mi corazón ya ufano,-->
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="corazón" type="NCMS000">co|ra|zón|</w>
          <w lemma="ya" type="RG">ya</w>
          <w lemma="ufano" type="AQ0MS00">u|fa|no|</w>
        </l>
        <l met="1|2|-|-|-|6|7|-|-|10|-|" n="32">
          <!--tan próspero estará y tan victorioso,-->
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="próspero" type="AQ0MS00">prós|pe|ro</w>
          <w lemma="estar" type="VMIF3S0">es|ta|rá</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="victorioso" type="AQ0MS00">vic|to|rio|so|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="33">
          <!--que por tan soberano-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="soberano" type="AQ0MS00">so|be|ra|no|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="34">
          <!--bien, al nombre glorïoso-->
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="nombre" type="NCMS000">nom|bre|</w>
          <w lemma="glorïoso" type="AQ0MS00">glorï|o|so|</w>
        </l>
        <l met="-|-|3|4|-|-|-|8|-|10|-|" n="35">
          <!--vuestro mil palmas cantará gozoso.-->
          <w lemma="vuestro" type="DP2MSP">vues|tro|</w>
          <w lemma="1000" type="Z">mil|</w>
          <w lemma="palma" type="NCFP000">pal|mas|</w>
          <w lemma="cantar" type="VMIF3S0">can|ta|rá|</w>
          <w lemma="gozoso" type="AQ0MS00">go|zo|so|</w>
        </l>
      </lg>
    </body>
