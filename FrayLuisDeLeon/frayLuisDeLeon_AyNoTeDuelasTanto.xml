  <text>
    <body>
      <head>
        <title>¡Ay!, no te duelas tanto,</title>
      </head>
      <lg cert="high" type="Sextetos-lira">
        <l met="-|2|-|4|-|6|-|" n="1">
          <!--¡Ay!, no te duelas tanto,-->
          <w lemma="ay" type="I">ay|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="dolar" type="VMIP2S0">due|las|</w>
          <w lemma="tanto" type="RG">tan|to|</w>
        </l>
        <l met="1|-|-|-|-|6|-|-|-|10|-|" n="2">
          <!--Tíbulo, ni te acuerdes del olvido-->
          <w lemma="tíbulo" type="NP00000">tí|bu|lo|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="acordar" type="VMSP2S0">a|cuer|des|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="olvido" type="NCMS000">ol|vi|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="3">
          <!--de Glícera, ni en canto-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="glícera" type="NP00000">glí|ce|ra|</w>
          <w lemma="ni" type="CC">ni</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="canto" type="NCMS000">can|to|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="4">
          <!--publiques tus querellas dolorido,-->
          <w lemma="publicar" type="VMSP2S0">pu|bli|ques|</w>
          <w lemma="tu" type="DP2CPS">tus|</w>
          <w lemma="querella" type="NCFP000">que|re|llas|</w>
          <w lemma="dolorido" type="AQ0MS00">do|lo|ri|do|</w>
        </l>
        <l met="-|-|3|4|-|6|-|" n="5">
          <!--si, por un bien dispuesto-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="disponer" type="VMP00SM">dis|pues|to|</w>
        </l>
        <l met="1|-|-|-|-|6|-|8|-|10|-|" n="6">
          <!--mozo, la fementida te ha pospuesto.-->
          <w lemma="mozo" type="NCMS000">mo|zo|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="fementido" type="AQ0FS00">fe|men|ti|da|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="haber" type="VAIP3S0">ha|</w>
          <w lemma="posponer" type="VMP00SM">pos|pues|to|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="7">
          <!--Porque sabrás que muere-->
          <w lemma="porque" type="CS">por|que|</w>
          <w lemma="saber" type="VMIF2S0">sa|brás|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="morir" type="VMIP3S0">mue|re|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="8">
          <!--por Ciro, Licorisa, la hermosa;-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="ciro" type="NP00000">ci|ro|</w>
          <w lemma="licorisa" type="NP00000">li|co|ri|sa|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="hermoso" type="AQ0FS00">her|mo|sa|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="9">
          <!--y Ciro no la quiere,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="ciro" type="NP00000">ci|ro|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="querer" type="VMIP3S0">quie|re|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="10">
          <!--y vase en pos de Fóloe desdeñosa;-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="vase" type="NCMS000">va|se</w>
          <w lemma="en_pos_de" type="SP">en_|pos_|de|</w>
          <w lemma="fóloe" type="NP00000">fó|loe|</w>
          <w lemma="desdeñoso" type="AQ0FS00">des|de|ño|sa|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="11">
          <!--y yo sé que primero-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="saber" type="VMIP1S0">sé|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="primero" type="RG">pri|me|ro|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="12">
          <!--se amistarán el lobo y el cordero.-->
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="amistar" type="VMIF3P0">a|mis|ta|rán|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="lobo" type="NCMS000">lo|bo</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cordero" type="NCMS000">cor|de|ro|</w>
        </l>
        <l met="-|2|-|-|5|6|-|" n="13">
          <!--A Venus ansí place-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="venus" type="NP00000">ve|nus|</w>
          <w lemma="ansí" type="RG">an|sí|</w>
          <w lemma="placer" type="VMIP3S0">pla|ce|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="14">
          <!--de aprisionar diversos corazones-->
          <w lemma="de" type="SP">de</w>
          <w lemma="aprisionar" type="VMN0000">a|pri|sio|nar|</w>
          <w lemma="diverso" type="DI0MP0">di|ver|sos|</w>
          <w lemma="corazón" type="NCMP000">co|ra|zo|nes|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="15">
          <!--en duro lazo, que hace-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="duro" type="AQ0MS00">du|ro|</w>
          <w lemma="lazo" type="NCMS000">la|zo|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="hacer" type="VMIP3S0">ha|ce|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="16">
          <!--compuesto de disformes condiciones,-->
          <w lemma="componer" type="VMP00SM">com|pues|to|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="disforme" type="AQ0CP00">dis|for|mes|</w>
          <w lemma="condición" type="NCFP000">con|di|cio|nes|</w>
        </l>
        <l met="-|-|-|-|5|6|-|" n="17">
          <!--y de nuestro error ciego-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="nuestro" type="DP1MSP">nues|tro</w>
          <w lemma="error" type="NCMS000">e|rror|</w>
          <w lemma="ciego" type="AQ0MS00">cie|go|</w>
        </l>
        <l met="1|-|-|-|-|6|-|8|-|10|-|" n="18">
          <!--saca su pasatiempo y crudo juego.-->
          <w lemma="sacar" type="VMIP3S0">sa|ca|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="pasatiempo" type="NCMS000">pa|sa|tiem|po</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="crudo" type="NCMS000">cru|do|</w>
          <w lemma="juego" type="NCMS000">jue|go|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="19">
          <!--Por mí lo sé, que siendo-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="saber" type="VMIP1S0">sé|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="ser" type="VSG0000">sien|do|</w>
        </l>
        <l met="1|-|-|4|-|6|7|-|-|10|-|" n="20">
          <!--de un principal amor muy recuestado,-->
          <w lemma="de" type="SP">de</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="principal" type="AQ0CS00">prin|ci|pal|</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
          <w lemma="muy" type="RG">muy|</w>
          <w lemma="recuestar" type="VMP00SM">re|cues|ta|do|</w>
        </l>
        <l met="1|2|-|-|-|6|-|" n="21">
          <!--yo mismo consintiendo,-->
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="mismo" type="PI0MS00">mis|mo|</w>
          <w lemma="consentir" type="VMG0000">con|sin|tien|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="22">
          <!--la Mírtale me tiene aherrojado,-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mírtale" type="NP00000">mír|ta|le|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="tener" type="VMIP3S0">tie|ne</w>
          <w lemma="aherrojar" type="VMP00SM">a|he|rro|ja|do|</w>
        </l>
        <l met="-|-|3|4|-|6|-|" n="23">
          <!--la cual es medio esclava,-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="cual" type="PR0CS00">cual|</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="medio" type="RG">me|dio</w>
          <w lemma="esclavo" type="AQ0FS00">es|cla|va|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|9|10|-|" n="24">
          <!--y más enojadiza que mar brava.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="enojadizo" type="AQ0FS00">e|no|ja|di|za|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="mar" type="NCCS000">mar|</w>
          <w lemma="bravo" type="AQ0FS00">bra|va|</w>
        </l>
      </lg>
    </body>
