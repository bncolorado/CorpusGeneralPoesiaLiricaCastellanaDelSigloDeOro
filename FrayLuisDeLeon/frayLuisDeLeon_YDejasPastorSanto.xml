    <body>
      <head>
        <title>¿Y dejas, Pastor santo,</title>
      </head>
      <lg cert="high" type="Liras">
        <l met="-|2|-|-|5|6|-|" n="1">
          <!--¿Y dejas, Pastor santo,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="deja" type="NCFP000">de|jas|</w>
          <w lemma="pastor" type="NP00000">pas|tor|</w>
          <w lemma="santo" type="AQ0MS00">san|to|</w>
        </l>
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="2">
          <!--tu grey en este valle hondo, oscuro,-->
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="grey" type="NCFS000">grey|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="este" type="DD0MS0">es|te|</w>
          <w lemma="valle" type="NCMS000">va|lle|</w>
          <w lemma="hondo" type="AQ0MS00">hon|do</w>
          <w lemma="oscuro" type="AQ0MS00">os|cu|ro|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="3">
          <!--con soledad y llanto,-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="soledad" type="NCFS000">so|le|dad|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="llanto" type="NCMS000">llan|to|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="4">
          <!--y tú rompiendo el puro-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="tú" type="PP2CSN0">tú|</w>
          <w lemma="romper" type="VMG0000">rom|pien|do</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="puro" type="AQ0MS00">pu|ro|</w>
        </l>
        <l met="-|-|-|4|-|-|-|8|-|10|-|" n="5">
          <!--aire, te vas al inmortal seguro?-->
          <w lemma="aire" type="I">ai|re|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="ir" type="VMIP2S0">vas|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="inmortal" type="AQ0CS00">in|mor|tal|</w>
          <w lemma="seguro" type="NCMS000">se|gu|ro|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="6">
          <!--Los antes bienhadados,-->
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="antes" type="RG">an|tes|</w>
          <w lemma="bienhadado" type="AQ0MP00">bie|nha|da|dos|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="7">
          <!--y los agora tristes y afligidos,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="aágora" type="NCFS000">a|go|ra|</w>
          <w lemma="triste" type="AQ0CP00">tris|tes|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="afligir" type="VMP00PM">a|fli|gi|dos|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="8">
          <!--a tus pechos criados,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="tu" type="DP2CPS">tus|</w>
          <w lemma="pecho" type="NCMP000">pe|chos|</w>
          <w lemma="criar" type="VMP00PM">cria|dos|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="9">
          <!--de ti desposeídos,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="ti" type="PP2CSO0">ti|</w>
          <w lemma="desposeído" type="NCMP000">des|po|seí|dos|</w>
        </l>
        <l met="-|2|-|-|-|6|7|-|-|10|-|" n="10">
          <!--¿a dó convertirán ya sus sentidos?-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="dó" type="VMIS3S0">dó|</w>
          <w lemma="convertir" type="VMIF3P0">con|ver|ti|rán|</w>
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="sentido" type="NCMP000">sen|ti|dos|</w>
        </l>
        <l met="1|-|-|4|-|6|-|" n="11">
          <!--¿Qué mirarán los ojos-->
          <w lemma="qué" type="PT00000">qué|</w>
          <w lemma="mirar" type="VMIF3P0">mi|ra|rán|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="ojo" type="NCMP000">o|jos|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="12">
          <!--que vieron de tu rostro la hermosura,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="ver" type="VMIS3P0">vie|ron|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="rostro" type="NCMS000">ros|tro|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="hermosura" type="NCFS000">her|mo|su|ra|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="13">
          <!--que no les sea enojos?-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="le" type="PP3CPD0">les|</w>
          <w lemma="ser" type="VSSP1S0">sea|</w>
          <w lemma="enojo" type="NCMP000">e|no|jos|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="14">
          <!--Quien oyó tu dulzura,-->
          <w lemma="quien" type="PR0CS00">quien|</w>
          <w lemma="oír" type="VMIS3S0">o|yó|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="dulzura" type="NCFS000">dul|zu|ra|</w>
        </l>
        <l met="1|2|-|4|-|6|-|-|-|10|-|" n="15">
          <!--¿qué no tendrá por sordo y desventura?-->
          <w lemma="qué" type="PT00000">qué|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="tener" type="VMIF3S0">ten|drá|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="sordo" type="NCMS000">sor|do</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="desventura" type="NCFS000">des|ven|tu|ra|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="16">
          <!--Aqueste mar turbado,-->
          <w lemma="aqueste" type="NP00000">a|ques|te|</w>
          <w lemma="mar" type="NCCS000">mar|</w>
          <w lemma="turbar" type="VMP00SM">tur|ba|do|</w>
        </l>
        <l met="1|-|-|4|5|6|-|-|9|-|" n="17">
          <!--¿quién le pondrá ya freno? ¿Quién concierto-->
          <w lemma="quién" type="PT0CS00">quién|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="poner" type="VMIF3S0">pon|drá|</w>
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="frenoQuién" type="NCMS000">PT0CS00</w>
          <w lemma="concertar" type="VMIP1S0">con|cier|to|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="18">
          <!--al viento fiero, airado?;-->
          <w lemma="al" type="SP">al|</w>
          <w lemma="viento" type="NCMS000">vien|to|</w>
          <w lemma="fiero" type="AQ0MS00">fie|ro</w>
          <w lemma="airar" type="VMP00SM">ai|ra|do|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="19">
          <!--estando tú encubierto,-->
          <w lemma="estar" type="VMG0000">es|tan|do|</w>
          <w lemma="tú" type="PP2CSN0">tú</w>
          <w lemma="encubrir" type="VMP00SM">en|cu|bier|to|</w>
        </l>
        <l met="1|2|-|-|5|-|7|-|-|10|-|" n="20">
          <!--¿qué norte guiará la nave al puerto?-->
          <w lemma="qué" type="DT0CN0">qué|</w>
          <w lemma="norte" type="NCMS000">nor|te|</w>
          <w lemma="guiar" type="VMIF3S0">guia|rá|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="nave" type="NCFS000">na|ve|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="puerto" type="NCMS000">puer|to|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="21">
          <!--¡Ay, nube envidïosa!-->
          <w lemma="ay" type="I">ay|</w>
          <w lemma="nube" type="NCFS000">nu|be|</w>
          <w lemma="envidïosa" type="AQ0FS00">en|vidï|o|sa|</w>
        </l>
        <l met="1|2|-|4|-|6|-|8|-|10|-|" n="22">
          <!--aun de este breve gozo, ¿qué te aquejas?-->
          <w lemma="aun" type="RG">aun|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="este" type="DD0MS0">es|te|</w>
          <w lemma="breve" type="AQ0CS00">bre|ve|</w>
          <w lemma="gozo" type="NCMS000">go|zo|</w>
          <w lemma="qué" type="PT00000">qué|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="aquejar" type="VMIP2S0">a|que|jas|</w>
        </l>
        <l met="1|2|-|-|-|6|-|" n="23">
          <!--¿Dó vuelas presurosa?-->
          <w lemma="dó" type="NP00000">dó|</w>
          <w lemma="volar" type="VMIP2S0">vue|las|</w>
          <w lemma="presuroso" type="AQ0FS00">pre|su|ro|sa|</w>
        </l>
        <l met="1|2|-|4|-|6|-|" n="24">
          <!--¡Cuan rica tú te alejas!-->
          <w lemma="cuan" type="RG">cuan|</w>
          <w lemma="rico" type="AQ0FS00">ri|ca|</w>
          <w lemma="tú" type="PP2CSN0">tú|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="alejar" type="VMIP2S0">a|le|jas|</w>
        </l>
        <l met="1|2|-|-|5|6|-|-|-|10|-|" n="25">
          <!--¡Cuan pobres y cuan ciegos, ay, nos dejas!-->
          <w lemma="cuan" type="RG">cuan|</w>
          <w lemma="cuan" type="RG">cuan|</w>
          <w lemma="pobre" type="AQ0CP00">po|bres|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="cuan" type="RG">cuan|</w>
          <w lemma="cuan" type="RG">cuan|</w>
          <w lemma="ciego" type="AQ0MP00">cie|gos|</w>
          <w lemma="ay" type="I">ay|</w>
          <w lemma="nos" type="PP1CP00">nos|</w>
          <w lemma="deja" type="NCFP000">de|jas|</w>
        </l>
      </lg>
    </body>
