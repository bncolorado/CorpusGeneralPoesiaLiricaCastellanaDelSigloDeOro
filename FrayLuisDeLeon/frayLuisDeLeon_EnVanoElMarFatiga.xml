    <body>
      <head>
        <title>En vano el mar fatiga</title>
      </head>
      <lg cert="high" type="Liras">
        <l met="-|2|-|4|-|6|-|" n="1">
          <!--En vano el mar fatiga-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="vano" type="NCMS000">va|no</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="mar" type="NCCS000">mar|</w>
          <w lemma="fatiga" type="NCFS000">fa|ti|ga|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="2">
          <!--la vela portuguesa, que ni el seno-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="vela" type="NCFS000">ve|la|</w>
          <w lemma="portugués" type="AQ0FS00">por|tu|gue|sa|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="ni" type="CC">ni</w>
          <w lemma="el" type="DA0MS0">e|l</w>
          <w lemma="seno" type="NCMS000">se|no|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="3">
          <!--de Persia, ni la amiga-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="persia" type="NP00000">per|sia|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="amigo" type="NCFS000">a|mi|ga|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="4">
          <!--Maluca da árbol bueno,-->
          <w lemma="maluca" type="NP00000">ma|lu|ca|</w>
          <w lemma="dar" type="VMIP3S0">da</w>
          <w lemma="árbol" type="NCMS000">ár|bol|</w>
          <w lemma="bueno" type="AQ0MS00">bue|no|</w>
        </l>
        <l met="-|2|-|4|5|6|-|-|-|10|-|" n="5">
          <!--que pueda hacer un ánimo sereno.-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="poder" type="VMSP3S0">pue|da</w>
          <w lemma="hacer" type="VMN0000">ha|cer|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="ánimo" type="NCMS000">á|ni|mo|</w>
          <w lemma="sereno" type="AQ0MS00">se|re|no|</w>
        </l>
        <l met="1|2|-|4|-|6|-|" n="6">
          <!--No da reposo al pecho,-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="dar" type="VMIP3S0">da|</w>
          <w lemma="reposo" type="NCMS000">re|po|so</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="pecho" type="NCMS000">pe|cho|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="7">
          <!--Felipe, ni la India, ni la rara-->
          <w lemma="felipe" type="NP00000">fe|li|pe|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="india" type="NP00000">in|dia|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="raro" type="AQ0FS00">ra|ra|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="8">
          <!--esmeralda provecho;-->
          <w lemma="esmeralda" type="NCFS000">es|me|ral|da|</w>
          <w lemma="provecho" type="NCMS000">pro|ve|cho|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="9">
          <!--que más tuerce la cara-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="torcer" type="VMIP3S0">tuer|ce|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="cara" type="NCFS000">ca|ra|</w>
        </l>
        <l met="1|-|-|4|-|6|-|8|-|10|-|" n="10">
          <!--cuanto posee más el alma avara.-->
          <w lemma="cuanto" type="RG">cuan|to|</w>
          <w lemma="poseer" type="VMIP3S0">po|se|e|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="alma" type="NCFS000">al|ma</w>
          <w lemma="avaro" type="AQ0FS00">a|va|ra|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="11">
          <!--Al capitán romano-->
          <w lemma="a" type="SP">a</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="capitán" type="NCMS000">ca|pi|tán|</w>
          <w lemma="romano" type="AQ0MS00">ro|ma|no|</w>
        </l>
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="12">
          <!--la vida, y no la sed, quitó el bebido-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="vida" type="NCFS000">vi|da</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="ser" type="VSM02P0">sed|</w>
          <w lemma="quitar" type="VMIS3S0">qui|tó</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="beber" type="VMP00SM">be|bi|do|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="13">
          <!--tesoro persïano;-->
          <w lemma="tesoro" type="NCMS000">te|so|ro|</w>
          <w lemma="persïano" type="AQ0MS00">persï|a|no|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="14">
          <!--y Tántalo, metido-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="tántalo" type="NP00000">tán|ta|lo|</w>
          <w lemma="meter" type="VMP00SM">me|ti|do|</w>
        </l>
        <l met="-|-|-|-|-|6|-|-|-|10|-|" n="15">
          <!--en medio de las aguas, afligido-->
          <w lemma="en_medio_de" type="SP">en_|me|dio_|de|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="agua" type="NCFP000">a|guas|</w>
          <w lemma="afligir" type="VMP00SM">a|fli|gi|do|</w>
        </l>
        <l met="-|2|-|4|5|6|-|" n="16">
          <!--de sed está; y más dura-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="sed" type="NCFS000">sed|</w>
          <w lemma="estar" type="VMIP3S0">es|tá</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="duro" type="AQ0FS00">du|ra|</w>
        </l>
        <l met="-|2|3|-|-|6|-|-|-|10|-|" n="17">
          <!--la suerte es del mezquino, que sin tasa-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="suerte" type="NCFS000">suer|te</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="mezquino" type="NCMS000">mez|qui|no|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="tasa" type="NCFS000">ta|sa|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="18">
          <!--se cansa ansí, y endura-->
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="cansar" type="VMIP3S0">can|sa</w>
          <w lemma="ansí" type="RG">an|sí</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="endurar" type="VMIP3S0">en|du|ra|</w>
        </l>
        <l met="-|2|-|-|5|6|-|" n="19">
          <!--el oro, y la mar pasa-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="oro" type="NCMS000">o|ro</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mar" type="NCCS000">mar|</w>
          <w lemma="pasar" type="VMIP3S0">pa|sa|</w>
        </l>
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="20">
          <!--osado, y no osa abrir la mano escasa,-->
          <w lemma="osar" type="VMP00SM">o|sa|do</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="osar" type="VMIP3S0">o|sa|</w>
          <w lemma="abrir" type="VMN0000">a|brir|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mano" type="NCFS000">ma|no</w>
          <w lemma="escaso" type="AQ0FS00">es|ca|sa|</w>
        </l>
        <l met="1|2|-|4|-|6|-|" n="21">
          <!--¿Qué vale el no tocado-->
          <w lemma="qué" type="PT00000">qué|</w>
          <w lemma="valer" type="VMIP3S0">va|le</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="tocar" type="VMP00SM">to|ca|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="22">
          <!--tesoro, si corrompe el dulce sueño,-->
          <w lemma="tesoro" type="NCMS000">te|so|ro|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="corromper" type="VMIP3S0">co|rrom|pe</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="dulce" type="AQ0CS00">dul|ce|</w>
          <w lemma="sueño" type="NCMS000">sue|ño|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="23">
          <!--si estrecha el ñudo dado,-->
          <w lemma="si" type="CS">si</w>
          <w lemma="estrechar" type="VMIP3S0">es|tre|cha</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="ñudo" type="NCMS000">ñu|do|</w>
          <w lemma="dar" type="VMP00SM">da|do|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="24">
          <!--si más enturbia el ceño,-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="enturbiar" type="VMIP3S0">en|tur|bia</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="ceño" type="NCMS000">ce|ño|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="25">
          <!--y deja en la riqueza pobre al dueño?-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="dejar" type="VMIP3S0">de|ja</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="riqueza" type="NCFS000">ri|que|za|</w>
          <w lemma="pobre" type="AQ0CS00">po|bre</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="dueño" type="NCMS000">due|ño|</w>
        </l>
      </lg>
