          <w lemma="o" type="CC">o|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="tierra" type="NCFS000">tie|rra|</w>
          <w lemma="crudo" type="AQ0FS00">cru|da|</w>
        </l>
        <l met="-|2|-|4|-|-|-|8|-|10|-|" n="12">
          <!--de nieve llena y de piedad desnuda.-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="nieve" type="NCFS000">nie|ve|</w>
          <w lemma="lleno" type="AQ0FS00">lle|na</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="piedad" type="NCFS000">pie|dad|</w>
          <w lemma="desnudo" type="AQ0FS00">des|nu|da|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="13">
          <!--De mí sé que al encuentro,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
          <w lemma="saber" type="VMIP1S0">sé|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="encuentro" type="NCMS000">en|cuen|tro|</w>
        </l>
        <l met="-|-|-|-|-|6|-|-|-|10|-|" n="14">
          <!--mientras por las montañas vagueando-->
          <w lemma="mientras" type="CS">mien|tras|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="montaña" type="NCFP000">mon|ta|ñas|</w>
          <w lemma="vaguear" type="VMG0000">va|gue|an|do|</w>
        </l>
        <l met="1|-|-|4|-|6|-|" n="15">
