          <w lemma="o" type="CC">o|</w>
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="poner" type="VMSP3S0">pon|ga</w>
          <w lemma="alguno" type="PI0MS00">al|gu|no|</w>
        </l>
        <l met="-|-|-|4|-|6|7|-|-|10|-|" n="32">
