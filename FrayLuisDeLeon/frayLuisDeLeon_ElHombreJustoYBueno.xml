          <w lemma="más" type="RG">más|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="justo" type="RG">jus|to|</w>
          <w lemma="entrar" type="VMIP1S0">en|tro|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="16">
          <!--sin armas, y de Lálage cantando,-->
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="arma" type="NCFP000">ar|mas|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="lálage" type="NP00000">lá|la|ge|</w>
          <w lemma="cantar" type="VMG0000">can|tan|do|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="17">
          <!--me vido, y más ligero-->
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="vido" type="NCMS000">vi|do</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="ligero" type="AQ0MS00">li|ge|ro|</w>
        </l>
        <l met="-|2|-|4|5|6|-|-|-|10|-|" n="18">
          <!--huyó que rayo, un lobo carnicero.-->
          <w lemma="huir" type="VMIS3S0">hu|yó|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="rayo" type="NCMS000">ra|yo</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="lobo" type="NCMS000">lo|bo|</w>
          <w lemma="carnicero" type="AQ0MS00">car|ni|ce|ro|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="19">
          <!--Y creo que alimaña-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="creer" type="VMIP1S0">creo|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="alimaña" type="NCFS000">a|li|ma|ña|</w>
        </l>
        <l met="1|2|-|-|-|6|-|8|-|10|-|" n="20">
          <!--más fiera y espantosa no mantiene-->
          <w lemma="más" type="RG">más|</w>
          <w lemma="fiero" type="AQ0FS00">fie|ra</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="espantoso" type="AQ0FS00">es|pan|to|sa|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="mantener" type="VMIP3S0">man|tie|ne|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="21">
          <!--la más alta Alemaña-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="alto" type="AQ0FS00">al|ta</w>
          <w lemma="alemaña" type="NP00000">a|le|ma|ña|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="22">
          <!--en sus espesos bosques, ni la tiene-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="espeso" type="AQ0MP00">es|pe|sos|</w>
          <w lemma="bosque" type="NCMP000">bos|ques|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="tener" type="VMIP3S0">tie|ne|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="23">
          <!--la tierra donde mora-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="tierra" type="NCFS000">tie|rra|</w>
          <w lemma="donde" type="PR00000">don|de|</w>
          <w lemma="morar" type="VMIP3S0">mo|ra|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="24">
          <!--el moro, de fiereza engendradora.-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="moro" type="NCMS000">mo|ro|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="fiereza" type="NCFS000">fie|re|za</w>
          <w lemma="engendrador" type="AQ0FS00">en|gen|dra|do|ra|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="25">
          <!--O ya en aquella parte,-->
          <w lemma="o" type="CC">o|</w>
          <w lemma="ya" type="RG">ya</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="aquel" type="DD0FS0">a|que|lla|</w>
          <w lemma="parte" type="NCCS000">par|te|</w>
        </l>
        <l met="-|2|-|4|-|6|-|-|-|10|-|" n="26">
          <!--que siempre está sujeta al inclemente-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="siempre" type="RG">siem|pre</w>
          <w lemma="estar" type="VMIP3S0">es|tá|</w>
          <w lemma="sujeto" type="AQ0FS00">su|je|ta</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="inclemente" type="AQ0CS00">in|cle|men|te|</w>
        </l>
        <l met="1|-|3|4|-|6|-|" n="27">
          <!--cielo, do no se parte-->
          <w lemma="cielo" type="NCMS000">cie|lo|</w>
          <w lemma="do" type="NCMS000">do|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="partir" type="VMIP3S0">par|te|</w>
        </l>
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="28">
          <!--espesa y fría niebla eternamente,-->
          <w lemma="espeso" type="AQ0FS00">es|pe|sa</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="frío" type="AQ0FS00">frí|a|</w>
          <w lemma="niebla" type="NCFS000">nie|bla</w>
          <w lemma="eternamente" type="RG">e|ter|na|men|te|</w>
        </l>
        <l met="1|2|-|4|-|6|-|" n="29">
