          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="región" type="NCFS000">re|gión|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="sol" type="NCMS000">sol|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="allegar" type="VMP00SF">a|lle|ga|da|</w>
        </l>
        <l met="1|2|3|-|-|6|-|" n="33">
          <!--do no vive ninguno,-->
          <w lemma="do" type="NCMS000">do|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="vivir" type="VMIP3S0">vi|ve|</w>
          <w lemma="ninguno" type="PI0MS00">nin|gu|no|</w>
        </l>
        <l met="1|-|-|4|-|6|7|-|-|10|-|" n="34">
