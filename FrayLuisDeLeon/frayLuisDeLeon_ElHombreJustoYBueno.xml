          <w lemma="mí" type="PP1CSO0">mí|</w>
          <w lemma="lálage" type="NP00000">lá|la|ge</w>
          <w lemma="amar" type="VMP00SF">a|ma|da|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="35">
