          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="reír" type="VMN0000">re|ír|</w>
          <w lemma="gracioso" type="AQ0MS00">gra|cio|so|</w>
        </l>
        <l met="-|-|-|4|5|6|-|8|-|10|-|" n="36">
