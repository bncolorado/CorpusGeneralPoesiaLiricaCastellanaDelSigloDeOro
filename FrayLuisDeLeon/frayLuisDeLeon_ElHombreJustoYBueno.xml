    <body>
      <head>
        <title>El hombre justo y bueno,</title>
      </head>
      <lg cert="high" type="Sextetos-lira">
        <l met="-|2|-|4|-|6|-|" n="1">
          <!--El hombre justo y bueno,-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="hombre" type="NCMS000">hom|bre|</w>
          <w lemma="justo" type="AQ0MS00">jus|to</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="bueno" type="AQ0MS00">bue|no|</w>
        </l>
        <l met="-|-|-|4|-|6|-|8|-|10|-|" n="2">
          <!--el que de culpa está y mancilla puro,-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="culpa" type="NCFS000">cul|pa</w>
          <w lemma="estar" type="VMIP3S0">es|tá</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="mancilla" type="NCFS000">man|ci|lla|</w>
          <w lemma="puro" type="AQ0MS00">pu|ro|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="3">
          <!--las manos en el seno,-->
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="mano" type="NCFP000">ma|nos|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="seno" type="NCMS000">se|no|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="4">
          <!--sin dardo ni azagaya va seguro,-->
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="dardo" type="NCMS000">dar|do|</w>
          <w lemma="ni" type="CC">ni</w>
          <w lemma="azagaya" type="NCFS000">a|za|ga|ya|</w>
          <w lemma="ir" type="VMIP3S0">va|</w>
          <w lemma="seguro" type="RG">se|gu|ro|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="5">
          <!--y sin llevar cargada-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="llevar" type="VMN0000">lle|var|</w>
          <w lemma="cargar" type="VMP00SF">car|ga|da|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="6">
          <!--la aljaba de saeta enherbolada.-->
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="aljaba" type="NCFS000">al|ja|ba|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="saeta" type="NCFS000">sa|e|ta</w>
          <w lemma="enherbolar" type="VMP00SF">e|nher|bo|la|da|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="7">
          <!--O vaya por la arena-->
          <w lemma="o" type="CC">o|</w>
          <w lemma="ir" type="VMM03S0">va|ya|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="arena" type="NCFS000">a|re|na|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="8">
          <!--ardiente de la Libia ponzoñosa,-->
          <w lemma="ardiente" type="AQ0CS00">ar|dien|te|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="libia" type="NP00000">li|bia|</w>
          <w lemma="ponzoñoso" type="AQ0FS00">pon|zo|ño|sa|</w>
        </l>
        <l met="-|2|-|-|5|6|-|" n="9">
          <!--o vaya por do suena-->
          <w lemma="o" type="CC">o|</w>
          <w lemma="ir" type="VMM03S0">va|ya|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="do" type="NCMS000">do|</w>
          <w lemma="sonar" type="VMIP3S0">sue|na|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="10">
          <!--de Hidaspes la corriente fabulosa,-->
          <w lemma="de" type="SP">de</w>
          <w lemma="hidaspes" type="NP00000">hi|das|pes|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="corriente" type="NCFS000">co|rrien|te|</w>
          <w lemma="fabuloso" type="AQ0FS00">fa|bu|lo|sa|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="11">
