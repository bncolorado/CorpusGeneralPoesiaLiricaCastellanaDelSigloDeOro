  <text>
    <body>
      <head>
        <title>Invictísimo César, cuyo nombre</title>
      </head>
      <lg cert="high" type="Octavas reales">
        <l met="-|-|3|-|-|-|-|-|-|10|-|" n="1">
          <!--Invictísimo César, cuyo nombre-->
          <w lemma="invictísimo_césar" type="NP00000">in|vic|tí|si|mo_|cé|sar|</w>
          <w lemma="cuyo" type="PR0MS00">cu|yo|</w>
          <w lemma="nombre" type="NCMS000">nom|bre|</w>
        </l>
        <l met="-|-|-|4|-|6|7|-|-|10|-|" n="2">
          <!--el del antiguo Carlo ha renovado,-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="antiguo" type="AQ0MS00">an|ti|guo|</w>
          <w lemma="carlo" type="NP00000">ca|rlo</w>
          <w lemma="haber" type="VAIP3S0">ha|</w>
          <w lemma="renovar" type="VMP00SM">re|no|va|do|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|-|10|-|" n="3">
          <!--al sonido del cual tiemble y se asombre-->
          <w lemma="al" type="SP">al|</w>
          <w lemma="sonido" type="NCMS000">so|ni|do|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="cual" type="PR0CS00">cual|</w>
          <w lemma="temblar" type="VMSP3S0">tiem|ble</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="asombrar" type="VMSP3S0">a|som|bre|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|-|10|-|" n="4">
          <!--la tierra, el mar y todo lo criado;-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="tierra" type="NCFS000">tie|rra|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="mar" type="NCCS000">mar|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="todo" type="DI0MS0">to|do|</w>
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="criado" type="NCMS000">cria|do|</w>
        </l>
        <l met="-|-|3|-|-|6|-|8|-|10|-|" n="5">
          <!--en quien Roma su imperio y gran renombre-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="quien" type="PR0CS00">quien|</w>
          <w lemma="roma" type="NP00000">ro|ma|</w>
          <w lemma="su" type="DP3CSN">su</w>
          <w lemma="imperio" type="NCMS000">im|pe|rio</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="gran" type="AQ0CS00">gran|</w>
          <w lemma="renombre" type="NCMS000">re|nom|bre|</w>
        </l>
        <l met="-|2|-|4|-|6|-|-|-|10|-|" n="6">
          <!--conoce más que nunca sublimado,-->
          <w lemma="conocer" type="VMIP3S0">co|no|ce|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="nunca" type="RG">nun|ca|</w>
          <w lemma="sublimar" type="VMP00SM">su|bli|ma|do|</w>
        </l>
        <l met="-|2|-|4|-|6|-|-|-|10|-|" n="7">
          <!--y do el dichoso siglo que os alcanza-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="do" type="NCMS000">do</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="dichoso" type="AQ0MS00">di|cho|so|</w>
          <w lemma="siglo" type="NCMS000">si|glo|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="os" type="PP2CP00">o|s</w>
          <w lemma="alcanzar" type="VMIP3S0">al|can|za|</w>
        </l>
        <l met="1|-|-|4|-|6|-|-|-|10|-|" n="8">
          <!--pone primera y última esperanza.-->
          <w lemma="poner" type="VMIP3S0">po|ne|</w>
          <w lemma="1" type="AO0FS00">pri|me|ra</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="último" type="AO0FS00">úl|ti|ma</w>
          <w lemma="esperanza" type="NCFS000">es|pe|ran|za|</w>
        </l>
        <l met="1|-|-|4|-|-|-|-|-|10|-|" n="9">
          <!--Vos, pues, Señor, en cuya fortaleza-->
          <w lemma="vos" type="PP2CS0P">vos|</w>
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="señor" type="NP00000">se|ñor|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="cuyo" type="PR0FS00">cu|ya|</w>
          <w lemma="fortaleza" type="NCFS000">for|ta|le|za|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="10">
          <!--el nombre se sustenta y ser cristiano,-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="nombre" type="NCMS000">nom|bre|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="sustentar" type="VMIP3S0">sus|ten|ta</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="cristiano" type="AQ0MS00">cris|tia|no|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="11">
          <!--y en el supremo grado de grandeza-->
          <w lemma="y" type="CC">y</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="supremo" type="AQ0MS00">su|pre|mo|</w>
          <w lemma="grado" type="NCMS000">gra|do|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="grandeza" type="NCFS000">gran|de|za|</w>
        </l>
        <l met="-|2|3|-|-|6|-|8|-|10|-|" n="12">
          <!--tenéis siempre delante el ser humano;-->
          <w lemma="tener" type="VMIP2P0">te|néis|</w>
          <w lemma="siempre" type="RG">siem|pre|</w>
          <w lemma="delante" type="RG">de|lan|te</w>
          <w lemma="el" type="DA0MS0">e|l</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="humano" type="AQ0MS00">hu|ma|no|</w>
        </l>
        <l met="-|-|3|4|-|6|-|-|-|10|-|" n="13">
          <!--si del don bajo suple la bajeza-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="don" type="NCMS000">don|</w>
          <w lemma="bajo" type="AQ0MS00">ba|jo|</w>
          <w lemma="suplir" type="VMIP3S0">su|ple|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="bajeza" type="NCFS000">ba|je|za|</w>
        </l>
        <l met="1|2|-|-|-|6|-|8|-|10|-|" n="14">
          <!--un puro corazón sincero y sano,-->
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="puro" type="AQ0MS00">pu|ro|</w>
          <w lemma="corazón" type="NCMS000">co|ra|zón|</w>
          <w lemma="sincero" type="AQ0MS00">sin|ce|ro</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="sano" type="AQ0MS00">sa|no|</w>
        </l>
        <l met="1|-|-|4|5|-|-|8|-|10|-|" n="15">
          <!--dél acetad esta señal presente,-->
          <w lemma="dél" type="AQ0CS00">dél|</w>
          <w lemma="acetad" type="NCFS000">a|ce|tad|</w>
          <w lemma="este" type="DD0FS0">es|ta|</w>
          <w lemma="señal" type="NCFS000">se|ñal|</w>
          <w lemma="presente" type="AQ0CS00">pre|sen|te|</w>
        </l>
        <l met="-|-|3|-|-|6|-|8|-|10|-|" n="16">
          <!--como César humano, humanamente.-->
          <w lemma="como" type="CS">co|mo|</w>
          <w lemma="césar" type="NP00000">cé|sar|</w>
          <w lemma="humano" type="AQ0MS00">hu|ma|no</w>
          <w lemma="humanamente" type="RG">hu|ma|na|men|te|</w>
        </l>
      </lg>
    </body>
  </text>
