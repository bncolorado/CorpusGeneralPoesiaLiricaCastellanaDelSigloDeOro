  <text>
    <body>
      <head>
        <title> En el tiempo, señora, que encubría</title>
      </head>
      <lg cert="high" type="Madrigal">
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="1">
          <!--En el tiempo, señora, que encubría-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="tiempo" type="NCMS000">tiem|po|</w>
          <w lemma="señor" type="NCFS000">se|ño|ra|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="encubrir" type="VMII3S0">en|cu|brí|a|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="2">
          <!--lo que publico agora,-->
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="publicar" type="VMIP1S0">pu|bli|co</w>
          <w lemma="aágora" type="NCFS000">a|go|ra|</w>
        </l>
        <l met="1|2|-|-|-|6|-|8|9|10|-|" n="3">
          <!--no tuve de descanso sola un hora.-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="tener" type="VMIS1S0">tu|ve|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="descanso" type="NCMS000">des|can|so|</w>
          <w lemma="solo" type="AQ0FS00">so|la</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="hora" type="NCFS000">ho|ra|</w>
        </l>
        <l met="-|-|-|4|-|-|-|8|-|10|-|" n="4">
          <!--Lo que sentía me forzó a quejarme,-->
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="sentir" type="VMII3S0">sen|tí|a|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="forzar" type="VMIS3S0">for|zó</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="quejar" type="VMN0000">que|jar|</w>
          <w lemma="me" type="PP1CS00">me||</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="5">
          <!--y quedo más quejoso,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="quedo" type="I">que|do|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="quejoso" type="AQ0MS00">que|jo|so|</w>
        </l>
        <l met="-|-|-|-|-|6|-|-|-|10|-|" n="6">
          <!--porque lo que busqué para aliviarme-->
          <w lemma="porque" type="CS">por|que|</w>
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="buscar" type="VMIS1S0">bus|qué|</w>
          <w lemma="para" type="SP">pa|ra</w>
          <w lemma="aliviar" type="VMN0000">a|li|viar|</w>
          <w lemma="me" type="PP1CS00">me|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="7">
          <!--me da menos reposo;-->
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="dar" type="VMIP3S0">da|</w>
          <w lemma="menos" type="RG">me|nos|</w>
          <w lemma="reposo" type="NCMS000">re|po|so|</w>
        </l>
        <l met="-|-|3|-|-|6|7|8|-|10|-|" n="8">
          <!--y pues todo camino es tan dañoso,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="todo" type="DI0MS0">to|do|</w>
          <w lemma="camino" type="NCMS000">ca|mi|no</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="dañoso" type="AQ0MS00">da|ño|so|</w>
        </l>
        <l met="1|2|-|-|-|6|-|" n="9">
          <!--yo tomo por mejor-->
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="tomo" type="NCMS000">to|mo|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="mejor" type="AQ0CS00">me|jor|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="10">
          <!--dejarme en vuestra mano y la de Amor.-->
          <w lemma="dejar" type="VMN0000">de|jar|</w>
          <w lemma="me" type="PP1CS00">me</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="vuestro" type="DP2FSP">vues|tra|</w>
          <w lemma="mano" type="NCFS000">ma|no</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="amor" type="NP00000">a|mor|</w>
        </l>
      </lg>
    </body>
  </text>
