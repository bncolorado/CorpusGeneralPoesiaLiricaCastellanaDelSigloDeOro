  <text>
    <body>
      <head>
        <title>Acaso supo, a mi ver,</title>
      </head>
      <lg cert="high" type="Copla">
        <l met="-|2|-|4|-|-|7|-|" n="1">
          <!--Acaso supo, a mi ver,-->
          <w lemma="acaso" type="RG">a|ca|so|</w>
          <w lemma="saber" type="VMIS3S0">su|po</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="2">
          <!--y por acierto quereros-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="acierto" type="NCMS000">a|cier|to|</w>
          <w lemma="querer" type="VMN0000">que|rer|</w>
          <w lemma="os" type="PP2CP00">os|</w>
        </l>
        <l met="-|2|3|-|5|-|7|-|" n="3">
          <!--quien tal yerro fue a hacer:-->
          <w lemma="quien" type="PR0CS00">quien|</w>
          <w lemma="tal" type="DD0CS0">tal|</w>
          <w lemma="yerro" type="NCMS000">ye|rro|</w>
          <w lemma="ser" type="VSIS3S0">fue|</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="hacer" type="VMN0000">ha|cer|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="4">
          <!--como partirse de veros-->
          <w lemma="como" type="CS">co|mo|</w>
          <w lemma="partir" type="VMN0000">par|tir|</w>
          <w lemma="se" type="PP3CN00">se|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
          <w lemma="os" type="PP2CP00">os|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="5">
          <!--donde os dejase de ver.-->
          <w lemma="donde" type="PR00000">don|de</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="dejar" type="VMSI3S0">de|ja|se|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="ver" type="VMN0000">ver|</w>
        </l>
        <l met="-|-|3|4|5|-|7|-|" n="6">
          <!--Imposible es que este tal,-->
          <w lemma="imposible" type="AQ0CS00">im|po|si|ble</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="este" type="DD0MS0">es|te|</w>
          <w lemma="tal" type="PD0CS00">tal|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="7">
          <!--pensando que os conocía,-->
          <w lemma="pensar" type="VMG0000">pen|san|do|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="conocer" type="VMII3S0">co|no|cí|a|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="8">
          <!--supiese lo que hacía-->
          <w lemma="saber" type="VMSI3S0">su|pie|se|</w>
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="hacer" type="VMII3S0">ha|cí|a|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="9">
          <!--cuando su bien y su mal-->
          <w lemma="cuando" type="CS">cuan|do|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="bien" type="NCMS000">bien|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="mal" type="NCMS000">mal|</w>
        </l>
        <l met="1|-|-|-|5|6|7|-|" n="10">
          <!--junto os entregó en un día.-->
          <w lemma="junto" type="RG">jun|to</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="entregar" type="VMIS3S0">en|tre|gó</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="día" type="NCMS000">dí|a|</w>
        </l>
        <l met="-|-|3|4|-|-|7|-|" n="11">
          <!--Acertó acaso a hacer-->
          <w lemma="acertar" type="VMIS3S0">a|cer|tó</w>
          <w lemma="acaso" type="RG">a|ca|so|</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="hacer" type="VMN0000">ha|cer|</w>
        </l>
        <l met="-|-|-|-|-|-|7|-|" n="12">
          <!--lo que, si por conoceros-->
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="conocer" type="VMN0000">co|no|cer|</w>
          <w lemma="os" type="PP2CP00">os|</w>
        </l>
        <l met="-|2|-|4|-|6|7|-|" n="13">
          <!--hiciera, no podía ser:-->
          <w lemma="hacer" type="VMSI3S0">hi|cie|ra|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="poder" type="VMII3S0">po|día|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="14">
          <!--partirse y, con solo veros,-->
          <w lemma="partir" type="VMN0000">par|tir|</w>
          <w lemma="se" type="PP3CN00">se</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="solo" type="RG">so|lo|</w>
          <w lemma="vero" type="NCMP000">ve|ros|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="15">
          <!--dejaros siempre de ver.-->
          <w lemma="dejar" type="VMN0000">de|jar|</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="siempre" type="RG">siem|pre|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
        </l>
      </lg>
    </body>
  </text>
