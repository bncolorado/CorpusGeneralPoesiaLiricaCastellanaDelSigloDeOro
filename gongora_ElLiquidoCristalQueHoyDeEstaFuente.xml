  <text>
    <body>
      <head>
        <title>El líquido cristal que hoy de esta fuente</title>
      </head>
      <lg cert="high" type="Madrigal">
        <l met="-|2|-|-|-|6|7|8|-|10|-|" n="1">
          <!--El líquido cristal que hoy de esta fuente-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="líquido" type="AQ0MS00">lí|qui|do|</w>
          <w lemma="cristal" type="NCMS000">cris|tal|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="hoy" type="RG">hoy|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="este" type="DD0FS0">es|ta|</w>
          <w lemma="fuente" type="NCFS000">fuen|te|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="2">
          <!--admiras, caminante,-->
          <w lemma="admirar" type="VMIP2S0">ad|mi|ras|</w>
          <w lemma="caminante" type="NCCS000">ca|mi|nan|te|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="3">
          <!--el mismo es de Helicona;-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="mismo" type="AQ0MS00">mis|mo</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="helicona" type="NP00000">he|li|co|na|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="4">
          <!--si pudieres, perdona-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="poder" type="VMSF2S0">pu|die|res|</w>
          <w lemma="perdonar" type="VMIP3S0">per|do|na|</w>
        </l>
        <l met="-|2|3|4|-|6|-|" n="5">
          <!--al paso un solo instante:-->
          <w lemma="al" type="SP">al|</w>
          <w lemma="paso" type="NCMS000">pa|so</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="solo" type="AQ0MS00">so|lo</w>
          <w lemma="instante" type="NCMS000">ins|tan|te|</w>
        </l>
        <l met="-|-|3|4|-|6|-|" n="6">
          <!--beberás (cultamente)-->
          <w lemma="beber" type="VMIF2S0">be|be|rás|</w>
          <w lemma="cultamente" type="RG">cul|ta|men|te|</w>
        </l>
        <l met="1|-|-|-|-|6|-|" n="7">
          <!--ondas que del Parnaso-->
          <w lemma="onda" type="NCFP000">on|das|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="parnaso" type="NP00000">par|na|so|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="8">
          <!--a su Vega tradujo Garcilaso.-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="vega" type="NP00000">ve|ga|</w>
          <w lemma="traducir" type="VMIS3S0">tra|du|jo|</w>
          <w lemma="garcilaso" type="NP00000">gar|ci|la|so|</w>
        </l>
      </lg>
    </body>
  </text>
