  <text>
    <body>
      <head>
        <title> Hoy metimos tres varones</title>
      </head>
      <lg cert="low" type="Coplas">
        <l met="1|-|3|-|5|-|7|-|" n="1">
          <!--Hoy metimos tres varones-->
          <w lemma="hoy" type="RG">hoy|</w>
          <w lemma="meter" type="VMIS1P0">me|ti|mos|</w>
          <w lemma="3" type="Z">tres|</w>
          <w lemma="varón" type="NCMP000">va|ro|nes|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="2">
          <!--en el horno aprisionados,-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="horno" type="NCMS000">hor|no</w>
          <w lemma="aprisionar" type="VMP00PM">a|pri|sio|na|dos|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="3">
          <!--y ahora siendo mirados,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="ahora" type="RG">a|ho|ra|</w>
          <w lemma="ser" type="VSG0000">sien|do|</w>
          <w lemma="mirar" type="VMP00PM">mi|ra|dos|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="4">
          <!--veo cuatro sin prisiones;-->
          <w lemma="ver" type="VMIP1S0">ve|o|</w>
          <w lemma="4" type="Z">cua|tro|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="prisión" type="NCFP000">pri|sio|nes|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="5">
          <!--y el fuego no les empece-->
          <w lemma="y" type="CC">y</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="fuego" type="NCMS000">fue|go|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="le" type="PP3CPD0">les|</w>
          <w lemma="empecer" type="VMIP3S0">em|pe|ce|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="6">
          <!--ni les toca en los cabellos;-->
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="le" type="PP3CPD0">les|</w>
          <w lemma="tocar" type="VMIP3S0">to|ca</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="cabello" type="NCMP000">ca|be|llos|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="7">
          <!--la vista del cuarto dellos-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="vista" type="NCFS000">vis|ta|</w>
          <w lemma="de" type="SP">de</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="4" type="AO0MS00">cuar|to|</w>
          <w lemma="de" type="SP">de</w>
	  <w lemma="ellos" type="PP3MP00">e|llos|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="8">
          <!--Hijo de Dios me parece.-->
          <w lemma="hijo_de_dios" type="NP00000">hi|jo_|de_|dios|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="parecer" type="VMIP3S0">pa|re|ce|</w>
        </l>
      </lg>
    </body>
  </text>
