  <text>
    <body>
      <head>
        <title> No sé a cuál crea de los dos,</title>
      </head>
      <lg cert="high" type="Redondillas">
        <l met="1|2|3|4|-|-|7|-|" n="1">
          <!--No sé a cuál crea de los dos,-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="saber" type="VMIP1S0">sé</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="cuál" type="PT0CS00">cuál|</w>
          <w lemma="crear" type="VMIP3S0">crea|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="2" type="Z">dos|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="2">
          <!--viéndoos, Ana, cual os veis;-->
          <w lemma="ver" type="VMG0000">vien|do</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="ana" type="NP00000">a|na|</w>
          <w lemma="cual" type="PR0CS00">cual|</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="ver" type="VMIP2P0">veis|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="3">
          <!--si vos la Muerte traéis,-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="vos" type="PP2CS0P">vos|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="muerte" type="NP00000">muer|te|</w>
          <w lemma="traer" type="VMIP2P0">traéis|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="4">
          <!--o si os trae la Muerte a vos.-->
          <w lemma="o" type="CC">o|</w>
          <w lemma="si" type="CS">si</w>
          <w lemma="os" type="PP2CP00">o|s</w>
          <w lemma="traer" type="VMIP3S0">trae|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="muerte" type="NP00000">muer|te</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="vos" type="PP2CS0P">vos|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="5">
          <!--Queredme la muerte dar,-->
          <w lemma="querer" type="VMM02P0">que|red|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="muerte" type="NCFS000">muer|te|</w>
          <w lemma="dar" type="VMN0000">dar|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="6">
          <!--por que mis males remate,-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="mi" type="DP1CPS">mis|</w>
          <w lemma="mal" type="NCMP000">ma|les|</w>
          <w lemma="remate" type="NCMS000">re|ma|te|</w>
        </l>
        <l met="-|2|3|4|-|-|7|-|" n="7">
          <!--que en mí tiene hambre que mate-->
          <w lemma="que" type="CS">que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
          <w lemma="tener" type="VMIP3S0">tie|ne</w>
          <w lemma="hambre" type="NCFS000">ham|bre|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="matar" type="VMSP3S0">ma|te|</w>
        </l>
        <l met="-|2|3|4|5|-|7|-|" n="8">
          <!--y en vos no hay ya qué matar.-->
          <w lemma="y" type="CC">y</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="vos" type="PP2CS0P">vos|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="haber" type="VMIP3S0">hay|</w>
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="qué" type="PT00000">qué|</w>
          <w lemma="matar" type="VMN0000">ma|tar|</w>
        </l>
      </lg>
    </body>
