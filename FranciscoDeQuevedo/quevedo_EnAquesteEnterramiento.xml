  <text>
    <body>
      <head>
        <title> En aqueste enterramiento</title>
      </head>
      <lg cert="high" type="Redondillas">
        <l met="-|-|3|-|-|-|7|-|" n="1">
          <!--En aqueste enterramiento-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="aqueste" type="VMSP3S0">a|ques|te</w>
          <w lemma="enterramiento" type="NCMS000">en|te|rra|mien|to|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="2">
          <!--humilde, pobre y mezquino,-->
          <w lemma="humilde" type="AQ0CS00">hu|mil|de|</w>
          <w lemma="pobre" type="AQ0CS00">po|bre</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="mezquino" type="AQ0MS00">mez|qui|no|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="3">
          <!--yace envuelto en oro fino-->
          <w lemma="yacer" type="VMIP3S0">ya|ce</w>
          <w lemma="envolver" type="VMP00SM">en|vuel|to</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="oro" type="NCMS000">o|ro|</w>
          <w lemma="fino" type="AQ0MS00">fi|no|</w>
        </l>
        <l met="1|2|-|4|-|-|7|-|" n="4">
          <!--un hombre rico avariento.-->
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="hombre" type="NCMS000">hom|bre|</w>
          <w lemma="rico" type="AQ0MS00">ri|co</w>
          <w lemma="avariento" type="AQ0MS00">a|va|rien|to|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="5">
          <!--Murió con cien mil dolores,-->
          <w lemma="morir" type="VMIS3S0">mu|rió|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="100000" type="Z">cien_|mil|</w>
          <w lemma="dolor" type="NCMP000">do|lo|res|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="6">
          <!--sin poderlo remediar.-->
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="poder" type="VMN0000">po|der|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="remediar" type="VMN0000">re|me|diar|</w>
        </l>
        <l met="1|2|-|-|5|-|7|-|" n="7">
          <!--Tan sólo por no gastar,-->
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="sólo" type="RG">só|lo|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="gastar" type="VMN0000">gas|tar|</w>
        </l>
        <l met="1|2|-|4|-|-|7|-|" n="8">
          <!--ni aun gasta malos humores.-->
          <w lemma="ni" type="CC">ni</w>
          <w lemma="aun" type="RG">aun|</w>
          <w lemma="gastar" type="VMIP3S0">gas|ta|</w>
          <w lemma="malo" type="AQ0MP00">ma|los|</w>
          <w lemma="humor" type="NCMP000">hu|mo|res|</w>
        </l>
      </lg>
    </body>
  </text>
