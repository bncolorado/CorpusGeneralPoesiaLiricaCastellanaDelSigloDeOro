  <text>
    <body>
      <head>
        <title>Buscas en Roma a Roma, ¡oh, peregrino!,</title>
      </head>
      <lg cert="high" type="Soneto">
        <l met="1|-|-|4|-|6|-|-|-|10|-|" n="1">
          <!--Buscas en Roma a Roma, ¡oh, peregrino!,-->
          <w lemma="busca" type="NCFP000">bus|cas|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="roma" type="NP00000">ro|ma</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="roma" type="NP00000">ro|ma</w>
          <w lemma="oh" type="I">oh|</w>
          <w lemma="peregrino" type="NCMS000">pe|re|gri|no|</w>
        </l>
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="2">
          <!--y en Roma misma a Roma, no la hallas:-->
          <w lemma="y" type="CC">y</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="roma" type="NP00000">ro|ma|</w>
          <w lemma="mismo" type="AQ0FS00">mis|ma</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="roma" type="NP00000">ro|ma|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="hallar" type="VMIP2S0">ha|llas|</w>
        </l>
        <l met="-|2|-|4|-|-|-|8|-|10|-|" n="3">
          <!--cadáver son las que ostentó murallas,-->
          <w lemma="cadáver" type="NCMS000">ca|dá|ver|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="ostentar" type="VMIS3S0">os|ten|tó|</w>
          <w lemma="muralla" type="NCFP000">mu|ra|llas|</w>
        </l>
        <l met="-|2|-|-|5|6|-|-|-|10|-|" n="4">
          <!--y tumba de sí proprio el Aventino.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="tumba" type="NCFS000">tum|ba|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="sí" type="RG">sí|</w>
          <w lemma="proprio" type="AQ0MS00">pro|prio</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="aventino" type="NP00000">a|ven|ti|no|</w>
        </l>
        <l met="1|-|-|-|-|6|-|-|-|10|-|" n="5">
          <!--Yace donde reinaba el Palatino;-->
          <w lemma="yacer" type="VMIP3S0">ya|ce|</w>
          <w lemma="donde" type="PR00000">don|de|</w>
          <w lemma="reinar" type="VMII3S0">rei|na|ba</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="palatino" type="NP00000">pa|la|ti|no|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|-|10|-|" n="6">
          <!--y limadas del tiempo las medallas,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="limar" type="VMP00PF">li|ma|das|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="tiempo" type="NCMS000">tiem|po|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="medalla" type="NCFP000">me|da|llas|</w>
        </l>
        <l met="1|-|3|-|-|6|-|-|-|10|-|" n="7">
          <!--más se muestran destrozo a las batallas-->
          <w lemma="más" type="RG">más|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="mostrar" type="VMIP3P0">mues|tran|</w>
          <w lemma="destrozo" type="NCMS000">des|tro|zo</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="batalla" type="NCFP000">ba|ta|llas|</w>
        </l>
        <l met="-|-|-|4|-|-|-|8|-|10|-|" n="8">
          <!--de las edades que blasón latino.-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="edad" type="NCFP000">e|da|des|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="blasón" type="NCMS000">bla|són|</w>
          <w lemma="latino" type="AQ0MS00">la|ti|no|</w>
        </l>
        <l met="1|-|3|-|-|6|-|-|-|10|-|" n="9">
          <!--Sólo el Tibre quedó, cuya corriente,-->
          <w lemma="sólo" type="RG">só|lo</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="tibre" type="NP00000">ti|bre|</w>
          <w lemma="quedar" type="VMIS3S0">que|dó|</w>
          <w lemma="cuyo" type="PR0FS00">cu|ya|</w>
          <w lemma="corriente" type="NCFS000">co|rrien|te|</w>
        </l>
        <l met="-|-|3|-|-|6|7|-|-|10|-|" n="10">
          <!--si ciudad la regó, ya sepultura-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="ciudad" type="NCFS000">ciu|dad|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="regar" type="VMIS3S0">re|gó|</w>
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="sepultura" type="NCFS000">se|pul|tu|ra|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="11">
          <!--la llora con funesto son doliente.-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="llorar" type="VMIP3S0">llo|ra|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="funesto" type="AQ0MS00">fu|nes|to|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="doliente" type="AQ0CS00">do|lien|te|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="12">
          <!--¡Oh, Roma!, en tu grandeza, en tu hermosura,-->
          <w lemma="oh" type="I">oh|</w>
          <w lemma="roma" type="NP00000">ro|ma</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="grandeza" type="NCFS000">gran|de|za</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="hermosura" type="NCFS000">her|mo|su|ra|</w>
        </l>
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="13">
          <!--huyó lo que era firme, y solamente-->
          <w lemma="huir" type="VMIS3S0">hu|yó|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="ser" type="VSII3S0">e|ra|</w>
          <w lemma="firme" type="AQ0CS00">fir|me</w>
          <w lemma="y" type="CC">y</w>
          <w lemma="solamente" type="RG">so|la|men|te|</w>
        </l>
        <l met="-|-|-|4|-|-|-|8|-|10|-|" n="14">
          <!--lo fugitivo permanece y dura.-->
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="fugitivo" type="NCMS000">fu|gi|ti|vo|</w>
          <w lemma="permanecer" type="VMIP3S0">per|ma|ne|ce</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="duro" type="AQ0FS00">du|ra|</w>
        </l>
      </lg>
    </body>
  </text>
