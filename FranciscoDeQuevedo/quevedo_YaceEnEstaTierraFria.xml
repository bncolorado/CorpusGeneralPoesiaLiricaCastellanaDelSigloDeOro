  <text>
    <body>
      <head>
        <title>Yace en esta tierra fría,</title>
      </head>
      <lg cert="high" type="Redondillas">
        <l met="1|-|3|-|5|-|7|-|" n="1">
          <!--Yace en esta tierra fría,-->
          <w lemma="yacer" type="VMIP3S0">ya|ce</w>
          <w lemma="en" type="SP">en</w>
          <w lemma="este" type="DD0FS0">es|ta</w>
          <w lemma="tierra" type="NCFS000">tie|rra</w>
          <w lemma="frío" type="AQ0FS00">frí|a|</w>
        </l>
        <l met="1|-|-|4|-|6|-|" n="2">
          <!--digna de toda crianza,-->
          <w lemma="digno" type="AQ0FS00">dig|na|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="todo" type="DI0FS0">to|da|</w>
          <w lemma="crianza" type="NCFS000">crian|za|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="3">
          <!--la vieja cuya alabanza-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="viejo" type="AQ0FS00">vie|ja|</w>
          <w lemma="cuyo" type="PR0FS00">cu|ya</w>
          <w lemma="alabanza" type="NCFS000">a|la|ban|za|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="4">
          <!--tantas plumas merecía.-->
          <w lemma="tanto" type="DI0FP0">tan|tas|</w>
          <w lemma="pluma" type="NCFP000">plu|mas|</w>
          <w lemma="merecer" type="VMII3S0">me|re|cí|a|</w>
        </l>
        <l met="1|2|-|-|5|-|7|-|" n="5">
          <!--No quiso en el cielo entrar-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="querer" type="VMIS3S0">qui|so</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cielo" type="NCMS000">cie|lo</w>
          <w lemma="entrar" type="VMN0000">en|trar|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="6">
          <!--a gozar de las estrellas,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="gozar" type="VMN0000">go|zar|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="estrella" type="NCFP000">es|tre|llas|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="7">
          <!--por no estar entre doncellas-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="estar" type="VMN0000">es|tar|</w>
          <w lemma="entre" type="SP">en|tre|</w>
          <w lemma="doncella" type="NCFP000">don|ce|llas|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="8">
          <!--que no pudiese manchar.-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="poder" type="VMSI3S0">pu|die|se|</w>
          <w lemma="manchar" type="VMN0000">man|char|</w>
        </l>
      </lg>
    </body>
