          <w lemma="yo" type="PP1CSN0">yo</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="este" type="DD0MP0">es|tos|</w>
          <w lemma="sauce" type="NCMP000">sau|ces|</w>
