          <w lemma="dar" type="VMIS1S0">di|</w>
          <w lemma="bárbaro" type="AQ0FS00">bár|ba|ra</w>
          <w lemma="arena" type="NCFS000">a|re|na|</w>
        </l>
        <l met="-|2|-|4|-|6|-|-|-|10|-|" n="20">
