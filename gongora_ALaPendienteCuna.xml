          <w lemma="oh" type="I">oh|</w>
          <w lemma="huésped" type="NCFS000">hués|pe|da</w>
          <w lemma="importuno" type="AQ0FS00">im|por|tu|na|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|9|10|-|" n="4">
