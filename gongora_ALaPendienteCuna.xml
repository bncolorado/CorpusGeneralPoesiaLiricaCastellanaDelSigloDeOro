          <w lemma="aplauso" type="NCMS000">a|plau|so</w>
          <w lemma="a" type="SP">a</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="ruiseñor" type="NCMS000">rui|se|ñor|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="negar" type="VMIP1S0">nie|go|</w>
          <w lemma="breve" type="AQ0CS00">bre|ve|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|9|10|-|" n="18">
          <!--sobre la yerba que ese cristal bebe.-->
          <w lemma="sobre" type="SP">so|bre|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="yerba" type="NCFS000">yer|ba|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="ese" type="DD0MS0">e|se|</w>
          <w lemma="cristal" type="NCMS000">cris|tal|</w>
          <w lemma="beber" type="VMIP3S0">be|be|</w>
        </l>
        <l met="1|2|3|-|-|6|-|" n="19">
