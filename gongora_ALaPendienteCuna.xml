    <body>
      <head>
        <title>A la pendiente cuna</title>
      </head>
      <lg cert="high" type="Canción">
        <l met="-|-|-|4|-|6|-|" n="1">
          <!--A la pendiente cuna-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="pendiente" type="AQ0CS00">pen|dien|te|</w>
          <w lemma="cuna" type="NCFS000">cu|na|</w>
        </l>
        <l met="1|-|-|-|-|6|-|8|-|10|-|" n="2">
          <!--vuelves, al que fiaste nido estrecho,-->
          <w lemma="volver" type="VMIP2S0">vuel|ves|</w>
          <w lemma="a" type="SP">a</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="fiar" type="VMIS2S0">fi|as|te|</w>
          <w lemma="nido" type="NCMS000">ni|do</w>
