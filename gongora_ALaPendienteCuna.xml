          <w lemma="querer" type="VMIP2S0">quie|res|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="ruido" type="NCMS000">ru|i|do|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="presumir" type="VMSP3S0">pre|su|ma|</w>
        </l>
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="12">
