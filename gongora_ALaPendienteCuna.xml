          <w lemma="de" type="SP">de</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="techo" type="NCMS000">te|cho|</w>
        </l>
        <l met="-|-|3|-|-|6|7|8|-|10|-|" n="5">
