          <w lemma="de" type="SP">de|</w>
          <w lemma="sierpe" type="NCFP000">sier|pes|</w>
          <w lemma="haber" type="VAIP2S0">has|</w>
          <w lemma="dejar" type="VMP00SM">de|ja|do</w>
          <w lemma="engendrador" type="AQ0FS00">en|gen|dra|do|ra|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="21">
