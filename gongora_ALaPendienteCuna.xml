          <w lemma="por" type="SP">por|</w>
          <w lemma="turbar" type="VMN0000">tur|bar|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="serena" type="AQ0FS00">se|re|na|</w>
        </l>
        <l met="1|-|-|-|-|6|-|8|-|10|-|" n="22">
