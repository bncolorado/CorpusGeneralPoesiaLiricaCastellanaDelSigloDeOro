          <w lemma="que" type="CS">que|</w>
          <w lemma="mentir" type="VMSP3S0">mien|te|</w>
          <w lemma="voz" type="NCFS000">voz|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="invidia" type="NCFS000">in|vi|dia</w>
