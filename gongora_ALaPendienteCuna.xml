          <w lemma="este" type="DD0MP0">es|tos|</w>
          <w lemma="quicio" type="NCMP000">qui|cios|</w>
          <w lemma="negar" type="VMIP1S0">nie|go</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="cultura" type="NCFS000">cul|tu|ra|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="27">
