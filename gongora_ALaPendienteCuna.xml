          <w lemma="sellar" type="VMP00SM">se|lla|do</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="labio" type="NCMS000">la|bio|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="quietud" type="NCFS000">quie|tud|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="esconder" type="VMIP3S0">es|con|de|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="25">
          <!--Aquí, pues, al cuidado-->
          <w lemma="aquí" type="RG">a|quí|</w>
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="a" type="SP">a</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cuidado" type="NCMS000">cui|da|do|</w>
        </l>
        <l met="1|2|-|4|-|6|-|-|-|10|-|" n="26">
