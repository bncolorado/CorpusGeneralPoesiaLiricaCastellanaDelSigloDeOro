  <text>
    <body>
      <head>
        <title>¿Cuál es el ave ligera</title>
      </head>
      <lg cert="high" type="Sextillas">
        <l met="1|2|-|4|-|-|7|-|" n="1">
          <!--¿Cuál es el ave ligera-->
          <w lemma="cuál" type="PT0CS00">cuál|</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="ave" type="NCFS000">a|ve|</w>
          <w lemma="ligero" type="AQ0FS00">li|ge|ra|</w>
        </l>
        <l met="-|2|3|-|5|-|7|-|" n="2">
          <!--que está siempre en un lugar-->
          <w lemma="que" type="CS">que</w>
          <w lemma="estar" type="VMIP3S0">es|tá|</w>
          <w lemma="siempre" type="RG">siem|pre</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="lugar" type="NCMS000">lu|gar|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="3">
          <!--y anda siempre caminando,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="andar" type="VMIP3S0">an|da|</w>
          <w lemma="siempre" type="RG">siem|pre|</w>
          <w lemma="caminar" type="VMG0000">ca|mi|nan|do|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="4">
          <!--penetra y entra doquiera,-->
          <w lemma="penetrar" type="VMIP3S0">pe|ne|tra</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="entrar" type="VMIP3S0">en|tra|</w>
          <w lemma="doquier" type="RG">do|quie|ra|</w>
        </l>
        <l met="1|2|-|4|-|-|7|-|" n="5">
          <!--de un vuelo pasa la mar,-->
          <w lemma="de" type="SP">de</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="vuelo" type="NCMS000">vue|lo|</w>
          <w lemma="pasar" type="VMIP3S0">pa|sa|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mar" type="NCCS000">mar|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="6">
          <!--las nubes sobrepujando?-->
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="nube" type="NCFP000">nu|bes|</w>
          <w lemma="sobrepujar" type="VMG0000">so|bre|pu|jan|do|</w>
        </l>
        <l met="-|2|3|-|5|-|7|-|" n="7">
          <!--Así verla no podemos,-->
          <w lemma="así" type="RG">a|sí|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="poder" type="VMIP1P0">po|de|mos|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="8">
          <!--y quien la está descubriendo-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="quien" type="PR0CS00">quien|</w>
          <w lemma="lo" type="PP3FSA0">la</w>
          <w lemma="estar" type="VAIP3S0">es|tá|</w>
          <w lemma="descubrir" type="VMG0000">des|cu|brien|do|</w>
        </l>
        <l met="1|-|3|-|5|6|7|-|" n="9">
          <!--sabio queda en sola una hora;-->
          <w lemma="sabio" type="AQ0MS00">sa|bio|</w>
          <w lemma="quedar" type="VMIP3S0">que|da</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="solo" type="AQ0FS00">so|la</w>
          <w lemma="uno" type="DI0FS0">u|na</w>
          <w lemma="hora" type="NCFS000">ho|ra|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="10">
          <!--mas tal vez la conocemos-->
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="tal_vez" type="RG">tal_|vez|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="conocer" type="VMIP1P0">co|no|ce|mos|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="11">
          <!--las paredes solas viendo-->
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="pared" type="NCFP000">pa|re|des|</w>
          <w lemma="solo" type="AQ0FP00">so|las|</w>
          <w lemma="ver" type="VMG0000">vien|do|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="12">
          <!--de la casa donde mora.-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="casa" type="NCFS000">ca|sa|</w>
          <w lemma="donde" type="PR00000">don|de|</w>
          <w lemma="morar" type="VMIP3S0">mo|ra|</w>
        </l>
      </lg>
    </body>
