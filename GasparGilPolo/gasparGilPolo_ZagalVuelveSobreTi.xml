      <head>
        <title>Zagal, vuelve sobre ti,</title>
      </head>
      <lg cert="high" type="Canción">
        <l met="-|2|3|-|-|-|7|-|" n="1">
          <!--Zagal, vuelve sobre ti,-->
          <w lemma="zagal" type="NP00000">za|gal|</w>
