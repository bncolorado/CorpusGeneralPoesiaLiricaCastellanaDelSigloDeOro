  <text>
    <body>
      <head>
        <title>¿Quién jamás caballo vio</title>
      </head>
      <lg cert="high" type="Quintillas">
        <l met="1|-|3|-|5|-|7|-|" n="1">
          <!--¿Quién jamás caballo vio-->
          <w lemma="quién" type="PT0CS00">quién|</w>
          <w lemma="jamás" type="RG">ja|más|</w>
          <w lemma="caballo" type="NCMS000">ca|ba|llo|</w>
          <w lemma="ver" type="VMIS3S0">vio|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="2">
          <!--que por extraña manera,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="extraño" type="AQ0FS00">ex|tra|ña|</w>
          <w lemma="manera" type="NCFS000">ma|ne|ra|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="3">
          <!--sin jamás haber comido,-->
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="jamás" type="RG">ja|más|</w>
          <w lemma="haber" type="VAN0000">ha|ber|</w>
          <w lemma="comer" type="VMP00SM">co|mi|do|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="4">
          <!--con el viento sostenido,-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="viento" type="NCMS000">vien|to|</w>
          <w lemma="sostener" type="VMP00SM">sos|te|ni|do|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="5">
          <!--se le iguale en la carrera?-->
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="le" type="PP3CSD0">le</w>
          <w lemma="igualar" type="VMSP3S0">i|gua|le</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="carrera" type="NCFS000">ca|rre|ra|</w>
        </l>
        <l met="1|-|3|4|-|-|7|-|" n="6">
          <!--Obra muy grandes hazañas-->
          <w lemma="obra" type="NCFS000">o|bra|</w>
          <w lemma="muy" type="RG">muy|</w>
          <w lemma="grande" type="AQ0CP00">gran|des|</w>
          <w lemma="hazaña" type="NCFP000">ha|za|ñas|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="7">
          <!--y en sus corridas extrañas-->
          <w lemma="y" type="CC">y</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="corrida" type="NCFP000">co|rri|das|</w>
          <w lemma="extraño" type="AQ0FP00">ex|tra|ñas|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="8">
          <!--va arrastrando el duro pecho,-->
          <w lemma="ir" type="VMIP3S0">va</w>
          <w lemma="arrastrar" type="VMG0000">a|rras|tran|do</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="duro" type="AQ0MS00">du|ro|</w>
          <w lemma="pecho" type="NCMS000">pe|cho|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="9">
          <!--sus riendas, por más provecho,-->
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="rienda" type="NCFP000">rien|das|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="provecho" type="NCMS000">pro|ve|cho|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="10">
          <!--metidas en sus entrañas.-->
          <w lemma="meter" type="VMP00PF">me|ti|das|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="entraña" type="NCFP000">en|tra|ñas|</w>
        </l>
      </lg>
    </body>
  </text>
