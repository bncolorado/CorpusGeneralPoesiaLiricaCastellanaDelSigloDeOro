  <text>
    <body>
      <head>
        <title>Morir debiera sin verte,</title>
      </head>
      <lg cert="high" type="Canción">
        <l met="-|2|-|4|-|-|7|-|" n="1">
          <!--Morir debiera sin verte,-->
          <w lemma="morir" type="VMN0000">mo|rir|</w>
          <w lemma="deber" type="VMSI3S0">de|bie|ra|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
          <w lemma="te" type="PP2CS00">te|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="2">
          <!--hermosísima pastora,-->
          <w lemma="hermoso" type="AQSFS00">her|mo|sí|si|ma|</w>
          <w lemma="pastor" type="NCFS000">pas|to|ra|</w>
        </l>
        <l met="-|-|3|4|5|6|7|-|" n="3">
          <!--pues que osé tan sola una hora-->
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="osar" type="VMIS1S0">o|sé|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="solo" type="AQ0FS00">so|la</w>
          <w lemma="uno" type="DI0FS0">u|na</w>
          <w lemma="hora" type="NCFS000">ho|ra|</w>
        </l>
        <l met="-|2|3|-|5|-|7|-|" n="4">
          <!--estar vivo y no quererte.-->
          <w lemma="estar" type="VMN0000">es|tar|</w>
          <w lemma="vivo" type="AQ0MS00">vi|vo</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="querer" type="VMN0000">que|rer|</w>
          <w lemma="te" type="PP2CS00">te|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="5">
          <!--De un dichoso amor gozara,-->
          <w lemma="de" type="SP">de</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="dichoso" type="AQ0MS00">di|cho|so</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
          <w lemma="gozar" type="VMSI3S0">go|za|ra|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="6">
          <!--dejado el tormento aparte,-->
          <w lemma="dejar" type="VMP00SM">de|ja|do</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="tormento" type="NCMS000">tor|men|to</w>
          <w lemma="aparte" type="RG">a|par|te|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="7">
          <!--si en acordarme de amarte,-->
          <w lemma="si" type="CS">si</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="acordar" type="VMN0000">a|cor|dar|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="amar" type="VMN0000">a|mar|</w>
          <w lemma="te" type="PP2CS00">te|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="8">
          <!--de mi olvido me olvidara.-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="mi" type="DP1CSS">mi</w>
          <w lemma="olvido" type="NCMS000">ol|vi|do|</w>
          <w lemma="me" type="PP1CS00">me</w>
          <w lemma="olvidar" type="VMSI3S0">ol|vi|da|ra|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="9">
          <!--Que de morirme y perderte-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="morir" type="VMN0000">mo|rir|</w>
          <w lemma="me" type="PP1CS00">me</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="perder" type="VMN0000">per|der|</w>
          <w lemma="te" type="PP2CS00">te|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="10">
          <!--tengo recelo, pastora,-->
          <w lemma="tener" type="VMIP1S0">ten|go|</w>
          <w lemma="recelo" type="NCMS000">re|ce|lo|</w>
          <w lemma="pastor" type="NCFS000">pas|to|ra|</w>
        </l>
        <l met="-|-|3|4|5|6|7|-|" n="11">
          <!--pues que osé tan sola una hora-->
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="osar" type="VMIS1S0">o|sé|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="solo" type="AQ0FS00">so|la</w>
          <w lemma="uno" type="DI0FS0">u|na</w>
          <w lemma="hora" type="NCFS000">ho|ra|</w>
        </l>
        <l met="-|2|3|-|5|-|7|-|" n="12">
          <!--estar vivo y no quererte.-->
          <w lemma="estar" type="VMN0000">es|tar|</w>
          <w lemma="vivo" type="AQ0MS00">vi|vo</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="querer" type="VMN0000">que|rer|</w>
          <w lemma="te" type="PP2CS00">te|</w>
        </l>
      </lg>
