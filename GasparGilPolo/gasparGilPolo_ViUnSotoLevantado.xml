  <text>
    <body>
      <head>
        <title>Vi un soto levantado</title>
      </head>
      <lg cert="high" type="Quintillas">
        <l met="1|2|3|-|-|-|7|-|" n="1">
          <!--Vi un soto levantado-->
          <w lemma="ver" type="VMIS1S0">vi|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="soto" type="NCMS000">so|to|</w>
          <w lemma="levantar" type="VMP00SM">le|van|ta|do|</w>
        </l>
        <l met="-|-|-|4|-|6|7|-|" n="2">
          <!--sobre los aires un día,-->
          <w lemma="sobre" type="SP">so|bre|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="aire" type="NCMP000">ai|res|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="día" type="NCMS000">dí|a|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="3">
          <!--el cual, con sangre regado,-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cual" type="PR0CS00">cual|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="sangre" type="NCFS000">san|gre|</w>
          <w lemma="regar" type="VMP00SM">re|ga|do|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="4">
          <!--con gran ansia cultivado-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="gran" type="AQ0CS00">gran|</w>
          <w lemma="ansia" type="NCFS000">an|sia|</w>
          <w lemma="cultivar" type="VMP00SM">cul|ti|va|do|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="5">
          <!--muchas yerbas producía.-->
          <w lemma="mucho" type="DI0FP0">mu|chas|</w>
          <w lemma="yerba" type="NCFP000">yer|bas|</w>
          <w lemma="producir" type="VMII3S0">pro|du|cí|a|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="6">
          <!--De allí un manojo arrancando,-->
          <w lemma="de" type="SP">de</w>
          <w lemma="allí" type="RG">a|llí</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="manojo" type="NCMS000">ma|no|jo</w>
          <w lemma="arrancar" type="VMG0000">a|rran|can|do|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="7">
          <!--y solo con él tocando,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="solo" type="RG">so|lo|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="él" type="PP3MS00">él|</w>
          <w lemma="tocar" type="VMG0000">to|can|do|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="8">
          <!--una sabia y cuerda gente,-->
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="sabio" type="AQ0FS00">sa|bia</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="cuerdo" type="AQ0FS00">cuer|da|</w>
          <w lemma="gente" type="NCFS000">gen|te|</w>
        </l>
        <l met="-|-|3|4|5|-|7|-|" n="9">
          <!--la dejé cabe una puente,-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="dejar" type="VMIS1S0">de|jé|</w>
          <w lemma="caber" type="VMIP3S0">ca|be</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="puente" type="NCMS000">puen|te|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="10">
          <!--sin dolores lamentando.-->
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="dolor" type="NCMP000">do|lo|res|</w>
          <w lemma="lamentar" type="VMG0000">la|men|tan|do|</w>
        </l>
      </lg>
    </body>
  </text>
