  <text>
    <body>
      <head>
        <title> En las internas entrañas</title>
      </head>
      <lg cert="..." type="...">
        <l met="-|-|-|4|-|-|7|-|" n="1">
          <!--En las internas entrañas-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="interno" type="AQ0FP00">in|ter|nas|</w>
          <w lemma="entraña" type="NCFP000">en|tra|ñas|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="2">
          <!--Sentí un golpe repentino:-->
          <w lemma="sentir" type="VMIS1S0">sen|tí</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="golpe" type="NCMS000">gol|pe|</w>
          <w lemma="repentino" type="AQ0MS00">re|pen|ti|no|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="3">
          <!--El blason era divino,-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="blason" type="NCMS000">bla|son|</w>
          <w lemma="ser" type="VSII3S0">e|ra|</w>
          <w lemma="divino" type="AQ0MS00">di|vi|no|</w>
        </l>
        <l met="-|-|3|4|-|-|7|-|" n="4">
          <!--Porque obró grandes hazañas.-->
          <w lemma="porque" type="CS">por|que</w>
          <w lemma="obrar" type="VMIS3S0">o|bró|</w>
          <w lemma="grande" type="AQ0CP00">gran|des|</w>
          <w lemma="hazaña" type="NCFP000">ha|za|ñas|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="5">
          <!--Con el golpe fuí herida,-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="golpe" type="NCMS000">gol|pe|</w>
          <w lemma="fuí" type="RG">fuí|</w>
          <w lemma="herir" type="VMP00SF">he|ri|da|</w>
        </l>
        <l met="-|-|-|4|5|-|7|-|" n="6">
          <!--Y aunque la herida es mortal,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="aunque" type="CC">aun|que|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="herida" type="NCFS000">he|ri|da</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="mortal" type="AQ0CS00">mor|tal|</w>
        </l>
        <l met="1|2|-|4|-|-|7|-|" n="7">
          <!--Y es un dolor sin igual,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="dolor" type="NCMS000">do|lor|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="igual" type="RG">i|gual|</w>
        </l>
        <l met="1|2|-|-|5|-|7|-|" n="8">
          <!--Es muerte que causa vida.-->
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="muerte" type="NCFS000">muer|te|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="causar" type="VMIP3S0">cau|sa|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
        </l>
        <l met="-|2|-|4|-|6|7|-|" n="9">
          <!--Si mata, ¿cómo da vida?-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="matar" type="VMIP3S0">ma|ta|</w>
          <w lemma="cómo" type="PT00000">có|mo|</w>
          <w lemma="dar" type="VMIP3S0">da|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="10">
          <!--Y si vida, ¿cómo muere?-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
          <w lemma="cómo" type="PT00000">có|mo|</w>
          <w lemma="morir" type="VMIP3S0">mue|re|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="11">
          <!--¿Cómo sana, cuando hiere,-->
          <w lemma="cómo" type="PT00000">có|mo|</w>
          <w lemma="sanar" type="VMIP3S0">sa|na|</w>
          <w lemma="cuando" type="CS">cuan|do|</w>
          <w lemma="herir" type="VMIP3S0">hie|re|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="12">
          <!--Y se ve con él unida?-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="ver" type="VMIP3S0">ve|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="él" type="PP3MS00">él|</w>
          <w lemma="unir" type="VMP00SF">u|ni|da|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="13">
          <!--Tiene tan divinas mañas,-->
          <w lemma="tener" type="VMIP3S0">tie|ne|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="divino" type="AQ0FP00">di|vi|nas|</w>
          <w lemma="maña" type="NCFP000">ma|ñas|</w>
        </l>
        <l met="-|2|3|-|5|-|7|-|" n="14">
          <!--Que en un tan acerbo trance-->
          <w lemma="que" type="CS">que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="acerbo" type="AQ0MS00">a|cer|bo|</w>
          <w lemma="trance" type="NCMS000">tran|ce|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="15">
          <!--Sale triunfando del lance,-->
          <w lemma="salir" type="VMIP3S0">sa|le|</w>
          <w lemma="triunfar" type="VMG0000">triun|fan|do|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="lance" type="NCMS000">lan|ce|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="16">
          <!--Obrando grandes hazañas.-->
          <w lemma="obrar" type="VMG0000">o|bran|do|</w>
          <w lemma="grande" type="AQ0CP00">gran|des|</w>
          <w lemma="hazaña" type="NCFP000">ha|za|ñas|</w>
        </l>
      </lg>
    </body>
  </text>
