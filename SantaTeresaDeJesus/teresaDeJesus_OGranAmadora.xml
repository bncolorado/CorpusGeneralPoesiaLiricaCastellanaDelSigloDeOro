  <text>
    <body>
      <head>
        <title>O gran amadora</title>
      </head>
      <lg cert="..." type="...">
        <l met="-|2|-|-|5|-|" n="1">
          <!--O gran amadora-->
          <w lemma="o" type="CC">o|</w>
          <w lemma="gran" type="AQ0CS00">gran|</w>
          <w lemma="amadora" type="AQ0FS00">a|ma|do|ra|</w>
        </l>
        <l met="-|-|-|-|5|-|" n="2">
          <!--Del Eterno Dios,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="eterno_dios" type="NP00000">e|ter|no_|dios|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="3">
          <!--Estrella luciente,-->
          <w lemma="estrella" type="NCFS000">es|tre|lla|</w>
          <w lemma="luciente" type="AQ0CS00">lu|cien|te|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="4">
          <!--Amparadnos vos.-->
          <w lemma="amparar" type="VMM02P0">am|pa|rad|</w>
          <w lemma="nos" type="PP1CP00">nos|</w>
          <w lemma="vos" type="PP2CS0P">vos|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="5">
          <!--Desde tierna edad-->
          <w lemma="desde" type="SP">des|de|</w>
          <w lemma="tierno" type="AQ0FS00">tier|na</w>
          <w lemma="edad" type="NCFS000">e|dad|</w>
        </l>
        <l met="-|-|-|4|-|" n="6">
          <!--Tomastes Esposo,-->
          <w lemma="tomastes_esposo" type="NP00000">to|mas|tes_es|po|so|</w>
        </l>
        <l met="1|2|-|-|5|-|" n="7">
          <!--Fué tanto el amor,-->
          <w lemma="fué" type="NP00000">fué|</w>
          <w lemma="tanto" type="RG">tan|to</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
        </l>
        <l met="-|2|3|-|5|-|" n="8">
          <!--Que no os dió reposo:-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="dió" type="VMIS3S0">dió|</w>
          <w lemma="reposo" type="NCMS000">re|po|so|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="9">
          <!--Quien es temeroso,-->
          <w lemma="quien" type="PR0CS00">quien|</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="temeroso" type="AQ0MS00">te|me|ro|so|</w>
        </l>
        <l met="1|-|3|4|5|-|" n="10">
          <!--No se llegue á vos-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="llegar" type="VMSP3S0">lle|gue</w>
          <w lemma="á" type="RG">á|</w>
          <w lemma="vos" type="PP2CS0P">vos|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="11">
          <!--Si estima la vida-->
          <w lemma="si" type="CS">si</w>
          <w lemma="estimar" type="VMIP3S0">es|ti|ma|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="12">
          <!--Y el morir por Dios.-->
          <w lemma="y" type="CC">y</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="morir" type="VMN0000">mo|rir|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="dios" type="NP00000">dios|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="13">
          <!--Mirad los cobardes-->
          <w lemma="mirar" type="VMM02P0">mi|rad|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="cobarde" type="NCCP000">co|bar|des|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="14">
          <!--Aquesta doncella,-->
          <w lemma="aquesta" type="NP00000">a|ques|ta|</w>
          <w lemma="doncella" type="NCFS000">don|ce|lla|</w>
        </l>
        <l met="-|2|3|-|5|-|" n="15">
          <!--Que no estima el oro-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="estimar" type="VMIP3S0">es|ti|ma</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="oro" type="NCMS000">o|ro|</w>
        </l>
        <l met="-|2|-|4|5|-|" n="16">
          <!--Ni verse tan bella:-->
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
          <w lemma="se" type="PP3CN00">se|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="bello" type="AQ0FS00">be|lla|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="17">
          <!--Metida en la guerra-->
          <w lemma="meter" type="VMP00SF">me|ti|da</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="guerra" type="NCFS000">gue|rra|</w>
        </l>
        <l met="-|-|-|4|-|" n="18">
          <!--De persecucion,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="persecucion" type="NCFS000">per|se|cu|cion|</w>
        </l>
        <l met="-|-|-|-|5|-|" n="19">
          <!--Para padecer-->
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="padecer" type="VMN0000">pa|de|cer|</w>
        </l>
        <l met="-|2|-|4|-|" n="20">
          <!--Con gran corazon.-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="gran" type="AQ0CS00">gran|</w>
          <w lemma="corazon" type="VMIS3P0">co|ra|zon|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="21">
          <!--Mas pena le da-->
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="pena" type="NCFS000">pe|na|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="dar" type="VMIP3S0">da|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="22">
          <!--Vivir sin su Esposo,-->
          <w lemma="vivir" type="VMN0000">vi|vir|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="su" type="DP3CSN">su</w>
          <w lemma="esposo" type="NP00000">es|po|so|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="23">
          <!--Y así en los tormentos-->
          <w lemma="y" type="CC">y</w>
          <w lemma="así" type="RG">a|sí</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="tormento" type="NCMP000">tor|men|tos|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="24">
          <!--Hallaba reposo:-->
          <w lemma="hallar" type="VMII3S0">ha|lla|ba|</w>
          <w lemma="reposo" type="NCMS000">re|po|so|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="25">
          <!--Todo le es gozoso,-->
          <w lemma="todo" type="PI0MS00">to|do|</w>
          <w lemma="le" type="PP3CSD0">le</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="gozoso" type="AQ0MS00">go|zo|so|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="26">
          <!--Quiere ya morir,-->
          <w lemma="querer" type="VMIP3S0">quie|re|</w>
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="morir" type="VMN0000">mo|rir|</w>
        </l>
        <l met="-|-|-|-|5|-|" n="27">
          <!--Pues que con la vida-->
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
        </l>
        <l met="1|2|-|-|5|-|" n="28">
          <!--No puede vivir.-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="poder" type="VMIP3S0">pue|de|</w>
          <w lemma="vivir" type="VMN0000">vi|vir|</w>
        </l>
        <l met="-|-|-|-|5|-|" n="29">
          <!--Las que pretendemos-->
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="pretender" type="VMIP1P0">pre|ten|de|mos|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="30">
          <!--Gozar de su gozo,-->
          <w lemma="gozar" type="VMN0000">go|zar|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="gozo" type="NCMS000">go|zo|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="31">
          <!--Nunca nos cansemos,-->
          <w lemma="nunca" type="RG">nun|ca|</w>
          <w lemma="nos" type="PP1CP00">nos|</w>
          <w lemma="cansar" type="VMSP1P0">can|se|mos|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="32">
          <!--Por hallar reposo,-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="hallar" type="VMN0000">ha|llar|</w>
          <w lemma="reposo" type="NCMS000">re|po|so|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="33">
          <!--¡Oh engaño engañoso,-->
          <w lemma="oh" type="I">oh</w>
          <w lemma="engaño" type="NCMS000">en|ga|ño</w>
          <w lemma="engañoso" type="AQ0MS00">en|ga|ño|so|</w>
        </l>
        <l met="-|-|-|-|5|-|" n="34">
          <!--Y que sin amor,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="35">
          <!--Es querer sanar,-->
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="querer" type="VMN0000">que|rer|</w>
          <w lemma="sanar" type="VMN0000">sa|nar|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="36">
          <!--Viviendo el dolor.-->
          <w lemma="vivir" type="VMG0000">vi|vien|do</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="dolor" type="NCMS000">do|lor|</w>
        </l>
      </lg>
    </body>
  </text>
