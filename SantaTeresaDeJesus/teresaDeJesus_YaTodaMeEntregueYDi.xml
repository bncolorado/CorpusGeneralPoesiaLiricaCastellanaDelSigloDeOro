          <w lemma="cobrar" type="VMG0000">co|bran|do|</w>
          <w lemma="nuevo" type="AQ0FS00">nue|va|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
        </l>
        <l met="-|2|-|4|5|-|7|-|" n="10">
