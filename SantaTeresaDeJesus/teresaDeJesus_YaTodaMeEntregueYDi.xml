    <body>
      <head>
        <title>  Ya toda me entregué y dí,</title>
      </head>
      <lg cert="..." type="...">
        <l met="1|2|-|-|-|6|7|-|" n="1">
          <!--Ya toda me entregué y dí,-->
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="todo" type="DI0FS0">to|da|</w>
          <w lemma="me" type="PP1CS00">me</w>
          <w lemma="entregar" type="VMIS1S0">en|tre|gué</w>
          <w lemma="y" type="CC">y</w>
          <w lemma="dí" type="VMIS1S0">dí|</w>
        </l>
        <l met="-|-|3|4|5|-|7|-|" n="2">
          <!--Y de tal suerte he trocado,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="tal" type="DD0CS0">tal|</w>
          <w lemma="suerte" type="NCFS000">suer|te</w>
          <w lemma="haber" type="VAIP1S0">he|</w>
          <w lemma="trocar" type="VMP00SM">tro|ca|do|</w>
        </l>
        <l met="-|-|3|4|-|-|7|-|" n="3">
          <!--Que mi Amado es para mí-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="mi" type="DP1CSS">mi</w>
          <w lemma="amado" type="NP00000">a|ma|do</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="4">
          <!--Y yo soy para mi Amado.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="ser" type="VSIP1S0">soy|</w>
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="mi" type="DP1CSS">mi</w>
          <w lemma="amado" type="NP00000">a|ma|do|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="5">
