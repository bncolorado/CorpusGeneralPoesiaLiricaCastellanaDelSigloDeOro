          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="12">
          <!--Y yo soy para mi Amado.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="ser" type="VSIP1S0">soy|</w>
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="mi" type="DP1CSS">mi</w>
          <w lemma="amado" type="NP00000">a|ma|do|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="13">
          <!--Tiróme con una flecha-->
          <w lemma="tiróme" type="NP00000">ti|ró|me|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="flecha" type="NCFS000">fle|cha|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="14">
          <!--Enarbolada de amor,-->
          <w lemma="enarbolar" type="VMP00SF">e|nar|bo|la|da|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="15">
          <!--Y mi alma quedó hecha-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="alma" type="NCFS000">al|ma|</w>
          <w lemma="quedar" type="VMIS3S0">que|dó</w>
          <w lemma="hacer" type="VMP00SF">he|cha|</w>
        </l>
        <l met="1|-|-|-|-|6|-|" n="16">
