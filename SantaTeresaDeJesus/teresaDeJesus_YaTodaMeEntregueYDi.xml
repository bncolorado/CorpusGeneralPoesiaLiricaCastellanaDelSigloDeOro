          <w lemma="pues" type="CS">pues|</w>
          <w lemma="á" type="VMIF3S0">á|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="dios" type="NP00000">dios|</w>
          <w lemma="me" type="PP1CS00">me</w>
          <w lemma="haber" type="VAIP1S0">he</w>
          <w lemma="entregar" type="VMP00SM">en|tre|ga|do|</w>
        </l>
        <l met="-|-|3|4|-|-|7|-|" n="19">
          <!--Y mi Amado es para mí-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="mi" type="DP1CSS">mi</w>
          <w lemma="amado" type="NP00000">a|ma|do</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="20">
          <!--Y yo soy para mi Amado.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="ser" type="VSIP1S0">soy|</w>
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="mi" type="DP1CSS">mi</w>
          <w lemma="amado" type="NP00000">a|ma|do|</w>
        </l>
