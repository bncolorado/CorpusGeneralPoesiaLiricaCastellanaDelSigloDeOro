          <w lemma="uno" type="PI0FS00">u|na|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="criador" type="NP00000">cria|dor|</w>
        </l>
        <l met="1|2|3|4|5|-|7|-|" n="17">
