          <w lemma="ya" type="RG">ya|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="querer" type="VMIP1S0">quie|ro</w>
          <w lemma="otro" type="DI0MS0">o|tro</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
        </l>
        <l met="-|2|-|4|5|-|7|-|" n="18">
