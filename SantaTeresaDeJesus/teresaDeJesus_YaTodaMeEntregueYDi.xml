          <w lemma="cuando" type="CS">cuan|do</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="dulce" type="NCMS000">dul|ce|</w>
          <w lemma="cazador" type="NP00000">ca|za|dor|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="6">
          <!--Me tiró y dejó rendida,-->
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="tirar" type="VMIS3S0">ti|ró</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="dejar" type="VMIS3S0">de|jó|</w>
          <w lemma="rendir" type="VMP00SF">ren|di|da|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="7">
          <!--En los brazos del amor-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="brazo" type="NCMP000">bra|zos|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
        </l>
        <l met="1|-|-|4|5|-|" n="8">
          <!--Mi alma quedó caida,-->
          <w lemma="mi" type="DP1CSS">mi</w>
          <w lemma="alma" type="NCFS000">al|ma|</w>
          <w lemma="quedar" type="VMIS3S0">que|dó|</w>
          <w lemma="caida" type="VMP00SF">cai|da|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="9">
