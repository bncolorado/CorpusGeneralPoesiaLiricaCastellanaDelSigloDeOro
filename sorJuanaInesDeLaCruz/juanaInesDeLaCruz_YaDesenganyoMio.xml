  <text>
    <body>
      <head>
        <title>Ya, desengaño mío,</title>
      </head>
      <lg cert="high" type="Endechas">
        <l met="1|-|-|4|-|6|-|" n="1">
          <!--Ya, desengaño mío,-->
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="desengaño" type="NCMS000">de|sen|ga|ño|</w>
          <w lemma="mío" type="AP0MS1S">mí|o|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="2">
          <!--llegasteis al extremo-->
          <w lemma="llegar" type="VMIS2P0">lle|gas|teis|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="extremo" type="NCMS000">ex|tre|mo|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="3">
          <!--que pudo en vuestro ser-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="poder" type="VMIS3S0">pu|do</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="vuestro" type="DP2MSP">vues|tro|</w>
          <w lemma="ser" type="NCMS000">ser|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="4">
          <!--verificar el serlo.-->
          <w lemma="verificar" type="VMN0000">ve|ri|fi|car|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
        </l>
        <l met="1|-|-|4|-|6|-|" n="5">
          <!--Todo lo habéis perdido:-->
          <w lemma="todo" type="PI0MS00">to|do|</w>
          <w lemma="lo" type="PP3MSA0">lo</w>
          <w lemma="haber" type="VAIP2P0">ha|béis|</w>
          <w lemma="perder" type="VMP00SM">per|di|do|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="6">
          <!--mas no todo, pues creo-->
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="todo" type="PI0MS00">to|do|</w>
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="creer" type="VMIP1S0">cre|o|</w>
        </l>
        <l met="1|-|3|4|-|6|-|" n="7">
          <!--que aun a costa es de todo-->
          <w lemma="que" type="CS">que</w>
          <w lemma="aun" type="RG">aun|</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="costa" type="NCFS000">cos|ta</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="todo" type="DI0MS0">to|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="8">
          <!--barato el escarmiento.-->
          <w lemma="barato" type="RG">ba|ra|to</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="escarmiento" type="NCMS000">es|car|mien|to|</w>
        </l>
        <l met="1|-|-|4|-|6|-|" n="9">
          <!--No envidiaréis de amor-->
          <w lemma="no" type="RN">no</w>
          <w lemma="envidiar" type="VMIF2P0">en|vi|dia|réis|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="10">
          <!--los gustos lisonjeros-->
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="gusto" type="NCMP000">gus|tos|</w>
          <w lemma="lisonjero" type="AQ0MP00">li|son|je|ros|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="11">
          <!--que está un escarmentado-->
          <w lemma="que" type="CS">que</w>
          <w lemma="estar" type="VMIP3S0">es|tá</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="escarmentado" type="NCMS000">es|car|men|ta|do|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="12">
          <!--muy remoto del riesgo.-->
          <w lemma="muy" type="RG">muy|</w>
          <w lemma="remoto" type="AQ0MS00">re|mo|to|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="riesgo" type="NCMS000">ries|go|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="13">
          <!--El no esperar alguno-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="esperar" type="VMN0000">es|pe|rar|</w>
          <w lemma="alguno" type="PI0MS00">al|gu|no|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="14">
          <!--me sirve de consuelo,-->
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="servir" type="VMIP3S0">sir|ve|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="consuelo" type="NCMS000">con|sue|lo|</w>
        </l>
        <l met="-|-|3|4|-|6|-|" n="15">
          <!--que también es alivio-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="también" type="RG">tam|bién|</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="alivio" type="NCMS000">a|li|vio|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="16">
          <!--el no buscar remedio.-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="buscar" type="VMN0000">bus|car|</w>
          <w lemma="remedio" type="NCMS000">re|me|dio|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="17">
          <!--En la pérdida misma-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="pérdida" type="NCFS000">pér|di|da|</w>
          <w lemma="mismo" type="AQ0FS00">mis|ma|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="18">
          <!--los alivios encuentro,-->
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="alivio" type="NCMP000">a|li|vios|</w>
          <w lemma="encuentro" type="NCMS000">en|cuen|tro|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="19">
          <!--pues si perdí el tesoro,-->
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="perder" type="VMIS1S0">per|dí</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="tesoro" type="NCMS000">te|so|ro|</w>
        </l>
        <l met="-|2|-|-|5|6|-|" n="20">
          <!--también se perdió el miedo-->
          <w lemma="también" type="RG">tam|bién|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="perder" type="VMIS3S0">per|dió</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="miedo" type="NCMS000">mie|do|</w>
        </l>
        <l met="1|-|3|4|-|6|-|" n="21">
          <!--No tener qué perder-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="tener" type="VMN0000">te|ner|</w>
          <w lemma="qué" type="PT00000">qué|</w>
          <w lemma="perder" type="VMN0000">per|der|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="22">
          <!--me sirve de sosiego,-->
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="servir" type="VMIP3S0">sir|ve|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="sosiego" type="NCMS000">so|sie|go|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="23">
          <!--que no teme ladrones-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="temer" type="VMIP3S0">te|me|</w>
          <w lemma="ladrón" type="NCMP000">la|dro|nes|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="24">
          <!--desnudo el pasajero.-->
          <w lemma="desnudar" type="VMIP1S0">des|nu|do</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="pasajero" type="NCMS000">pa|sa|je|ro|</w>
        </l>
        <l met="1|-|-|-|5|6|-|" n="25">
          <!--Ni aun la libertad misma-->
          <w lemma="ni" type="CC">ni</w>
          <w lemma="aun" type="RG">aun|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="libertad" type="NCFS000">li|ber|tad|</w>
          <w lemma="mismo" type="AQ0FS00">mis|ma|</w>
        </l>
        <l met="-|2|-|-|5|6|-|" n="26">
          <!--tenerla por bien quiero,-->
          <w lemma="tener" type="VMN0000">te|ner|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="querer" type="VMIP1S0">quie|ro|</w>
        </l>
        <l met="-|2|-|-|5|6|-|" n="27">
          <!--que luego será daño-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="luego" type="RG">lue|go|</w>
          <w lemma="ser" type="VSIF3S0">se|rá|</w>
          <w lemma="daño" type="NCMS000">da|ño|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="28">
          <!--si por tal la poseo.-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="tal" type="PD0CS00">tal|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="poseer" type="VMIP1S0">po|se|o|</w>
        </l>
        <l met="1|2|-|4|-|6|-|" n="29">
          <!--No quiero más cuidados-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="querer" type="VMIP1S0">quie|ro|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="cuidar" type="VMP00PM">cui|da|dos|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="30">
          <!--de bienes tan inciertos,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="bien" type="NCMP000">bie|nes|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="incierto" type="AQ0MP00">in|cier|tos|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="31">
          <!--sino tener el alma-->
          <w lemma="sino" type="CC">si|no|</w>
          <w lemma="tener" type="VMN0000">te|ner|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="alma" type="NCFS000">al|ma|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="32">
          <!--como que no la tengo.-->
          <w lemma="como" type="CS">co|mo|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="tener" type="VMIP1S0">ten|go|</w>
        </l>
      </lg>
    </body>
