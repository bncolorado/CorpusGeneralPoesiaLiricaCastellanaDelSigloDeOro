  <text>
    <body>
      <head>
        <title>Para estar tan bien parida</title>
      </head>
      <lg cert="low" type="Coplas">
        <l met="-|-|3|4|5|-|7|-|" n="1">
          <!--Para estar tan bien parida-->
          <w lemma="para" type="SP">pa|ra</w>
          <w lemma="estar" type="VMN0000">es|tar|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="parir" type="VMP00SF">pa|ri|da|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="2">
          <!--y tan bien acompañada,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="acompañar" type="VMP00SF">a|com|pa|ña|da|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="3">
          <!--mal estáis aposentada,-->
          <w lemma="mal" type="RG">mal|</w>
          <w lemma="estar" type="VAIP2P0">es|táis|</w>
          <w lemma="aposentar" type="VMP00SF">a|po|sen|ta|da|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="4">
          <!--Virgen, y mal proveída.-->
          <w lemma="virgen" type="NCCS000">vir|gen|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="mal" type="RG">mal|</w>
          <w lemma="proveer" type="VMP00SF">pro|ve|í|da|</w>
        </l>
        <l met="1|2|3|-|5|-|7|-|" n="5">
          <!--Yo no sé, ni nadie sabe,-->
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="saber" type="VMIP1S0">sé|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="nadie" type="PI0CS00">na|die|</w>
          <w lemma="saber" type="VMIP3S0">sa|be|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="6">
          <!--se qué manera os alabe;-->
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="qué" type="DT0CN0">qué|</w>
          <w lemma="manera" type="NCFS000">ma|ne|ra</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="alabar" type="VMSP3S0">a|la|be|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="7">
          <!--que sin sentir embarazo-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="sentir" type="VMN0000">sen|tir|</w>
          <w lemma="embarazo" type="NCMS000">em|ba|ra|zo|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="8">
          <!--tenéis en vuestro regazo-->
          <w lemma="tener" type="VMIP2P0">te|néis|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="vuestro" type="DP2MSP">vues|tro|</w>
          <w lemma="regazo" type="NCMS000">re|ga|zo|</w>
        </l>
        <l met="-|-|-|4|-|6|7|-|" n="9">
          <!--al que en el cielo no cabe.-->
          <w lemma="al" type="SP">al|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cielo" type="NCMS000">cie|lo|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="caber" type="VMIP3S0">ca|be|</w>
        </l>
      </lg>
    </body>
  </text>
