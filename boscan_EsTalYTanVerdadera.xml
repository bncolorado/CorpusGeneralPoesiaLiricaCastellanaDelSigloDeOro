  <text>
    <body>
      <head>
        <title>Es tal y tan verdadera</title>
      </head>
      <lg cert="high" type="Canción - Cuartetas">
        <l met="1|2|-|-|-|-|7|-|" n="1">
          <!--Es tal y tan verdadera-->
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="tal" type="PD0CS00">tal|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="verdadero" type="AQ0FS00">ver|da|de|ra|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="2">
          <!--mi pena por conoceros-->
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="pena" type="NCFS000">pe|na|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="conocer" type="VMN0000">co|no|cer|</w>
          <w lemma="os" type="PP2CP00">os|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="3">
          <!--que, si tanto no os quisiera,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="tanto" type="RG">tan|to|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="querer" type="VMSI3S0">qui|sie|ra|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="4">
          <!--yo quisiera no quereros.-->
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="querer" type="VMSI3S0">qui|sie|ra|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="querer" type="VMN0000">que|rer|</w>
          <w lemma="os" type="PP2CP00">os|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="5">
          <!--Que nuevo caso de amor-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="nuevo" type="AQ0MS00">nue|vo|</w>
          <w lemma="caso" type="NCMS000">ca|so|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="6">
          <!--ordenáis que en mí comience:-->
          <w lemma="ordenar" type="VMIP2P0">or|de|náis|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
          <w lemma="comenzar" type="VMSP3S0">co|mien|ce|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="7">
          <!--combatirme el desamor,-->
          <w lemma="combatir" type="VMN0000">com|ba|tir|</w>
          <w lemma="me" type="PP1CS00">me</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="desamor" type="NCMS000">de|sa|mor|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="8">
          <!--adonde el amor me vence.-->
          <w lemma="adonde" type="CS">a|don|de</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="vencer" type="VMIP3S0">ven|ce|</w>
        </l>
        <l met="1|2|-|-|-|-|7|-|" n="9">
          <!--No es mucho, pues tan entera-->
          <w lemma="no" type="RN">no</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="mucho" type="RG">mu|cho|</w>
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="entero" type="AQ0FS00">en|te|ra|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="10">
          <!--es mi pena en conoceros,-->
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="pena" type="NCFS000">pe|na</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="conocer" type="VMN0000">co|no|cer|</w>
          <w lemma="os" type="PP2CP00">os|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="11">
          <!--que, si tanto no os quisiera,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="tanto" type="RG">tan|to|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="querer" type="VMSI3S0">qui|sie|ra|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="12">
          <!--yo quisiera no quereros.-->
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="querer" type="VMSI3S0">qui|sie|ra|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="querer" type="VMN0000">que|rer|</w>
          <w lemma="os" type="PP2CP00">os|</w>
        </l>
      </lg>
    </body>
  </text>
