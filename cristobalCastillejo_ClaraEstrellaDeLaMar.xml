    <body>
      <head>
        <title>Clara estrella de la mar,</title>
      </head>
      <lg cert="high" type="Canción">
        <l met="1|-|3|-|-|-|7|-|" n="1">
          <!--Clara estrella de la mar,-->
          <w lemma="claro" type="AQ0FS00">cla|ra</w>
          <w lemma="estrella" type="NCFS000">es|tre|lla|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mar" type="NCCS000">mar|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="2">
          <!--dichosa puerta del cielo,-->
          <w lemma="dichoso" type="AQ0FS00">di|cho|sa|</w>
          <w lemma="puerta" type="NCFS000">puer|ta|</w>
          <w lemma="de" type="SP">de</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cielo" type="NCMS000">cie|lo|</w>
        </l>
        <l met="1|-|-|-|-|-|7|-|" n="3">
          <!--Madre de nuestro consuelo,-->
          <w lemma="madre" type="NP00000">ma|dre|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="nuestro" type="DP1MSP">nues|tro|</w>
          <w lemma="consuelo" type="NCMS000">con|sue|lo|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="4">
