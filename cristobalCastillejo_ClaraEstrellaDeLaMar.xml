          <w lemma="otro" type="PI0MP00">o|tros|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="marinero" type="AQ0MP00">ma|ri|ne|ros|</w>
        </l>
        <l met="1|-|-|-|-|-|7|-|" n="36">
          <!--puestos para ejecutar.-->
          <w lemma="poner" type="VMP00PM">pues|tos|</w>
          <w lemma="para" type="SP">pa|ra</w>
          <w lemma="ejecutar" type="VMN0000">e|je|cu|tar|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="37">
          <!--Las velas por do se guía,-->
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="vela" type="NCFP000">ve|las|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="do" type="CS">do|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="guía" type="NCCS000">guí|a|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="38">
          <!--que son los cinco sentidos,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="5" type="Z">cin|co|</w>
          <w lemma="sentido" type="NCMP000">sen|ti|dos|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="39">
          <!--sean de vientos heridos-->
          <w lemma="ser" type="VSSP3P0">se|an|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="viento" type="NCMP000">vien|tos|</w>
          <w lemma="herir" type="VMP00PM">he|ri|dos|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="40">
          <!--que vengan sin travesía;-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="vengar" type="VMIP3P0">ven|gan|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="travesía" type="NCFS000">tra|ve|sí|a|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="41">
          <!--y si no pudiere andar-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="poder" type="VMSF1S0">pu|die|re</w>
          <w lemma="andar" type="VMN0000">an|dar|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="42">
          <!--nuestra flaqueza mezquina,-->
          <w lemma="nuestro" type="DP1FSP">nues|tra|</w>
          <w lemma="flaqueza" type="NCFS000">fla|que|za|</w>
          <w lemma="mezquino" type="AQ0FS00">mez|qui|na|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="43">
          <!--viento en popa a la bolina-->
          <w lemma="viento" type="NCMS000">vien|to</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="popa" type="NCFS000">po|pa</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="bolina" type="NCFS000">bo|li|na|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="44">
          <!--sepa al menos navegar.-->
          <w lemma="saber" type="VMSP3S0">se|pa</w>
          <w lemma="a" type="SP">a</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="menos" type="RG">me|nos|</w>
          <w lemma="navegar" type="VMN0000">na|ve|gar|</w>
        </l>
