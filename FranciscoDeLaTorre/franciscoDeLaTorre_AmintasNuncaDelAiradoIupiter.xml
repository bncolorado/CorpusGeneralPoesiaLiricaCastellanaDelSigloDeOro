          <w lemma="presencia" type="NCFS000">pre|sen|cia|</w>
          <w lemma="divino" type="AQ0FS00">di|ui|na|</w>
        </l>
        <l met="1|-|-|4|-|-|-|8|-|10|-|" n="5">
