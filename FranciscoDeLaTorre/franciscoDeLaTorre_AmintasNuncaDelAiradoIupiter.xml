          <w lemma="nube" type="NCFP000">nu|bes|</w>
          <w lemma="claro" type="AQ0MS00">cla|ro|</w>
          <w lemma="sol" type="NP00000">sol|</w>
          <w lemma="desocupar" type="VMG0000">de|so|cu|pan|do|</w>
        </l>
        <l met="1|-|3|-|-|6|-|-|-|10|-|" n="27">
