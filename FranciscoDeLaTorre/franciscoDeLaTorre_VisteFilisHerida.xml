          <w lemma="mano" type="NCFS000">ma|no|</w>
          <w lemma="torcer" type="VMG0000">tor|cien|do</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="buelo" type="NCMS000">bue|lo|</w>
        </l>
        <l met="1|-|-|-|-|6|-|-|-|10|-|" n="20">
          <!--Sacre, que la derriba por el suelo?-->
          <w lemma="sacre" type="NP00000">sa|cre|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="derribar" type="VMIP3S0">de|rri|ba|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="suelo" type="NCMS000">sue|lo|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="21">
