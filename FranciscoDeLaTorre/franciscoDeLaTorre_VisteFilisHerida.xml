    <body>
      <head>
        <title> ¿Viste, Filis, herida</title>
      </head>
      <lg cert="high" type="Liras">
        <l met="1|-|3|-|-|6|-|" n="1">
          <!--¿Viste, Filis, herida-->
          <w lemma="vestir" type="VMIP3S0">vis|te|</w>
          <w lemma="filis" type="NP00000">fi|lis|</w>
          <w lemma="herida" type="NCFS000">he|ri|da|</w>
