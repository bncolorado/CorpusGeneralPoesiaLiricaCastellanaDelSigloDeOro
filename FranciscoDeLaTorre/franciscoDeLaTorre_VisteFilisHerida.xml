          <w lemma="luzidas" type="NP00000">lu|zi|das|</w>
          <w lemma="flor" type="NCFP000">flo|res|</w>
          <w lemma="ver" type="VMIS2S0">vis|te|</w>
        </l>
        <l met="-|-|-|4|-|6|-|-|-|10|-|" n="22">
