          <w lemma="no" type="RN">no|</w>
          <w lemma="viuas" type="NCFP000">viuas|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="seguro" type="AQ0FS00">se|gu|ra|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="29">
