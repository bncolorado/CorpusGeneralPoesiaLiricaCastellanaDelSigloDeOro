          <w lemma="rojo" type="AQ0FS00">ro|ja</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="acelerar" type="VMP00SF">a|ce|le|ra|da|</w>
        </l>
        <l met="1|-|-|4|-|6|-|" n="14">
