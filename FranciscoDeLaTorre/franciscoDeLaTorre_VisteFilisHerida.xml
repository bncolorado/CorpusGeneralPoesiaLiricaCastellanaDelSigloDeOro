          <w lemma="cumbre" type="NCFS000">cum|bre|</w>
          <w lemma="sacar" type="VMN0000">sa|car|</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="febo" type="NP00000">fe|bo|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="cabeça" type="NCFS000">ca|beça|</w>
        </l>
        <l met="1|-|-|-|-|6|-|" n="13">
