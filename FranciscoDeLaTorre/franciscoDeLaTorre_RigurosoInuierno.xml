          <w lemma="quanto" type="NP00000">quan|to</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cielo" type="NCMS000">cie|lo</w>
          <w lemma="amigo" type="AQ0MS00">a|mi|go|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="18">
