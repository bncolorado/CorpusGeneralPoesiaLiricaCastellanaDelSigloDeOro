    <body>
      <head>
        <title>Riguroso inuierno,</title>
      </head>
      <lg cert="high" type="Endecha">
        <l met="-|-|3|-|5|-|" n="1">
          <!--Riguroso inuierno,-->
          <w lemma="riguroso" type="AQ0MS00">ri|gu|ro|so</w>
          <w lemma="inuierno" type="NCMS000">i|nuier|no|</w>
        </l>
        <l met="-|-|3|4|-|" n="2">
