          <w lemma="soler" type="VMIP2S0">sue|les|</w>
          <w lemma="acabar" type="VMN0000">a|ca|bar|</w>
          <w lemma="te" type="PP2CS00">te|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="11">
