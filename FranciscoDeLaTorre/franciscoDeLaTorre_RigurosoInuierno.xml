          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="suerte" type="NCFS000">suer|te|</w>
          <w lemma="propicio" type="AQ0FS00">pro|pi|cia|</w>
        </l>
        <l met="-|2|3|-|5|-|" n="25">
