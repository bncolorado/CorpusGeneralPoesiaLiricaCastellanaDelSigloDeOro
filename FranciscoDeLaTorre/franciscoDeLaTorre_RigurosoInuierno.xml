          <w lemma="mucho" type="DI0MP0">mu|chos|</w>
          <w lemma="enemigo" type="NCMP000">e|ne|mi|gos|</w>
        </l>
        <l met="1|-|-|4|5|-|" n="38">
          <!--tiene mi cruel vida,-->
          <w lemma="tener" type="VMIP3S0">tie|ne|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="cruel" type="AQ0CS00">cruel|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="39">
