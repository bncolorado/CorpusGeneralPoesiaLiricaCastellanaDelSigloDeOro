          <w lemma="a" type="SP">a|</w>
          <w lemma="ti" type="PP2CSO0">ti|</w>
          <w lemma="sólo" type="RG">só|lo</w>
          <w lemma="aágora" type="NCFS000">a|go|ra|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="26">
          <!--me bueluo llorando,-->
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="bueluo" type="AQ0MS00">bue|luo|</w>
          <w lemma="llorar" type="VMG0000">llo|ran|do|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="27">
