          <w lemma="no" type="RN">no|</w>
          <w lemma="poder" type="VMIF3S0">po|drá|</w>
          <w lemma="sufrir" type="VMN0000">su|frir|</w>
          <w lemma="me" type="PP1CS00">me|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="17">
