          <w lemma="tanto" type="RG">tan|to|</w>
          <w lemma="mal" type="RG">mal|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="vario" type="AQ0MS00">va|rio|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="37">
