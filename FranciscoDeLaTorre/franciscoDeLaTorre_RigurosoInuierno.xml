          <w lemma="que" type="CS">que|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="hallar" type="VMIP1S0">ha|llo|</w>
          <w lemma="blando" type="AQ0MS00">blan|do|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="28">
