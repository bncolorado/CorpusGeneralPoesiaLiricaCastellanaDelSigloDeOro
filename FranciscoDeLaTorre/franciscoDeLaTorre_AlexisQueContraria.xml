  <text>
    <body>
      <head>
        <title>Alexis, ¿qué contraria</title>
      </head>
      <lg cert="low" type="Cuartetas asonantadas">
        <l met="-|2|-|4|-|6|-|" n="1">
          <!--Alexis, ¿qué contraria-->
          <w lemma="alexis" type="NP00000">a|le|xis|</w>
          <w lemma="qué" type="DE0CN0">qué|</w>
          <w lemma="contrario" type="NCFS000">con|tra|ria|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="2">
          <!--influencia del cielo-->
          <w lemma="influencia" type="NCFS000">in|fluen|cia|</w>
          <w lemma="del" type="SP">de</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cielo" type="NCMS000">cie|lo|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="3">
          <!--persigue nuestros ánimos-->
          <w lemma="perseguir" type="VMIP3S0">per|si|gue|</w>
          <w lemma="nuestro" type="DP1MPP">nues|tros|</w>
          <w lemma="ánimo" type="NCMP000">á|ni|mos|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="4">
          <!--con las cosas del mundo?-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="cosa" type="NCFP000">co|sas|</w>
          <w lemma="de" type="SP">de</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="mundo" type="NCMS000">mun|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="5">
          <!--Ninguno con la suerte-->
          <w lemma="ninguno" type="PI0MS00">nin|gu|no|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="suerte" type="NCFS000">suer|te|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="6">
          <!--que le priuino el hado,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="prevenir" type="VMIS3S0">pri|ui|no</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="hado" type="NCMS000">ha|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="7">
          <!--dichosa o miserable,-->
          <w lemma="dichoso" type="AQ0FS00">di|cho|sa</w>
          <w lemma="o" type="CC">o|</w>
          <w lemma="miserable" type="AQ0CS00">mi|se|ra|ble|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="8">
          <!--alegremente viue.-->
          <w lemma="alegremente" type="RG">a|le|gre|men|te|</w>
          <w lemma="vivir" type="VMIP3S0">vi|ue|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="9">
          <!--El nauegante, quando-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="navegante" type="AQ0CS00">na|ue|gan|te|</w>
          <w lemma="cuando" type="CS">quan|do|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="10">
          <!--turbado cielo ruega-->
          <w lemma="turbar" type="VMP00SM">tur|ba|do|</w>
          <w lemma="cielo" type="NCMS000">cie|lo|</w>
          <w lemma="rogar" type="VMIP3S0">rue|ga|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="11">
          <!--con lágrimas y votos,-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="lágrima" type="NCFP000">lá|gri|mas|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="voto" type="NCMP000">vo|tos|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="12">
          <!--su ventura maldize.-->
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="ventura" type="NCFS000">ven|tu|ra|</w>
          <w lemma="maldecir" type="VMIP3S0">mal|di|ze|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="13">
          <!--El labrador, cansado-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="labrador" type="NCMS000">la|bra|dor|</w>
          <w lemma="cansar" type="VMP00SM">can|sa|do|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="14">
          <!--de abrir la tierra, huyendo-->
          <w lemma="de" type="SP">de</w>
          <w lemma="abrir" type="VMN0000">a|brir|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="tierra" type="NCFS000">tie|rra</w>
          <w lemma="huir" type="VMG0000">hu|yen|do|</w>
        </l>
        <l met="1|-|-|4|-|6|-|" n="15">
          <!--fiero león del cielo,-->
          <w lemma="fiero" type="AQ0MS00">fie|ro|</w>
          <w lemma="león" type="NCMS000">le|ón|</w>
          <w lemma="de" type="SP">de</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cielo" type="NCMS000">cie|lo|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="16">
          <!--maldize su ventura.-->
          <w lemma="maldecir" type="VMIP3S0">mal|di|ze|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="ventura" type="NCFS000">ven|tu|ra|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="17">
          <!--La más dichosa suerte,-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="dichoso" type="AQ0FS00">di|cho|sa|</w>
          <w lemma="suerte" type="NCFS000">suer|te|</w>
        </l>
        <l met="1|2|-|-|-|6|-|" n="18">
          <!--si es propia, desagrada;-->
          <w lemma="si" type="CS">si</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="propio" type="AQ0FS00">pro|pia|</w>
          <w lemma="desagradar" type="VMIP3S0">de|sa|gra|da|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="19">
          <!--y si tras ella vamos,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="tras" type="SP">tras|</w>
          <w lemma="él" type="PP3FS00">e|lla|</w>
          <w lemma="ir" type="VMIP1P0">va|mos|</w>
        </l>
        <l met="1|2|-|4|-|6|-|" n="20">
          <!--no ay cosa más diuina.-->
          <w lemma="no" type="RN">no</w>
          <w lemma="haber" type="VMIP3S0">ay|</w>
          <w lemma="cosa" type="NCFS000">co|sa|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="divino" type="AQ0FS00">di|ui|na|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="21">
          <!--A mí, que el campo habito,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="campo" type="NCMS000">cam|po</w>
          <w lemma="habitar" type="VMIP1S0">ha|bi|to|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="22">
          <!--me tienes por dichoso:-->
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="tener" type="VMIP2S0">tie|nes|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="dichoso" type="AQ0MS00">di|cho|so|</w>
        </l>
        <l met="1|-|-|4|5|6|-|" n="23">
          <!--oy para mí no ay cosa-->
          <w lemma="hoy" type="RG">oy|</w>
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="haber" type="VMIP3S0">ay|</w>
          <w lemma="cosa" type="NCFS000">co|sa|</w>
        </l>
        <l met="-|-|3|-|5|6|-|" n="24">
          <!--en los hados más triste.-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="hado" type="NCMP000">ha|dos|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="triste" type="AQ0CS00">tris|te|</w>
        </l>
        <l met="1|-|-|-|5|6|-|" n="25">
          <!--Tú, que la ciudad honras,-->
          <w lemma="tú" type="PP2CSN0">tú|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="ciudad" type="NCFS000">ciu|dad|</w>
          <w lemma="honrar" type="VMIP2S0">ho|nras|</w>
        </l>
        <l met="1|-|-|-|-|6|-|" n="26">
          <!--eres el inuidiado;-->
          <w lemma="ser" type="VSIP2S0">e|res|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="envidiar" type="VMP00SM">in|ui|dia|do|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="27">
          <!--a ti te agrada el mío,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="ti" type="PP2CSO0">ti|</w>
          <w lemma="te" type="PP2CS00">te</w>
          <w lemma="agradar" type="VMIP3S0">a|gra|da</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="mío" type="AP0MS1S">mí|o|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="28">
          <!--y a mí tu dulce estado.-->
          <w lemma="y" type="CC">y</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="mí" type="PP1CSO0">mí|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="dulce" type="NCMS000">dul|ce</w>
          <w lemma="estar" type="VMP00SM">es|ta|do|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="29">
          <!--Y la dichosa suerte-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="dichoso" type="AQ0FS00">di|cho|sa|</w>
          <w lemma="suerte" type="NCFS000">suer|te|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="30">
          <!--a los dos agradable,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="2" type="Z">dos|</w>
          <w lemma="agradable" type="AQ0CS00">a|gra|da|ble|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="31">
          <!--a ti por el contrario-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="ti" type="PP2CSO0">ti|</w>
          <w lemma="por_el_contrario" type="RG">por_el_|con|tra|rio|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="32">
          <!--y a mí es aborrecible.-->
          <w lemma="y" type="CC">y</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="mí" type="PP1CSO0">mí</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="aborrecible" type="AQ0CS00">a|bo|rre|ci|ble|</w>
        </l>
        <l met="1|2|-|4|-|6|-|" n="33">
          <!--No son la causa desto-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="causa" type="NCFS000">cau|sa|</w>
          <w lemma="de" type="SP">de</w>
	  <w lemma="este" type="PD00S00">es|to|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="34">
          <!--lugares ni ocasiones;-->
          <w lemma="lugar" type="NCMP000">lu|ga|res|</w>
          <w lemma="ni" type="CC">ni</w>
          <w lemma="ocasión" type="NCFP000">o|ca|sio|nes|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="35">
          <!--nuestro ánimo es la causa-->
          <w lemma="nuestro" type="DP1MSP">nues|tro</w>
          <w lemma="ánimo" type="NCMS000">á|ni|mo</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="causa" type="NCFS000">cau|sa|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="36">
          <!--que se estraña del mundo,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="extrañar" type="VMIP3S0">es|tra|ña|</w>
          <w lemma="de" type="SP">de</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="mundo" type="NCMS000">mun|do|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="37">
          <!--y no bien satisfecho-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="satisfacer" type="VMP00SM">sa|tis|fe|cho|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="38">
          <!--del mal seguro gozo-->
          <w lemma="de" type="SP">de</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="mal" type="NCMS000">mal|</w>
          <w lemma="seguro" type="AQ0MS00">se|gu|ro|</w>
          <w lemma="gozo" type="NCMS000">go|zo|</w>
        </l>
        <l met="1|-|-|4|-|6|-|" n="39">
          <!--desta mudable vida,-->
          <w lemma="de" type="AQ0FS00">de</w>
	  <w lemma="este" type="DD0FS0">es|ta|</w>
          <w lemma="mudable" type="AQ0CS00">mu|da|ble|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="40">
          <!--al que es eterno aspira.-->
          <w lemma="a" type="SP">a</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="eterno" type="AQ0MS00">e|ter|no</w>
          <w lemma="aspirar" type="VMIP3S0">as|pi|ra|</w>
        </l>
      </lg>
    </body>
  </text>
