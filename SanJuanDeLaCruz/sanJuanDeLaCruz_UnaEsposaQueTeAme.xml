  <text>
    <body>
      <head>
        <title>Una esposa que te ame,</title>
      </head>
      <lg cert="..." type="...">
        <l met="1|-|3|-|-|-|7|-|" n="1">
          <!--Una esposa que te ame,-->
          <w lemma="uno" type="DI0FS0">u|na</w>
          <w lemma="esposo" type="NCFS000">es|po|sa|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="amar" type="VMSP3S0">a|me|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="2">
          <!--mi Hijo, darte quería,-->
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="hijo" type="NP00000">hi|jo|</w>
          <w lemma="dar" type="VMN0000">dar|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="querer" type="VMII3S0">que|rí|a|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="3">
          <!--que por tu valor merezca-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="valor" type="NCMS000">va|lor|</w>
          <w lemma="merecer" type="VMSP3S0">me|rez|ca|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="4">
          <!--tener nuestra compañía-->
          <w lemma="tener" type="VMN0000">te|ner|</w>
          <w lemma="nuestro" type="DP1FSP">nues|tra|</w>
          <w lemma="compañía" type="NCFS000">com|pa|ñí|a|</w>
        </l>
        <l met="-|-|3|4|5|-|7|-|" n="5">
          <!--y comer pan a una mesa,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="comer" type="VMN0000">co|mer|</w>
          <w lemma="pan" type="NCMS000">pan|</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="mesa" type="NCFS000">me|sa|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="6">
          <!--del mismo que yo comía,-->
          <w lemma="del" type="SP">del|</w>
          <w lemma="mismo" type="AQ0MS00">mis|mo|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="comer" type="VMII3S0">co|mí|a|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="7">
          <!--porque conozco los bienes-->
          <w lemma="porque" type="CS">por|que|</w>
          <w lemma="conocer" type="VMIP1S0">co|noz|co|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="bien" type="NCMP000">bie|nes|</w>
        </l>
        <l met="-|2|3|-|5|-|7|-|" n="8">
          <!--que en tal Hijo yo tenía,-->
          <w lemma="que" type="CS">que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="tal" type="DD0CS0">tal|</w>
          <w lemma="hijo" type="NP00000">hi|jo|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="tener" type="VMII3S0">te|ní|a|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="9">
          <!--y se congracie commigo-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="congraciar" type="VMSP3S0">con|gra|cie|</w>
          <w lemma="commigo" type="NCMS000">com|mi|go|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="10">
          <!--de tu gracia y lozanía.-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="gracia" type="NCFS000">gra|cia</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="lozanía" type="NCFS000">lo|za|ní|a|</w>
        </l>
        <l met="1|-|-|-|5|-|7|-|" n="11">
          <!--Mucho lo agradezco, Padre-->
          <w lemma="mucho" type="RG">mu|cho|</w>
          <w lemma="lo" type="PP3MSA0">lo</w>
          <w lemma="agradecer" type="VMIP1S0">a|gra|dez|co|</w>
          <w lemma="padre" type="NP00000">pa|dre|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="12">
          <!---el Hijo le respondía-;-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="hijo" type="NP00000">hi|jo|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="responder" type="VMII3S0">res|pon|dí|a|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="13">
          <!--a la esposa que me dieres-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="esposo" type="NCFS000">es|po|sa|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="dar" type="VMSF2S0">die|res|</w>
        </l>
        <l met="1|-|-|-|5|-|7|-|" n="14">
          <!--yo mi claridad daría-->
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="claridad" type="NCFS000">cla|ri|dad|</w>
          <w lemma="dar" type="VMIC3S0">da|rí|a|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="15">
          <!--para que por ella vea-->
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="él" type="PP3FS00">e|lla|</w>
          <w lemma="ver" type="VMSP3S0">ve|a|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="16">
          <!--cuánto mi Padre valía-->
          <w lemma="cuánto" type="PT0MS00">cuán|to|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="padre" type="NP00000">pa|dre|</w>
          <w lemma="valía" type="NCFS000">va|lí|a|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="17">
          <!--y cómo el ser que poseo-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="cómo" type="PT00000">có|mo</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="ser" type="NCMS000">ser|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="poseer" type="VMIP1S0">po|se|o|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="18">
          <!--de su ser lo recibía.-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="ser" type="NCMS000">ser|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="recibir" type="VMII3S0">re|ci|bí|a|</w>
        </l>
        <l met="-|-|3|4|5|-|7|-|" n="19">
          <!--Reclinarla he yo en mi brazo,-->
          <w lemma="reclinar" type="VMN0000">re|cli|nar|</w>
          <w lemma="lo" type="PP3FSA0">la</w>
          <w lemma="haber" type="VMIP1S0">he|</w>
          <w lemma="yo" type="PP1CSN0">yo</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="brazo" type="NCMS000">bra|zo|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="20">
          <!--y en tu amor se abrasaría,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="tu" type="DP2CSS">tu</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="abrasar" type="VMIC3S0">a|bra|sa|rí|a|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="21">
          <!--y con eterno deleite-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="eterno" type="AQ0MS00">e|ter|no|</w>
          <w lemma="deleite" type="NCMS000">de|lei|te|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="22">
          <!--tu bondad sublimaría.-->
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="bondad" type="NCFS000">bon|dad|</w>
          <w lemma="sublimar" type="VMIC3S0">su|bli|ma|rí|a|</w>
        </l>
      </lg>
    </body>
  </text>
