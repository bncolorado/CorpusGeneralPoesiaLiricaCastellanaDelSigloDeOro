  <text>
    <body>
      <head>
        <title>Entonces llamó un arcángel</title>
      </head>
      <lg cert="..." type="...">
        <l met="-|2|-|-|5|-|7|-|" n="1">
          <!--Entonces llamó un arcángel-->
          <w lemma="entonces" type="RG">en|ton|ces|</w>
          <w lemma="llamar" type="VMIS3S0">lla|mó</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="arcángel" type="NCMS000">ar|cán|gel|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="2">
          <!--que San Gabriel se decía,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="san_gabriel" type="NP00000">san_|ga|briel|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="decir" type="VMII3S0">de|cí|a|</w>
        </l>
        <l met="-|-|3|4|-|-|7|-|" n="3">
          <!--y enviolo a una doncella-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="enviolo" type="RG">en|vio|lo</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="doncella" type="NCFS000">don|ce|lla|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="4">
          <!--que se llamaba María,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="llamar" type="VMII3S0">lla|ma|ba|</w>
          <w lemma="maría" type="NP00000">ma|rí|a|</w>
        </l>
        <l met="-|-|-|-|-|-|7|-|" n="5">
          <!--de cuyo consentimiento-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="cuyo" type="PR0MS00">cu|yo|</w>
          <w lemma="consentimiento" type="NCMS000">con|sen|ti|mien|to|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="6">
          <!--el misterio se hacía;-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="misterio" type="NCMS000">mis|te|rio|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="hacer" type="VMII3S0">ha|cí|a|</w>
        </l>
        <l met="-|-|-|-|-|-|7|-|" n="7">
          <!--en el cual la Trinidad-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cual" type="PR0CS00">cual|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="trinidad" type="NP00000">tri|ni|dad|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="8">
          <!--de carne al Verbo vestía;-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="carne" type="NCFS000">car|ne</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="verbo" type="NP00000">ver|bo|</w>
          <w lemma="vestir" type="VMII3S0">ves|tí|a|</w>
        </l>
        <l met="-|-|3|4|-|-|7|-|" n="9">
          <!--y aunque tres hacen la obra,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="aunque" type="CC">aun|que|</w>
          <w lemma="3" type="Z">tres|</w>
          <w lemma="hacer" type="VMIP3P0">ha|cen|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="obra" type="NCFS000">o|bra|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="10">
          <!--en el uno se hacía;-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="uno" type="PI0MS00">u|no|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="hacer" type="VMII3S0">ha|cí|a|</w>
        </l>
        <l met="-|-|3|4|-|-|7|-|" n="11">
          <!--y quedó el Verbo encarnado-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="quedar" type="VMIS3S0">que|dó</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="verbo" type="NP00000">ver|bo</w>
          <w lemma="encarnar" type="VMP00SM">en|car|na|do|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="12">
          <!--en el vientre de María.-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="vientre" type="NCMS000">vien|tre|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="maría" type="NP00000">ma|rí|a|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="13">
          <!--Y el que tiene solo Padre,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="tener" type="VMIP3S0">tie|ne|</w>
          <w lemma="solo" type="RG">so|lo|</w>
          <w lemma="padre" type="NP00000">pa|dre|</w>
        </l>
        <l met="1|-|3|4|-|-|7|-|" n="14">
          <!--ya también madre tenía,-->
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="también" type="RG">tam|bién|</w>
          <w lemma="madre" type="NCFS000">ma|dre|</w>
          <w lemma="tener" type="VMII3S0">te|ní|a|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="15">
          <!--aunque no como cualquiera-->
          <w lemma="aunque" type="CC">aun|que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="como" type="CS">co|mo|</w>
          <w lemma="cualquiera" type="PI0CS00">cual|quie|ra|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="16">
          <!--que de varón concebía,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="varón" type="NCMS000">va|rón|</w>
          <w lemma="concebir" type="VMII3S0">con|ce|bí|a|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="17">
          <!--que de las entrañas de ella-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="entraña" type="NCFP000">en|tra|ñas|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="él" type="PP3FS00">e|lla|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="18">
          <!--él su carne recibía,-->
          <w lemma="él" type="PP3MS00">él|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="carne" type="NCFS000">car|ne|</w>
          <w lemma="recibir" type="VMII3S0">re|ci|bí|a|</w>
        </l>
        <l met="-|-|-|-|-|6|-|" n="19">
          <!--por lo cual Hijo de Dios-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="cual" type="PR0CS00">cual|</w>
          <w lemma="hijo_de_dios" type="NP00000">hi|jo_|de_|dios|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="20">
          <!--y del hombre se decía.-->
          <w lemma="y" type="CC">y</w>
          <w lemma="del" type="SP">del</w>
          <w lemma="hombre" type="NCMS000">hom|bre</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="decir" type="VMII3S0">de|cí|a|</w>
        </l>
      </lg>
    </body>
  </text>
