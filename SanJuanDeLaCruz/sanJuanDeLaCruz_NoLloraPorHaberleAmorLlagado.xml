  <text>
    <body>
      <head>
        <title>No llora por haberle amor llagado, </title>
      </head>
      <lg cert="..." type="...">
        <l met="1|2|-|-|-|6|-|8|-|10|-|" n="1">
          <!--No llora por haberle amor llagado,-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="llorar" type="VMIP3S0">llo|ra|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="haber" type="VMN0000">ha|ber|</w>
          <w lemma="le" type="PP3CSD0">le</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
          <w lemma="llagar" type="VMP00SM">lla|ga|do|</w>
        </l>
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="2">
          <!--que no se pena en verse así afligido,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="penar" type="VMIP3S0">pe|na</w>
          <w lemma="en" type="SP">e|n</w>
          <w lemma="ver" type="VMN0000">ver|</w>
          <w lemma="se" type="PP3CN00">se|</w>
          <w lemma="así" type="RG">a|sí</w>
          <w lemma="afligir" type="VMP00SM">a|fli|gi|do|</w>
        </l>
        <l met="-|-|-|-|-|6|-|8|-|10|-|" n="3">
          <!--aunque en el corazón está herido;-->
          <w lemma="aunque" type="CC">aun|que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="corazón" type="NCMS000">co|ra|zón|</w>
          <w lemma="estar" type="VMIP3S0">es|tá|</w>
          <w lemma="herir" type="VMP00SM">he|ri|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="4">
          <!--mas llora por pensar que está olvidado.-->
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="llorar" type="VMIP3S0">llo|ra|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="pensar" type="VMN0000">pen|sar|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="estar" type="VMIP3S0">es|tá</w>
          <w lemma="olvidar" type="VMP00SM">ol|vi|da|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="5">
          <!--Que solo de pensar que está olvidado-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="solo" type="RG">so|lo|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="pensar" type="VMN0000">pen|sar|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="estar" type="VMIP3S0">es|tá</w>
          <w lemma="olvidar" type="VMP00SM">ol|vi|da|do|</w>
        </l>
        <l met="-|-|3|-|-|6|-|-|9|10|-|" n="6">
          <!--de su bella pastora, con gran pena-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="bello" type="AQ0FS00">be|lla|</w>
          <w lemma="pastor" type="NCFS000">pas|to|ra|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="gran" type="AQ0CS00">gran|</w>
          <w lemma="pena" type="NCFS000">pe|na|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|10|-|" n="7">
          <!--se deja maltratar en tierra ajena,-->
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="dejar" type="VMIP3S0">de|ja|</w>
          <w lemma="maltratar" type="VMN0000">mal|tra|tar|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="tierra" type="NCFS000">tie|rra</w>
          <w lemma="ajeno" type="AQ0FS00">a|je|na|</w>
        </l>
        <l met="-|2|-|-|-|6|7|-|-|10|-|" n="8">
          <!--el pecho del amor muy lastimado.-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="pecho" type="NCMS000">pe|cho|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
          <w lemma="muy" type="RG">muy|</w>
          <w lemma="lastimar" type="VMP00SM">las|ti|ma|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|-|-|10|-|" n="9">
          <!--Y dice el pastorcico: ¡Ay, desdichado-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="decir" type="VMIP3S0">di|ce</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="pastorcico" type="NCMS000">pas|tor|ci|co</w>
          <w lemma="ay" type="I">ay|</w>
          <w lemma="desdichado" type="NCMS000">des|di|cha|do|</w>
        </l>
        <l met="-|2|-|-|-|6|7|-|-|10|-|" n="10">
          <!--de aquel que de mi amor ha hecho ausencia-->
          <w lemma="de" type="SP">de</w>
          <w lemma="aquel" type="PD0MS00">a|quel|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="mi" type="DP1CSS">mi</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
          <w lemma="haber" type="VAIP3S0">ha</w>
          <w lemma="hacer" type="VMP00SM">he|cho|</w>
          <w lemma="ausencia" type="NCFS000">au|sen|cia|</w>
        </l>
        <l met="-|2|3|-|-|6|-|-|-|10|-|" n="11">
          <!--y no quiere gozar la mi presencia,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="querer" type="VMIP3S0">quie|re|</w>
          <w lemma="gozar" type="VMN0000">go|zar|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="presencia" type="NCFS000">pre|sen|cia|</w>
        </l>
        <l met="-|2|-|-|-|6|7|-|-|10|-|" n="12">
          <!--el pecho por su amor muy lastimado!.-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="pecho" type="NCMS000">pe|cho|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="su" type="DP3CSN">su</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
          <w lemma="muy" type="RG">muy|</w>
          <w lemma="lastimar" type="VMP00SM">las|ti|ma|do|</w>
        </l>
        <l met="-|2|-|4|5|6|-|8|-|10|-|" n="13">
          <!--Y a cabo de un gran rato se ha encumbrado-->
          <w lemma="y" type="CC">y</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="cabo" type="NCMS000">ca|bo|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="gran" type="AQ0CS00">gran|</w>
          <w lemma="rato" type="NCMS000">ra|to|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="haber" type="VAIP3S0">ha</w>
          <w lemma="encumbrar" type="VMP00SM">en|cum|bra|do|</w>
        </l>
        <l met="-|2|3|-|5|6|-|8|-|10|-|" n="14">
          <!--sobre un árbol, do abrió sus brazos bellos,-->
          <w lemma="sobre" type="SP">so|bre</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="árbol" type="NCMS000">ár|bol|</w>
          <w lemma="do" type="NCMS000">do</w>
          <w lemma="abrir" type="VMIS3S0">a|brió|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="brazo" type="NCMP000">bra|zos|</w>
          <w lemma="bello" type="AQ0MP00">be|llos|</w>
        </l>
        <l met="-|2|-|4|-|6|-|8|-|10|-|" n="15">
          <!--y muerto se ha quedado asido dellos,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="morir" type="VMP00SM">muer|to|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="haber" type="VAIP3S0">ha|</w>
          <w lemma="quedar" type="VMP00SM">que|da|do</w>
          <w lemma="asir" type="VMP00SM">a|si|do|</w>
          <w lemma="dellos" type="NCMP000">de|llos|</w>
        </l>
        <l met="-|2|-|-|-|6|7|-|-|10|-|" n="16">
          <!--el pecho del amor muy lastimado.-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="pecho" type="NCMS000">pe|cho|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
          <w lemma="muy" type="RG">muy|</w>
          <w lemma="lastimar" type="VMP00SM">las|ti|ma|do|</w>
        </l>
      </lg>
    </body>
  </text>
