  <text>
    <body>
      <head>
        <title>Ya que era llegado el tiempo</title>
      </head>
      <lg cert="..." type="...">
        <l met="-|2|-|-|5|-|7|-|" n="1">
          <!--Ya que era llegado el tiempo-->
          <w lemma="ya_que" type="CS">ya_|que</w>
          <w lemma="ser" type="VSII3S0">e|ra|</w>
          <w lemma="llegar" type="VMP00SM">lle|ga|do</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="tiempo" type="NCMS000">tiem|po|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="2">
          <!--en que de nacer había,-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="nacer" type="VMN0000">na|cer|</w>
          <w lemma="haber" type="VAII3S0">ha|bí|a|</w>
        </l>
        <l met="-|-|-|-|-|-|7|-|" n="3">
          <!--así como desposado-->
          <w lemma="así_como" type="CC">a|sí_|co|mo|</w>
          <w lemma="desposar" type="VMP00SM">des|po|sa|do|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="4">
          <!--de su tálamo salía-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="tálamo" type="NCMS000">tá|la|mo|</w>
          <w lemma="salir" type="VMII3S0">sa|lí|a|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="5">
          <!--abrazado con su esposa,-->
          <w lemma="abrazar" type="VMP00SM">a|bra|za|do|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="su" type="DP3CSN">su</w>
          <w lemma="esposo" type="NCFS000">es|po|sa|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="6">
          <!--que en sus brazos la traía,-->
          <w lemma="que" type="CS">que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="brazo" type="NCMP000">bra|zos|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="traer" type="VMII3S0">tra|í|a|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="7">
          <!--al cual la graciosa Madre-->
          <w lemma="al" type="SP">al|</w>
          <w lemma="cual" type="PR0CS00">cual|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="gracioso" type="AQ0FS00">gra|cio|sa|</w>
          <w lemma="madre" type="NP00000">ma|dre|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="8">
          <!--en un pesebre ponía-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="pesebre" type="NCMS000">pe|se|bre|</w>
          <w lemma="poner" type="VMII3S0">po|ní|a|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="9">
          <!--entre unos animales-->
          <w lemma="entre" type="SP">en|tre|</w>
          <w lemma="uno" type="DI0MP0">u|nos|</w>
          <w lemma="animal" type="NCMP000">a|ni|ma|les|</w>
        </l>
        <l met="-|-|-|4|-|6|7|-|" n="10">
          <!--que a la sazón allí había.-->
          <w lemma="que" type="CS">que</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="sazón" type="NCFS000">sa|zón|</w>
          <w lemma="allí" type="RG">a|llí</w>
          <w lemma="haber" type="VAII3S0">ha|bí|a|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="11">
          <!--Los hombres decían cantares,-->
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="hombre" type="NCMP000">hom|bres|</w>
          <w lemma="decir" type="VMII3P0">de|cían|</w>
          <w lemma="cantar" type="NCMP000">can|ta|res|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="12">
          <!--los ángeles melodía,-->
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="ángel" type="NCMP000">án|ge|les|</w>
          <w lemma="melodía" type="NCFS000">me|lo|dí|a|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="13">
          <!--festejando el desposorio-->
          <w lemma="festejar" type="VMG0000">fes|te|jan|do</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="desposorio" type="NCMS000">des|po|so|rio|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="14">
          <!--que entre tales dos había.-->
          <w lemma="que" type="CS">que</w>
          <w lemma="entre" type="SP">en|tre|</w>
          <w lemma="tal" type="DD0CP0">ta|les|</w>
          <w lemma="2" type="Z">dos|</w>
          <w lemma="haber" type="VAII3S0">ha|bí|a|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="15">
          <!--Pero Dios en el pesebre-->
          <w lemma="pero" type="CC">pe|ro|</w>
          <w lemma="dios" type="NP00000">dios|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="pesebre" type="NCMS000">pe|se|bre|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="16">
          <!--allí lloraba y gemía,-->
          <w lemma="allí" type="RG">a|llí|</w>
          <w lemma="llorar" type="VMII3S0">llo|ra|ba</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="gemir" type="VMII3S0">ge|mí|a|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="17">
          <!--que eran joyas que la esposa-->
          <w lemma="que" type="CS">que</w>
          <w lemma="ser" type="VSII3P0">e|ran|</w>
          <w lemma="joya" type="NCFP000">jo|yas|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="esposo" type="NCFS000">es|po|sa|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="18">
          <!--al desposorio traía.-->
          <w lemma="al" type="SP">al|</w>
          <w lemma="desposorio" type="NCMS000">des|po|so|rio|</w>
          <w lemma="traer" type="VMII3S0">tra|í|a|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="19">
          <!--Y la Madre estaba en pasmo-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="madre" type="NP00000">ma|dre</w>
          <w lemma="estar" type="VMII3S0">es|ta|ba</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="pasmo" type="NCMS000">pas|mo|</w>
        </l>
        <l met="-|-|3|4|-|-|7|-|" n="20">
          <!--de que tal trueque veía;-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="tal" type="DD0CS0">tal|</w>
          <w lemma="trueque" type="NCMS000">true|que|</w>
          <w lemma="ver" type="VMII3S0">ve|í|a|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="21">
          <!--el llanto del hombre en Dios-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="llanto" type="NCMS000">llan|to|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="hombre" type="NCMS000">hom|bre</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="dios" type="NP00000">dios|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="22">
          <!--y en el hombre el alegría,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="hombre" type="NCMS000">hom|bre</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="alegría" type="NCFS000">a|le|grí|a|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="23">
          <!--lo cual del uno y del otro-->
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="cual" type="PR0CS00">cual|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="uno" type="PI0MS00">u|no</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="otro" type="PI0MS00">o|tro|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="24">
          <!--tan ajeno ser solía.-->
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="ajeno" type="AQ0MS00">a|je|no|</w>
          <w lemma="ser" type="NCMS000">ser|</w>
          <w lemma="soler" type="VMII3S0">so|lí|a|</w>
        </l>
      </lg>
    </body>
  </text>
