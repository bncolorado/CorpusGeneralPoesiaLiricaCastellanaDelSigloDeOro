      <head>
        <title>Manzanares claro,</title>
      </head>
      <lg cert="high" type="Seguidilla">
        <l met="-|-|3|-|5|-|" n="1">
          <!--Manzanares claro,-->
          <w lemma="manzanares" type="NP00000">man|za|na|res|</w>
          <w lemma="claro" type="AQ0MS00">cla|ro|</w>
        </l>
        <l met="1|-|-|4|-|" n="2">
          <!--río pequeño,-->
          <w lemma="río" type="NCMS000">rí|o|</w>
          <w lemma="pequeño" type="AQ0MS00">pe|que|ño|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="3">
          <!--por faltarle el agua-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="faltar" type="VMN0000">fal|tar|</w>
          <w lemma="le" type="PP3CSD0">le</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="agua" type="NCCS000">a|gua|</w>
        </l>
        <l met="1|-|-|4|-|" n="4">
          <!--corre con fuego.-->
          <w lemma="correr" type="VMIP3S0">co|rre|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="fuego" type="NCMS000">fue|go|</w>
        </l>
      </lg>
