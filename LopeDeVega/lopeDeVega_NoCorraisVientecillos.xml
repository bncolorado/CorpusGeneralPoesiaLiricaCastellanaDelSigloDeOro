      <head>
        <title>   No corráis, vientecillos,</title>
      </head>
      <lg cert="high" type="Seguidilla">
        <l met="1|-|3|-|-|6|-|" n="1">
          <!--No corráis, vientecillos,-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="correr" type="VMSP2P0">co|rráis|</w>
          <w lemma="viente" type="NCMP00V">vien|te|ci|llos|</w>
        </l>
        <l met="-|2|-|4|-|" n="2">
          <!--con tanta prisa,-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="tanto" type="DI0FS0">tan|ta|</w>
          <w lemma="prisa" type="NCFS000">pri|sa|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="3">
          <!--porque al son de las aguas-->
          <w lemma="porque" type="CS">por|que</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="son" type="NCMS000">son|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="agua" type="NCFP000">a|guas|</w>
        </l>
        <l met="1|-|-|4|-|" n="4">
          <!--duerme la niña.-->
          <w lemma="dormir" type="VMIP3S0">duer|me|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="niño" type="NCFS000">ni|ña|</w>
        </l>
      </lg>
