        <l met="1|-|-|-|-|" n="1">
          <!--Río de Sevilla,-->
          <w lemma="río_de_sevilla" type="NP00000">rí|o_|de_|se|vi|lla|</w>
        </l>
        <l met="1|2|-|4|-|" n="2">
          <!--¡cuán bien pareces,-->
          <w lemma="cuán" type="RG">cuán|</w>
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="parecer" type="VMIP2S0">pa|re|ces|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="3">
          <!--con galeras blancas-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="galera" type="NCFP000">ga|le|ras|</w>
          <w lemma="blanco" type="AQ0FP00">blan|cas|</w>
        </l>
        <l met="-|2|-|4|-|" n="4">
          <!--y ramos verdes!-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="ramo" type="NCMP000">ra|mos|</w>
          <w lemma="verde" type="AQ0CP00">ver|des|</w>
        </l>
