  <text>
    <body>
      <head>
        <title>A los carreteros</title>
      </head>
      <lg cert="..." type="...">
        <l met="-|-|-|-|5|-|" n="1">
          <!--A los carreteros-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="carretero" type="NCMP000">ca|rre|te|ros|</w>
        </l>
        <l met="-|2|-|4|-|" n="2">
          <!--del buen Getafe-->
          <w lemma="del" type="SP">del|</w>
          <w lemma="bueno" type="AQ0MS00">buen|</w>
          <w lemma="getafe" type="NP00000">ge|ta|fe|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="3">
          <!--les rogaba la niña-->
          <w lemma="le" type="PP3CPD0">les|</w>
          <w lemma="rogar" type="VMII3S0">ro|ga|ba|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="niño" type="NCFS000">ni|ña|</w>
        </l>
        <l met="-|-|-|4|-|" n="4">
          <!--que la llevasen.-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="llevar" type="VMSI3P0">lle|va|sen|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="5">
          <!--Pásese, señora,-->
          <w lemma="pasar" type="VMM03S0">pa|se|</w>
          <w lemma="se" type="PP3CN00">se|</w>
          <w lemma="señor" type="NCFS000">se|ño|ra|</w>
        </l>
        <l met="-|2|-|4|-|" n="6">
          <!--desotra banda,-->
          <w lemma="desotra" type="RG">de|so|tra|</w>
          <w lemma="banda" type="NCFS000">ban|da|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="7">
          <!--que es aquella mula-->
          <w lemma="que" type="CS">que</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="aquel" type="DD0FS0">a|que|lla|</w>
          <w lemma="mulo" type="NCFS000">mu|la|</w>
        </l>
        <l met="-|-|-|4|-|" n="8">
          <!--falsificada.-->
          <w lemma="falsificar" type="VMP00SF">fal|si|fi|ca|da|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="9">
          <!--Unta aquellas ruedas,-->
          <w lemma="untar" type="VMIP3S0">un|ta</w>
          <w lemma="aquel" type="DD0FP0">a|que|llas|</w>
          <w lemma="rueda" type="NCFP000">rue|das|</w>
        </l>
        <l met="1|-|-|4|-|" n="10">
          <!--mozo de Judas,-->
          <w lemma="mozo" type="NCMS000">mo|zo|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="judas" type="NP00000">ju|das|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="11">
          <!--que ninguno se mueve-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="ninguno" type="PI0MS00">nin|gu|no|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="mover" type="VMIP3S0">mue|ve|</w>
        </l>
        <l met="-|2|-|4|-|" n="12">
          <!--si no le untan.-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="untar" type="VMIP3P0">un|tan|</w>
        </l>
        <l met="-|-|3|4|-|" n="13">
          <!--Hacia atrás se hagan-->
          <w lemma="hacia" type="SP">ha|cia</w>
          <w lemma="atrás" type="RG">a|trás|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="hacer" type="VMSP3P0">ha|gan|</w>
        </l>
        <l met="-|-|-|4|-|" n="14">
          <!--los de delante,-->
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="delante" type="RG">de|lan|te|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="15">
          <!--que se ahorcan las mulas-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="ahorcar" type="VMIP3P0">ahor|can|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="mulo" type="NCFP000">mu|las|</w>
        </l>
        <l met="-|2|-|4|-|" n="16">
          <!--sin ser tratantes.-->
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="tratante" type="AQ0CP00">tra|tan|tes|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="17">
          <!--¡Cómo se arrellana-->
          <w lemma="cómo" type="PT00000">có|mo|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="arrellanar" type="VMIP3S0">a|rre|lla|na|</w>
        </l>
        <l met="-|2|-|4|-|" n="18">
          <!--la madre tía!-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="madre" type="NCFS000">ma|dre|</w>
          <w lemma="tío" type="NCFS000">tí|a|</w>
        </l>
        <l met="1|-|3|-|5|6|-|" n="19">
          <!--¡Vive Dios que no lleve-->
          <w lemma="vivir" type="VMIP3S0">vi|ve|</w>
          <w lemma="dios" type="NP00000">dios|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="llevar" type="VMSP3S0">lle|ve|</w>
        </l>
        <l met="1|-|-|4|-|" n="20">
          <!--vieja en mi vida!-->
          <w lemma="viejo" type="AQ0FS00">vie|ja</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="21">
          <!--Si en mi carro llevara-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="carro" type="NCMS000">ca|rro|</w>
          <w lemma="llevar" type="VMSI3S0">lle|va|ra|</w>
        </l>
        <l met="1|-|3|-|" n="22">
          <!--poetas solos,-->
          <w lemma="poeta" type="NCMP000">poe|tas|</w>
          <w lemma="solo" type="AQ0MP00">so|los|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="23">
          <!--no llevara un adarme-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="llevar" type="VMSI3S0">lle|va|ra|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="adarme" type="NCMS000">a|dar|me|</w>
        </l>
        <l met="-|2|-|4|-|" n="24">
          <!--de viento en todos.-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="viento" type="NCMS000">vien|to</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="todo" type="PI0MP00">to|dos|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="25">
          <!--Dale âquella rucia,-->
          <w lemma="dar" type="VMM02S0">da|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="âquella" type="NCFS000">â|que|lla|</w>
          <w lemma="rucio" type="AQ0FS00">ru|cia|</w>
        </l>
        <l met="-|-|-|4|-|" n="26">
          <!--que se desmanda;-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="desmandar" type="VMIP3S0">des|man|da|</w>
        </l>
        <l met="-|-|3|-|5|6|-|" n="27">
          <!--alcaceres ha visto;-->
          <w lemma="alcacer" type="NCMP000">al|ca|ce|res|</w>
          <w lemma="haber" type="VAIP3S0">ha|</w>
          <w lemma="ver" type="VMP00SM">vis|to|</w>
        </l>
        <l met="1|2|-|4|-|" n="28">
          <!--ser hembra basta.-->
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="hembra" type="NCFS000">hem|bra|</w>
          <w lemma="bastar" type="VMIP3S0">bas|ta|</w>
        </l>
      </lg>
    </body>
  </text>
