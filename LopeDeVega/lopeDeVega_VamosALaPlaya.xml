  <text>
    <body>
      <head>
        <title> Vamos a la playa</title>
      </head>
      <lg cert="high" type="Seguidillas">
        <l met="1|-|-|-|5|-|" n="1">
          <!--Vamos a la playa-->
          <w lemma="ir" type="VMIP1P0">va|mos|</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="playa" type="NCFS000">pla|ya|</w>
        </l>
        <l met="1|-|-|4|-|" n="2">
          <!--noche de San Juan-->
          <w lemma="noche" type="NCFS000">no|che|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="san_juan" type="NP00000">san_|juan|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="3">
          <!--que alegra la tierra-->
          <w lemma="que" type="CS">que</w>
          <w lemma="alegrar" type="VMIP3S0">a|le|gra|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="tierra" type="NCFS000">tie|rra|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="4">
          <!--y retumba el mar.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="retumbar" type="VMIP3S0">re|tum|ba</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="marzo" type="NCMS000">mar.|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="5">
          <!--En la paya hagamos-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="payo" type="NCFS000">pa|ya</w>
          <w lemma="hacer" type="VMSP1P0">ha|ga|mos|</w>
        </l>
        <l met="1|-|-|4|5|-|" n="6">
          <!--fiestas de mil modos,-->
          <w lemma="fiesta" type="NCFP000">fies|tas|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="1000" type="Z">mil|</w>
          <w lemma="modo" type="NCMP000">mo|dos|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="7">
          <!--coronados todos-->
          <w lemma="coronar" type="VMP00PM">co|ro|na|dos|</w>
          <w lemma="todo" type="DI0MP0">to|dos|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="8">
          <!--de verbena y ramos.-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="verbena" type="NCFS000">ver|be|na</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="ramo" type="NCMP000">ra|mos|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="9">
          <!--A su arena vamos,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="su" type="DP3CSN">su</w>
          <w lemma="arena" type="NCFS000">a|re|na|</w>
          <w lemma="ir" type="VMIP1P0">va|mos|</w>
        </l>
        <l met="1|-|-|4|-|" n="10">
          <!--noche de San Juan,-->
          <w lemma="noche" type="NCFS000">no|che|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="san_juan" type="NP00000">san_|juan|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="11">
          <!--que alegra la tierra-->
          <w lemma="que" type="CS">que</w>
          <w lemma="alegrar" type="VMIP3S0">a|le|gra|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="tierra" type="NCFS000">tie|rra|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="12">
          <!--y retumba el mar.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="retumbar" type="VMIP3S0">re|tum|ba</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="marzo" type="NCMS000">mar.|</w>
        </l>
      </lg>
    </body>
