    <body>
      <head>
        <title> Esta novia se lleva la flor,</title>
      </head>
      <lg cert="..." type="...">
        <l met="1|-|3|-|-|6|-|-|9|-|" n="1">
          <!--Esta novia se lleva la flor,-->
          <w lemma="este" type="DD0FS0">es|ta|</w>
          <w lemma="novio" type="NCFS000">no|via|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="llevar" type="VMIP3S0">lle|va|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="flor" type="NCFS000">flor|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="2">
          <!--que las otras no.-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="otro" type="PI0FP00">o|tras|</w>
          <w lemma="no" type="RN">no|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="3">
          <!--Bendiga Dios el molino-->
          <w lemma="bendecir" type="VMSP3S0">ben|di|ga|</w>
          <w lemma="dios" type="NP00000">dios|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="molino" type="NCMS000">mo|li|no|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="4">
          <!--que tales novias sustenta,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="tal" type="DD0CP0">ta|les|</w>
          <w lemma="novio" type="NCFP000">no|vias|</w>
          <w lemma="sustentar" type="VMIP3S0">sus|ten|ta|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="5">
          <!--muela su harina sin cuenta-->
          <w lemma="moler" type="VMSP3S0">mue|la|</w>
          <w lemma="su" type="DP3CSN">su</w>
          <w lemma="harina" type="NCFS000">ha|ri|na|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="cuenta" type="NCFS000">cuen|ta|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="6">
          <!--a costa de tal padrino.-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="costa" type="NCFS000">cos|ta|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="tal" type="DD0CS0">tal|</w>
          <w lemma="padrino" type="NCMS000">pa|dri|no|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="7">
          <!--Éstas muelen de lo fino-->
          <w lemma="este" type="PD0FP00">és|tas|</w>
          <w lemma="moler" type="VMIP3P0">mue|len|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="fino" type="AQ0MS00">fi|no|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="8">
          <!--del trigo que muele amor,-->
          <w lemma="del" type="SP">del|</w>
          <w lemma="trigo" type="NCMS000">tri|go|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="moler" type="VMIP3S0">mue|le</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="9">
          <!--que las otras no.-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="otro" type="PI0FP00">o|tras|</w>
          <w lemma="no" type="RN">no|</w>
        </l>
      </lg>
