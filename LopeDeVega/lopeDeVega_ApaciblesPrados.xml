    <body>
      <head>
        <title> Apacibles prados,</title>
      </head>
      <lg cert="high" type="Seguidilla">
        <l met="-|-|3|-|5|-|" n="1">
          <!--Apacibles prados,-->
          <w lemma="apacible" type="AQ0CP00">a|pa|ci|bles|</w>
          <w lemma="prado" type="NCMP000">pra|dos|</w>
        </l>
        <l met="-|2|-|4|-|" n="2">
          <!--creced las hierbas,-->
          <w lemma="crecer" type="VMM02P0">cre|ced|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="hierba" type="NCFP000">hier|bas|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="3">
          <!--que ganado de oro-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="ganado" type="NCMS000">ga|na|do|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="oro" type="NCMS000">o|ro|</w>
        </l>
        <l met="1|-|-|4|-|" n="4">
          <!--pasa por ellas.-->
          <w lemma="pasar" type="VMIP3S0">pa|sa|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="ellos" type="PP3FP00">e|llas|</w>
        </l>
      </lg>
