  <text>
    <body>
      <head>
        <title> Blancas coge Lucinda</title>
      </head>
      <lg cert="high" type="Seguidillas">
        <l met="1|-|3|-|-|6|-|" n="1">
          <!--Blancas coge Lucinda-->
          <w lemma="blancas" type="NP00000">blan|cas|</w>
          <w lemma="coger" type="VMIP3S0">co|ge|</w>
          <w lemma="lucinda" type="NP00000">lu|cin|da|</w>
        </l>
        <l met="-|-|-|4|-|" n="2">
          <!--las azucenas-->
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="azucena" type="NCFP000">a|zu|ce|nas|</w>
        </l>
        <l met="-|-|-|4|-|-|-|8|-|" n="3">
          <!--y en llegando a sus manos-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="llegar" type="VMG0000">lle|gan|do|</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="mano" type="NCFP000">ma|nos|</w>
        </l>
        <l met="-|2|-|4|-|" n="4">
          <!--parecen negras.-->
          <w lemma="parecer" type="VMIP3P0">pa|re|cen|</w>
          <w lemma="negro" type="AQ0FP00">ne|gras|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="5">
          <!--Cuando sale el alba,-->
          <w lemma="cuando" type="CS">cuan|do|</w>
          <w lemma="salir" type="VMIP3S0">sa|le</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="alba" type="NCFS000">al|ba|</w>
        </l>
        <l met="-|2|-|4|-|" n="6">
          <!--Lucinda bella,-->
          <w lemma="lucinda" type="NP00000">lu|cin|da|</w>
          <w lemma="bello" type="AQ0FS00">be|lla|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="7">
          <!--sale más hermosa,-->
          <w lemma="salir" type="VMIP3S0">sa|le|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="hermoso" type="AQ0FS00">her|mo|sa|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="8">
          <!--la tierra alegra.-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="tierra" type="NCFS000">tie|rra|</w>
          <w lemma="alegrar" type="VMIP3S0">a|le|gra|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="9">
          <!--Con su sol enjuga-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="sol" type="NCMS000">sol|</w>
          <w lemma="enjugar" type="VMIP3S0">en|ju|ga|</w>
        </l>
        <l met="-|2|-|4|-|" n="10">
          <!--sus blancas perlas;-->
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="blanco" type="AQ0FP00">blan|cas|</w>
          <w lemma="perla" type="NCFP000">pe|rlas|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="11">
          <!--si una flor le quita,-->
          <w lemma="si" type="CS">si</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="flor" type="NCFS000">flor|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="quitar" type="VMIP3S0">qui|ta|</w>
        </l>
        <l met="-|2|-|4|-|" n="12">
          <!--dos mil engendra.-->
          <w lemma="2000" type="Z">dos_|mil|</w>
          <w lemma="engendrar" type="VMIP3S0">en|gen|dra|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="13">
          <!--Porque son sus plantas-->
          <w lemma="porque" type="CS">por|que|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="planta" type="NCFP000">plan|tas|</w>
        </l>
        <l met="-|-|-|4|-|" n="14">
          <!--de primavera-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="primavera" type="NCFS000">pri|ma|ve|ra|</w>
        </l>
        <l met="-|-|-|-|5|-|" n="15">
          <!--y como cristales-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="como" type="CS">co|mo|</w>
          <w lemma="cristal" type="NCMP000">cris|ta|les|</w>
        </l>
        <l met="-|2|-|4|-|" n="16">
          <!--sus manos bellas.-->
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="mano" type="NCFP000">ma|nos|</w>
          <w lemma="bello" type="AQ0FP00">be|llas|</w>
        </l>
        <l met="-|2|-|4|5|-|" n="17">
          <!--Y ansí, con ser bellas-->
          <w lemma="y" type="CC">y</w>
          <w lemma="ansí" type="RG">an|sí|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="bello" type="AQ0FP00">be|llas|</w>
        </l>
        <l met="-|-|-|4|-|" n="18">
          <!--las azucenas,-->
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="azucena" type="NCFP000">a|zu|ce|nas|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="19">
          <!--en llegando a sus manos-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="llegar" type="VMG0000">lle|gan|do</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="mano" type="NCFP000">ma|nos|</w>
        </l>
        <l met="-|2|-|4|-|" n="20">
          <!--parecen negras.-->
          <w lemma="parecer" type="VMIP3P0">pa|re|cen|</w>
          <w lemma="negro" type="AQ0FP00">ne|gras|</w>
        </l>
      </lg>
    </body>
  </text>
