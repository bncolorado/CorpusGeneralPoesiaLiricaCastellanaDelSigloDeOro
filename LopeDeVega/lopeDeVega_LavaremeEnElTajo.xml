        <l met="-|-|3|-|-|6|-|" n="1">
          <!--Lavareme en el Tajo-->
          <w lemma="lavareme" type="NP00000">la|va|re|me</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="tajo" type="NP00000">ta|jo|</w>
        </l>
        <l met="1|-|-|4|-|" n="2">
          <!--muerta de risa,-->
          <w lemma="morir" type="VMP00SF">muer|ta|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="risa" type="NCFS000">ri|sa|</w>
        </l>
        <l met="-|-|-|4|-|-|-|8|-|" n="3">
          <!--que el arena en los dedos-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="arena" type="NCFS000">a|re|na|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="dedo" type="NCMP000">de|dos|</w>
        </l>
        <l met="1|-|-|4|-|" n="4">
          <!--me hace cosquillas.-->
          <w lemma="me" type="PP1CS00">me</w>
          <w lemma="hacer" type="VMIP3S0">ha|ce|</w>
          <w lemma="cosquillas" type="NCFP000">cos|qui|llas|</w>
        </l>
