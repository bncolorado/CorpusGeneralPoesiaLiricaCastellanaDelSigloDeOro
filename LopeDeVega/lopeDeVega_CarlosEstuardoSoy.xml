      <head>
        <title>Carlos Estuardo soy,</title>
      </head>
      <lg cert="high" type="Cuarteto">
        <l met="-|-|3|-|5|-|" n="1">
          <!--Carlos Estuardo soy,-->
          <w lemma="carlos_estuardo" type="NP00000">ca|rlos_es|tuar|do|</w>
          <w lemma="ser" type="VSIP1S0">soy|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="2">
          <!--que, siendo Amor mi guía,-->
          <w lemma="que" type="CS">que</w>
          <w lemma="ser" type="VSG0000">sien|do</w>
          <w lemma="amor" type="NP00000">a|mor</w>
          <w lemma="mi" type="DP1CSS">mi</w>
          <w lemma="guía" type="NCCS000">guí|a|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="3">
          <!--al cielo de España voy-->
          <w lemma="al" type="SP">al|</w>
          <w lemma="cielo" type="NCMS000">cie|lo|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="españa" type="NP00000">es|pa|ña|</w>
          <w lemma="ir" type="VMIP1S0">voy|</w>
        </l>
        <l met="-|2|-|-|5|-|-|8|-|" n="4">
          <!--por ver mi estrella María.-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="estrella" type="NCFS000">es|tre|lla|</w>
          <w lemma="maría" type="NP00000">ma|rí|a|</w>
        </l>
      </lg>
