  <text>
    <body>
      <head>
        <title>Álamos del seto,</title>
      </head>
      <lg cert="high" type="Seguidilla">
        <l met="1|-|-|-|5|-|" n="1">
          <!--Álamos del seto,-->
          <w lemma="álamos" type="NP00000">á|la|mos|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="seto" type="NCMS000">se|to|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="2">
          <!--¿dónde está mi amor?-->
          <w lemma="dónde" type="PT00000">dón|de</w>
          <w lemma="estar" type="VMIP3S0">es|tá|</w>
          <w lemma="mi" type="DP1CSS">mi</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="3">
          <!--Si se fue con otro-->
          <w lemma="si" type="CS">si|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="ser" type="VSIS3S0">fue|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="otro" type="DI0MS0">o|tro|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="4">
          <!--morireme yo.-->
          <w lemma="morireme" type="VMIP3S0">mo|ri|re|me|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
        </l>
      </lg>
    </body>
