  <text>
    <body>
      <head>
        <title>  Caminad, suspiros</title>
      </head>
      <lg cert="high" type="Seguidilla">
        <l met="-|-|3|-|5|-|" n="1">
          <!--Caminad, suspiros-->
          <w lemma="caminar" type="VMM02P0">ca|mi|nad|</w>
          <w lemma="suspiro" type="NCMP000">sus|pi|ros|</w>
        </l>
        <l met="-|-|-|-|5|-|" n="2">
          <!--adonde soléis,-->
          <w lemma="adonde" type="CS">a|don|de|</w>
          <w lemma="solar" type="VMSP2P0">so|léis|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="3">
          <!--y si duerme mi niña-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="dormir" type="VMIP3S0">duer|me|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="niño" type="NCFS000">ni|ña|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="4">
          <!--no la recordéis.-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
          <w lemma="recordar" type="VMSP2P0">re|cor|déis|</w>
        </l>
      </lg>
    </body>
