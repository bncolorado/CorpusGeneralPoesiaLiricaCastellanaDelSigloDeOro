          <w lemma="llevar" type="VMII3S0">lle|va|ba</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="cuello" type="NCMS000">cue|llo|</w>
          <w lemma="pendiente" type="AQ0CS00">pen|dien|te|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="27">
