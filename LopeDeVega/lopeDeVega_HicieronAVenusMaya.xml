          <w lemma="de" type="SP">de|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
          <w lemma="cómo" type="PT00000">có|mo|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="cantar" type="VMII3P0">can|ta|ban|</w>
        </l>
        <l met="-|-|-|4|-|-|-|8|-|" n="40">
