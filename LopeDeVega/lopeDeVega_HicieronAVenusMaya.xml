          <w lemma="y" type="CC">y|</w>
          <w lemma="fabio" type="NP00000">fa|bio|</w>
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="chapín" type="NCMP000">cha|pi|nes|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="18">
