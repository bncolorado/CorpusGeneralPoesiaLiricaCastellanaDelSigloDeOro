          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="colmenero" type="NCFS000">col|me|ne|ra|</w>
        </l>
        <l met="-|-|3|-|5|-|-|8|-|-|-|12|13|-|" n="42">
