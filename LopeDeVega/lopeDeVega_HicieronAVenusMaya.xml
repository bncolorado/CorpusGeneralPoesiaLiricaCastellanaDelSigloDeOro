          <w lemma="pasar" type="VMIS3S0">pa|só</w>
          <w lemma="amor" type="NP00000">a|mor|</w>
          <w lemma="y" type="CC">y</w>
          <w lemma="aunque" type="CC">aun|que|</w>
          <w lemma="desnudo" type="AQ0MS00">des|nu|do|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="26">
