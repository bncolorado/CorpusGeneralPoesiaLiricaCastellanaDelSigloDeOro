          <w lemma="amor" type="NP00000">a|mor|</w>
          <w lemma="entre" type="SP">en|tre|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="pastor" type="NCFP000">pas|to|ras|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="34">
