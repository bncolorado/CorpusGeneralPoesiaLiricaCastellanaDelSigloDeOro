          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="carcaj" type="NCMS000">car|caj|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="flecha" type="NCFP000">fle|chas|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="oro" type="NCMS000">o|ro|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="28">
          <!--con plumas blancas y verdes.-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="pluma" type="NCFP000">plu|mas|</w>
          <w lemma="blanco" type="AQ0FP00">blan|cas|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="verde" type="AQ0CP00">ver|des|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="29">
          <!--Dad para la maya,-->
          <w lemma="dar" type="VMM02P0">dad|</w>
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="maya" type="NCCS000">ma|ya|</w>
        </l>
        <l met="-|-|-|4|-|" n="30">
          <!--el caballero,-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="caballero" type="NCMS000">ca|ba|lle|ro|</w>
        </l>
        <l met="-|2|3|-|5|-|" n="31">
          <!--que más vale honra-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="valer" type="VMIP3S0">va|le|</w>
          <w lemma="honra" type="NCFS000">ho|nra|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="32">
          <!--que no el dinero.-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="dinero" type="NCMS000">di|ne|ro|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="33">
