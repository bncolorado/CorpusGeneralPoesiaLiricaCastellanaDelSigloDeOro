          <w lemma="y" type="CC">y</w>
          <w lemma="a_puñados" type="RG">a_|pu|ña|dos|</w>
          <w lemma="lo" type="PP3FPA0">las|</w>
          <w lemma="coger" type="VMII3P0">co|gí|an|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="37">
