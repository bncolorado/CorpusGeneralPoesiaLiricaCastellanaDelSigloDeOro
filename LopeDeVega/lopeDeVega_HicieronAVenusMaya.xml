          <w lemma="y" type="CC">y|</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="propósito" type="NCMS000">pro|pó|si|to|</w>
          <w lemma="decir" type="VMII3P0">de|cí|an|</w>
        </l>
        <l met="1|-|-|4|5|-|-|-|9|-|" n="41">
