          <w lemma="y" type="CC">y|</w>
          <w lemma="picole" type="VMSP3S0">pi|co|le|</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="abeja" type="NCFS000">a|be|ja|</w>
