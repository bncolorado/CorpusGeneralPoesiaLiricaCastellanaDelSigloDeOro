    <body>
      <head>
        <title> Hicieron a Venus maya,</title>
      </head>
      <lg cert="..." type="...">
        <l met="-|2|-|-|5|-|7|-|" n="1">
          <!--Hicieron a Venus maya,-->
          <w lemma="hacer" type="VMIS3P0">hi|cie|ron|</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="venus" type="NP00000">ve|nus|</w>
          <w lemma="maya" type="AQ0CS00">ma|ya|</w>
        </l>
        <l met="1|-|-|-|5|-|7|-|" n="2">
          <!--diosa interesable siempre,-->
          <w lemma="dios" type="NCFS000">dio|sa</w>
          <w lemma="interesable" type="AQ0CS00">in|te|re|sa|ble|</w>
          <w lemma="siempre" type="RG">siem|pre|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="3">
          <!--los pastores de la isla-->
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="pastor" type="NCMP000">pas|to|res|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="isla" type="NCFS000">i|sla|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="4">
          <!--donde más imperio tiene.-->
          <w lemma="donde" type="PR00000">don|de|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="imperio" type="NCMS000">im|pe|rio|</w>
          <w lemma="tener" type="VMIP3S0">tie|ne|</w>
        </l>
        <l met="-|-|-|4|-|-|-|" n="5">
          <!--Como los meses de mayo-->
          <w lemma="como" type="CS">co|mo|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="mes" type="NCMP000">me|ses|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="[??:??/5/??:??.??:??]" type="W">ma|yo|</w>
        </l>
        <l met="1|-|-|-|5|-|7|-|" n="6">
          <!--eran sus mejores meses,-->
          <w lemma="ser" type="VSII3P0">e|ran|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="mejor" type="AQ0CP00">me|jo|res|</w>
          <w lemma="mes" type="NCMP000">me|ses|</w>
        </l>
        <l met="1|-|-|4|5|-|7|-|" n="7">
          <!--ya porque está verde todo-->
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="porque" type="CS">por|que</w>
          <w lemma="estar" type="VMIP3S0">es|tá|</w>
          <w lemma="verde" type="AQ0CS00">ver|de|</w>
          <w lemma="todo" type="DI0MS0">to|do|</w>
        </l>
        <l met="1|-|-|-|5|6|7|-|" n="8">
          <!--ya porque la diosa es verde,-->
          <w lemma="ya" type="RG">ya|</w>
          <w lemma="porque" type="CS">por|que|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="dios" type="NCFS000">dio|sa</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="verde" type="AQ0CS00">ver|de|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="9">
          <!--Belisa y la bella Antandra-->
          <w lemma="belisa" type="NP00000">be|li|sa</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="bello" type="AQ0FS00">be|lla</w>
          <w lemma="antandra" type="NP00000">an|tan|dra|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="10">
          <!--pedían con una fuente-->
          <w lemma="pedir" type="VMII3P0">pe|dí|an|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="fuente" type="NCFS000">fuen|te|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="11">
          <!--y a la gente que pasaba-->
          <w lemma="y" type="CC">y</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="gente" type="NCFS000">gen|te|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="pasar" type="VMII3S0">pa|sa|ba|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="12">
          <!--esto cantaban alegres-->
          <w lemma="este" type="PD00S00">es|to|</w>
          <w lemma="cantar" type="VMII3P0">can|ta|ban|</w>
          <w lemma="alegre" type="AQ0CP00">a|le|gres|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="13">
          <!--Den para la maya,-->
          <w lemma="dar" type="VMSP3P0">den|</w>
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="maya" type="NCCS000">ma|ya|</w>
        </l>
        <l met="-|2|-|4|-|-|-|8|-|" n="14">
          <!--que es hermosa y galana.-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="hermoso" type="AQ0FS00">her|mo|sa|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="galano" type="AQ0FS00">ga|la|na|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="15">
          <!--Pasó Riselo y les dio-->
          <w lemma="pasar" type="VMIS3S0">pa|só|</w>
          <w lemma="riselo" type="NP00000">ri|se|lo</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="le" type="PP3CPD0">les|</w>
          <w lemma="dar" type="VMIS3S0">dio|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="16">
