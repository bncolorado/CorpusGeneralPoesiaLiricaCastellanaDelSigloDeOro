          <w lemma="que" type="CS">que|</w>
          <w lemma="pie" type="NCMP000">pies|</w>
          <w lemma="celebrar" type="VMII3S0">ce|le|bra|ba|</w>
          <w lemma="siempre" type="RG">siem|pre|</w>
        </l>
        <l met="-|2|3|-|5|6|7|-|" n="19">
          <!--Pasó Balo y no dio nada-->
          <w lemma="pasar" type="VMIS3S0">pa|só|</w>
          <w lemma="balo" type="NP00000">ba|lo</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="dar" type="VMIS3S0">dio|</w>
          <w lemma="nada" type="PI0CS00">na|da|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="20">
          <!--y las pastoras al verle-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="pastor" type="NCFP000">pas|to|ras|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="21">
          <!--tan cobarde en el dativo-->
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="cobarde" type="AQ0CS00">co|bar|de</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="dativo" type="NCMS000">da|ti|vo|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="22">
          <!--le cantaron de esta suerte:-->
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="cantar" type="VMIS3P0">can|ta|ron|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="este" type="DD0FS0">es|ta|</w>
          <w lemma="suerte" type="NCFS000">suer|te|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="23">
          <!--Pase, pase al palado,-->
          <w lemma="pasar" type="VMM03S0">pa|se|</w>
          <w lemma="pasar" type="VMM03S0">pa|se|</w>
          <w lemma="pasar" type="VMM03S0">pa|se|</w>
          <w lemma="pasar" type="VMM03S0">pa|se|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="palado" type="AQ0MS00">pa|la|do|</w>
        </l>
        <l met="-|2|3|-|5|-|-|-|9|-|" n="24">
