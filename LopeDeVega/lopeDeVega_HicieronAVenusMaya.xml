          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="llevar" type="VMIP3S0">lle|va|</w>
          <w lemma="blanco" type="AQ0FS00">blan|ca|</w>
          <w lemma="ni" type="CC">ni|</w>
          <w lemma="cornado" type="VMP00SM">cor|na|do|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="25">
