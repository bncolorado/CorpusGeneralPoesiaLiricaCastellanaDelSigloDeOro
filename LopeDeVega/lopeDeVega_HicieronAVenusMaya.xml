          <w lemma="quedar" type="VMIS3P0">que|da|ron|</w>
          <w lemma="enamorar" type="VMP00PF">e|na|mo|ra|das|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="38">
          <!--y Venus muerta de risa-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="venus" type="NP00000">ve|nus|</w>
          <w lemma="morir" type="VMP00SF">muer|ta|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="risa" type="NCFS000">ri|sa|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="39">
