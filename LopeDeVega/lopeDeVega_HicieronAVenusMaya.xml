          <w lemma="flecha" type="NCFP000">fle|chas|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="oro" type="NCMS000">o|ro|</w>
          <w lemma="repartir" type="VMII3S0">re|par|tí|a|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="35">
