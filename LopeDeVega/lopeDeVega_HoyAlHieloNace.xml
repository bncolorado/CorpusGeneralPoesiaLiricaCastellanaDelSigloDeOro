          <w lemma="él" type="PP3MS00">él|</w>
          <w lemma="llorar" type="VMIP3S0">llo|ra|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="21">
          <!--No lloréis, mi vida,-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="llorar" type="VMSP2P0">llo|réis|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="22">
          <!--que me dais pasión,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="dar" type="VMIP2P0">dais|</w>
          <w lemma="pasión" type="NCFS000">pa|sión|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="23">
          <!--le dice la Niña-->
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="decir" type="VMIP3S0">di|ce|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="niña" type="NP00000">ni|ña|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="24">
          <!--que al Niño parió.-->
          <w lemma="que" type="CS">que</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="niño" type="NP00000">ni|ño|</w>
          <w lemma="parir" type="VMIS3S0">pa|rió|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="25">
          <!--Témplanse los aires-->
          <w lemma="témplanse" type="NP00000">tém|plan|se|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="aire" type="NCMP000">ai|res|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="26">
          <!--a su dulce voz;-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="dulce" type="AQ0CS00">dul|ce|</w>
          <w lemma="voz" type="NCFS000">voz|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="27">
          <!--cántale su Madre-->
          <w lemma="cantar" type="VMM02S0">can|ta|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="madre" type="NP00000">ma|dre|</w>
