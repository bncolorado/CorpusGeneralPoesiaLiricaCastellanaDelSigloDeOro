          <w lemma="como" type="CS">co|mo|</w>
          <w lemma="ser" type="VSIS3S0">fue|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="madre" type="NP00000">ma|dre|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="14">
          <!--de tal perfección,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="tal" type="DD0CS0">tal|</w>
          <w lemma="perfección" type="NCFS000">per|fec|ción|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="15">
          <!--un precioso nácar-->
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="precioso" type="AQ0MS00">pre|cio|so|</w>
          <w lemma="nácar" type="NCMS000">ná|car|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="16">
          <!--sólo abierto al sol,-->
          <w lemma="sólo" type="RG">só|lo</w>
          <w lemma="abrir" type="VMP00SM">a|bier|to</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="sol" type="NCMS000">sol|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="17">
          <!--las que llora al Niño-->
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="llorar" type="VMIP3S0">llo|ra</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="niño" type="NP00000">ni|ño|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="18">
          <!--finas perlas son.-->
          <w lemma="fino" type="AQ0FP00">fi|nas|</w>
          <w lemma="perla" type="NCFP000">pe|rlas|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="19">
          <!--Cántale su Madre-->
          <w lemma="cantar" type="VMM02S0">can|ta|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="madre" type="NP00000">ma|dre|</w>
