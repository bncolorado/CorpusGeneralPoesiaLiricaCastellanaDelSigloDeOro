    <body>
      <head>
        <title> Hoy al hielo nace</title>
      </head>
      <lg cert="..." type="...">
        <l met="1|-|3|-|5|-|" n="1">
          <!--Hoy al hielo nace-->
          <w lemma="hoy" type="RG">hoy|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="hielo" type="NCMS000">hie|lo|</w>
          <w lemma="nacer" type="VMIP3S0">na|ce|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="2">
          <!--en Belén mi Dios,-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="belén" type="NP00000">be|lén|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="dios" type="NP00000">dios|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="3">
          <!--cántale su Madre-->
          <w lemma="cantar" type="VMM02S0">can|ta|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="madre" type="NP00000">ma|dre|</w>
        </l>
        <l met="1|2|-|-|5|-|" n="4">
          <!--y él llora de amor.-->
          <w lemma="y" type="CC">y</w>
          <w lemma="él" type="PP3MS00">él|</w>
          <w lemma="llorar" type="VMIP3S0">llo|ra|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
        </l>
        <l met="-|2|3|-|5|-|" n="5">
          <!--Aquel Verbo santo,-->
          <w lemma="aquel" type="DD0MS0">a|quel|</w>
          <w lemma="verbo" type="NP00000">ver|bo|</w>
          <w lemma="santo" type="AQ0MS00">san|to|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="6">
          <!--luz y resplandor-->
          <w lemma="luz" type="NCFS000">luz|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="resplandor" type="NCMS000">res|plan|dor|</w>
        </l>
        <l met="-|-|-|-|5|-|" n="7">
          <!--de su Padre Eterno,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="padre_eterno" type="NP00000">pa|dre_e|ter|no|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="8">
          <!--que es quien le engendró,-->
          <w lemma="que" type="CS">que</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="quien" type="PR0CS00">quien|</w>
          <w lemma="le" type="PP3CSD0">le</w>
          <w lemma="engendrar" type="VMIS3S0">en|gen|dró|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="9">
