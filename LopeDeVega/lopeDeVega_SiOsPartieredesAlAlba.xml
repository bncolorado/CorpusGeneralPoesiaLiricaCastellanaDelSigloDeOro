  <text>
    <body>
      <head>
        <title> Si os partiéredes al alba</title>
      </head>
      <lg cert="..." type="...">
        <l met="-|-|3|-|-|-|7|-|" n="1">
          <!--Si os partiéredes al alba-->
          <w lemma="si" type="CS">si</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="partiéredes" type="NCFP000">par|tié|re|des|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="alba" type="NCFS000">al|ba|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="2">
          <!--quedito, pasito, amor,-->
          <w lemma="quedo" type="AQVMS00">que|di|to|</w>
          <w lemma="pasito" type="NCMS000">pa|si|to</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="3">
          <!--no espantéis al ruiseñor.-->
          <w lemma="no" type="RN">no</w>
          <w lemma="espantar" type="VMSP2P0">es|pan|téis|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="ruiseñor" type="NCMS000">rui|se|ñor|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="4">
          <!--Si os levantáis de mañana-->
          <w lemma="si" type="CS">si</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="levantar" type="VMIP2P0">le|van|táis|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="mañana" type="NCCS000">ma|ña|na|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="5">
          <!--de los brazos que os desean,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="brazo" type="NCMP000">bra|zos|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="desear" type="VMIP3P0">de|se|an|</w>
        </l>
        <l met="-|-|-|4|-|6|7|-|" n="6">
          <!--porque en los brazos no os vean-->
          <w lemma="porque" type="CS">por|que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="brazo" type="NCMP000">bra|zos|</w>
          <w lemma="no" type="RN">no</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="ver" type="VMSP3P0">ve|an|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="7">
          <!--de alguna envidia liviana,-->
          <w lemma="de" type="SP">de</w>
          <w lemma="alguno" type="DI0FS0">al|gu|na</w>
          <w lemma="envidia" type="NCFS000">en|vi|dia|</w>
          <w lemma="liviano" type="AQ0FS00">li|via|na|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="8">
          <!--pisad con planta de lana,-->
          <w lemma="pisar" type="VMM02P0">pi|sad|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="planta" type="NCFS000">plan|ta|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="lana" type="NCFS000">la|na|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="9">
          <!--quedito, pasito, amor,-->
          <w lemma="quedo" type="AQVMS00">que|di|to|</w>
          <w lemma="pasito" type="NCMS000">pa|si|to</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="10">
          <!--no espantéis al ruiseñor.-->
          <w lemma="no" type="RN">no</w>
          <w lemma="espantar" type="VMSP2P0">es|pan|téis|</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="ruiseñor" type="NCMS000">rui|se|ñor|</w>
        </l>
      </lg>
