  <text>
    <body>
      <head>
        <title>  Mañanicas floridas</title>
      </head>
      <lg cert="..." type="...">
        <l met="-|-|3|-|-|6|-|" n="1">
          <!--Mañanicas floridas-->
          <w lemma="mañanicas" type="NP00000">ma|ña|ni|cas|</w>
          <w lemma="florido" type="AQ0FP00">flo|ri|das|</w>
        </l>
        <l met="-|2|-|4|-|" n="2">
          <!--del frío invierno,-->
          <w lemma="del" type="SP">del|</w>
          <w lemma="frío" type="NCMS000">frío|</w>
          <w lemma="invierno" type="NCMS000">in|vier|no|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="3">
          <!--recordad a mi niño-->
          <w lemma="recordar" type="VMM02P0">re|cor|dad|</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="niño" type="NCMS000">ni|ño|</w>
        </l>
        <l met="-|2|-|4|-|" n="4">
          <!--que duerme al hielo.-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="dormir" type="VMIP3S0">duer|me</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="hielo" type="NCMS000">hie|lo|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="5">
          <!--Mañanas dichosas-->
          <w lemma="mañana" type="NCCP000">ma|ña|nas|</w>
          <w lemma="dichoso" type="AQ0FP00">di|cho|sas|</w>
        </l>
        <l met="-|2|-|-|-|" n="6">
          <!--del frío diciembre,-->
          <w lemma="del" type="SP">del|</w>
          <w lemma="frío" type="AQ0MS00">frí|o|</w>
          <w lemma="[??:??/12/??:??.??:??]" type="W">di|ciem|bre|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="7">
          <!--aunque el cielo os siembre-->
          <w lemma="aunque" type="CC">aun|que</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cielo" type="NCMS000">cie|lo</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="sembrar" type="VMSP3S0">siem|bre|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="8">
          <!--de flores y rosas,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="flor" type="NCFP000">flo|res|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="rosa" type="NCCP000">ro|sas|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="9">
          <!--pues sois rigurosas-->
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="ser" type="VSIP2P0">sois|</w>
          <w lemma="riguroso" type="AQ0FP00">ri|gu|ro|sas|</w>
        </l>
        <l met="-|2|3|4|-|" n="10">
          <!--y Dios es tierno,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="dios" type="NP00000">dios|</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="tierno" type="AQ0MS00">tier|no|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="11">
          <!--recordad a mi niño-->
          <w lemma="recordar" type="VMM02P0">re|cor|dad|</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="mi" type="DP1CSS">mi|</w>
          <w lemma="niño" type="NCMS000">ni|ño|</w>
        </l>
        <l met="-|2|-|4|-|" n="12">
          <!--que duerme al hielo.-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="dormir" type="VMIP3S0">duer|me</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="hielo" type="NCMS000">hie|lo|</w>
        </l>
      </lg>
    </body>
