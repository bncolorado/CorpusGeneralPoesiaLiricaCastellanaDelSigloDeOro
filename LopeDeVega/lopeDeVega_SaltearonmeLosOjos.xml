  <text>
    <body>
      <head>
        <title> Salteáronme los ojos</title>
      </head>
      <lg cert="..." type="...">
        <l met="-|2|-|-|-|6|-|" n="1">
          <!--Salteáronme los ojos-->
          <w lemma="salteáronme" type="NP00000">sal|teá|ron|me|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="ojo" type="NCMP000">o|jos|</w>
        </l>
        <l met="-|-|-|4|-|" n="2">
          <!--de la mozuela;-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mozuelo" type="NCFS000">mo|zue|la|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="3">
          <!--diles más que pedían,-->
          <w lemma="decir" type="VMM02S0">di</w>
          <w lemma="le" type="PP3CPD0">les</w>
          <w lemma="más" type="RG">más</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="pedir" type="VMII3P0">pe|dí|an|</w>
        </l>
        <l met="-|2|-|4|-|" n="4">
          <!--¿de qué se quejan?-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="qué" type="PT00000">qué|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="quejar" type="VMIP3P0">que|jan|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="5">
          <!--Érase la niña-->
          <w lemma="erar" type="VMM02S0">e|ra|</w>
          <w lemma="se" type="PP3CN00">se|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="niño" type="NCFS000">ni|ña|</w>
        </l>
        <l met="1|-|-|-|5|-|" n="6">
          <!--libre de las penas-->
          <w lemma="libre" type="AQ0CS00">li|bre|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="pena" type="NCFP000">pe|nas|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="7">
          <!--que el amor me causa-->
          <w lemma="que" type="CS">que</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="causar" type="VMIP3S0">cau|sa|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="8">
          <!--porque vine a verla.-->
          <w lemma="porque" type="CS">por|que|</w>
          <w lemma="venir" type="VMIS1S0">vi|ne</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="ver" type="VMN0000">ver|</w>
          <w lemma="lo" type="PP3FSA0">la|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="9">
          <!--Era yo arrogante,-->
          <w lemma="ser" type="VSII3S0">e|ra|</w>
          <w lemma="yo" type="PP1CSN0">yo</w>
          <w lemma="arrogante" type="AQ0CS00">a|rro|gan|te|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="10">
          <!--burlé de sus flechas,-->
          <w lemma="burlar" type="VMIS1S0">bu|rlé|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="flecha" type="NCFP000">fle|chas|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="11">
          <!--pero destas burlas-->
          <w lemma="pero" type="CC">pe|ro|</w>
          <w lemma="destas" type="NCMN000">des|tas|</w>
          <w lemma="burla" type="NCFP000">bu|rlas|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="12">
          <!--vine a tantas veras.-->
          <w lemma="venir" type="VMIS1S0">vi|ne</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="tanto" type="DI0FP0">tan|tas|</w>
          <w lemma="vera" type="NCFP000">ve|ras|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="13">
          <!--Vi los bellos ojos-->
          <w lemma="ver" type="VMIS1S0">vi|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="bello" type="AQ0MP00">be|llos|</w>
          <w lemma="ojo" type="NCMP000">o|jos|</w>
        </l>
        <l met="-|-|-|4|-|" n="14">
          <!--de la mozuela;-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mozuelo" type="NCFS000">mo|zue|la|</w>
        </l>
        <l met="1|-|3|-|-|6|-|" n="15">
          <!--diles más que pedían,-->
          <w lemma="decir" type="VMM02S0">di</w>
          <w lemma="le" type="PP3CPD0">les</w>
          <w lemma="más" type="RG">más</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="pedir" type="VMII3P0">pe|dí|an|</w>
        </l>
        <l met="-|2|-|4|-|" n="16">
          <!--¿de qué se quejan?-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="qué" type="PT00000">qué|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="quejar" type="VMIP3P0">que|jan|</w>
        </l>
      </lg>
