      <head>
        <title> Que no quiero bonetes,</title>
      </head>
      <lg cert="..." type="...">
        <l met="-|2|3|-|-|6|-|" n="1">
          <!--Que no quiero bonetes,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="querer" type="VMIP1S0">quie|ro|</w>
          <w lemma="bonete" type="NCMP000">bo|ne|tes|</w>
        </l>
        <l met="-|2|3|4|-|" n="2">
          <!--que soy muy boba,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="ser" type="VSIP1S0">soy|</w>
          <w lemma="muy" type="RG">muy|</w>
          <w lemma="bobo" type="AQ0FS00">bo|ba|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="3">
          <!--y en andando con picos-->
          <w lemma="y" type="CC">y</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="andar" type="VMG0000">an|dan|do|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="pico" type="NCMP000">pi|cos|</w>
        </l>
        <l met="-|2|-|4|-|" n="4">
          <!--me pico toda.-->
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="picar" type="VMIP1S0">pi|co|</w>
          <w lemma="todo" type="DI0FS0">to|da|</w>
        </l>
      </lg>
