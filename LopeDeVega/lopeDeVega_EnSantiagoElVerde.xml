          <w lemma="el" type="DA0MS0">el</w>
          <w lemma="día" type="NCMS000">dí|a|</w>
        </l>
        <l met="-|2|-|4|-|" n="4">
          <!--vengarme pienso.-->
          <w lemma="vengar" type="VMN0000">ven|gar|</w>
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="pensar" type="VMIP1S0">pien|so|</w>
