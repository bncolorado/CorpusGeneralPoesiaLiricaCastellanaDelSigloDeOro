    <body>
      <head>
        <title> Barcos enramados</title>
      </head>
      <lg cert="high" type="Seguidilla">
        <l met="1|-|-|-|5|-|" n="1">
          <!--Barcos enramados-->
          <w lemma="barcos" type="NP00000">bar|cos|</w>
          <w lemma="enramar" type="VMP00PM">e|nra|ma|dos|</w>
        </l>
        <l met="1|-|3|-|" n="2">
          <!--van a Triana,-->
          <w lemma="ir" type="VMIP3P0">van|</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="triana" type="NP00000">tria|na|</w>
        </l>
        <l met="-|-|3|-|-|6|-|" n="3">
          <!--el primero de todos-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="1" type="AO0MS00">pri|me|ro|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="todo" type="DI0MP0">to|dos|</w>
        </l>
        <l met="-|2|-|4|-|" n="4">
          <!--me lleva el alma.-->
          <w lemma="me" type="PP1CS00">me|</w>
          <w lemma="llevar" type="VMIP3S0">lle|va</w>
          <w lemma="el" type="DA0MS0">e|l</w>
          <w lemma="alma" type="NCFS000">al|ma|</w>
        </l>
      </lg>
