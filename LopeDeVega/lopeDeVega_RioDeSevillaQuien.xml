    <body>
      <head>
        <title>Río de Sevilla,</title>
      </head>
      <lg cert="high" type="Seguidillas">
        <l met="1|-|-|-|-|" n="1">
          <!--Río de Sevilla,-->
          <w lemma="río_de_sevilla" type="NP00000">rí|o_|de_|se|vi|lla|</w>
        </l>
        <l met="1|-|-|4|-|" n="2">
          <!--¡quién te pasase-->
          <w lemma="quién" type="PT0CS00">quién|</w>
          <w lemma="te" type="PP2CS00">te|</w>
          <w lemma="pasar" type="VMSI3S0">pa|sa|se|</w>
        </l>
        <l met="-|-|-|-|-|6|-|" n="3">
