          <w lemma="como" type="CS">co|mo</w>
          <w lemma="estar" type="VMIP1S0">es|toy</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="orilla" type="NCFS000">o|ri|lla|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="10">
