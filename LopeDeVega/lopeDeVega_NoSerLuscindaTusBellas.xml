      <lg cert="..." type="...">
        <l met="1|2|-|4|-|-|7|-|" n="1">
          <!--No ser, Luscinda, tus bellas-->
          <w lemma="no" type="RN">no|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
