          <w lemma="sin" type="SP">sin|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="blanco" type="AQ0FS00">blan|ca|</w>
          <w lemma="niño" type="NCFS000">ni|ña|</w>
        </l>
        <l met="-|-|3|-|" n="4">
