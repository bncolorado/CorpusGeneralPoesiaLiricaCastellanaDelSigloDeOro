          <w lemma="en" type="SP">en|</w>
          <w lemma="casa" type="NCFS000">ca|sa|</w>
          <w lemma="desierto" type="AQ0FS00">de|sier|ta|</w>
        </l>
        <l met="-|2|-|-|5|-|" n="10">
          <!--del bien que tenía?-->
          <w lemma="del" type="SP">del|</w>
          <w lemma="bien" type="NCMS000">bien|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="tener" type="VMII3S0">te|ní|a|</w>
        </l>
        <l met="1|-|" n="11">
          <!--CORO-->
          <w lemma="coro" type="NP00000">co|ro|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="12">
