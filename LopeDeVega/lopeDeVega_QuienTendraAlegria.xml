      <lg cert="..." type="...">
        <l met="1|-|" n="1">
          <!--CORO-->
          <w lemma="coro" type="NP00000">co|ro|</w>
        </l>
        <l met="1|-|3|-|5|-|" n="2">
          <!--¿Quién tendrá alegría-->
          <w lemma="quién" type="PT0CS00">quién|</w>
          <w lemma="tener" type="VMIF3S0">ten|drá</w>
          <w lemma="alegría" type="NCFS000">a|le|grí|a|</w>
        </l>
        <l met="-|-|3|-|5|-|" n="3">
