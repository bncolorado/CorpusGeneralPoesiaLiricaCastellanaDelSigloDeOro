          <w lemma="aquel" type="DD0MS0">a|quel|</w>
          <w lemma="fruto" type="NCMS000">fru|to|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="2" type="AO0MS00">se|gun|do|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="5">
