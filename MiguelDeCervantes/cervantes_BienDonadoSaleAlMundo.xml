          <w lemma="y" type="CC">y</w>
          <w lemma="enriquecer" type="VMP00SM">en|ri|que|ci|do|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="lengua" type="NCFS000">len|gua|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="35">
