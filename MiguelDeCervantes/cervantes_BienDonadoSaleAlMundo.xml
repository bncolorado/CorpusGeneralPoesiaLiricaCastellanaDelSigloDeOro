          <w lemma="ir" type="VMIP3S0">va|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="rico" type="AQ0MS00">ri|co</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="donar" type="VMP00SM">do|na|do|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="8">
