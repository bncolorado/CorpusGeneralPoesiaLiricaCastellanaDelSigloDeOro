          <w lemma="sentimiento" type="NCMS000">sen|ti|mien|to</w>
          <w lemma="amoroso" type="AQ0MS00">a|mo|ro|so|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="12">
          <!--del pecho más encendido-->
          <w lemma="de" type="SP">de</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="pecho" type="NCMS000">pe|cho|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="encender" type="VMP00SM">en|cen|di|do|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="13">
