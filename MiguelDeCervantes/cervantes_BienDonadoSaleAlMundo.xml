          <w lemma="flor" type="NCFP000">flo|res|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="pradería" type="NCFP000">pra|de|rí|as|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="22">
