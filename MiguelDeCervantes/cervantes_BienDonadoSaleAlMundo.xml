          <w lemma="de" type="SP">de|</w>
          <w lemma="ciencia" type="NCFS000">scien|cia</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="discreción" type="NCFS000">dis|cre|ción|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="9">
