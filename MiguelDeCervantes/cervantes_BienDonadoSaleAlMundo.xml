    <body>
      <head>
        <title>Bien donado sale al mundo</title>
      </head>
      <lg cert="..." type="...">
        <l met="1|-|3|-|5|-|7|-|" n="1">
          <!--Bien donado sale al mundo-->
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="donar" type="VMP00SM">do|na|do|</w>
          <w lemma="salir" type="VMIP3S0">sa|le</w>
          <w lemma="a" type="SP">a</w>
	  <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="mundo" type="NCMS000">mun|do|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="2">
