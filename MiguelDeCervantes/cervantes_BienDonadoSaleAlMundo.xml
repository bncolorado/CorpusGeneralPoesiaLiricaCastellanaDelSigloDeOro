          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FP0">las|</w>
          <w lemma="empresa" type="NCFP000">em|pre|sas|</w>
          <w lemma="de" type="SP">de</w>
          <w lemma="amor" type="NCMS000">a|mor|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="19">
          <!--aquí muestra su valor,-->
          <w lemma="aquí" type="RG">a|quí|</w>
          <w lemma="mostrar" type="VMIP3S0">mues|tra|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="valor" type="NCMS000">va|lor|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="20">
          <!--su buena o su mala andanza.-->
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="bueno" type="AQ0FS00">bue|na</w>
          <w lemma="o" type="CC">o|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="malo" type="AQ0FS00">ma|la</w>
          <w lemma="andanza" type="NCFS000">an|dan|za|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="21">
