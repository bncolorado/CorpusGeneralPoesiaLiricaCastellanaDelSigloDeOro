  <text>
    <body>
      <head>
        <title>Tal secretario formays,</title>
      </head>
      <lg cert="high" type="Octavillas">
        <l met="1|-|-|4|-|-|7|-|" n="1">
          <!--Tal secretario formays,-->
          <w lemma="tal" type="DD0CS0">tal|</w>
          <w lemma="secretario" type="NCMS000">se|cre|ta|rio|</w>
          <w lemma="formays" type="AQ0CP00">for|mays|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="2">
          <!--Gabriel, en vuestros escritos,-->
          <w lemma="gabriel" type="NP00000">ga|briel|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="vuestro" type="DP2MPP">vues|tros|</w>
          <w lemma="escrito" type="NCMP000">es|cri|tos|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="3">
          <!--que por siglos infinitos-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="siglo" type="NCMP000">si|glos|</w>
          <w lemma="infinito" type="AQ0MP00">in|fi|ni|tos|</w>
        </l>
        <l met="-|-|-|-|-|-|7|-|" n="4">
          <!--en el os eternizays.-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="o" type="NCFP000">os|</w>
          <w lemma="eternizays" type="AQ0CP00">e|ter|ni|zays|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="5">
          <!--De la ignorancia sacays-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="ignorancia" type="NCFS000">ig|no|ran|cia|</w>
          <w lemma="sacays" type="AQ0CP00">sa|cays|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="6">
          <!--la pluma, y, en presto buelo,-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="pluma" type="NCFS000">plu|ma</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="presto" type="AQ0MS00">pres|to|</w>
          <w lemma="buelo" type="NCMS000">bue|lo|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="7">
          <!--de lo mas baxo del suelo,-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="baxo" type="NCMS000">ba|xo|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="suelo" type="NCMS000">sue|lo|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="8">
          <!--al cielo la leuantays.-->
          <w lemma="al" type="SP">al|</w>
          <w lemma="cielo" type="NCMS000">cie|lo|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="leuantays" type="NCMP000">leuan|tays|</w>
        </l>
        <l met="-|2|-|-|-|6|-|" n="9">
          <!--Desde oy mas, la discrecion-->
          <w lemma="desde" type="SP">des|de</w>
          <w lemma="oy" type="RG">oy|</w>
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="discrecion" type="NCFS000">dis|cre|cion|</w>
        </l>
        <l met="-|-|3|4|-|-|-|8|-|" n="10">
          <!--quedará puesta en su punto,-->
          <w lemma="quedar" type="VMIF3S0">que|da|rá|</w>
          <w lemma="poner" type="VMP00SF">pues|ta|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="punto" type="NCMS000">pun|to|</w>
        </l>
        <l met="-|-|3|-|5|6|-|" n="11">
          <!--y el hablar y escriuir junto,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="hablar" type="VMN0000">ha|blar|</w>
          <w lemma="y" type="CC">y</w>
          <w lemma="escriuir" type="VMN0000">es|criuir|</w>
          <w lemma="junto" type="AQ0MS00">jun|to|</w>
        </l>
        <l met="-|-|-|4|-|6|-|" n="12">
          <!--en su mayor perfecion,-->
          <w lemma="en" type="SP">en|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="mayor" type="AQ0CS00">ma|yor|</w>
          <w lemma="perfecion" type="NCFS000">per|fe|cion|</w>
        </l>
        <l met="-|2|-|4|-|6|-|" n="13">
          <!--que, en esta nueua ocasion-->
          <w lemma="que" type="CS">que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="este" type="DD0FS0">es|ta|</w>
          <w lemma="nueua" type="AQ0FS00">nueua|</w>
          <w lemma="ocasion" type="NCFS000">o|ca|sion|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="14">
          <!--nos muestre, en breue distancia,-->
          <w lemma="nos" type="PP1CP00">nos|</w>
          <w lemma="mostrar" type="VMSP3S0">mues|tre|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="breue" type="NCMS000">breue|</w>
          <w lemma="distancia" type="NCFS000">dis|tan|cia|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="15">
          <!--Demostenes su elegancia-->
          <w lemma="demostenes" type="NP00000">de|mos|te|nes|</w>
          <w lemma="su" type="DP3CSN">su</w>
          <w lemma="elegancia" type="NCFS000">e|le|gan|cia|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="16">
          <!--y su estilo Ciceron.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="estilo" type="NCMS000">es|ti|lo|</w>
          <w lemma="ciceron" type="NP00000">ci|ce|ron|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="17">
          <!--España os está obligada,-->
          <w lemma="españa" type="NP00000">es|pa|ña</w>
          <w lemma="os" type="PP2CP00">os|</w>
          <w lemma="estar" type="VMIP3S0">es|tá</w>
          <w lemma="obligar" type="VMP00SF">o|bli|ga|da|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="18">
          <!--y con ella el mundo todo,-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="él" type="PP3FS00">e|lla</w>
          <w lemma="el" type="DA0MS0">e|l</w>
          <w lemma="mundo" type="NCMS000">mun|do|</w>
          <w lemma="todo" type="PI0MS00">to|do|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="19">
          <!--por la subtileza y modo-->
          <w lemma="por" type="SP">por|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="subtileza" type="NCFS000">sub|ti|le|za</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="modo" type="NCMS000">mo|do|</w>
        </l>
        <l met="-|2|-|4|5|-|7|-|" n="20">
          <!--de pluma tan bien cortada.-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="pluma" type="NCFS000">plu|ma|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="bien" type="RG">bien|</w>
          <w lemma="cortar" type="VMP00SF">cor|ta|da|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="21">
          <!--La adulación, defraudada-->
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="adulación" type="NCFS000">a|du|la|ción|</w>
          <w lemma="defraudar" type="VMP00SF">de|frau|da|da|</w>
        </l>
        <l met="1|-|-|-|5|-|7|-|" n="22">
          <!--queda, y la lisonja en ella;-->
          <w lemma="quedar" type="VMIP3S0">que|da</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="lisonja" type="NCFS000">li|son|ja</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="él" type="PP3FS00">e|lla|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="23">
          <!--la mentira se atropella,-->
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mentira" type="NCFS000">men|ti|ra|</w>
          <w lemma="se" type="P00CN00">se</w>
          <w lemma="atropellar" type="VMIP3S0">a|tro|pe|lla|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="24">
          <!--y es la verdad leuantada.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="verdad" type="NCFS000">ver|dad|</w>
          <w lemma="leuantada" type="VMP00SF">leuan|ta|da|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="25">
          <!--Vuestro libro nos informa-->
          <w lemma="vuestro" type="DP2MSP">vues|tro|</w>
          <w lemma="libro" type="NCMS000">li|bro|</w>
          <w lemma="nos" type="PP1CP00">nos|</w>
          <w lemma="informar" type="VMIP3S0">in|for|ma|</w>
        </l>
        <l met="-|2|-|4|5|6|-|" n="26">
          <!--que solo vos aueis dado-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="solo" type="RG">so|lo|</w>
          <w lemma="vos" type="PP2CS0P">vos|</w>
          <w lemma="aueis" type="VMIP2P0">aueis|</w>
          <w lemma="dar" type="VMP00SM">da|do|</w>
        </l>
        <l met="-|-|-|4|-|-|-|8|-|" n="27">
          <!--a la materia de Estado-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="materia" type="NCFS000">ma|te|ria|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="estado" type="NP00000">es|ta|do|</w>
        </l>
        <l met="-|2|-|-|-|6|-|8|-|" n="28">
          <!--hermosa y christiana forma.-->
          <w lemma="hermoso" type="AQ0FS00">her|mo|sa</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="christiana" type="AQ0FS00">c|hris|tia|na|</w>
          <w lemma="forma" type="NCFS000">for|ma|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="29">
          <!--Con la razon se conforma-->
          <w lemma="con" type="SP">con|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="razon" type="NCMS000">ra|zon|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="conformar" type="VMIP3S0">con|for|ma|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="30">
          <!--de tal suerte, que en el veo-->
          <w lemma="de" type="SP">de|</w>
          <w lemma="tal" type="DD0CS0">tal|</w>
          <w lemma="suerte" type="NCFS000">suer|te|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="ver" type="VMIP1S0">ve|o|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="31">
          <!--que, contentando al desseo,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="contentar" type="VMG0000">con|ten|tan|do</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="desseo" type="NCCS000">des|se|o|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="32">
          <!--al que es mas libre reforma.-->
          <w lemma="al" type="SP">al|</w>
          <w lemma="que" type="PR0CN00">que</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="mas" type="CC">mas|</w>
          <w lemma="libre" type="AQ0CS00">li|bre|</w>
          <w lemma="reforma" type="NCFS000">re|for|ma|</w>
        </l>
      </lg>
    </body>
