          <w lemma="ser" type="VSIF3S0">se|rá</w>
          <w lemma="aquésta" type="AQ0CS00">a|qués|ta|</w>
          <w lemma="si" type="CS">si|</w>
          <w lemma="delante" type="RG">de|lan|te|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="40">
          <!--del mismo Dios resplandece?-->
          <w lemma="del" type="SP">del|</w>
          <w lemma="mismo" type="AQ0MS00">mis|mo|</w>
          <w lemma="dios" type="NP00000">dios|</w>
          <w lemma="resplandecer" type="VMIP3S0">res|plan|de|ce|</w>
