          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="restituir" type="VMIS3S0">res|ti|tu|yó|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="él" type="PP3MS00">él|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="26">
          <!--Crezca gloriosa la mina-->
          <w lemma="crecer" type="VMSP3S0">crez|ca|</w>
          <w lemma="glorioso" type="AQ0FS00">glo|rio|sa|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="mina" type="NCFS000">mi|na|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="27">
          <!--que de su luz jacintina-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="luz" type="NCFS000">luz|</w>
          <w lemma="jacintino" type="AQ0FS00">ja|cin|ti|na|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="28">
          <!--tiene el cielo y tierra llenos,-->
          <w lemma="tener" type="VMIP3S0">tie|ne</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cielo" type="NCMS000">cie|lo</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="tierra" type="NCFS000">tie|rra|</w>
          <w lemma="lleno" type="AQ0MP00">lle|nos|</w>
        </l>
        <l met="-|2|-|-|5|6|7|-|" n="29">
          <!--pues no mereció estar menos-->
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="merecer" type="VMIS3S0">me|re|ció</w>
          <w lemma="estar" type="VMN0000">es|tar|</w>
          <w lemma="menos" type="RG">me|nos|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="30">
