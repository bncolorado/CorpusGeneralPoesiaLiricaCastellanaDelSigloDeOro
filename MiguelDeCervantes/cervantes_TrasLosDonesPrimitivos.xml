          <w lemma="vivo" type="AQ0FS00">vi|va</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="rutilante" type="AQ0CS00">ru|ti|lan|te|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="39">
