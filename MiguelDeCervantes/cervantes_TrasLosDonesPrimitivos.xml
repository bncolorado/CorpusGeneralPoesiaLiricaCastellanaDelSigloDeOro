          <w lemma="vivir" type="VMG0000">vi|vien|do|</w>
          <w lemma="jacinto" type="NP00000">ja|cin|to|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="él" type="PP3MS00">él|</w>
        </l>
        <l met="-|-|-|4|-|6|-|8|-|" n="23">
