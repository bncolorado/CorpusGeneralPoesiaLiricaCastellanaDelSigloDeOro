          <w lemma="porque" type="CS">por|que</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="hombre" type="NCMS000">hom|bre|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="gracia" type="NCFS000">gra|cia|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="12">
          <!--tantas veces se retira,-->
          <w lemma="tanto" type="DI0FP0">tan|tas|</w>
          <w lemma="vez" type="NCFP000">ve|ces|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="retirar" type="VMIP3S0">re|ti|ra|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="13">
          <!--y el Jacinto, al que le mira,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="jacinto" type="NP00000">ja|cin|to</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="mirar" type="VMIP3S0">mi|ra|</w>
        </l>
        <l met="1|2|3|-|-|-|7|-|" n="14">
          <!--es tan grande su eficacia-->
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="grande" type="AQ0CS00">gran|de|</w>
          <w lemma="su" type="DP3CSN">su</w>
          <w lemma="eficacia" type="NCFS000">e|fi|ca|cia|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="15">
          <!--que le sosiega la ira,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="le" type="PP3CSD0">le|</w>
          <w lemma="sosegar" type="VMIP3S0">so|sie|ga|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="ira" type="NCFS000">i|ra|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="16">
