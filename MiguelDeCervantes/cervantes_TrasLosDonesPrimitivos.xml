          <w lemma="otro" type="DI0FS0">o|tra|</w>
          <w lemma="luz" type="NCFS000">luz|</w>
          <w lemma="desfallecer" type="VMIP3S0">des|fa|lle|ce|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="37">
