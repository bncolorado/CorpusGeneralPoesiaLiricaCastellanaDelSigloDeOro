          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="mismo" type="AQ0FS00">mis|ma|</w>
          <w lemma="piedad" type="NCFS000">pie|dad|</w>
          <w lemma="lo" type="PP3MSA0">lo</w>
          <w lemma="inclinar" type="VMIP3S0">in|cli|na|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="17">
          <!--a darlo por medicina,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="dar" type="VMN0000">dar|</w>
          <w lemma="lo" type="PP3MSA0">lo|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="medicina" type="NCFS000">me|di|ci|na|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="18">
          <!--que, en su jüicio profundo,-->
          <w lemma="que" type="CS">que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="jüicio" type="NCMS000">jü|i|cio|</w>
          <w lemma="profundo" type="AQ0MS00">pro|fun|do|</w>
        </l>
        <l met="1|2|-|-|5|-|7|-|" n="19">
          <!--ve que ha menester el mundo,-->
          <w lemma="ver" type="VMIP3S0">ve|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="haber" type="VMIP3S0">ha|</w>
          <w lemma="menester" type="NCMS000">me|nes|ter|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="mundo" type="NCMS000">mun|do|</w>
        </l>
        <l met="1|2|-|4|-|6|7|-|" n="20">
          <!--hoy una piedra tan fina.-->
          <w lemma="hoy" type="RG">hoy|</w>
          <w lemma="uno" type="DI0FS0">u|na|</w>
          <w lemma="piedra" type="NCFS000">pie|dra|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="fino" type="AQ0FS00">fi|na|</w>
        </l>
        <l met="-|2|3|4|-|-|7|-|" n="21">
          <!--Obró tanto esta virtud,-->
          <w lemma="obrar" type="VMIS3S0">o|bró|</w>
          <w lemma="tanto" type="RG">tan|to</w>
          <w lemma="este" type="DD0FS0">es|ta|</w>
          <w lemma="virtud" type="NCFS000">vir|tud|</w>
        </l>
        <l met="-|2|-|-|5|-|-|8|-|" n="22">
