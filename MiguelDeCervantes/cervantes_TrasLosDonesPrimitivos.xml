    <body>
      <head>
        <title> Tras los dones primitivos</title>
      </head>
      <lg cert="high" type="Glosa">
        <l met="-|-|3|-|-|-|7|-|" n="1">
          <!--Tras los dones primitivos-->
          <w lemma="tras" type="SP">tras|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="don" type="NCMP000">do|nes|</w>
          <w lemma="primitivo" type="AQ0MP00">pri|mi|ti|vos|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="2">
          <!--que, en el fervor de su celo,-->
          <w lemma="que" type="CS">que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="fervor" type="NCMS000">fer|vor|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="su" type="DP3CSN">su|</w>
          <w lemma="celo" type="NCMS000">ce|lo|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="3">
          <!--ofreció la iglesia al cielo,-->
          <w lemma="ofrecer" type="VMIS3S0">o|fre|ció|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="iglesia" type="NCFS000">i|gle|sia</w>
          <w lemma="al" type="SP">al|</w>
          <w lemma="cielo" type="NCMS000">cie|lo|</w>
        </l>
        <l met="-|-|-|-|5|-|7|-|" n="4">
          <!--a sus edificios vivos-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="su" type="DP3CPN">sus|</w>
          <w lemma="edificio" type="NCMP000">e|di|fi|cios|</w>
          <w lemma="vivo" type="AQ0MP00">vi|vos|</w>
        </l>
        <l met="1|2|-|4|-|-|7|-|" n="5">
          <!--dio nuevas piedras el suelo;-->
          <w lemma="dar" type="VMIS3S0">dio|</w>
          <w lemma="nuevo" type="AQ0FP00">nue|vas|</w>
          <w lemma="piedra" type="NCFP000">pie|dras|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="suelo" type="NCMS000">sue|lo|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="6">
          <!--estos dones agradece-->
          <w lemma="este" type="DD0MP0">es|tos|</w>
          <w lemma="don" type="NCMP000">do|nes|</w>
          <w lemma="agradecer" type="VMIP3S0">a|gra|de|ce|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="7">
          <!--a su esposa y la ennoblece,-->
          <w lemma="a" type="SP">a|</w>
          <w lemma="su" type="DP3CSN">su</w>
          <w lemma="esposo" type="NCFS000">es|po|sa</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="lo" type="PP3FSA0">la</w>
          <w lemma="ennoblecer" type="VMIP3S0">en|no|ble|ce|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="8">
          <!--pues, de parte del esposo,-->
          <w lemma="pues" type="CS">pues|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="parte" type="NCCS000">par|te|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="esposo" type="NCMS000">es|po|so|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="9">
          <!--un Hiacinto, el más precioso,-->
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="hiacinto" type="NP00000">hia|cin|to</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="precioso" type="AQ0MS00">pre|cio|so|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="10">
          <!--el cielo a la iglesia ofrece.-->
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cielo" type="NCMS000">cie|lo</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="el" type="DA0FS0">la</w>
          <w lemma="iglesia" type="NCFS000">i|gle|sia</w>
          <w lemma="ofrecer" type="VMIP3S0">o|fre|ce|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="11">
