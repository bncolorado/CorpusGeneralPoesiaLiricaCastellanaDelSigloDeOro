          <w lemma="lucir" type="VMIP3S0">lu|ce</w>
          <w lemma="ante" type="SP">an|te|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="ojo" type="NCMP000">o|jos|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="32">
