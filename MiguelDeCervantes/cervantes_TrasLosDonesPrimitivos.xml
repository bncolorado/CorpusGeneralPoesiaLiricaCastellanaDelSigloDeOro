          <w lemma="que" type="CS">que</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="corona" type="NCFS000">co|ro|na|</w>
          <w lemma="divino" type="AQ0FS00">di|vi|na|</w>
        </l>
        <l met="-|2|3|-|-|-|7|-|" n="31">
