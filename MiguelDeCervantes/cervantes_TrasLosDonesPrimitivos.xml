          <w lemma="acá" type="RG">a|cá</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="glorioso" type="AQ0FS00">glo|rio|sa|</w>
          <w lemma="memoria" type="NCFS000">me|mo|ria|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="34">
