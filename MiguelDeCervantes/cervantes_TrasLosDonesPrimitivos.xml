          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="triunfo" type="NCMP000">triun|fos|</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="despojo" type="NCMP000">des|po|jos|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="35">
          <!--que sacó de la vitoria,-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="sacar" type="VMIS3S0">sa|có|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="vitoria" type="AQ0FS00">vi|to|ria|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="36">
