  <text>
    <body>
      <head>
        <title> Para darnos a entender</title>
      </head>
      <lg cert="low" type="Coplas">
        <l met="-|-|3|-|-|-|7|-|" n="1">
          <!--Para darnos a entender-->
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="dar" type="VMN0000">dar|</w>
          <w lemma="nos" type="PP1CP00">nos|</w>
          <w lemma="a" type="SP">a</w>
          <w lemma="entender" type="VMN0000">en|ten|der|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="2">
          <!--que no venís a holgar-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="venir" type="VMIP2P0">ve|nís|</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="holgar" type="VMN0000">hol|gar|</w>
        </l>
        <l met="-|2|3|-|-|6|-|" n="3">
          <!--queréis luego començar,-->
          <w lemma="querer" type="VMIP2P0">que|réis|</w>
          <w lemma="luego" type="RG">lue|go|</w>
          <w lemma="comenzar" type="VMN0000">co|men|çar|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="4">
          <!--rey de gloria, a padecer;-->
          <w lemma="rey" type="NCMS000">rey|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="gloria" type="NCFS000">glo|ria</w>
          <w lemma="a" type="SP">a|</w>
          <w lemma="padecer" type="VMN0000">pa|de|cer|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="5">
          <!--y ponéis en amargura-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="poner" type="VMIP2P0">po|néis|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="amargura" type="NCFS000">a|mar|gu|ra|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="6">
          <!--vuestra carne tierna y pura-->
          <w lemma="vuestro" type="DP2FSP">vues|tra|</w>
          <w lemma="carne" type="NCFS000">car|ne|</w>
          <w lemma="tierno" type="AQ0FS00">tier|na</w>
          <w lemma="y" type="CC">y|</w>
          <w lemma="puro" type="AQ0FS00">pu|ra|</w>
        </l>
        <l met="-|-|-|4|-|-|7|-|" n="7">
          <!--para mostrarnos, Señor,-->
          <w lemma="para" type="SP">pa|ra|</w>
          <w lemma="mostrar" type="VMN0000">mos|trar|</w>
          <w lemma="nos" type="PP1CP00">nos|</w>
          <w lemma="señor" type="NP00000">se|ñor|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="8">
          <!--lo que, siendo criador,-->
          <w lemma="el" type="DA00S0">lo|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="ser" type="VSG0000">sien|do|</w>
          <w lemma="criador" type="AQ0MS00">cri|a|dor|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="9">
          <!--sufrís por la criatura.-->
          <w lemma="sufrir" type="VMIP2P0">su|frís|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="el" type="DA0FS0">la|</w>
          <w lemma="criatura" type="NCFS000">cri|a|tu|ra|</w>
        </l>
      </lg>
    </body>
  </text>
