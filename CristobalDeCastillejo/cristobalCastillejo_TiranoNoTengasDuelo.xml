  <text>
    <body>
      <head>
        <title>Tirano, no tengas duelo;</title>
      </head>
      <lg cert="low" type="Coplas">
        <l met="-|2|-|4|5|-|7|-|" n="1">
          <!--Tirano, no tengas duelo;-->
          <w lemma="tirano" type="NCMS000">ti|ra|no|</w>
          <w lemma="no" type="RN">no|</w>
          <w lemma="tener" type="VMSP2S0">ten|gas|</w>
          <w lemma="duelo" type="NCMS000">due|lo|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="2">
          <!--que estos que matas temprano-->
          <w lemma="que" type="CS">que</w>
          <w lemma="este" type="DD0MP0">es|tos|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="matar" type="VMIP2S0">ma|tas|</w>
          <w lemma="temprano" type="RG">tem|pra|no|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="3">
          <!--plantas son que de tu mano-->
          <w lemma="planta" type="NCFP000">plan|tas|</w>
          <w lemma="ser" type="VSIP3P0">son|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="tu" type="DP2CSS">tu|</w>
          <w lemma="mano" type="NCFS000">ma|no|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="4">
          <!--se trasponen en el cielo.-->
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="trasponer" type="VMIP3P0">tras|po|nen|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="cielo" type="NCMS000">cie|lo|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="5">
          <!--Y el que buscas sin reposo,-->
          <w lemma="y" type="CC">y</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="que" type="PR0CN00">que|</w>
          <w lemma="buscar" type="VMIP2S0">bus|cas|</w>
          <w lemma="sin" type="SP">sin|</w>
          <w lemma="reposo" type="NCMS000">re|po|so|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="6">
          <!--sabe que es tan poderoso,-->
          <w lemma="saber" type="VMIP3S0">sa|be|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="poderoso" type="AQ0MS00">po|de|ro|so|</w>
        </l>
        <l met="1|-|-|4|-|-|7|-|" n="7">
          <!--que éstos, muriendo por él,-->
          <w lemma="que" type="CS">que</w>
          <w lemma="este" type="PD0MP00">és|tos|</w>
          <w lemma="morir" type="VMG0000">mu|rien|do|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="él" type="PP3MS00">él|</w>
        </l>
        <l met="1|-|-|4|5|-|7|-|" n="8">
          <!--ganan en ser tú cruel-->
          <w lemma="ganar" type="VMIP3P0">ga|nan|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="tú" type="PP2CSN0">tú|</w>
          <w lemma="cruel" type="AQ0CS00">cru|el|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="9">
          <!--más que siendo piadoso.-->
          <w lemma="más" type="RG">más|</w>
          <w lemma="que" type="CS">que|</w>
          <w lemma="ser" type="VSG0000">sien|do|</w>
          <w lemma="piadoso" type="AQ0MS00">pi|a|do|so|</w>
        </l>
      </lg>
    </body>
  </text>
