  <text>
    <body>
      <head>
        <title>¡Qué vida de tantos males,</title>
      </head>
      <lg cert="high" type="Canción - Quintillas">
        <l met="1|2|-|-|5|-|7|-|" n="1">
          <!--¡Qué vida de tantos males,-->
          <w lemma="qué" type="DE0CN0">qué|</w>
          <w lemma="vida" type="NCFS000">vi|da|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="tanto" type="DI0MP0">tan|tos|</w>
          <w lemma="mal" type="NCMP000">ma|les|</w>
        </l>
        <l met="1|2|-|4|-|-|7|-|" n="2">
          <!--qué mundo tan desigual,-->
          <w lemma="qué" type="DE0CN0">qué|</w>
          <w lemma="mundo" type="NCMS000">mun|do|</w>
          <w lemma="tan" type="RG">tan|</w>
          <w lemma="desigual" type="AQ0CS00">de|si|gual|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="3">
          <!--do los bienes con el mal-->
          <w lemma="do" type="NCMS000">do|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="bien" type="NCMP000">bie|nes|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="mal" type="NCMS000">mal|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="4">
          <!--nunca pueden ser iguales-->
          <w lemma="nunca" type="RG">nun|ca|</w>
          <w lemma="poder" type="VMIP3P0">pue|den|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="igual" type="AQ0CP00">i|gua|les|</w>
        </l>
        <l met="-|-|3|-|5|-|7|-|" n="5">
          <!--aunque sean de un igual!-->
          <w lemma="aunque" type="CC">aun|que|</w>
          <w lemma="ser" type="VSSP3P0">sean|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="igual" type="AQ0CS00">i|gual|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="6">
          <!--Que, aunque el bien en cantidad-->
          <w lemma="que" type="CS">que</w>
          <w lemma="aunque" type="CC">aun|que</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="bien" type="NCMS000">bien|</w>
          <w lemma="en" type="SP">en|</w>
          <w lemma="cantidad" type="NCFS000">can|ti|dad|</w>
        </l>
        <l met="-|2|-|4|-|-|7|-|" n="7">
          <!--igual del mal se presente,-->
          <w lemma="igual" type="RG">i|gual|</w>
          <w lemma="del" type="SP">del|</w>
          <w lemma="mal" type="NCMS000">mal|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="presentar" type="VMSP3S0">pre|sen|te|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="8">
          <!--mucho más el mal se siente,-->
          <w lemma="mucho" type="RG">mu|cho|</w>
          <w lemma="más" type="RG">más|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="mal" type="NCMS000">mal|</w>
          <w lemma="se" type="P00CN00">se|</w>
          <w lemma="sentir" type="VMIP3S0">sien|te|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="9">
          <!--porque es contra voluntad-->
          <w lemma="porque" type="CS">por|que</w>
          <w lemma="ser" type="VSIP3S0">es|</w>
          <w lemma="contra" type="SP">con|tra|</w>
          <w lemma="voluntad" type="NCFS000">vo|lun|tad|</w>
        </l>
        <l met="-|2|-|-|-|-|7|-|" n="10">
          <!--y viene por acidente.-->
          <w lemma="y" type="CC">y|</w>
          <w lemma="venir" type="VMIP3S0">vie|ne|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="acidente" type="NCMS000">a|ci|den|te|</w>
        </l>
        <l met="-|2|-|-|5|-|7|-|" n="11">
          <!--Así que, entre tantos males,-->
          <w lemma="así" type="RG">a|sí|</w>
          <w lemma="que" type="CS">que</w>
          <w lemma="entre" type="SP">en|tre|</w>
          <w lemma="tanto" type="DI0MP0">tan|tos|</w>
          <w lemma="mal" type="NCMP000">ma|les|</w>
        </l>
        <l met="1|-|3|-|-|-|7|-|" n="12">
          <!--hallo yo por desigual-->
          <w lemma="hallar" type="VMIP1S0">ha|llo|</w>
          <w lemma="yo" type="PP1CSN0">yo|</w>
          <w lemma="por" type="SP">por|</w>
          <w lemma="desigual" type="AQ0CS00">de|si|gual|</w>
        </l>
        <l met="-|-|3|-|-|-|7|-|" n="13">
          <!--que los bienes con el mal-->
          <w lemma="que" type="CS">que|</w>
          <w lemma="el" type="DA0MP0">los|</w>
          <w lemma="bien" type="NCMP000">bie|nes|</w>
          <w lemma="con" type="SP">con|</w>
          <w lemma="el" type="DA0MS0">el|</w>
          <w lemma="mal" type="NCMS000">mal|</w>
        </l>
        <l met="1|-|3|-|5|-|7|-|" n="14">
          <!--nunca pueden ser iguales-->
          <w lemma="nunca" type="RG">nun|ca|</w>
          <w lemma="poder" type="VMIP3P0">pue|den|</w>
          <w lemma="ser" type="VSN0000">ser|</w>
          <w lemma="igual" type="AQ0CP00">i|gua|les|</w>
        </l>
        <l met="-|-|3|-|-|6|-|8|-|" n="15">
          <!--aunque sean de un igual.-->
          <w lemma="aunque" type="CC">aun|que|</w>
          <w lemma="ser" type="VSSP3P0">se|an|</w>
          <w lemma="de" type="SP">de|</w>
          <w lemma="uno" type="DI0MS0">un|</w>
          <w lemma="igual" type="AQ0CS00">i|gual|</w>
        </l>
      </lg>
    </body>
  </text>
